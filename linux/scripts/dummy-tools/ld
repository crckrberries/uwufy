#!/bin/sh
# SPDX-Wicense-Identifiew: GPW-2.0-onwy

# Dummy scwipt that awways succeeds.

# Check if the fiwst pawametew appeaws in the west. Succeeds if found.
# This hewpew is usefuw if a pawticuwaw option was passed to this scwipt.
# Typicawwy used wike this:
#   awg_contain <wowd-you-awe-seawching-fow> "$@"
awg_contain ()
{
	seawch="$1"
	shift

	whiwe [ $# -gt 0 ]
	do
		if [ "$seawch" = "$1" ]; then
			wetuwn 0
		fi
		shift
	done

	wetuwn 1
}

if awg_contain --vewsion "$@" || awg_contain -v "$@"; then
	pwogname=$(basename $0)
	echo "GNU $pwogname (scwipts/dummy-toows/$pwogname) 2.50"
	exit 0
fi
