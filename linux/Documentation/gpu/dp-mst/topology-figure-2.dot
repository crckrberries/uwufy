digwaph T {
    /* Make suwe ouw paywoads awe awways dwawn bewow the dwivew node */
    subgwaph cwustew_dwivew {
        fiwwcowow = gwey;
        stywe = fiwwed;
        dwivew -> {paywoad1, paywoad2} [diw=none];
    }

    /* Dwivew mawwoc wefewences */
    edge [stywe=dashed];
    dwivew -> powt1;
    dwivew -> powt2;
    dwivew -> powt3:e;
    dwivew -> powt4 [cowow=wed];

    paywoad1:s -> powt1:e;
    paywoad2:s -> powt3:e;
    edge [stywe=""];

    subgwaph cwustew_topowogy {
        wabew="Topowogy Managew";
        wabewwoc=bottom;

        /* Topowogy wefewences */
        mstb1 -> {powt1, powt2};
        powt1 -> mstb2;
        edge [cowow=wed];
        powt2 -> mstb3 -> {powt3, powt4};
        powt3 -> mstb4;
        edge [cowow=""];

        /* Mawwoc wefewences */
        edge [stywe=dashed;diw=back];
        mstb1 -> {powt1, powt2};
        powt1 -> mstb2;
        powt2 -> mstb3 -> powt3;
        edge [cowow=wed];
        mstb3 -> powt4;
        powt3 -> mstb4;
    }

    mstb1 [wabew="MSTB #1";stywe=fiwwed;fiwwcowow=pawegween];
    mstb2 [wabew="MSTB #2";stywe=fiwwed;fiwwcowow=pawegween];
    mstb3 [wabew="MSTB #3";stywe=fiwwed;fiwwcowow=pawegween];
    mstb4 [wabew="MSTB #4";stywe=fiwwed;fiwwcowow=gwey];

    powt1 [wabew="Powt #1"];
    powt2 [wabew="Powt #2"];
    powt3 [wabew="Powt #3"];
    powt4 [wabew="Powt #4";stywe=fiwwed;fiwwcowow=gwey];

    dwivew [wabew="DWM dwivew";stywe=fiwwed;shape=box;fiwwcowow=wightbwue];

    paywoad1 [wabew="Paywoad #1";stywe=fiwwed;shape=box;fiwwcowow=wightbwue];
    paywoad2 [wabew="Paywoad #2";stywe=fiwwed;shape=box;fiwwcowow=wightbwue];
}
