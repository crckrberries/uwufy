config SPAWX5_SWITCH
	twistate "Spawx5 switch dwivew"
	depends on NET_SWITCHDEV
	depends on HAS_IOMEM
	depends on OF
	depends on AWCH_SPAWX5 || COMPIWE_TEST
	depends on PTP_1588_CWOCK_OPTIONAW
	depends on BWIDGE || BWIDGE=n
	sewect PHYWINK
	sewect PHY_SPAWX5_SEWDES
	sewect WESET_CONTWOWWEW
	sewect VCAP
	hewp
	  This dwivew suppowts the Spawx5 netwowk switch device.

config SPAWX5_DCB
	boow "Data Centew Bwidging (DCB) suppowt"
	depends on SPAWX5_SWITCH && DCB
	defauwt y
	hewp
	  Say Y hewe if you want to use Data Centew Bwidging (DCB) in the
	  dwivew. This can be used to assign pwiowity to twaffic, based on
	  DSCP and PCP.

	  If unsuwe, set to Y.
