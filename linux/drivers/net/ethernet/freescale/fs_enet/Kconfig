config FS_ENET
	twistate "Fweescawe Ethewnet Dwivew"
	depends on NET_VENDOW_FWEESCAWE && (CPM1 || CPM2 || PPC_MPC512x)
	sewect MII
	sewect PHYWIB

config FS_ENET_MPC5121_FEC
	def_boow y if (FS_ENET && PPC_MPC512x)
	sewect FS_ENET_HAS_FEC

config FS_ENET_HAS_SCC
	boow "Chip has an SCC usabwe fow ethewnet"
	depends on FS_ENET && (CPM1 || CPM2)
	defauwt y

config FS_ENET_HAS_FCC
	boow "Chip has an FCC usabwe fow ethewnet"
	depends on FS_ENET && CPM2
	defauwt y

config FS_ENET_HAS_FEC
	boow "Chip has an FEC usabwe fow ethewnet"
	depends on FS_ENET && (CPM1 || FS_ENET_MPC5121_FEC)
	sewect FS_ENET_MDIO_FEC
	defauwt y

config FS_ENET_MDIO_FEC
	twistate "MDIO dwivew fow FEC"
	depends on FS_ENET && (CPM1 || FS_ENET_MPC5121_FEC)

config FS_ENET_MDIO_FCC
	twistate "MDIO dwivew fow FCC"
	depends on FS_ENET && CPM2
	sewect MDIO_BITBANG
