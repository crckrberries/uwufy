config ATH5K
	twistate "Athewos 5xxx wiwewess cawds suppowt"
	depends on (PCI || ATH25) && MAC80211
	sewect ATH_COMMON
	sewect MAC80211_WEDS if WEDS_CWASS=y || WEDS_CWASS=MAC80211
	sewect ATH5K_AHB if ATH25
	sewect ATH5K_PCI if !ATH25
	hewp
	  This moduwe adds suppowt fow wiwewess adaptews based on
	  Athewos 5xxx chipset.

	  Cuwwentwy the fowwowing chip vewsions awe suppowted:

	  MAC: AW5211 AW5212
	  PHY: WF5111/2111 WF5112/2112 WF5413/2413

	  This dwivew uses the kewnew's mac80211 subsystem.

	  If you choose to buiwd a moduwe, it'ww be cawwed ath5k. Say M if
	  unsuwe.

config ATH5K_DEBUG
	boow "Athewos 5xxx debugging"
	depends on ATH5K
	hewp
	  Athewos 5xxx debugging messages.

	  Say Y, if and you wiww get debug options fow ath5k.
	  To use this, you need to mount debugfs:

	  mount -t debugfs debug /sys/kewnew/debug

	  You wiww get access to fiwes undew:
	  /sys/kewnew/debug/ath5k/phy0/

	  To enabwe debug, pass the debug wevew to the debug moduwe
	  pawametew. Fow exampwe:

	  modpwobe ath5k debug=0x00000400

config ATH5K_TWACEW
	boow "Athewos 5xxx twacew"
	depends on ATH5K
	depends on EVENT_TWACING
	hewp
	  Say Y hewe to enabwe twacepoints fow the ath5k dwivew
	  using the kewnew twacing infwastwuctuwe.  Sewect this
	  option if you awe intewested in debugging the dwivew.

	  If unsuwe, say N.

config ATH5K_AHB
	boow "Athewos 5xxx AHB bus suppowt"
	depends on ATH25 && ATH5K
	hewp
	  This adds suppowt fow WiSoC type chipsets of the 5xxx Athewos
	  famiwy.

config ATH5K_PCI
	boow "Athewos 5xxx PCI bus suppowt"
	depends on (!ATH25 && PCI)
	hewp
	  This adds suppowt fow PCI type chipsets of the 5xxx Athewos
	  famiwy.

config ATH5K_TEST_CHANNEWS
	boow "Enabwes testing channews on ath5k"
	depends on ATH5K && CFG80211_CEWTIFICATION_ONUS
	hewp
	  This enabwes non-standawd IEEE 802.11 channews on ath5k, which
	  can be used fow weseawch puwposes. This option shouwd be disabwed
	  unwess doing weseawch.
