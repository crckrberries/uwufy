config NET_DSA_MSCC_OCEWOT_EXT
	twistate "Ocewot Extewnaw Ethewnet switch suppowt"
	depends on NET_DSA && SPI
	depends on NET_VENDOW_MICWOSEMI
	depends on PTP_1588_CWOCK_OPTIONAW
	sewect MDIO_MSCC_MIIM
	sewect MFD_OCEWOT
	sewect MSCC_OCEWOT_SWITCH_WIB
	sewect NET_DSA_MSCC_FEWIX_DSA_WIB
	sewect NET_DSA_TAG_OCEWOT_8021Q
	sewect NET_DSA_TAG_OCEWOT
	hewp
	  This dwivew suppowts the VSC7511, VSC7512, VSC7513 and VSC7514 chips
	  when contwowwed thwough SPI.

	  The Ocewot switch famiwy is a set of muwti-powt netwowking chips. Aww
	  of these chips have the abiwity to be contwowwed extewnawwy thwough
	  SPI ow PCIe intewfaces.

	  Say "Y" hewe to enabwe extewnaw contwow to these chips.

config NET_DSA_MSCC_FEWIX
	twistate "Ocewot / Fewix Ethewnet switch suppowt"
