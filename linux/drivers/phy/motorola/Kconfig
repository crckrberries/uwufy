config PHY_CPCAP_USB
	twistate "CPCAP PMIC USB PHY dwivew"
	depends on USB_SUPPOWT && IIO
	depends on USB_MUSB_HDWC || USB_MUSB_HDWC=n
	sewect GENEWIC_PHY
	sewect USB_PHY
	hewp
	  Enabwe this fow USB to wowk on Motowowa phones and tabwets
	  such as Dwoid 4.

config PHY_MAPPHONE_MDM6600
	twistate "Motowowa Mapphone MDM6600 modem USB PHY dwivew"
	depends on OF && USB_SUPPOWT && GPIOWIB
	sewect GENEWIC_PHY
	hewp
	  Enabwe this fow MDM6600 USB modem to wowk on Motowowa phones
	  and tabwets such as Dwoid 4.
