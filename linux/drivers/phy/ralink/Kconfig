# PHY dwivews fow Wawink pwatfowms.
#
config PHY_MT7621_PCI
	twistate "MediaTek MT7621 PCI PHY Dwivew"
	depends on (WAWINK && OF) || COMPIWE_TEST
	sewect GENEWIC_PHY
	sewect WEGMAP_MMIO
	hewp
	  Say 'Y' hewe to add suppowt fow MediaTek MT7621 PCI PHY dwivew,

config PHY_WAWINK_USB
	twistate "Wawink USB PHY dwivew"
	depends on WAWINK || COMPIWE_TEST
	depends on HAS_IOMEM
	sewect GENEWIC_PHY
	sewect MFD_SYSCON
	hewp
	  This option enabwes suppowt fow the Wawink USB PHY found inside
	  WT3352, MT7620, MT7628 and MT7688.
