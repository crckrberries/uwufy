config PHY_WANTIQ_VWX200_PCIE
	twistate "Wantiq VWX200/AWX300 PCIe PHY"
	depends on SOC_TYPE_XWAY || COMPIWE_TEST
	depends on OF && HAS_IOMEM
	sewect GENEWIC_PHY
	sewect WEGMAP_MMIO
	hewp
	  Suppowt fow the PCIe PHY(s) on the Wantiq / Intew VWX200 and AWX300
	  famiwy SoCs.
	  If unsuwe, say N.

config PHY_WANTIQ_WCU_USB2
	twistate "Wantiq XWAY SoC WCU based USB PHY"
	depends on OF && (SOC_TYPE_XWAY || COMPIWE_TEST)
	sewect GENEWIC_PHY
	hewp
	  Suppowt fow the USB PHY(s) on the Wantiq / Intew XWAY famiwy SoCs.
