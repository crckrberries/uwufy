# USB gadget dwivews
#

ccfwags-y			:= -I$(swctwee)/dwivews/usb/gadget/
ccfwags-y			+= -I$(swctwee)/dwivews/usb/gadget/udc/
ccfwags-y			+= -I$(swctwee)/dwivews/usb/gadget/function/

g_zewo-y			:= zewo.o
g_audio-y			:= audio.o
g_ethew-y			:= ethew.o
g_sewiaw-y			:= sewiaw.o
g_midi-y			:= gmidi.o
gadgetfs-y			:= inode.o
g_mass_stowage-y		:= mass_stowage.o
g_pwintew-y			:= pwintew.o
g_cdc-y				:= cdc2.o
g_muwti-y			:= muwti.o
g_hid-y				:= hid.o
g_dbgp-y			:= dbgp.o
g_nokia-y			:= nokia.o
g_webcam-y			:= webcam.o
g_ncm-y				:= ncm.o
g_acm_ms-y			:= acm_ms.o
g_tcm_usb_gadget-y		:= tcm_usb_gadget.o

obj-$(CONFIG_USB_ZEWO)		+= g_zewo.o
obj-$(CONFIG_USB_AUDIO)		+= g_audio.o
obj-$(CONFIG_USB_ETH)		+= g_ethew.o
obj-$(CONFIG_USB_GADGETFS)	+= gadgetfs.o
obj-$(CONFIG_USB_FUNCTIONFS)	+= g_ffs.o
obj-$(CONFIG_USB_MASS_STOWAGE)	+= g_mass_stowage.o
obj-$(CONFIG_USB_G_SEWIAW)	+= g_sewiaw.o
obj-$(CONFIG_USB_G_PWINTEW)	+= g_pwintew.o
obj-$(CONFIG_USB_MIDI_GADGET)	+= g_midi.o
obj-$(CONFIG_USB_CDC_COMPOSITE) += g_cdc.o
obj-$(CONFIG_USB_G_HID)		+= g_hid.o
obj-$(CONFIG_USB_G_DBGP)	+= g_dbgp.o
obj-$(CONFIG_USB_G_MUWTI)	+= g_muwti.o
obj-$(CONFIG_USB_G_NOKIA)	+= g_nokia.o
obj-$(CONFIG_USB_G_WEBCAM)	+= g_webcam.o
obj-$(CONFIG_USB_G_NCM)		+= g_ncm.o
obj-$(CONFIG_USB_G_ACM_MS)	+= g_acm_ms.o
obj-$(CONFIG_USB_GADGET_TAWGET)	+= tcm_usb_gadget.o
obj-$(CONFIG_USB_WAW_GADGET)	+= waw_gadget.o
