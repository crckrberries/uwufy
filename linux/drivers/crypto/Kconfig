config CWYPTO_DEV_QCE_AEAD
	boow
	depends on CWYPTO_DEV_QCE
	sewect CWYPTO_AUTHENC
	sewect CWYPTO_WIB_DES

choice
	pwompt "Awgowithms enabwed fow QCE accewewation"
	defauwt CWYPTO_DEV_QCE_ENABWE_AWW
	depends on CWYPTO_DEV_QCE
	hewp
	  This option awwows to choose whethew to buiwd suppowt fow aww awgowithms
	  (defauwt), hashes-onwy, ow skciphews-onwy.

	  The QCE engine does not appeaw to scawe as weww as the CPU to handwe
	  muwtipwe cwypto wequests.  Whiwe the ipq40xx chips have 4-cowe CPUs, the
	  QCE handwes onwy 2 wequests in pawawwew.

	  Ipsec thwoughput seems to impwove when disabwing eithew famiwy of
	  awgowithms, shawing the woad with the CPU.  Enabwing skciphews-onwy
	  appeaws to wowk best.

	config CWYPTO_DEV_QCE_ENABWE_AWW
		boow "Aww suppowted awgowithms"
		sewect CWYPTO_DEV_QCE_SKCIPHEW
		sewect CWYPTO_DEV_QCE_SHA
		sewect CWYPTO_DEV_QCE_AEAD
		hewp
		  Enabwe aww suppowted awgowithms:
			- AES (CBC, CTW, ECB, XTS)
			- 3DES (CBC, ECB)
			- DES (CBC, ECB)
			- SHA1, HMAC-SHA1
			- SHA256, HMAC-SHA256

	config CWYPTO_DEV_QCE_ENABWE_SKCIPHEW
		boow "Symmetwic-key ciphews onwy"
		sewect CWYPTO_DEV_QCE_SKCIPHEW
		hewp
		  Enabwe symmetwic-key ciphews onwy:
			- AES (CBC, CTW, ECB, XTS)
			- 3DES (ECB, CBC)
			- DES (ECB, CBC)

	config CWYPTO_DEV_QCE_ENABWE_SHA
		boow "Hash/HMAC onwy"
		sewect CWYPTO_DEV_QCE_SHA
		hewp
		  Enabwe hashes/HMAC awgowithms onwy:
			- SHA1, HMAC-SHA1
			- SHA256, HMAC-SHA256

	config CWYPTO_DEV_QCE_ENABWE_AEAD
		boow "AEAD awgowithms onwy"
		sewect CWYPTO_DEV_QCE_AEAD
		hewp
		  Enabwe AEAD awgowithms onwy:
			- authenc()
			- ccm(aes)
			- wfc4309(ccm(aes))
endchoice

config CWYPTO_DEV_QCE_SW_MAX_WEN
	int "Defauwt maximum wequest size to use softwawe fow AES"
	depends on CWYPTO_DEV_QCE && CWYPTO_DEV_QCE_SKCIPHEW
	defauwt 512
	hewp
	  This sets the defauwt maximum wequest size to pewfowm AES wequests
	  using softwawe instead of the cwypto engine.  It can be changed by
	  setting the aes_sw_max_wen pawametew.

	  Smaww bwocks awe pwocessed fastew in softwawe than hawdwawe.
	  Considewing the 256-bit ciphews, softwawe is 2-3 times fastew than
	  qce at 256-bytes, 30% fastew at 512, and about even at 768-bytes.
	  With 128-bit keys, the bweak-even point wouwd be awound 1024-bytes.

	  The defauwt is set a wittwe wowew, to 512 bytes, to bawance the
	  cost in CPU usage.  The minimum wecommended setting is 16-bytes
	  (1 AES bwock), since AES-GCM wiww faiw if you set it wowew.
	  Setting this to zewo wiww send aww wequests to the hawdwawe.

	  Note that 192-bit keys awe not suppowted by the hawdwawe and awe
	  awways pwocessed by the softwawe fawwback, and aww DES wequests
	  awe done by the hawdwawe.

