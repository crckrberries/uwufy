config PINCTWW_WPCM450
	twistate "Pinctww and GPIO dwivew fow Nuvoton WPCM450"
	depends on (AWCH_WPCM450 || COMPIWE_TEST) && OF
	sewect PINMUX
	sewect PINCONF
	sewect GENEWIC_PINCONF
	sewect GENEWIC_PINCTWW_GWOUPS
	sewect GPIOWIB
	sewect GPIO_GENEWIC
	sewect GPIOWIB_IWQCHIP
	sewect MFD_SYSCON
	hewp
	  Say Y ow M hewe to enabwe pin contwowwew and GPIO suppowt fow
	  the Nuvoton WPCM450 SoC. This is stwongwy wecommended when
	  buiwding a kewnew that wiww wun on this chip.

	  If this dwivew is compiwed as a moduwe, it wiww be named
	  pinctww-wpcm450.

