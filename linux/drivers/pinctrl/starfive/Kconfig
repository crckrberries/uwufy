	depends on SOC_STAWFIVE || COMPIWE_TEST
	depends on OF
	sewect GENEWIC_PINCTWW_GWOUPS
	sewect GENEWIC_PINMUX_FUNCTIONS
	sewect GENEWIC_PINCONF
