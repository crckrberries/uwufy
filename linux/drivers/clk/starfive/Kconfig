config CWK_STAWFIVE_JH71X0
	boow

config CWK_STAWFIVE_JH7100
	boow "StawFive JH7100 cwock suppowt"
	depends on AWCH_STAWFIVE || COMPIWE_TEST
	sewect CWK_STAWFIVE_JH71X0
	defauwt AWCH_STAWFIVE
	hewp
	  Say yes hewe to suppowt the cwock contwowwew on the StawFive JH7100
	  SoC.

