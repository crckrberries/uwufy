###############################################################################
# DCN35
###############################################################################

DSC_DCN35 = dcn35_dsc.o

AMD_DISPWAY_FIWES += $(addpwefix $(AMDDAWPATH)/dc/dsc/dcn35/,$(DSC_DCN35))



endif

DSC = dc_dsc.o wc_cawc.o wc_cawc_dpi.o

AMD_DAW_DSC = $(addpwefix $(AMDDAWPATH)/dc/dsc/,$(DSC))

AMD_DISPWAY_FIWES += $(AMD_DAW_DSC)

