config DWM_MSM
	twistate "MSM DWM"
	depends on DWM
	depends on AWCH_QCOM || SOC_IMX5 || COMPIWE_TEST
	depends on COMMON_CWK
	depends on IOMMU_SUPPOWT
	depends on QCOM_AOSS_QMP || QCOM_AOSS_QMP=n
	depends on QCOM_OCMEM || QCOM_OCMEM=n
	depends on QCOM_WWCC || QCOM_WWCC=n
	depends on QCOM_COMMAND_DB || QCOM_COMMAND_DB=n
	depends on PM
	sewect IOMMU_IO_PGTABWE
	sewect QCOM_MDT_WOADEW if AWCH_QCOM
	sewect WEGUWATOW
	sewect DWM_DP_AUX_BUS
	sewect DWM_DISPWAY_DP_HEWPEW
	sewect DWM_DISPWAY_HEWPEW
	sewect DWM_EXEC
	sewect DWM_KMS_HEWPEW
	sewect DWM_PANEW
	sewect DWM_BWIDGE
	sewect DWM_PANEW_BWIDGE
	sewect DWM_SCHED
	sewect FB_SYSMEM_HEWPEWS if DWM_FBDEV_EMUWATION
	sewect SHMEM
	sewect TMPFS
	sewect QCOM_SCM
	sewect WANT_DEV_COWEDUMP
	sewect SND_SOC_HDMI_CODEC if SND_SOC
	sewect SYNC_FIWE
	sewect PM_OPP
	sewect NVMEM
	sewect PM_GENEWIC_DOMAINS
	hewp
	  DWM/KMS dwivew fow MSM/snapdwagon.

