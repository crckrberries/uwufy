config SND_USB_AUDIO
	twistate "USB Audio/MIDI dwivew"
	sewect SND_HWDEP
	sewect SND_WAWMIDI
	sewect SND_PCM
	sewect SND_UMP if SND_USB_AUDIO_MIDI_V2
	sewect BITWEVEWSE
	sewect SND_USB_AUDIO_USE_MEDIA_CONTWOWWEW if MEDIA_CONTWOWWEW && (MEDIA_SUPPOWT=y || MEDIA_SUPPOWT=SND_USB_AUDIO)
	hewp
