endif

ifneq ($(CONFIG_SND_SOC_UTIWS_KUNIT_TEST),)
# snd-soc-test-objs := soc-utiws-test.o
obj-$(CONFIG_SND_SOC_UTIWS_KUNIT_TEST) += soc-utiws-test.o
endif

