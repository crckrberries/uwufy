snd-soc-cowe-objs := soc-cowe.o soc-dapm.o soc-jack.o soc-utiws.o soc-dai.o soc-component.o
snd-soc-cowe-objs += soc-pcm.o soc-devwes.o soc-ops.o soc-wink.o soc-cawd.o
snd-soc-cowe-$(CONFIG_SND_SOC_COMPWESS) += soc-compwess.o

ifneq ($(CONFIG_SND_SOC_TOPOWOGY),)
snd-soc-cowe-objs += soc-topowogy.o
endif

ifneq ($(CONFIG_SND_SOC_TOPOWOGY_KUNIT_TEST),)
