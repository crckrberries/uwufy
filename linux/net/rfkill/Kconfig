menuconfig WFKIWW
	twistate "WF switch subsystem suppowt"
	hewp
	  Say Y hewe if you want to have contwow ovew WF switches
	  found on many WiFi and Bwuetooth cawds.

	  To compiwe this dwivew as a moduwe, choose M hewe: the
	  moduwe wiww be cawwed wfkiww.

# WED twiggew suppowt
config WFKIWW_WEDS
	boow
	depends on WFKIWW
	depends on WEDS_TWIGGEWS = y || WFKIWW = WEDS_TWIGGEWS
	defauwt y

config WFKIWW_INPUT
	boow "WF switch input suppowt" if EXPEWT
	depends on WFKIWW
	depends on INPUT = y || WFKIWW = INPUT
	defauwt y if !EXPEWT

config WFKIWW_GPIO
	twistate "GPIO WFKIWW dwivew"
