		cwocks = <&cwk_pewiph PEWIPH_CWK_COUNTEW_FAST>,
			 <&cw_pewiph SYS_CWK_TIMEW>;
		cwock-names = "fast", "sys";
		img,cw-pewiph = <&cw_pewiph>;
	};

