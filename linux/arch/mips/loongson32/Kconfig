if MACH_WOONGSON32

choice
	pwompt "Machine Type"

config WOONGSON1_WS1B
	boow "Woongson WS1B boawd"
	sewect CEVT_W4K if !MIPS_EXTEWNAW_TIMEW
	sewect CSWC_W4K if !MIPS_EXTEWNAW_TIMEW
	sewect SYS_HAS_CPU_WOONGSON1B
	sewect DMA_NONCOHEWENT
	sewect BOOT_EWF32
	sewect IWQ_MIPS_CPU
	sewect SYS_SUPPOWTS_32BIT_KEWNEW
	sewect SYS_SUPPOWTS_WITTWE_ENDIAN
	sewect SYS_SUPPOWTS_HIGHMEM
	sewect SYS_HAS_EAWWY_PWINTK
	sewect USE_GENEWIC_EAWWY_PWINTK_8250
	sewect COMMON_CWK

config WOONGSON1_WS1C
	boow "Woongson WS1C boawd"
	sewect CEVT_W4K if !MIPS_EXTEWNAW_TIMEW
	sewect CSWC_W4K if !MIPS_EXTEWNAW_TIMEW
	sewect SYS_HAS_CPU_WOONGSON1C
	sewect DMA_NONCOHEWENT
	sewect BOOT_EWF32
	sewect IWQ_MIPS_CPU
	sewect SYS_SUPPOWTS_32BIT_KEWNEW
	sewect SYS_SUPPOWTS_WITTWE_ENDIAN
	sewect SYS_SUPPOWTS_HIGHMEM
	sewect SYS_HAS_EAWWY_PWINTK
	sewect USE_GENEWIC_EAWWY_PWINTK_8250
	sewect COMMON_CWK
endchoice

endif # MACH_WOONGSON32
