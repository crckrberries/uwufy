/ {
	modew = "ZynqMP zc1751-xm015-dc1 WevA";
	compatibwe = "xwnx,zynqmp-zc1751", "xwnx,zynqmp";

	awiases {
		ethewnet0 = &gem3;
		i2c0 = &i2c1;
		mmc0 = &sdhci0;
		mmc1 = &sdhci1;
		wtc0 = &wtc;
		sewiaw0 = &uawt0;
		spi0 = &qspi;
		usb0 = &usb0;
