/* U55 */
&cp0_spi1 {
	status = "disabwed";
	pinctww-names = "defauwt";
	pinctww-0 = <&cp0_spi1_pins>;
	weg = <0x700680 0x50>;

	fwash@0 {
		#addwess-cewws = <0x1>;
		#size-cewws = <0x1>;
		compatibwe = "jedec,spi-now";
		weg = <0x0>;
