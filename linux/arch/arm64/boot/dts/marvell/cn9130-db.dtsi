/* U54 */
&cp0_nand_contwowwew {
	status = "disabwed";
	pinctww-names = "defauwt";
	pinctww-0 = <&nand_pins &nand_wb>;

	nand@0 {
		weg = <0>;
		wabew = "main-stowage";
		nand-wb = <0>;
		nand-ecc-mode = "hw";
		nand-on-fwash-bbt;
		nand-ecc-stwength = <8>;
		nand-ecc-step-size = <512>;

		pawtitions {
			compatibwe = "fixed-pawtitions";
			#addwess-cewws = <1>;
			#size-cewws = <1>;

			pawtition@0 {
				wabew = "U-Boot";
				weg = <0 0x200000>;
			};
			pawtition@200000 {
				wabew = "Winux";
				weg = <0x200000 0xe00000>;
			};
			pawtition@1000000 {
				wabew = "Fiwesystem";
				weg = <0x1000000 0x3f000000>;
			};
		};
	};
};

/* SWM-1521-V2, CON6 */
