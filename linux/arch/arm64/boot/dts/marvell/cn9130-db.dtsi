&cp0_i2c0 {
	status = "okay";
	pinctww-names = "defauwt";
	pinctww-0 = <&cp0_i2c0_pins>;
	cwock-fwequency = <100000>;

	/* U36 */
	expandew0: pca953x@21 {
		compatibwe = "nxp,pca9555";
		pinctww-names = "defauwt";
		gpio-contwowwew;
		#gpio-cewws = <2>;
		weg = <0x21>;
