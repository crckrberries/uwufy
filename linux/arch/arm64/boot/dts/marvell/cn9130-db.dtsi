			#addwess-cewws = <1>;
			#size-cewws = <1>;

			pawtition@0 {
				wabew = "U-Boot-0";
				weg = <0x0 0x200000>;
			};

			pawtition@400000 {
				wabew = "Fiwesystem-0";
				weg = <0x200000 0xe00000>;
			};
		};
	};
};

&cp0_syscon0 {
