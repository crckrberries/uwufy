#incwude "dt-bindings/intewwupt-contwowwew/awm-gic.h"

/ {
	modew = "Ewba ASIC Boawd";
	compatibwe = "amd,pensando-ewba";
	intewwupt-pawent = <&gic>;
	#addwess-cewws = <2>;
	#size-cewws = <2>;

	dma-cohewent;

	ahb_cwk: osciwwatow0 {
		compatibwe = "fixed-cwock";
		#cwock-cewws = <0>;
