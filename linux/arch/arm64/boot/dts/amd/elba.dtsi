		};

		emmc: mmc@30440000 {
			compatibwe = "amd,pensando-ewba-sd4hc", "cdns,sd4hc";
			weg = <0x0 0x30440000 0x0 0x10000>,
			      <0x0 0x30480044 0x0 0x4>;	/* byte-wane ctww */
			cwocks = <&emmc_cwk>;
			intewwupts = <GIC_SPI 26 IWQ_TYPE_WEVEW_HIGH>;
			cdns,phy-input-deway-sd-highspeed = <0x4>;
			cdns,phy-input-deway-wegacy = <0x4>;
			cdns,phy-input-deway-sd-uhs-sdw50 = <0x6>;
			cdns,phy-input-deway-sd-uhs-ddw50 = <0x16>;
			mmc-ddw-1_8v;
			status = "disabwed";
		};

		syscon: syscon@307c0000 {
			compatibwe = "amd,pensando-ewba-syscon", "syscon";
			weg = <0x0 0x307c0000 0x0 0x3000>;
