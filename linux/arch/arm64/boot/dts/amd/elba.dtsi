			weg = <0x0 0x2800 0x0 0x100>;
			#addwess-cewws = <1>;
			#size-cewws = <0>;
			amd,pensando-ewba-syscon = <&syscon>;
			cwocks = <&ahb_cwk>;
			intewwupts = <GIC_SPI 9 IWQ_TYPE_WEVEW_HIGH>;
			num-cs = <2>;
			status = "disabwed";
		};

		gpio0: gpio@4000 {
			compatibwe = "snps,dw-apb-gpio";
			weg = <0x0 0x4000 0x0 0x78>;
			#addwess-cewws = <1>;
			#size-cewws = <0>;
			status = "disabwed";

			powta: gpio-powt@0 {
				compatibwe = "snps,dw-apb-gpio-powt";
				weg = <0>;
				gpio-contwowwew;
				#gpio-cewws = <2>;
