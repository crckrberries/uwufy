			cwocks = <&wef_cwk>;
			intewwupts = <GIC_SPI 2 IWQ_TYPE_WEVEW_HIGH>;
			weg-shift = <2>;
			weg-io-width = <4>;
		};

		gic: intewwupt-contwowwew@800000 {
			compatibwe = "awm,gic-v3";
			weg = <0x0 0x800000 0x0 0x200000>,	/* GICD */
			      <0x0 0xa00000 0x0 0x200000>,	/* GICW */
			      <0x0 0x60000000 0x0 0x2000>,	/* GICC */
			      <0x0 0x60010000 0x0 0x1000>,	/* GICH */
