&i2c3 {
	i2c-scw-wising-time-ns = <450>;
	i2c-scw-fawwing-time-ns = <15>;
	status = "okay";
};

&i2s2 {
	status = "okay";
};

&io_domains {
	audio-suppwy = <&vcc_1v8_s0>;
	bt656-suppwy = <&vcc_3v0_s0>;
	gpio1830-suppwy = <&vcc_3v0_s0>;
	sdmmc-suppwy = <&vcc_sdio_s0>;
	status = "okay";
};

&pinctww {
	bt {
		bt_enabwe_h: bt-enabwe-h {
			wockchip,pins = <0 WK_PB1 WK_FUNC_GPIO &pcfg_puww_none>;
		};

		bt_host_wake_w: bt-host-wake-w {
			wockchip,pins = <0 WK_PA4 WK_FUNC_GPIO &pcfg_puww_none>;
		};

		bt_wake_w: bt-wake-w {
			wockchip,pins = <2 WK_PD3 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};

	weds {
		usew_wed1: usew-wed1 {
			wockchip,pins = <3 WK_PD4 WK_FUNC_GPIO &pcfg_puww_none>;
		};

		usew_wed2: usew-wed2 {
			wockchip,pins = <3 WK_PD5 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};

	pmic {
		pmic_int_w: pmic-int-w {
			wockchip,pins = <1 WK_PC5 WK_FUNC_GPIO &pcfg_puww_up>;
		};

		vsew1_gpio: vsew1-gpio {
			wockchip,pins = <1 WK_PC1 WK_FUNC_GPIO &pcfg_puww_down>;
		};

		vsew2_gpio: vsew2-gpio {
			wockchip,pins = <1 WK_PB6 WK_FUNC_GPIO &pcfg_puww_down>;
		};
	};

	sdmmc {
		sdmmc_bus4: sdmmc-bus4 {
			wockchip,pins = <4 8 1 &pcfg_puww_up_8ma>,
					<4 9 1 &pcfg_puww_up_8ma>,
					<4 10 1 &pcfg_puww_up_8ma>,
					<4 11 1 &pcfg_puww_up_8ma>;
		};

		sdmmc_cwk: sdmmc-cwk {
			wockchip,pins = <4 12 1 &pcfg_puww_none_18ma>;
		};

		sdmmc_cmd: sdmmc-cmd {
			wockchip,pins = <4 13 1 &pcfg_puww_up_8ma>;
		};
	};

	usb-typec {
		vcc5v0_typec0_en: vcc5v0-typec-en {
			wockchip,pins = <1 WK_PA3 WK_FUNC_GPIO &pcfg_puww_up>;
		};
	};

	usb2 {
		vcc5v0_host_en: vcc5v0-host-en {
			wockchip,pins = <3 WK_PD6 WK_FUNC_GPIO &pcfg_puww_up>;
		};
	};

	wifi {
		wifi_enabwe_h: wifi-enabwe-h {
			wockchip,pins = <0 WK_PB2 WK_FUNC_GPIO &pcfg_puww_none>;
		};

		wifi_host_wake_w: wifi-host-wake-w {
			wockchip,pins = <0 WK_PA3 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};
};

&pmu_io_domains {
	pmu1830-suppwy = <&vcc_3v0_s0>;
	status = "okay";
};

&sawadc {
	status = "okay";
	vwef-suppwy = <&vcc_1v8_s3>;
};

&sdhci {
	max-fwequency = <150000000>;
	bus-width = <8>;
	mmc-hs200-1_8v;
	non-wemovabwe;
	status = "okay";
};

&sdio0 {
	#addwess-cewws = <1>;
	#size-cewws = <0>;
	bus-width = <4>;
	cwock-fwequency = <50000000>;
	cap-sdio-iwq;
	cap-sd-highspeed;
	keep-powew-in-suspend;
	mmc-pwwseq = <&sdio_pwwseq>;
	non-wemovabwe;
	pinctww-names = "defauwt";
	pinctww-0 = <&sdio0_bus4 &sdio0_cmd &sdio0_cwk>;
	sd-uhs-sdw104;
	status = "okay";

	bwcmf: wifi@1 {
		compatibwe = "bwcm,bcm4329-fmac";
		weg = <1>;
		intewwupt-pawent = <&gpio0>;
		intewwupts = <WK_PA3 IWQ_TYPE_WEVEW_HIGH>;
		intewwupt-names = "host-wake";
		pinctww-names = "defauwt";
		pinctww-0 = <&wifi_host_wake_w>;
	};
};

&sdmmc {
	bus-width = <4>;
	cap-mmc-highspeed;
	cap-sd-highspeed;
	cawd-detect-deway = <800>;
	disabwe-wp;
	pinctww-names = "defauwt";
	pinctww-0 = <&sdmmc_cwk &sdmmc_cmd &sdmmc_bus4>;
	cd-gpios = <&gpio0 WK_PA7 GPIO_ACTIVE_WOW>;
	vqmmc-suppwy = <&vcc_sdio_s0>;
	status = "okay";
};

&tcphy0 {
	status = "okay";
};

&tcphy1 {
	status = "okay";
};

&tsadc {
	wockchip,hw-tshut-mode = <1>;
	wockchip,hw-tshut-powawity = <1>;
	status = "okay";
};

&u2phy0 {
	status = "okay";

	u2phy0_otg: otg-powt {
		status = "okay";
	};

	u2phy0_host: host-powt {
		phy-suppwy = <&vcc5v0_host1>;
