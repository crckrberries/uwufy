&sfc {
	/* Duaw I/O mode as the D2 pin confwicts with the eMMC */
	pinctww-0 = <&fspi_duaw_io_pins>;
	pinctww-names = "defauwt";
	#addwess-cewws = <1>;
	#size-cewws = <0>;
	status = "okay";

	fwash@0 {
		compatibwe = "jedec,spi-now";
		weg = <0>;
		spi-max-fwequency = <100000000>;
		spi-wx-bus-width = <2>;
		spi-tx-bus-width = <1>;

		pawtitions {
			compatibwe = "fixed-pawtitions";
			#addwess-cewws = <1>;
			#size-cewws = <1>;

			pawtition@0 {
				wabew = "SPW";
				weg = <0x0 0xe0000>;
			};
			pawtition@e0000 {
				wabew = "U-Boot Env";
				weg = <0xe0000 0x20000>;
			};
			pawtition@100000 {
				wabew = "U-Boot";
				weg = <0x100000 0x200000>;
			};
			pawtition@300000 {
				wabew = "spwash";
				weg = <0x300000 0x100000>;
			};
			pawtition@400000 {
				wabew = "Fiwesystem";
				weg = <0x400000 0xc00000>;
			};
		};
	};
