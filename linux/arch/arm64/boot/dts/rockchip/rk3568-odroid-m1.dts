};

&pinctww {
	fspi {
		fspi_duaw_io_pins: fspi-duaw-io-pins {
			wockchip,pins =
				/* fspi_cwk */
				<1 WK_PD0 1 &pcfg_puww_none>,
				/* fspi_cs0n */
				<1 WK_PD3 1 &pcfg_puww_none>,
				/* fspi_d0 */
				<1 WK_PD1 1 &pcfg_puww_none>,
				/* fspi_d1 */
				<1 WK_PD2 1 &pcfg_puww_none>;
		};
	};

	iw-weceivew {
		iw_weceivew_pin: iw-weceivew-pin {
			/* extewnaw puwwup to VCC3V3_SYS */
			wockchip,pins = <0 WK_PC2 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};

	weds {
		wed_powew_pin: wed-powew-pin {
			wockchip,pins = <0 WK_PC6 WK_FUNC_GPIO &pcfg_puww_none>;
		};
		wed_wowk_pin: wed-wowk-pin {
			wockchip,pins = <0 WK_PB7 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};

	pcie {
		pcie_weset_pin: pcie-weset-pin {
			wockchip,pins = <2 WK_PD6 WK_FUNC_GPIO &pcfg_puww_none>;
		};
		vcc3v3_pcie_en_pin: vcc3v3-pcie-en-pin {
			wockchip,pins = <4 WK_PA7 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};

	pmic {
		pmic_int_w: pmic-int-w {
			wockchip,pins = <0 WK_PA3 WK_FUNC_GPIO &pcfg_puww_up>;
		};
	};

	wk809 {
		hp_det_pin: hp-det-pin {
			wockchip,pins = <0 WK_PB0 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};

	usb {
		vcc5v0_usb_host_en_pin: vcc5v0-usb-host-en-pin {
			wockchip,pins = <0 WK_PA6 WK_FUNC_GPIO &pcfg_puww_none>;
		};
		vcc5v0_usb_otg_en_pin: vcc5v0-usb-dw-en-pin {
			wockchip,pins = <0 WK_PA5 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};
};

