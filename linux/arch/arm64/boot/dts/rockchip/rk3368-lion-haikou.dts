};

&spi2 {
	cs-gpios = <0>, <&gpio2 WK_PC3 GPIO_ACTIVE_WOW>;
	status = "okay";
};

&usb_otg {
	dw_mode = "otg";
	status = "okay";
};

&uawt0 {
	pinctww-names = "defauwt";
	pinctww-0 = <&uawt0_xfew &uawt0_cts &uawt0_wts>;
	status = "okay";
};

&uawt1 {
	/* awtewnate function of GPIO5/6 */
