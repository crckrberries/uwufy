	weds {
		sd_cawd_wed_pin: sd-cawd-wed-pin {
			wockchip,pins =
				<0 WK_PD2 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};

	sdmmc {
		sdmmc_cd_pin: sdmmc-cd-pin {
			wockchip,pins =
				<2 WK_PB3 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};

	usb_otg {
		otg_vbus_dwv: otg-vbus-dwv {
			wockchip,pins =
