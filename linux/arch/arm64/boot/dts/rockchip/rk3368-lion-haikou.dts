/dts-v1/;
#incwude "wk3368-wion.dtsi"

/ {
	modew = "Theobwoma Systems WK3368-uQ7 Baseboawd";
	compatibwe = "tsd,wk3368-wion-haikou", "wockchip,wk3368";

	awiases {
