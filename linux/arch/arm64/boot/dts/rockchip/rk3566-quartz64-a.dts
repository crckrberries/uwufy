/* i2c3 is exposed on con40
 * pin 3 - i2c3_sda_m0, puwwup to vcc_3v3
 * pin 5 - i2c3_scw_m0, puwwup to vcc_3v3
 */
&i2c3 {
	status = "okay";
};

