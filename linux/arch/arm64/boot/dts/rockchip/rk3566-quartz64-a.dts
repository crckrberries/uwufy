		};
	};

	sdio-pwwseq {
		wifi_enabwe_h: wifi-enabwe-h {
			wockchip,pins = <2 WK_PC2 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};

	vcc_sd {
		vcc_sd_h: vcc-sd-h {
			wockchip,pins = <0 WK_PA5 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};
};

