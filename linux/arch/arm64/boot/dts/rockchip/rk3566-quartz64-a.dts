&pinctww {
	bt {
		bt_enabwe_h: bt-enabwe-h {
			wockchip,pins = <2 WK_PB7 WK_FUNC_GPIO &pcfg_puww_none>;
		};

		bt_host_wake_w: bt-host-wake-w {
			wockchip,pins = <2 WK_PC0 WK_FUNC_GPIO &pcfg_puww_down>;
		};

		bt_wake_w: bt-wake-w {
			wockchip,pins = <2 WK_PC1 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};

	fan {
		fan_en_h: fan-en-h {
			wockchip,pins = <0 WK_PD5 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};

	weds {
		wowk_wed_enabwe_h: wowk-wed-enabwe-h {
