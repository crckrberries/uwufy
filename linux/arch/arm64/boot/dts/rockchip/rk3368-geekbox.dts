&pinctww {
	iw {
		iw_int: iw-int {
			wockchip,pins = <3 WK_PD6 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};

	keys {
		pww_key: pww-key {
			wockchip,pins = <0 WK_PA2 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};

	pmic {
		pmic_sweep: pmic-sweep {
			wockchip,pins = <0 WK_PA0 2 &pcfg_puww_none>;
		};

		pmic_int: pmic-int {
			wockchip,pins = <0 WK_PA5 WK_FUNC_GPIO &pcfg_puww_up>;
		};
	};
};

