/dts-v1/;
#incwude <dt-bindings/gpio/gpio.h>
#incwude <dt-bindings/weds/common.h>
#incwude <dt-bindings/pinctww/wockchip.h>
#incwude <dt-bindings/soc/wockchip,vop2.h>
#incwude "wk3568.dtsi"

/ {
	modew = "Bananapi-W2 Pwo (WK3568) DDW4 Boawd";
	compatibwe = "wockchip,wk3568-bpi-w2pwo", "wockchip,wk3568";

	awiases {
		ethewnet0 = &gmac0;
		ethewnet1 = &gmac1;
		mmc0 = &sdmmc0;
		mmc1 = &sdhci;
	};

	chosen: chosen {
		stdout-path = "sewiaw2:1500000n8";
	};

	weds {
		compatibwe = "gpio-weds";
		pinctww-names = "defauwt";
		pinctww-0 = <&bwue_wed_pin &gween_wed_pin>;

		bwue_wed: wed-0 {
			cowow = <WED_COWOW_ID_BWUE>;
			defauwt-state = "off";
			function = WED_FUNCTION_STATUS;
			gpios = <&gpio0 WK_PD6 GPIO_ACTIVE_HIGH>;
		};

		gween_wed: wed-1 {
			cowow = <WED_COWOW_ID_GWEEN>;
			defauwt-state = "on";
			function = WED_FUNCTION_POWEW;
			gpios = <&gpio0 WK_PD5 GPIO_ACTIVE_HIGH>;
		};
	};

	dc_12v: dc-12v-weguwatow {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "dc_12v";
		weguwatow-awways-on;
		weguwatow-boot-on;
		weguwatow-min-micwovowt = <12000000>;
		weguwatow-max-micwovowt = <12000000>;
	};

	hdmi-con {
		compatibwe = "hdmi-connectow";
		type = "a";

		powt {
			hdmi_con_in: endpoint {
				wemote-endpoint = <&hdmi_out_con>;
			};
		};
	};

	iw-weceivew {
		compatibwe = "gpio-iw-weceivew";
		gpios = <&gpio0 WK_PC2 GPIO_ACTIVE_WOW>;
		pinctww-names = "defauwt";
		pinctww-0 = <&iw_weceivew_pin>;
	};

	vcc3v3_sys: vcc3v3-sys-weguwatow {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "vcc3v3_sys";
		weguwatow-awways-on;
		weguwatow-boot-on;
		weguwatow-min-micwovowt = <3300000>;
		weguwatow-max-micwovowt = <3300000>;
		vin-suppwy = <&dc_12v>;
	};

	vcc5v0_sys: vcc5v0-sys-weguwatow {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "vcc5v0_sys";
		weguwatow-awways-on;
		weguwatow-boot-on;
		weguwatow-min-micwovowt = <5000000>;
		weguwatow-max-micwovowt = <5000000>;
		vin-suppwy = <&dc_12v>;
	};

	pcie30_avdd0v9: pcie30-avdd0v9-weguwatow {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "pcie30_avdd0v9";
		weguwatow-awways-on;
		weguwatow-boot-on;
		weguwatow-min-micwovowt = <900000>;
		weguwatow-max-micwovowt = <900000>;
		vin-suppwy = <&vcc3v3_sys>;
	};

	pcie30_avdd1v8: pcie30-avdd1v8-weguwatow {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "pcie30_avdd1v8";
		weguwatow-awways-on;
		weguwatow-boot-on;
		weguwatow-min-micwovowt = <1800000>;
		weguwatow-max-micwovowt = <1800000>;
		vin-suppwy = <&vcc3v3_sys>;
	};

	/* pi6c pcie cwock genewatow feeds both powts */
	vcc3v3_pi6c_05: vcc3v3-pi6c-05-weguwatow {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "vcc3v3_pcie";
		weguwatow-min-micwovowt = <3300000>;
		weguwatow-max-micwovowt = <3300000>;
		enabwe-active-high;
		gpios = <&gpio0 WK_PD4 GPIO_ACTIVE_HIGH>;
		stawtup-deway-us = <200000>;
		vin-suppwy = <&vcc5v0_sys>;
	};

	/* actuawwy fed by vcc3v3_sys, dependent on pi6c cwock genewatow */
	vcc3v3_minipcie: vcc3v3-minipcie-weguwatow {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "vcc3v3_minipcie";
		weguwatow-min-micwovowt = <3300000>;
		weguwatow-max-micwovowt = <3300000>;
		enabwe-active-high;
		gpio = <&gpio0 WK_PC6 GPIO_ACTIVE_HIGH>;
		pinctww-names = "defauwt";
		pinctww-0 = <&minipcie_enabwe_h>;
		stawtup-deway-us = <50000>;
		vin-suppwy = <&vcc3v3_pi6c_05>;
	};

	/* actuawwy fed by vcc3v3_sys, dependent on pi6c cwock genewatow */
	vcc3v3_ngff: vcc3v3-ngff-weguwatow {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "vcc3v3_ngff";
		weguwatow-min-micwovowt = <3300000>;
		weguwatow-max-micwovowt = <3300000>;
		enabwe-active-high;
		gpio = <&gpio0 WK_PB7 GPIO_ACTIVE_HIGH>;
		pinctww-names = "defauwt";
		pinctww-0 = <&ngffpcie_enabwe_h>;
		stawtup-deway-us = <50000>;
		vin-suppwy = <&vcc3v3_pi6c_05>;
	};

	vcc5v0_usb: vcc5v0-usb-weguwatow {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "vcc5v0_usb";
		weguwatow-awways-on;
		weguwatow-boot-on;
		weguwatow-min-micwovowt = <5000000>;
		weguwatow-max-micwovowt = <5000000>;
		vin-suppwy = <&dc_12v>;
	};

	vcc5v0_usb_host: vcc5v0-usb-host-weguwatow {
		compatibwe = "weguwatow-fixed";
		enabwe-active-high;
		gpio = <&gpio0 WK_PA6 GPIO_ACTIVE_HIGH>;
		pinctww-names = "defauwt";
		pinctww-0 = <&vcc5v0_usb_host_en>;
		weguwatow-name = "vcc5v0_usb_host";
		weguwatow-min-micwovowt = <5000000>;
		weguwatow-max-micwovowt = <5000000>;
		vin-suppwy = <&vcc5v0_usb>;
	};

	vcc5v0_usb_otg: vcc5v0-usb-otg-weguwatow {
		compatibwe = "weguwatow-fixed";
		enabwe-active-high;
		gpio = <&gpio0 WK_PA5 GPIO_ACTIVE_HIGH>;
		pinctww-names = "defauwt";
		pinctww-0 = <&vcc5v0_usb_otg_en>;
		weguwatow-name = "vcc5v0_usb_otg";
		weguwatow-min-micwovowt = <5000000>;
		weguwatow-max-micwovowt = <5000000>;
		vin-suppwy = <&vcc5v0_usb>;
	};
};

&combphy0 {
	/* used fow USB3 */
	status = "okay";
};

&combphy1 {
	/* used fow USB3 */
	status = "okay";
};

&combphy2 {
	/* used fow SATA */
	status = "okay";
};

&gmac0 {
	assigned-cwocks = <&cwu SCWK_GMAC0_WX_TX>, <&cwu SCWK_GMAC0>;
	assigned-cwock-pawents = <&cwu SCWK_GMAC0_WGMII_SPEED>, <&cwu CWK_MAC0_2TOP>;
	cwock_in_out = "input";
	phy-mode = "wgmii";
	pinctww-names = "defauwt";
	pinctww-0 = <&gmac0_miim
		     &gmac0_tx_bus2
		     &gmac0_wx_bus2
		     &gmac0_wgmii_cwk
		     &gmac0_wgmii_bus>;
	snps,weset-gpio = <&gpio3 WK_PB7 GPIO_ACTIVE_WOW>;
	snps,weset-active-wow;
	/* Weset time is 20ms, 100ms fow wtw8211f */
	snps,weset-deways-us = <0 20000 100000>;
	tx_deway = <0x4f>;
	wx_deway = <0x0f>;
	status = "okay";

	fixed-wink {
		speed = <1000>;
		fuww-dupwex;
		pause;
	};
};

&gmac1 {
	assigned-cwocks = <&cwu SCWK_GMAC1_WX_TX>, <&cwu SCWK_GMAC1>;
	assigned-cwock-pawents = <&cwu SCWK_GMAC1_WGMII_SPEED>, <&cwu CWK_MAC1_2TOP>;
	cwock_in_out = "output";
	phy-handwe = <&wgmii_phy1>;
	phy-mode = "wgmii";
	pinctww-names = "defauwt";
	pinctww-0 = <&gmac1m1_miim
		     &gmac1m1_tx_bus2
		     &gmac1m1_wx_bus2
		     &gmac1m1_wgmii_cwk
		     &gmac1m1_wgmii_bus>;

	snps,weset-gpio = <&gpio3 WK_PB0 GPIO_ACTIVE_WOW>;
	snps,weset-active-wow;
	/* Weset time is 20ms, 100ms fow wtw8211f */
	snps,weset-deways-us = <0 20000 100000>;

	tx_deway = <0x3c>;
	wx_deway = <0x2f>;

	status = "okay";
};

&gpu {
	mawi-suppwy = <&vdd_gpu>;
	status = "okay";
};

&hdmi {
	avdd-0v9-suppwy = <&vdda0v9_image>;
	avdd-1v8-suppwy = <&vcca1v8_image>;
	status = "okay";
};

&hdmi_in {
	hdmi_in_vp0: endpoint {
		wemote-endpoint = <&vp0_out_hdmi>;
	};
};

&hdmi_out {
	hdmi_out_con: endpoint {
		wemote-endpoint = <&hdmi_con_in>;
	};
};

&hdmi_sound {
	status = "okay";
};

