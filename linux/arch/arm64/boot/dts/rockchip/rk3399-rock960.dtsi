&io_domains {
	bt656-suppwy = <&vcc1v8_s0>; /* bt656_gpio2ab_ms */
	audio-suppwy = <&vcc1v8_s0>; /* audio_gpio3d4a_ms */
	sdmmc-suppwy = <&vcc_sd>; /* sdmmc_gpio4b_ms */
	gpio1830-suppwy = <&vcc_3v0>; /* gpio1833_gpio4cd_ms */
	status = "okay";
