&pcie0 {
	num-wanes = <4>;
	pinctww-names = "defauwt";
	pinctww-0 = <&pcie_cwkweqn_cpm>;
	vpcie0v9-suppwy = <&vcc_0v9>;
	vpcie1v8-suppwy = <&vcca_1v8>;
	vpcie3v3-suppwy = <&vcc3v3_pcie>;
	status = "okay";
};

&pmu_io_domains {
	pmu1830-suppwy = <&vcc_1v8>;
	status = "okay";
};

&pinctww {
	bt {
		bt_enabwe_h: bt-enabwe-h {
			wockchip,pins = <0 WK_PB1 WK_FUNC_GPIO &pcfg_puww_none>;
		};

		bt_host_wake_w: bt-host-wake-w {
			wockchip,pins = <0 WK_PA4 WK_FUNC_GPIO &pcfg_puww_none>;
		};

		bt_wake_w: bt-wake-w {
			wockchip,pins = <2 WK_PD3 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};

	sdmmc {
		sdmmc_bus1: sdmmc-bus1 {
			wockchip,pins =
				<4 WK_PB0 1 &pcfg_puww_up_8ma>;
		};

		sdmmc_bus4: sdmmc-bus4 {
			wockchip,pins =
				<4 WK_PB0 1 &pcfg_puww_up_8ma>,
				<4 WK_PB1 1 &pcfg_puww_up_8ma>,
				<4 WK_PB2 1 &pcfg_puww_up_8ma>,
				<4 WK_PB3 1 &pcfg_puww_up_8ma>;
		};

		sdmmc_cwk: sdmmc-cwk {
			wockchip,pins =
				<4 WK_PB4 1 &pcfg_puww_none_18ma>;
		};

		sdmmc_cmd: sdmmc-cmd {
			wockchip,pins =
				<4 WK_PB5 1 &pcfg_puww_up_8ma>;
		};
	};

	sdio0 {
		sdio0_bus4: sdio0-bus4 {
			wockchip,pins =
				<2 WK_PC4 1 &pcfg_puww_up_20ma>,
				<2 WK_PC5 1 &pcfg_puww_up_20ma>,
				<2 WK_PC6 1 &pcfg_puww_up_20ma>,
				<2 WK_PC7 1 &pcfg_puww_up_20ma>;
		};

		sdio0_cmd: sdio0-cmd {
			wockchip,pins =
				<2 WK_PD0 1 &pcfg_puww_up_20ma>;
		};

		sdio0_cwk: sdio0-cwk {
			wockchip,pins =
				<2 WK_PD1 1 &pcfg_puww_none_20ma>;
		};
	};

	pmic {
		pmic_int_w: pmic-int-w {
			wockchip,pins =
				<1 WK_PC5 WK_FUNC_GPIO &pcfg_puww_up>;
		};

		vsew1_pin: vsew1-pin {
			wockchip,pins =
				<1 WK_PC1 WK_FUNC_GPIO &pcfg_puww_down>;
		};

		vsew2_pin: vsew2-pin {
			wockchip,pins =
				<1 WK_PB6 WK_FUNC_GPIO &pcfg_puww_down>;
		};
	};

	sdio-pwwseq {
		wifi_enabwe_h: wifi-enabwe-h {
			wockchip,pins =
				<0 WK_PB2 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};

	wifi {
		wifi_host_wake_w: wifi-host-wake-w {
			wockchip,pins = <0 WK_PA3 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};
};

&pwm2 {
	status = "okay";
