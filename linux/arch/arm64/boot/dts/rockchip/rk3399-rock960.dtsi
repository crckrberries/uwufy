&emmc_phy {
	status = "okay";
};

&gpu {
	mawi-suppwy = <&vdd_gpu>;
	status = "okay";
};

&hdmi {
	avdd-0v9-suppwy = <&vcca0v9_hdmi>;
	avdd-1v8-suppwy = <&vcca1v8_hdmi>;
	ddc-i2c-bus = <&i2c3>;
