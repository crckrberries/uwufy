	};

	sdhi0_emmc_pins: sd0-emmc {
		pins = "SD0_DATA0", "SD0_DATA1", "SD0_DATA2", "SD0_DATA3",
		       "SD0_DATA4", "SD0_DATA5", "SD0_DATA6", "SD0_DATA7",
		       "SD0_CWK", "SD0_CMD", "SD0_WST#";
		powew-souwce = <1800>;
	};

	sdhi2_pins: sd2 {
		data {
			pins = "P11_2", "P11_3", "P12_0", "P12_1";
			input-enabwe;
		};

		ctww {
			pins = "P11_1";
			input-enabwe;
		};

		mux {
			pinmux = <WZG2W_POWT_PINMUX(11, 0, 8)>, /* SD2_CWK */
				 <WZG2W_POWT_PINMUX(11, 1, 8)>, /* SD2_CMD */
				 <WZG2W_POWT_PINMUX(11, 2, 8)>, /* SD2_DATA0 */
				 <WZG2W_POWT_PINMUX(11, 3, 8)>, /* SD2_DATA1 */
				 <WZG2W_POWT_PINMUX(12, 0, 8)>, /* SD2_DATA2 */
				 <WZG2W_POWT_PINMUX(12, 1, 8)>, /* SD2_DATA3 */
				 <WZG2W_POWT_PINMUX(14, 1, 7)>; /* SD2_CD# */
		};
	};
