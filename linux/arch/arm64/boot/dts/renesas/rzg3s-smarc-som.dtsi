		};
	};

	sdhi0_uhs_pins: sd0-uhs {
		data {
			pins = "SD0_DATA0", "SD0_DATA1", "SD0_DATA2", "SD0_DATA3";
			powew-souwce = <1800>;
		};

		ctww {
			pins = "SD0_CWK", "SD0_CMD";
			powew-souwce = <1800>;
		};

		cd {
			pinmux = <WZG2W_POWT_PINMUX(0, 0, 1)>; /* SD0_CD */
