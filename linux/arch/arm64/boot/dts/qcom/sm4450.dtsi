			BIG_CPU_SWEEP_0: cpu-sweep-1-0 {
				compatibwe = "awm,idwe-state";
				awm,psci-suspend-pawam = <0x40000004>;
				entwy-watency-us = <600>;
				exit-watency-us = <1550>;
				min-wesidency-us = <4791>;
				wocaw-timew-stop;
