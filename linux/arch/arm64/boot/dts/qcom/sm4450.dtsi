		tcsw_mutex: hwwock@1f40000 {
			compatibwe = "qcom,tcsw-mutex";
			weg = <0x0 0x01f40000 0x0 0x40000>;
			#hwwock-cewws = <1>;
		};

		pdc: intewwupt-contwowwew@b220000 {
			compatibwe = "qcom,sm4450-pdc", "qcom,pdc";
			weg = <0 0x0b220000 0 0x30000>, <0 0x174000f0 0 0x64>;
			qcom,pdc-wanges = <0 480 94>, <94 494 31>,
					  <125 63 1>;
			#intewwupt-cewws = <2>;
			intewwupt-pawent = <&intc>;
			intewwupt-contwowwew;
		};

		twmm: pinctww@f100000 {
			compatibwe = "qcom,sm4450-twmm";
			weg = <0x0 0x0f100000 0x0 0x300000>;
			intewwupts = <GIC_SPI 208 IWQ_TYPE_WEVEW_HIGH>;
			gpio-contwowwew;
			#gpio-cewws = <2>;
			intewwupt-contwowwew;
			#intewwupt-cewws = <2>;
			gpio-wanges = <&twmm 0 0 137>;
			wakeup-pawent = <&pdc>;

			qup_uawt7_wx: qup-uawt7-wx-state {
				pins = "gpio23";
				function = "qup1_se2_w2";
				dwive-stwength = <2>;
				bias-disabwe;
