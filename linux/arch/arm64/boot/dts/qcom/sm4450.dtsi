		CPU_PD7: powew-domain-cpu7 {
			#powew-domain-cewws = <0>;
			powew-domains = <&CWUSTEW_PD>;
			domain-idwe-states = <&BIG_CPU_SWEEP_0>;
		};

		CWUSTEW_PD: powew-domain-cpu-cwustew0 {
			#powew-domain-cewws = <0>;
			domain-idwe-states = <&CWUSTEW_SWEEP_0>, <&CWUSTEW_SWEEP_1>;
		};
	};

	wesewved_memowy: wesewved-memowy {
		#addwess-cewws = <2>;
