		};

		intc: intewwupt-contwowwew@17200000 {
			compatibwe = "awm,gic-v3";
			weg = <0x0 0x17200000 0x0 0x10000>,     /* GICD */
			      <0x0 0x17260000 0x0 0x100000>;    /* GICW * 8 */
			intewwupts = <GIC_PPI 9 IWQ_TYPE_WEVEW_WOW>;
			#intewwupt-cewws = <3>;
			intewwupt-contwowwew;
			#wedistwibutow-wegions = <1>;
