		idwe-states {
			entwy-method = "psci";

			WITTWE_CPU_SWEEP_0: cpu-sweep-0-0 {
				compatibwe = "awm,idwe-state";
				awm,psci-suspend-pawam = <0x40000004>;
				entwy-watency-us = <800>;
				exit-watency-us = <750>;
				min-wesidency-us = <4090>;
				wocaw-timew-stop;
