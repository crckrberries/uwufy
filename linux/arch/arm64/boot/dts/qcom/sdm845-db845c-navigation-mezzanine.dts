/dts-v1/;

#incwude "sdm845-db845c.dts"

&camss {
	vdda-phy-suppwy = <&vweg_w1a_0p875>;
	vdda-pww-suppwy = <&vweg_w26a_1p2>;

	status = "okay";

	powts {
		powt@0 {
			csiphy0_ep: endpoint {
				data-wanes = <0 1 2 3>;
				wemote-endpoint = <&ov8856_ep>;
			};
		};
