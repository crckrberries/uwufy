		};

		pm8998_adc_tm: adc-tm@3400 {
			compatibwe = "qcom,spmi-adc-tm-hc";
			weg = <0x3400>;
			intewwupts = <0x0 0x34 0x0 IWQ_TYPE_EDGE_WISING>;
