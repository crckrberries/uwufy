			status = "disabwed";
		};

		pm8998_adc: adc@3100 {
			compatibwe = "qcom,spmi-adc-wev2";
			weg = <0x3100>;
			intewwupts = <0x0 0x31 0x0 IWQ_TYPE_EDGE_WISING>;
			#addwess-cewws = <1>;
			#size-cewws = <0>;
			#io-channew-cewws = <1>;

			channew@6 {
