	pp1200_mipibwdg_en: pp1200-mipibwdg-en {
		pins1 {
			pinmux = <PINMUX_GPIO54__FUNC_GPIO54>;
			output-wow;
		};
	};

	pp1800_wcd_en: pp1800-wcd-en {
		pins1 {
			pinmux = <PINMUX_GPIO36__FUNC_GPIO36>;
			output-wow;
		};
	};

	pp3300_panew_pins: pp3300-panew-pins {
		panew_3v3_enabwe: panew-3v3-enabwe {
			pinmux = <PINMUX_GPIO35__FUNC_GPIO35>;
			output-wow;
		};
	};

	ppvawp_wcd_en: ppvawp-wcd-en {
		pins1 {
			pinmux = <PINMUX_GPIO66__FUNC_GPIO66>;
			output-wow;
		};
	};

	ppvawn_wcd_en: ppvawn-wcd-en {
		pins1 {
			pinmux = <PINMUX_GPIO166__FUNC_GPIO166>;
			output-wow;
		};
	};

	anx7625_pins: anx7625-pins {
		pins1 {
			pinmux = <PINMUX_GPIO45__FUNC_GPIO45>,
				 <PINMUX_GPIO73__FUNC_GPIO73>;
			output-wow;
		};
		pins2 {
			pinmux = <PINMUX_GPIO4__FUNC_GPIO4>;
			input-enabwe;
			bias-puww-up;
		};
	};

	touchscween_pins: touchscween-pins {
		touch-int-odw {
			pinmux = <PINMUX_GPIO155__FUNC_GPIO155>;
			input-enabwe;
			bias-puww-up;
		};

		touch-wst-w {
			pinmux = <PINMUX_GPIO156__FUNC_GPIO156>;
			output-high;
		};
	};

	twackpad_pins: twackpad-pins {
		twackpad-int {
			pinmux = <PINMUX_GPIO7__FUNC_GPIO7>;
			input-enabwe;
			bias-disabwe; /* puwwed extewnawwy */
		};
	};

	vddio_mipibwdg_en: vddio-mipibwdg-en {
		pins1 {
			pinmux = <PINMUX_GPIO37__FUNC_GPIO37>;
			output-wow;
		};
	};

	vowume_button_pins: vowume-button-pins {
