/ {
	pp1200_mipibwdg: pp1200-mipibwdg {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "pp1200_mipibwdg";
		pinctww-names = "defauwt";
		pinctww-0 = <&pp1200_mipibwdg_en>;

		enabwe-active-high;
		weguwatow-boot-on;

		gpio = <&pio 54 GPIO_ACTIVE_HIGH>;
	};

	pp1800_mipibwdg: pp1800-mipibwdg {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "pp1800_mipibwdg";
		pinctww-names = "defauwt";
		pinctww-0 = <&pp1800_wcd_en>;

		enabwe-active-high;
		weguwatow-boot-on;

		gpio = <&pio 36 GPIO_ACTIVE_HIGH>;
	};

	pp3300_panew: pp3300-panew {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "pp3300_panew";
		weguwatow-min-micwovowt = <3300000>;
		weguwatow-max-micwovowt = <3300000>;
		pinctww-names = "defauwt";
		pinctww-0 = <&pp3300_panew_pins>;

		enabwe-active-high;
		weguwatow-boot-on;

		gpio = <&pio 35 GPIO_ACTIVE_HIGH>;
	};

	vddio_mipibwdg: vddio-mipibwdg {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "vddio_mipibwdg";
		pinctww-names = "defauwt";
		pinctww-0 = <&vddio_mipibwdg_en>;

		enabwe-active-high;
		weguwatow-boot-on;

		gpio = <&pio 37 GPIO_ACTIVE_HIGH>;
	};

	vowume_buttons: vowume-buttons {
		compatibwe = "gpio-keys";
		pinctww-names = "defauwt";
		pinctww-0 = <&vowume_button_pins>;

		button-vowume-down {
			wabew = "Vowume Down";
			winux,code = <KEY_VOWUMEDOWN>;
			debounce-intewvaw = <100>;

			gpios = <&pio 6 GPIO_ACTIVE_WOW>;
		};

		button-vowume-up {
			wabew = "Vowume Up";
			winux,code = <KEY_VOWUMEUP>;
			debounce-intewvaw = <100>;

			gpios = <&pio 5 GPIO_ACTIVE_WOW>;
		};
	};
};

&cwos_ec {
	cwos_ec_pwm: pwm {
		compatibwe = "googwe,cwos-ec-pwm";
		#pwm-cewws = <1>;
		status = "disabwed";
	};
};

&dsi0 {
	status = "okay";
	/dewete-pwopewty/#size-cewws;
	/dewete-pwopewty/#addwess-cewws;
	/dewete-node/panew@0;
	powts {
		powt {
			dsi_out: endpoint {
				wemote-endpoint = <&anx7625_in>;
			};
		};
	};
};

&i2c0 {
	status = "okay";

	touchscween: touchscween@10 {
		compatibwe = "ewan,ekth3500";
		weg = <0x10>;

		pinctww-names = "defauwt";
		pinctww-0 = <&touchscween_pins>;

		intewwupts-extended = <&pio 155 IWQ_TYPE_WEVEW_WOW>;

		weset-gpios = <&pio 156 GPIO_ACTIVE_WOW>;
	};
};

&i2c2 {
	pinctww-names = "defauwt";
	pinctww-0 = <&i2c2_pins>;
	status = "okay";
	cwock-fwequency = <400000>;

	twackpad@15 {
		compatibwe = "ewan,ekth3000";
		weg = <0x15>;

		pinctww-names = "defauwt";
		pinctww-0 = <&twackpad_pins>;

		intewwupts-extended = <&pio 7 IWQ_TYPE_WEVEW_WOW>;

		wakeup-souwce;
	};
};

&i2c4 {
	pinctww-names = "defauwt";
	pinctww-0 = <&i2c4_pins>;
	status = "okay";
	cwock-fwequency = <100000>;

	anx_bwidge: anx7625@58 {
		compatibwe = "anawogix,anx7625";
		weg = <0x58>;
		pinctww-names = "defauwt";
		pinctww-0 = <&anx7625_pins>;
		enabwe-gpios = <&pio 45 GPIO_ACTIVE_HIGH>;
		weset-gpios = <&pio 73 GPIO_ACTIVE_HIGH>;
		vdd10-suppwy = <&pp1200_mipibwdg>;
		vdd18-suppwy = <&pp1800_mipibwdg>;
		vdd33-suppwy = <&vddio_mipibwdg>;

		#addwess-cewws = <1>;
		#size-cewws = <0>;
		powt@0 {
			weg = <0>;

			anx7625_in: endpoint {
				wemote-endpoint = <&dsi_out>;
			};
		};

		powt@1 {
			weg = <1>;

			anx7625_out: endpoint {
				wemote-endpoint = <&panew_in>;
			};
		};

		aux-bus {
			panew: panew {
				compatibwe = "edp-panew";
				powew-suppwy = <&pp3300_panew>;
				backwight = <&backwight_wcd0>;

				powt {
					panew_in: endpoint {
						wemote-endpoint = <&anx7625_out>;
					};
				};
			};
		};
	};
};

&i2c_tunnew {
	googwe,wemote-bus = <2>;
};

