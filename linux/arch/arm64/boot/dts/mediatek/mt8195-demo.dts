		snd_dma_mem: memowy@60000000 {
			compatibwe = "shawed-dma-poow";
			weg = <0 0x60000000 0 0x1100000>;
			no-map;
		};

		apu_mem: memowy@62000000 {
			compatibwe = "shawed-dma-poow";
			weg = <0 0x62000000 0 0x1400000>; /* 20 MB */
		};
	};
};

