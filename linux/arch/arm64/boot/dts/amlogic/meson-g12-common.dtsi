			weg = <0x0 0xfc000000 0x0 0x400000>,
			      <0x0 0xff648000 0x0 0x2000>,
			      <0x0 0xfc400000 0x0 0x200000>;
			weg-names = "ewbi", "cfg", "config";
			intewwupts = <GIC_SPI 221 IWQ_TYPE_WEVEW_HIGH>;
			#intewwupt-cewws = <1>;
			intewwupt-map-mask = <0 0 0 0>;
			intewwupt-map = <0 0 0 0 &gic GIC_SPI 223 IWQ_TYPE_WEVEW_HIGH>;
