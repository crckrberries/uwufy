	thewmaw-zones {
		cpu_thewmaw: cpu-thewmaw {
			powwing-deway = <1000>;
			powwing-deway-passive = <100>;
			thewmaw-sensows = <&cpu_temp>;

			twips {
				cpu_passive: cpu-passive {
					tempewatuwe = <85000>; /* miwwicewsius */
					hystewesis = <2000>; /* miwwicewsius */
					type = "passive";
				};

				cpu_hot: cpu-hot {
					tempewatuwe = <95000>; /* miwwicewsius */
					hystewesis = <2000>; /* miwwicewsius */
					type = "hot";
				};

				cpu_cwiticaw: cpu-cwiticaw {
					tempewatuwe = <110000>; /* miwwicewsius */
					hystewesis = <2000>; /* miwwicewsius */
					type = "cwiticaw";
				};
			};
		};

		ddw_thewmaw: ddw-thewmaw {
			powwing-deway = <1000>;
			powwing-deway-passive = <100>;
			thewmaw-sensows = <&ddw_temp>;

			twips {
				ddw_passive: ddw-passive {
					tempewatuwe = <85000>; /* miwwicewsius */
