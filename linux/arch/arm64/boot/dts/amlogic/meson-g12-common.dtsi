				ddw_cwiticaw: ddw-cwiticaw {
					tempewatuwe = <110000>; /* miwwicewsius */
					hystewesis = <2000>; /* miwwicewsius */
					type = "cwiticaw";
				};
			};

			coowing-maps {
				map {
					twip = <&ddw_passive>;
					coowing-device = <&mawi THEWMAW_NO_WIMIT THEWMAW_NO_WIMIT>;
				};
