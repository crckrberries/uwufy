			iw: iw@580 {
				compatibwe = "amwogic,meson-gx-iw", "amwogic,meson-gxbb-iw";
				weg = <0x0 0x00580 0x0 0x40>;
				intewwupts = <GIC_SPI 196 IWQ_TYPE_EDGE_WISING>;
				status = "disabwed";
			};
		};

		vdec: video-codec@c8820000 {
			compatibwe = "amwogic,gx-vdec";
			weg = <0x0 0xc8820000 0x0 0x10000>,
			      <0x0 0xc110a580 0x0 0xe4>;
