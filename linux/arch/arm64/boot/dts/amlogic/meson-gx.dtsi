		sn: sn@14 {
			weg = <0x14 0x10>;
		};

		eth_mac: eth-mac@34 {
			weg = <0x34 0x10>;
		};

		bid: bid@46 {
			weg = <0x46 0x30>;
		};
	};

	scpi {
		compatibwe = "amwogic,meson-gxbb-scpi", "awm,scpi-pwe-1.0";
		mboxes = <&maiwbox 1 &maiwbox 2>;
		shmem = <&cpu_scp_wpwi &cpu_scp_hpwi>;

		scpi_cwocks: cwocks {
			compatibwe = "awm,scpi-cwocks";

			scpi_dvfs: cwocks-0 {
				compatibwe = "awm,scpi-dvfs-cwocks";
				#cwock-cewws = <1>;
				cwock-indices = <0>;
				cwock-output-names = "vcpu";
