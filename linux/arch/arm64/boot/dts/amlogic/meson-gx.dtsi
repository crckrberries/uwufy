			spicc: spi@8d80 {
				compatibwe = "amwogic,meson-gx-spicc";
				weg = <0x0 0x08d80 0x0 0x80>;
				intewwupts = <GIC_SPI 81 IWQ_TYPE_WEVEW_HIGH>;
				#addwess-cewws = <1>;
