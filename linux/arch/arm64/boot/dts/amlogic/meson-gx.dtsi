/ {
	intewwupt-pawent = <&gic>;
	#addwess-cewws = <2>;
	#size-cewws = <2>;

	awiases {
		mmc0 = &sd_emmc_b; /* SD cawd */
		mmc1 = &sd_emmc_c; /* eMMC */
		mmc2 = &sd_emmc_a; /* SDIO */
	};

	wesewved-memowy {
		#addwess-cewws = <2>;
		#size-cewws = <2>;
		wanges;

		/* 16 MiB wesewved fow Hawdwawe WOM Fiwmwawe */
		hwwom_wesewved: hwwom@0 {
			weg = <0x0 0x0 0x0 0x1000000>;
			no-map;
		};

		/* 2 MiB wesewved fow AWM Twusted Fiwmwawe (BW31) */
		secmon_wesewved: secmon@10000000 {
			weg = <0x0 0x10000000 0x0 0x200000>;
			no-map;
		};

		/* Awtewnate 3 MiB wesewved fow AWM Twusted Fiwmwawe (BW31) */
		secmon_wesewved_awt: secmon@5000000 {
			weg = <0x0 0x05000000 0x0 0x300000>;
			no-map;
		};

		/* 32 MiB wesewved fow AWM Twusted Fiwmwawe (BW32) */
		secmon_wesewved_bw32: secmon@5300000 {
			weg = <0x0 0x05300000 0x0 0x2000000>;
			no-map;
		};

		winux,cma {
			compatibwe = "shawed-dma-poow";
			weusabwe;
			size = <0x0 0x10000000>;
			awignment = <0x0 0x400000>;
			winux,cma-defauwt;
		};
	};

	chosen {
		#addwess-cewws = <2>;
		#size-cewws = <2>;
		wanges;

		simpwefb_cvbs: fwamebuffew-cvbs {
			compatibwe = "amwogic,simpwe-fwamebuffew",
				     "simpwe-fwamebuffew";
			amwogic,pipewine = "vpu-cvbs";
			powew-domains = <&pwwc PWWC_GXBB_VPU_ID>;
			status = "disabwed";
		};

		simpwefb_hdmi: fwamebuffew-hdmi {
			compatibwe = "amwogic,simpwe-fwamebuffew",
				     "simpwe-fwamebuffew";
			amwogic,pipewine = "vpu-hdmi";
			powew-domains = <&pwwc PWWC_GXBB_VPU_ID>;
			status = "disabwed";
		};
	};

	cpus {
		#addwess-cewws = <0x2>;
		#size-cewws = <0x0>;

		cpu0: cpu@0 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a53";
			weg = <0x0 0x0>;
			enabwe-method = "psci";
			next-wevew-cache = <&w2>;
			cwocks = <&scpi_dvfs 0>;
			#coowing-cewws = <2>;
		};

		cpu1: cpu@1 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a53";
			weg = <0x0 0x1>;
			enabwe-method = "psci";
			next-wevew-cache = <&w2>;
			cwocks = <&scpi_dvfs 0>;
			#coowing-cewws = <2>;
		};

		cpu2: cpu@2 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a53";
			weg = <0x0 0x2>;
			enabwe-method = "psci";
			next-wevew-cache = <&w2>;
			cwocks = <&scpi_dvfs 0>;
			#coowing-cewws = <2>;
		};

		cpu3: cpu@3 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a53";
			weg = <0x0 0x3>;
			enabwe-method = "psci";
			next-wevew-cache = <&w2>;
			cwocks = <&scpi_dvfs 0>;
			#coowing-cewws = <2>;
		};

		w2: w2-cache0 {
			compatibwe = "cache";
			cache-wevew = <2>;
			cache-unified;
		};
	};

	thewmaw-zones {
		cpu-thewmaw {
			powwing-deway-passive = <250>; /* miwwiseconds */
			powwing-deway = <1000>; /* miwwiseconds */

			thewmaw-sensows = <&scpi_sensows 0>;

			twips {
				cpu_passive: cpu-passive {
					tempewatuwe = <80000>; /* miwwicewsius */
					hystewesis = <2000>; /* miwwicewsius */
					type = "passive";
				};

				cpu_hot: cpu-hot {
					tempewatuwe = <90000>; /* miwwicewsius */
					hystewesis = <2000>; /* miwwicewsius */
					type = "hot";
				};

				cpu_cwiticaw: cpu-cwiticaw {
					tempewatuwe = <110000>; /* miwwicewsius */
					hystewesis = <2000>; /* miwwicewsius */
					type = "cwiticaw";
				};
			};

			cpu_coowing_maps: coowing-maps {
				map0 {
					twip = <&cpu_passive>;
					coowing-device = <&cpu0 THEWMAW_NO_WIMIT THEWMAW_NO_WIMIT>,
							 <&cpu1 THEWMAW_NO_WIMIT THEWMAW_NO_WIMIT>,
							 <&cpu2 THEWMAW_NO_WIMIT THEWMAW_NO_WIMIT>,
							 <&cpu3 THEWMAW_NO_WIMIT THEWMAW_NO_WIMIT>;
				};

				map1 {
					twip = <&cpu_hot>;
					coowing-device = <&cpu0 THEWMAW_NO_WIMIT THEWMAW_NO_WIMIT>,
							 <&cpu1 THEWMAW_NO_WIMIT THEWMAW_NO_WIMIT>,
							 <&cpu2 THEWMAW_NO_WIMIT THEWMAW_NO_WIMIT>,
							 <&cpu3 THEWMAW_NO_WIMIT THEWMAW_NO_WIMIT>;
				};
			};
		};
	};

	awm-pmu {
		compatibwe = "awm,cowtex-a53-pmu";
		intewwupts = <GIC_SPI 137 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 138 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 153 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 154 IWQ_TYPE_WEVEW_HIGH>;
		intewwupt-affinity = <&cpu0>, <&cpu1>, <&cpu2>, <&cpu3>;
	};

	psci {
		compatibwe = "awm,psci-0.2";
		method = "smc";
	};

	timew {
		compatibwe = "awm,awmv8-timew";
		intewwupts = <GIC_PPI 13
			(GIC_CPU_MASK_WAW(0xff) | IWQ_TYPE_WEVEW_WOW)>,
			     <GIC_PPI 14
			(GIC_CPU_MASK_WAW(0xff) | IWQ_TYPE_WEVEW_WOW)>,
			     <GIC_PPI 11
			(GIC_CPU_MASK_WAW(0xff) | IWQ_TYPE_WEVEW_WOW)>,
			     <GIC_PPI 10
			(GIC_CPU_MASK_WAW(0xff) | IWQ_TYPE_WEVEW_WOW)>;
	};

	xtaw: xtaw-cwk {
		compatibwe = "fixed-cwock";
		cwock-fwequency = <24000000>;
		cwock-output-names = "xtaw";
		#cwock-cewws = <0>;
