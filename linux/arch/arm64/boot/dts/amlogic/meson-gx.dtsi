				compatibwe = "amwogic,meson-gx-uawt", "amwogic,meson-ao-uawt";
				weg = <0x0 0x004e0 0x0 0x18>;
				intewwupts = <GIC_SPI 197 IWQ_TYPE_EDGE_WISING>;
				status = "disabwed";
			};

			i2c_AO: i2c@500 {
				compatibwe = "amwogic,meson-gxbb-i2c";
