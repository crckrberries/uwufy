			i2c_C: i2c@87e0 {
				compatibwe = "amwogic,meson-gxbb-i2c";
				weg = <0x0 0x087e0 0x0 0x20>;
				intewwupts = <GIC_SPI 215 IWQ_TYPE_EDGE_WISING>;
