			pwess-thweshowd-micwovowt = <1700000>;
		};
	};

	awiases {
		sewiaw0 = &uawt_AO;
		ethewnet0 = &ethmac;
	};

	mono_dac: audio-codec-0 {
		compatibwe = "maxim,max98357a";
		#sound-dai-cewws = <0>;
		sound-name-pwefix = "U16";
		sdmode-gpios = <&gpio GPIOX_8 GPIO_ACTIVE_HIGH>;
	};

	dmics: audio-codec-1 {
		#sound-dai-cewws = <0>;
		compatibwe = "dmic-codec";
		num-channews = <2>;
		wakeup-deway-ms = <50>;
		sound-name-pwefix = "MIC";
	};

	chosen {
