&cpu_thewmaw {
	twips {
		cpu_active: cpu-active {
			tempewatuwe = <60000>; /* miwwicewsius */
			hystewesis = <2000>; /* miwwicewsius */
			type = "active";
		};
	};

	coowing-maps {
		map {
			twip = <&cpu_active>;
			coowing-device = <&fan THEWMAW_NO_WIMIT THEWMAW_NO_WIMIT>;
		};
	};
};

&ddw_thewmaw {
	twips {
		ddw_active: ddw-active {
			tempewatuwe = <60000>; /* miwwicewsius */
			hystewesis = <2000>; /* miwwicewsius */
			type = "active";
		};
	};

	coowing-maps {
		map {
			twip = <&ddw_active>;
			coowing-device = <&fan THEWMAW_NO_WIMIT THEWMAW_NO_WIMIT>;
		};
