	status = "okay";
	pinctww-0 = <&uawt_ao_a_pins>;
	pinctww-names = "defauwt";
};

/* This is bwought out on the UAWT_WX_AO_B (15) and UAWT_TX_AO_B (16) pins: */
&uawt_AO_B {
	status = "okay";
	pinctww-0 = <&uawt_ao_b_pins>;
	pinctww-names = "defauwt";
};

&sawadc {
	status = "okay";
	vwef-suppwy = <&vddio_ao18>;
};

&usb {
