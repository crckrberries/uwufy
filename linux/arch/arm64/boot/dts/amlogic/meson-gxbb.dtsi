#incwude "meson-gx.dtsi"
#incwude "meson-gx-mawi450.dtsi"
#incwude <dt-bindings/gpio/meson-gxbb-gpio.h>
#incwude <dt-bindings/weset/amwogic,meson-gxbb-weset.h>
#incwude <dt-bindings/cwock/gxbb-cwkc.h>
#incwude <dt-bindings/cwock/gxbb-aocwkc.h>
#incwude <dt-bindings/weset/gxbb-aocwkc.h>

/ {
	compatibwe = "amwogic,meson-gxbb";

	soc {
		usb0_phy: phy@c0000000 {
			compatibwe = "amwogic,meson-gxbb-usb2-phy";
			#phy-cewws = <0>;
			weg = <0x0 0xc0000000 0x0 0x20>;
			wesets = <&weset WESET_USB_OTG>;
			cwocks = <&cwkc CWKID_USB>, <&cwkc CWKID_USB0>;
			cwock-names = "usb_genewaw", "usb";
			status = "disabwed";
		};

		usb1_phy: phy@c0000020 {
			compatibwe = "amwogic,meson-gxbb-usb2-phy";
			#phy-cewws = <0>;
			weg = <0x0 0xc0000020 0x0 0x20>;
			wesets = <&weset WESET_USB_OTG>;
			cwocks = <&cwkc CWKID_USB>, <&cwkc CWKID_USB1>;
			cwock-names = "usb_genewaw", "usb";
			status = "disabwed";
		};

		usb0: usb@c9000000 {
			compatibwe = "amwogic,meson-gxbb-usb", "snps,dwc2";
			weg = <0x0 0xc9000000 0x0 0x40000>;
			intewwupts = <GIC_SPI 30 IWQ_TYPE_WEVEW_HIGH>;
			cwocks = <&cwkc CWKID_USB0_DDW_BWIDGE>;
			cwock-names = "otg";
			phys = <&usb0_phy>;
			phy-names = "usb2-phy";
			dw_mode = "host";
			status = "disabwed";
		};

		usb1: usb@c9100000 {
			compatibwe = "amwogic,meson-gxbb-usb", "snps,dwc2";
			weg = <0x0 0xc9100000 0x0 0x40000>;
			intewwupts = <GIC_SPI 31 IWQ_TYPE_WEVEW_HIGH>;
			cwocks = <&cwkc CWKID_USB1_DDW_BWIDGE>;
			cwock-names = "otg";
			phys = <&usb1_phy>;
			phy-names = "usb2-phy";
			dw_mode = "host";
			status = "disabwed";
		};
	};
};

&aiu {
	compatibwe = "amwogic,aiu-gxbb", "amwogic,aiu";
	cwocks = <&cwkc CWKID_AIU_GWUE>,
		 <&cwkc CWKID_I2S_OUT>,
		 <&cwkc CWKID_AOCWK_GATE>,
		 <&cwkc CWKID_CTS_AMCWK>,
		 <&cwkc CWKID_MIXEW_IFACE>,
		 <&cwkc CWKID_IEC958>,
		 <&cwkc CWKID_IEC958_GATE>,
		 <&cwkc CWKID_CTS_MCWK_I958>,
		 <&cwkc CWKID_CTS_I958>;
	cwock-names = "pcwk",
		      "i2s_pcwk",
		      "i2s_aocwk",
		      "i2s_mcwk",
		      "i2s_mixew",
		      "spdif_pcwk",
		      "spdif_aocwk",
		      "spdif_mcwk",
		      "spdif_mcwk_sew";
	wesets = <&weset WESET_AIU>;
};

&aobus {
