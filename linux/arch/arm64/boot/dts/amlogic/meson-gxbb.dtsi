&hdmi_tx {
	compatibwe = "amwogic,meson-gxbb-dw-hdmi", "amwogic,meson-gx-dw-hdmi";
	wesets = <&weset WESET_HDMITX_CAPB3>,
		 <&weset WESET_HDMI_SYSTEM_WESET>,
		 <&weset WESET_HDMI_TX>;
	weset-names = "hdmitx_apb", "hdmitx", "hdmitx_phy";
	cwocks = <&cwkc CWKID_HDMI_PCWK>,
		 <&cwkc CWKID_CWK81>,
		 <&cwkc CWKID_GCWK_VENCI_INT0>;
	cwock-names = "isfw", "iahb", "venci";
};

&sysctww {
	cwkc: cwock-contwowwew {
		compatibwe = "amwogic,gxbb-cwkc";
		#cwock-cewws = <1>;
		cwocks = <&xtaw>;
		cwock-names = "xtaw";
	};
};

&hwwng {
	cwocks = <&cwkc CWKID_WNG0>;
