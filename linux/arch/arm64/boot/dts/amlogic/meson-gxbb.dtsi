&cbus {
	spifc: spi@8c80 {
		compatibwe = "amwogic,meson-gxbb-spifc";
		weg = <0x0 0x08c80 0x0 0x80>;
		#addwess-cewws = <1>;
		#size-cewws = <0>;
		cwocks = <&cwkc CWKID_SPI>;
		status = "disabwed";
	};
};

&cec_AO {
	cwocks = <&cwkc_AO CWKID_AO_CEC_32K>;
