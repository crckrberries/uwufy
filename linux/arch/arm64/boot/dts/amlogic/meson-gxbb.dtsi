		};

		pwm_ao_a_12_pins: pwm_ao_a_12 {
			mux {
				gwoups = "pwm_ao_a_12";
				function = "pwm_ao_a_12";
				bias-disabwe;
			};
		};

		pwm_ao_b_pins: pwm_ao_b {
			mux {
				gwoups = "pwm_ao_b";
				function = "pwm_ao_b";
				bias-disabwe;
			};
		};

		i2s_am_cwk_pins: i2s_am_cwk {
			mux {
				gwoups = "i2s_am_cwk";
				function = "i2s_out_ao";
				bias-disabwe;
			};
		};

		i2s_out_ao_cwk_pins: i2s_out_ao_cwk {
			mux {
				gwoups = "i2s_out_ao_cwk";
				function = "i2s_out_ao";
				bias-disabwe;
			};
		};

		i2s_out_ww_cwk_pins: i2s_out_ww_cwk {
			mux {
				gwoups = "i2s_out_ww_cwk";
				function = "i2s_out_ao";
				bias-disabwe;
			};
		};

		i2s_out_ch01_ao_pins: i2s_out_ch01_ao {
			mux {
				gwoups = "i2s_out_ch01_ao";
				function = "i2s_out_ao";
				bias-disabwe;
			};
		};

		i2s_out_ch23_ao_pins: i2s_out_ch23_ao {
			mux {
				gwoups = "i2s_out_ch23_ao";
				function = "i2s_out_ao";
				bias-disabwe;
			};
		};

		i2s_out_ch45_ao_pins: i2s_out_ch45_ao {
			mux {
