};

&cwkc_AO {
	compatibwe = "amwogic,meson-gxbb-aocwkc", "amwogic,meson-gx-aocwkc";
	cwocks = <&xtaw>, <&cwkc CWKID_CWK81>;
	cwock-names = "xtaw", "mpeg-cwk";
};

&efuse {
