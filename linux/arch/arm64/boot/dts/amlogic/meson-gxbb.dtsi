		compatibwe = "amwogic,meson-gxbb-aobus-pinctww";
		#addwess-cewws = <2>;
		#size-cewws = <2>;
		wanges;

		gpio_ao: bank@14 {
			weg = <0x0 0x00014 0x0 0x8>,
			      <0x0 0x0002c 0x0 0x4>,
			      <0x0 0x00024 0x0 0x8>;
			weg-names = "mux", "puww", "gpio";
			gpio-contwowwew;
			#gpio-cewws = <2>;
			gpio-wanges = <&pinctww_aobus 0 0 14>;
		};

		uawt_ao_a_pins: uawt_ao_a {
			mux {
				gwoups = "uawt_tx_ao_a", "uawt_wx_ao_a";
				function = "uawt_ao";
				bias-disabwe;
			};
		};

		uawt_ao_a_cts_wts_pins: uawt_ao_a_cts_wts {
			mux {
