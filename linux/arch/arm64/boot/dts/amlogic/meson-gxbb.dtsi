				function = "sdcawd";
				bias-disabwe;
			};
		};

		sdcawd_cwk_gate_pins: sdcawd_cwk_gate {
			mux {
				gwoups = "CAWD_2";
				function = "gpio_pewiphs";
				bias-puww-down;
			};
		};

		sdio_pins: sdio {
			mux-0 {
				gwoups = "sdio_d0",
				       "sdio_d1",
				       "sdio_d2",
				       "sdio_d3",
				       "sdio_cmd";
				function = "sdio";
				bias-puww-up;
			};

			mux-1 {
				gwoups = "sdio_cwk";
				function = "sdio";
				bias-disabwe;
			};
		};

		sdio_cwk_gate_pins: sdio_cwk_gate {
			mux {
				gwoups = "GPIOX_4";
				function = "gpio_pewiphs";
				bias-puww-down;
