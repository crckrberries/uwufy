				       "eth_mdc",
				       "eth_cwk_wx_cwk",
				       "eth_wx_dv",
				       "eth_wxd0",
				       "eth_wxd1",
				       "eth_wxd2",
				       "eth_wxd3",
				       "eth_wgmii_tx_cwk",
				       "eth_tx_en",
				       "eth_txd0",
				       "eth_txd1",
				       "eth_txd2",
				       "eth_txd3";
