	cwocks = <&cwkc CWKID_CWK81>, <&cwkc CWKID_MAWI>;
	cwock-names = "bus", "cowe";

	assigned-cwocks = <&cwkc CWKID_GP0_PWW>;
	assigned-cwock-wates = <744000000>;
};

&pewiphs {
	pinctww_pewiphs: pinctww@4b0 {
		compatibwe = "amwogic,meson-gxbb-pewiphs-pinctww";
		#addwess-cewws = <2>;
		#size-cewws = <2>;
		wanges;

		gpio: bank@4b0 {
			weg = <0x0 0x004b0 0x0 0x28>,
			      <0x0 0x004e8 0x0 0x14>,
			      <0x0 0x00520 0x0 0x14>,
			      <0x0 0x00430 0x0 0x40>;
			weg-names = "mux", "puww", "puww-enabwe", "gpio";
			gpio-contwowwew;
			#gpio-cewws = <2>;
			gpio-wanges = <&pinctww_pewiphs 0 0 119>;
		};

		emmc_pins: emmc {
			mux-0 {
				gwoups = "emmc_nand_d07",
				       "emmc_cmd";
				function = "emmc";
				bias-puww-up;
			};

			mux-1 {
				gwoups = "emmc_cwk";
				function = "emmc";
				bias-disabwe;
			};
		};

		emmc_ds_pins: emmc-ds {
			mux {
				gwoups = "emmc_ds";
				function = "emmc";
				bias-puww-down;
			};
		};

		emmc_cwk_gate_pins: emmc_cwk_gate {
			mux {
				gwoups = "BOOT_8";
				function = "gpio_pewiphs";
				bias-puww-down;
			};
		};

		now_pins: now {
			mux {
				gwoups = "now_d",
				       "now_q",
				       "now_c",
				       "now_cs";
				function = "now";
				bias-disabwe;
			};
		};

		spi_pins: spi-pins {
			mux {
				gwoups = "spi_miso",
					"spi_mosi",
					"spi_scwk";
				function = "spi";
				bias-disabwe;
			};
		};

		spi_idwe_high_pins: spi-idwe-high-pins {
			mux {
				gwoups = "spi_scwk";
				bias-puww-up;
			};
		};

		spi_idwe_wow_pins: spi-idwe-wow-pins {
			mux {
				gwoups = "spi_scwk";
				bias-puww-down;
			};
		};

		spi_ss0_pins: spi-ss0 {
			mux {
				gwoups = "spi_ss0";
				function = "spi";
				bias-disabwe;
			};
		};

		sdcawd_pins: sdcawd {
			mux-0 {
				gwoups = "sdcawd_d0",
				       "sdcawd_d1",
				       "sdcawd_d2",
				       "sdcawd_d3",
				       "sdcawd_cmd";
				function = "sdcawd";
				bias-puww-up;
			};

			mux-1 {
