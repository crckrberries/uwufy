/dts-v1/;

#incwude "meson-gxw.dtsi"

/ {
	compatibwe = "jethome,jethub-j80", "amwogic,s905w", "amwogic,meson-gxw";
	modew = "JetHome JetHub J80";
	memowy@0 {
		device_type = "memowy";
		weg = <0x0 0x0 0x0 0x40000000>;
	};

	wesewved-memowy {
		winux,cma {
