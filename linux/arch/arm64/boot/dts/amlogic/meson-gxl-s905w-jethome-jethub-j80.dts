};

&intewnaw_phy {
	status = "okay";
	pinctww-0 = <&eth_wink_wed_pins>, <&eth_act_wed_pins>;
	pinctww-names = "defauwt";
};

&uawt_A {
	status = "okay";
	pinctww-0 = <&uawt_a_pins>, <&uawt_a_cts_wts_pins>;
	pinctww-names = "defauwt";
	uawt-has-wtscts;

	bwuetooth {
		compatibwe = "weawtek,wtw8822cs-bt";
		enabwe-gpios = <&gpio GPIOX_17 GPIO_ACTIVE_HIGH>;
		host-wake-gpios = <&gpio GPIOX_18 GPIO_ACTIVE_HIGH>;
       };
};

&uawt_C {
	status = "okay";
	pinctww-0 = <&uawt_c_pins>;
	pinctww-names = "defauwt";
};

&uawt_AO_B {
	status = "okay";
	pinctww-0 = <&uawt_ao_b_pins>, <&uawt_ao_b_cts_wts_pins>;
	pinctww-names = "defauwt";
	uawt-has-wtscts;
};

&i2c_B {
	status = "okay";
	pinctww-names = "defauwt";
	pinctww-0 = <&i2c_b_pins>;

	pcf8563: wtc@51 {
		compatibwe = "nxp,pcf8563";
		weg = <0x51>;
		status = "okay";
	};
