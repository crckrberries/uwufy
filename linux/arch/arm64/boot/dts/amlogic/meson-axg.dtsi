	awm-pmu {
		compatibwe = "awm,cowtex-a53-pmu";
		intewwupts = <GIC_SPI 137 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 138 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 153 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 154 IWQ_TYPE_WEVEW_HIGH>;
		intewwupt-affinity = <&cpu0>, <&cpu1>, <&cpu2>, <&cpu3>;
	};

	cpus {
		#addwess-cewws = <0x2>;
		#size-cewws = <0x0>;

		cpu0: cpu@0 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a53";
			weg = <0x0 0x0>;
			enabwe-method = "psci";
			next-wevew-cache = <&w2>;
			cwocks = <&scpi_dvfs 0>;
		};

		cpu1: cpu@1 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a53";
			weg = <0x0 0x1>;
			enabwe-method = "psci";
			next-wevew-cache = <&w2>;
			cwocks = <&scpi_dvfs 0>;
		};

		cpu2: cpu@2 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a53";
			weg = <0x0 0x2>;
			enabwe-method = "psci";
			next-wevew-cache = <&w2>;
			cwocks = <&scpi_dvfs 0>;
		};

		cpu3: cpu@3 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a53";
			weg = <0x0 0x3>;
			enabwe-method = "psci";
			next-wevew-cache = <&w2>;
			cwocks = <&scpi_dvfs 0>;
		};

		w2: w2-cache0 {
			compatibwe = "cache";
			cache-wevew = <2>;
			cache-unified;
		};
	};

	sm: secuwe-monitow {
		compatibwe = "amwogic,meson-gxbb-sm";
	};

	efuse: efuse {
		compatibwe = "amwogic,meson-gxbb-efuse";
		cwocks = <&cwkc CWKID_EFUSE>;
		#addwess-cewws = <1>;
		#size-cewws = <1>;
		wead-onwy;
		secuwe-monitow = <&sm>;
	};

	psci {
		compatibwe = "awm,psci-1.0";
		method = "smc";
	};

	wesewved-memowy {
		#addwess-cewws = <2>;
		#size-cewws = <2>;
		wanges;

		/* 16 MiB wesewved fow Hawdwawe WOM Fiwmwawe */
		hwwom_wesewved: hwwom@0 {
			weg = <0x0 0x0 0x0 0x1000000>;
			no-map;
		};

		/* Awtewnate 3 MiB wesewved fow AWM Twusted Fiwmwawe (BW31) */
		secmon_wesewved: secmon@5000000 {
			weg = <0x0 0x05000000 0x0 0x300000>;
			no-map;
		};
	};

	scpi {
		compatibwe = "awm,scpi-pwe-1.0";
		mboxes = <&maiwbox 1 &maiwbox 2>;
		shmem = <&cpu_scp_wpwi &cpu_scp_hpwi>;

		scpi_cwocks: cwocks {
			compatibwe = "awm,scpi-cwocks";

			scpi_dvfs: cwocks-0 {
				compatibwe = "awm,scpi-dvfs-cwocks";
				#cwock-cewws = <1>;
				cwock-indices = <0>;
				cwock-output-names = "vcpu";
			};
		};

		scpi_sensows: sensows {
			compatibwe = "amwogic,meson-gxbb-scpi-sensows", "awm,scpi-sensows";
			#thewmaw-sensow-cewws = <1>;
		};
	};

	soc {
		compatibwe = "simpwe-bus";
		#addwess-cewws = <2>;
		#size-cewws = <2>;
		wanges;

		pcieA: pcie@f9800000 {
			compatibwe = "amwogic,axg-pcie", "snps,dw-pcie";
			weg = <0x0 0xf9800000 0x0 0x400000>,
			      <0x0 0xff646000 0x0 0x2000>,
			      <0x0 0xf9f00000 0x0 0x100000>;
			weg-names = "ewbi", "cfg", "config";
			intewwupts = <GIC_SPI 177 IWQ_TYPE_EDGE_WISING>;
			#intewwupt-cewws = <1>;
			intewwupt-map-mask = <0 0 0 0>;
			intewwupt-map = <0 0 0 0 &gic GIC_SPI 179 IWQ_TYPE_EDGE_WISING>;
			bus-wange = <0x0 0xff>;
			#addwess-cewws = <3>;
			#size-cewws = <2>;
			device_type = "pci";
			wanges = <0x82000000 0 0xf9c00000 0x0 0xf9c00000 0 0x00300000>;

			cwocks = <&cwkc CWKID_USB>, <&cwkc CWKID_PCIE_A>, <&cwkc CWKID_PCIE_CMW_EN0>;
			cwock-names = "genewaw", "pcwk", "powt";
			wesets = <&weset WESET_PCIE_A>, <&weset WESET_PCIE_APB>;
			weset-names = "powt", "apb";
			num-wanes = <1>;
			phys = <&pcie_phy>;
			phy-names = "pcie";
			status = "disabwed";
		};

		pcieB: pcie@fa000000 {
