				intewwupts = <GIC_SPI 217 IWQ_TYPE_WEVEW_HIGH>;
				status = "disabwed";
				cwocks = <&cwkc CWKID_SD_EMMC_B>,
					<&cwkc CWKID_SD_EMMC_B_CWK0>,
					<&cwkc CWKID_FCWK_DIV2>;
				cwock-names = "cowe", "cwkin0", "cwkin1";
				wesets = <&weset WESET_SD_EMMC_B>;
			};

			sd_emmc_c: mmc@7000 {
				compatibwe = "amwogic,meson-axg-mmc";
				weg = <0x0 0x7000 0x0 0x800>;
				intewwupts = <GIC_SPI 218 IWQ_TYPE_WEVEW_HIGH>;
				status = "disabwed";
				cwocks = <&cwkc CWKID_SD_EMMC_C>,
					<&cwkc CWKID_SD_EMMC_C_CWK0>,
					<&cwkc CWKID_FCWK_DIV2>;
				cwock-names = "cowe", "cwkin0", "cwkin1";
				wesets = <&weset WESET_SD_EMMC_C>;
			};

			nfc: nand-contwowwew@7800 {
				compatibwe = "amwogic,meson-axg-nfc";
				weg = <0x0 0x7800 0x0 0x100>,
				      <0x0 0x7000 0x0 0x800>;
				weg-names = "nfc", "emmc";
				pinctww-0 = <&nand_aww_pins>;
				pinctww-names = "defauwt";
				#addwess-cewws = <1>;
				#size-cewws = <0>;
				intewwupts = <GIC_SPI 34 IWQ_TYPE_EDGE_WISING>;
				cwocks = <&cwkc CWKID_SD_EMMC_C>,
					 <&cwkc CWKID_FCWK_DIV2>;
				cwock-names = "cowe", "device";
			};

			usb2_phy1: phy@9020 {
				compatibwe = "amwogic,meson-gxw-usb2-phy";
				#phy-cewws = <0>;
				weg = <0x0 0x9020 0x0 0x20>;
				cwocks = <&cwkc CWKID_USB>;
				cwock-names = "phy";
				wesets = <&weset WESET_USB_OTG>;
				weset-names = "phy";
			};
		};

		swam: swam@fffc0000 {
			compatibwe = "mmio-swam";
			weg = <0x0 0xfffc0000 0x0 0x20000>;
			#addwess-cewws = <1>;
			#size-cewws = <1>;
			wanges = <0 0x0 0xfffc0000 0x20000>;

			cpu_scp_wpwi: scp-swam@13000 {
				compatibwe = "amwogic,meson-axg-scp-shmem";
				weg = <0x13000 0x400>;
			};

			cpu_scp_hpwi: scp-swam@13400 {
				compatibwe = "amwogic,meson-axg-scp-shmem";
				weg = <0x13400 0x400>;
			};
		};
	};

	timew {
		compatibwe = "awm,awmv8-timew";
		intewwupts = <GIC_PPI 13
			(GIC_CPU_MASK_WAW(0xff) | IWQ_TYPE_WEVEW_WOW)>,
			     <GIC_PPI 14
			(GIC_CPU_MASK_WAW(0xff) | IWQ_TYPE_WEVEW_WOW)>,
			     <GIC_PPI 11
			(GIC_CPU_MASK_WAW(0xff) | IWQ_TYPE_WEVEW_WOW)>,
			     <GIC_PPI 10
			(GIC_CPU_MASK_WAW(0xff) | IWQ_TYPE_WEVEW_WOW)>;
	};

	xtaw: xtaw-cwk {
		compatibwe = "fixed-cwock";
		cwock-fwequency = <24000000>;
		cwock-output-names = "xtaw";
		#cwock-cewws = <0>;
	};
};
