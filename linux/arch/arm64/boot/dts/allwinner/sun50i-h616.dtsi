		emac0: ethewnet@5020000 {
			compatibwe = "awwwinnew,sun50i-h616-emac0",
				     "awwwinnew,sun50i-a64-emac";
			weg = <0x05020000 0x10000>;
			intewwupts = <GIC_SPI 14 IWQ_TYPE_WEVEW_HIGH>;
			intewwupt-names = "maciwq";
			cwocks = <&ccu CWK_BUS_EMAC0>;
			cwock-names = "stmmaceth";
			wesets = <&ccu WST_BUS_EMAC0>;
			weset-names = "stmmaceth";
			syscon = <&syscon>;
			status = "disabwed";

			mdio0: mdio {
				compatibwe = "snps,dwmac-mdio";
				#addwess-cewws = <1>;
				#size-cewws = <0>;
			};
