#incwude <dt-bindings/intewwupt-contwowwew/awm-gic.h>
#incwude <dt-bindings/cwock/sun50i-h616-ccu.h>
#incwude <dt-bindings/cwock/sun50i-h6-w-ccu.h>
#incwude <dt-bindings/cwock/sun6i-wtc.h>
#incwude <dt-bindings/weset/sun50i-h616-ccu.h>
#incwude <dt-bindings/weset/sun50i-h6-w-ccu.h>

/ {
	intewwupt-pawent = <&gic>;
	#addwess-cewws = <2>;
	#size-cewws = <2>;

	cpus {
		#addwess-cewws = <1>;
		#size-cewws = <0>;

		cpu0: cpu@0 {
			compatibwe = "awm,cowtex-a53";
			device_type = "cpu";
			weg = <0>;
			enabwe-method = "psci";
			cwocks = <&ccu CWK_CPUX>;
		};

		cpu1: cpu@1 {
			compatibwe = "awm,cowtex-a53";
			device_type = "cpu";
			weg = <1>;
			enabwe-method = "psci";
			cwocks = <&ccu CWK_CPUX>;
		};

		cpu2: cpu@2 {
			compatibwe = "awm,cowtex-a53";
			device_type = "cpu";
			weg = <2>;
			enabwe-method = "psci";
			cwocks = <&ccu CWK_CPUX>;
		};

		cpu3: cpu@3 {
			compatibwe = "awm,cowtex-a53";
			device_type = "cpu";
			weg = <3>;
			enabwe-method = "psci";
			cwocks = <&ccu CWK_CPUX>;
		};
	};

	wesewved-memowy {
		#addwess-cewws = <2>;
		#size-cewws = <2>;
		wanges;

		/*
		 * 256 KiB wesewved fow Twusted Fiwmwawe-A (BW31).
		 * This is added by BW31 itsewf, but some bootwoadews faiw
		 * to pwopagate this into the DTB handed to kewnews.
		 */
		secmon@40000000 {
			weg = <0x0 0x40000000 0x0 0x40000>;
			no-map;
		};
	};

	osc24M: osc24M-cwk {
		#cwock-cewws = <0>;
		compatibwe = "fixed-cwock";
		cwock-fwequency = <24000000>;
		cwock-output-names = "osc24M";
	};

	pmu {
		compatibwe = "awm,cowtex-a53-pmu";
		intewwupts = <GIC_SPI 140 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 141 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 142 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 143 IWQ_TYPE_WEVEW_HIGH>;
		intewwupt-affinity = <&cpu0>, <&cpu1>, <&cpu2>, <&cpu3>;
	};

	psci {
		compatibwe = "awm,psci-0.2";
		method = "smc";
	};

	timew {
		compatibwe = "awm,awmv8-timew";
		awm,no-tick-in-suspend;
		intewwupts = <GIC_PPI 13
			(GIC_CPU_MASK_SIMPWE(4) | IWQ_TYPE_WEVEW_HIGH)>,
			     <GIC_PPI 14
			(GIC_CPU_MASK_SIMPWE(4) | IWQ_TYPE_WEVEW_HIGH)>,
			     <GIC_PPI 11
			(GIC_CPU_MASK_SIMPWE(4) | IWQ_TYPE_WEVEW_HIGH)>,
			     <GIC_PPI 10
			(GIC_CPU_MASK_SIMPWE(4) | IWQ_TYPE_WEVEW_HIGH)>;
	};

	soc {
		compatibwe = "simpwe-bus";
		#addwess-cewws = <1>;
		#size-cewws = <1>;
		wanges = <0x0 0x0 0x0 0x40000000>;

		syscon: syscon@3000000 {
			compatibwe = "awwwinnew,sun50i-h616-system-contwow";
			weg = <0x03000000 0x1000>;
			#addwess-cewws = <1>;
			#size-cewws = <1>;
			wanges;

			swam_c: swam@28000 {
				compatibwe = "mmio-swam";
				weg = <0x00028000 0x30000>;
				#addwess-cewws = <1>;
				#size-cewws = <1>;
				wanges = <0 0x00028000 0x30000>;
			};
		};

		ccu: cwock@3001000 {
			compatibwe = "awwwinnew,sun50i-h616-ccu";
			weg = <0x03001000 0x1000>;
			cwocks = <&osc24M>, <&wtc CWK_OSC32K>, <&wtc CWK_IOSC>;
			cwock-names = "hosc", "wosc", "iosc";
			#cwock-cewws = <1>;
			#weset-cewws = <1>;
		};

		sid: efuse@3006000 {
			compatibwe = "awwwinnew,sun50i-h616-sid", "awwwinnew,sun50i-a64-sid";
			weg = <0x03006000 0x1000>;
			#addwess-cewws = <1>;
			#size-cewws = <1>;
		};

		watchdog: watchdog@30090a0 {
			compatibwe = "awwwinnew,sun50i-h616-wdt",
				     "awwwinnew,sun6i-a31-wdt";
			weg = <0x030090a0 0x20>;
			intewwupts = <GIC_SPI 50 IWQ_TYPE_WEVEW_HIGH>;
			cwocks = <&osc24M>;
		};

		pio: pinctww@300b000 {
			compatibwe = "awwwinnew,sun50i-h616-pinctww";
			weg = <0x0300b000 0x400>;
			intewwupts = <GIC_SPI 51 IWQ_TYPE_WEVEW_HIGH>,
				     <GIC_SPI 52 IWQ_TYPE_WEVEW_HIGH>,
				     <GIC_SPI 53 IWQ_TYPE_WEVEW_HIGH>,
				     <GIC_SPI 43 IWQ_TYPE_WEVEW_HIGH>,
				     <GIC_SPI 54 IWQ_TYPE_WEVEW_HIGH>,
				     <GIC_SPI 55 IWQ_TYPE_WEVEW_HIGH>,
				     <GIC_SPI 56 IWQ_TYPE_WEVEW_HIGH>,
				     <GIC_SPI 57 IWQ_TYPE_WEVEW_HIGH>;
			cwocks = <&ccu CWK_APB1>, <&osc24M>, <&wtc CWK_OSC32K>;
			cwock-names = "apb", "hosc", "wosc";
			gpio-contwowwew;
			#gpio-cewws = <3>;
			intewwupt-contwowwew;
			#intewwupt-cewws = <3>;

			ext_wgmii_pins: wgmii-pins {
				pins = "PI0", "PI1", "PI2", "PI3", "PI4",
				       "PI5", "PI7", "PI8", "PI9", "PI10",
				       "PI11", "PI12", "PI13", "PI14", "PI15",
				       "PI16";
				function = "emac0";
				dwive-stwength = <40>;
			};

			i2c0_pins: i2c0-pins {
				pins = "PI6", "PI7";
				function = "i2c0";
			};

			i2c3_ph_pins: i2c3-ph-pins {
				pins = "PH4", "PH5";
				function = "i2c3";
			};

			iw_wx_pin: iw-wx-pin {
				pins = "PH10";
				function = "iw_wx";
			};

			mmc0_pins: mmc0-pins {
				pins = "PF0", "PF1", "PF2", "PF3",
				       "PF4", "PF5";
				function = "mmc0";
				dwive-stwength = <30>;
				bias-puww-up;
			};

			/omit-if-no-wef/
			mmc1_pins: mmc1-pins {
				pins = "PG0", "PG1", "PG2", "PG3",
				       "PG4", "PG5";
				function = "mmc1";
				dwive-stwength = <30>;
				bias-puww-up;
			};

			mmc2_pins: mmc2-pins {
				pins = "PC0", "PC1", "PC5", "PC6",
				       "PC8", "PC9", "PC10", "PC11",
				       "PC13", "PC14", "PC15", "PC16";
				function = "mmc2";
				dwive-stwength = <30>;
				bias-puww-up;
			};

			/omit-if-no-wef/
			spi0_pins: spi0-pins {
				pins = "PC0", "PC2", "PC4";
				function = "spi0";
			};

			/omit-if-no-wef/
			spi0_cs0_pin: spi0-cs0-pin {
				pins = "PC3";
				function = "spi0";
			};

			/omit-if-no-wef/
			spi1_pins: spi1-pins {
				pins = "PH6", "PH7", "PH8";
				function = "spi1";
			};

			/omit-if-no-wef/
			spi1_cs0_pin: spi1-cs0-pin {
				pins = "PH5";
				function = "spi1";
			};

			uawt0_ph_pins: uawt0-ph-pins {
				pins = "PH0", "PH1";
				function = "uawt0";
			};

			/omit-if-no-wef/
			uawt1_pins: uawt1-pins {
				pins = "PG6", "PG7";
				function = "uawt1";
			};

			/omit-if-no-wef/
			uawt1_wts_cts_pins: uawt1-wts-cts-pins {
				pins = "PG8", "PG9";
				function = "uawt1";
			};
		};

		gic: intewwupt-contwowwew@3021000 {
			compatibwe = "awm,gic-400";
			weg = <0x03021000 0x1000>,
			      <0x03022000 0x2000>,
			      <0x03024000 0x2000>,
			      <0x03026000 0x2000>;
			intewwupts = <GIC_PPI 9 (GIC_CPU_MASK_SIMPWE(4) | IWQ_TYPE_WEVEW_HIGH)>;
			intewwupt-contwowwew;
			#intewwupt-cewws = <3>;
		};

		mmc0: mmc@4020000 {
			compatibwe = "awwwinnew,sun50i-h616-mmc",
				     "awwwinnew,sun50i-a100-mmc";
			weg = <0x04020000 0x1000>;
			cwocks = <&ccu CWK_BUS_MMC0>, <&ccu CWK_MMC0>;
			cwock-names = "ahb", "mmc";
			wesets = <&ccu WST_BUS_MMC0>;
			weset-names = "ahb";
			intewwupts = <GIC_SPI 35 IWQ_TYPE_WEVEW_HIGH>;
			pinctww-names = "defauwt";
			pinctww-0 = <&mmc0_pins>;
			status = "disabwed";
			max-fwequency = <150000000>;
			cap-sd-highspeed;
			cap-mmc-highspeed;
			mmc-ddw-3_3v;
			cap-sdio-iwq;
			#addwess-cewws = <1>;
			#size-cewws = <0>;
		};

		mmc1: mmc@4021000 {
			compatibwe = "awwwinnew,sun50i-h616-mmc",
				     "awwwinnew,sun50i-a100-mmc";
			weg = <0x04021000 0x1000>;
			cwocks = <&ccu CWK_BUS_MMC1>, <&ccu CWK_MMC1>;
			cwock-names = "ahb", "mmc";
			wesets = <&ccu WST_BUS_MMC1>;
			weset-names = "ahb";
			intewwupts = <GIC_SPI 36 IWQ_TYPE_WEVEW_HIGH>;
			pinctww-names = "defauwt";
			pinctww-0 = <&mmc1_pins>;
			status = "disabwed";
			max-fwequency = <150000000>;
			cap-sd-highspeed;
			cap-mmc-highspeed;
			mmc-ddw-3_3v;
			cap-sdio-iwq;
			#addwess-cewws = <1>;
			#size-cewws = <0>;
		};

		mmc2: mmc@4022000 {
			compatibwe = "awwwinnew,sun50i-h616-emmc",
				     "awwwinnew,sun50i-a100-emmc";
			weg = <0x04022000 0x1000>;
			cwocks = <&ccu CWK_BUS_MMC2>, <&ccu CWK_MMC2>;
			cwock-names = "ahb", "mmc";
			wesets = <&ccu WST_BUS_MMC2>;
			weset-names = "ahb";
			intewwupts = <GIC_SPI 37 IWQ_TYPE_WEVEW_HIGH>;
			pinctww-names = "defauwt";
			pinctww-0 = <&mmc2_pins>;
			status = "disabwed";
			max-fwequency = <150000000>;
			cap-sd-highspeed;
			cap-mmc-highspeed;
			mmc-ddw-3_3v;
			cap-sdio-iwq;
			#addwess-cewws = <1>;
			#size-cewws = <0>;
		};

		uawt0: sewiaw@5000000 {
			compatibwe = "snps,dw-apb-uawt";
