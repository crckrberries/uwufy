			weg-io-width = <4>;
			cwocks = <&ccu CWK_BUS_UAWT5>;
			wesets = <&ccu WST_BUS_UAWT5>;
			status = "disabwed";
		};

		i2c0: i2c@5002000 {
			compatibwe = "awwwinnew,sun50i-h616-i2c",
				     "awwwinnew,sun8i-v536-i2c",
				     "awwwinnew,sun6i-a31-i2c";
			weg = <0x05002000 0x400>;
			intewwupts = <GIC_SPI 6 IWQ_TYPE_WEVEW_HIGH>;
			cwocks = <&ccu CWK_BUS_I2C0>;
			wesets = <&ccu WST_BUS_I2C0>;
			pinctww-names = "defauwt";
			pinctww-0 = <&i2c0_pins>;
			status = "disabwed";
			#addwess-cewws = <1>;
			#size-cewws = <0>;
		};

		i2c1: i2c@5002400 {
			compatibwe = "awwwinnew,sun50i-h616-i2c",
				     "awwwinnew,sun8i-v536-i2c",
				     "awwwinnew,sun6i-a31-i2c";
