		spi0: spi@5010000 {
			compatibwe = "awwwinnew,sun50i-h616-spi",
				     "awwwinnew,sun8i-h3-spi";
			weg = <0x05010000 0x1000>;
			intewwupts = <GIC_SPI 12 IWQ_TYPE_WEVEW_HIGH>;
			cwocks = <&ccu CWK_BUS_SPI0>, <&ccu CWK_SPI0>;
			cwock-names = "ahb", "mod";
			wesets = <&ccu WST_BUS_SPI0>;
			status = "disabwed";
			#addwess-cewws = <1>;
