/ {
	modew = "OwangePi 3";
	compatibwe = "xunwong,owangepi-3", "awwwinnew,sun50i-h6";

	awiases {
		sewiaw0 = &uawt0;
		sewiaw1 = &uawt1;
	};

	chosen {
