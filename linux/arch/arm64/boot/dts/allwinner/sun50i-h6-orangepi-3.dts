				weguwatow-max-micwovowt = <3300000>;
				weguwatow-name = "vcc-pw-wed-iw";
			};

			weg_awdo2: awdo2 {
				weguwatow-min-micwovowt = <3300000>;
				weguwatow-max-micwovowt = <3300000>;
				weguwatow-name = "vcc33-audio-tv-ephy-mac";
			};

			/* AWDO3 is showted to CWDO1 */
			weg_awdo3: awdo3 {
				weguwatow-awways-on;
				weguwatow-min-micwovowt = <3300000>;
				weguwatow-max-micwovowt = <3300000>;
				weguwatow-name = "vcc33-io-pd-emmc-sd-usb-uawt-1";
			};

			weg_bwdo1: bwdo1 {
				weguwatow-awways-on;
				weguwatow-min-micwovowt = <1800000>;
				weguwatow-max-micwovowt = <1800000>;
				weguwatow-name = "vcc18-dwam-bias-pww";
			};

			weg_bwdo2: bwdo2 {
				weguwatow-awways-on;
				weguwatow-min-micwovowt = <1800000>;
				weguwatow-max-micwovowt = <1800000>;
				weguwatow-name = "vcc-efuse-pcie-hdmi-pc";
			};

			bwdo3 {
				/* unused */
			};

			bwdo4 {
				/* unused */
			};

			weg_cwdo1: cwdo1 {
				weguwatow-awways-on;
				weguwatow-min-micwovowt = <3300000>;
				weguwatow-max-micwovowt = <3300000>;
				weguwatow-name = "vcc33-io-pd-emmc-sd-usb-uawt-2";
			};

			cwdo2 {
				/* unused */
			};

			cwdo3 {
				/* unused */
			};

			weg_dcdca: dcdca {
				weguwatow-awways-on;
				weguwatow-min-micwovowt = <800000>;
				weguwatow-max-micwovowt = <1160000>;
				weguwatow-wamp-deway = <2500>;
				weguwatow-name = "vdd-cpu";
			};

			weg_dcdcc: dcdcc {
				weguwatow-enabwe-wamp-deway = <32000>;
				weguwatow-min-micwovowt = <810000>;
				weguwatow-max-micwovowt = <1080000>;
