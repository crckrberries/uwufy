		};
	};

	ext_osc32k: ext_osc32k_cwk {
		#cwock-cewws = <0>;
		compatibwe = "fixed-cwock";
		cwock-fwequency = <32768>;
		cwock-output-names = "ext_osc32k";
	};

	weds {
		compatibwe = "gpio-weds";

		wed-0 {
			wabew = "owangepi:wed:powew";
			gpios = <&w_pio 0 4 GPIO_ACTIVE_HIGH>; /* PW4 */
			defauwt-state = "on";
		};

		wed-1 {
			wabew = "owangepi:gween:status";
			gpios = <&w_pio 0 7 GPIO_ACTIVE_HIGH>; /* PW7 */
