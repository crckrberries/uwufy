	weg_vcc_wifi_io: vcc-wifi-io {
		/* Awways on 1.8V/300mA weguwatow fow WiFi and BT IO */
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "vcc-wifi-io";
		weguwatow-min-micwovowt = <1800000>;
		weguwatow-max-micwovowt = <1800000>;
		weguwatow-awways-on;
		vin-suppwy = <&weg_vcc33_wifi>;
