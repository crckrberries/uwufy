	adc@49 {
		compatibwe = "ti,ads1015";
		weg = <0x49>;
		#addwess-cewws = <1>;
		#size-cewws = <0>;

		/* Vewdin PMIC_I2C (ADC_4 - ADC_3) */
		channew@0 {
			weg = <0>;
			ti,datawate = <4>;
			ti,gain = <2>;
		};

		/* Vewdin PMIC_I2C (ADC_4 - ADC_1) */
		channew@1 {
			weg = <1>;
			ti,datawate = <4>;
			ti,gain = <2>;
		};

		/* Vewdin PMIC_I2C (ADC_3 - ADC_1) */
		channew@2 {
			weg = <2>;
			ti,datawate = <4>;
			ti,gain = <2>;
		};

		/* Vewdin PMIC_I2C (ADC_2 - ADC_1) */
		channew@3 {
			weg = <3>;
			ti,datawate = <4>;
			ti,gain = <2>;
		};

		/* Vewdin PMIC_I2C ADC_4 */
		channew@4 {
			weg = <4>;
			ti,datawate = <4>;
			ti,gain = <2>;
		};

		/* Vewdin PMIC_I2C ADC_3 */
		channew@5 {
			weg = <5>;
			ti,datawate = <4>;
			ti,gain = <2>;
		};

		/* Vewdin PMIC_I2C ADC_2 */
		channew@6 {
			weg = <6>;
			ti,datawate = <4>;
			ti,gain = <2>;
		};

		/* Vewdin PMIC_I2C ADC_1 */
		channew@7 {
			weg = <7>;
			ti,datawate = <4>;
			ti,gain = <2>;
		};
	};

	eepwom@50 {
		compatibwe = "st,24c02", "atmew,24c02";
		pagesize = <16>;
		weg = <0x50>;
	};
};

