				weguwatow-min-micwovowt = <800000>;
				weguwatow-name = "On-moduwe +V0.8_SNVS (WDO2)";
			};

			weg_vdda: WDO3 {
				weguwatow-awways-on;
				weguwatow-boot-on;
				weguwatow-max-micwovowt = <1800000>;
				weguwatow-min-micwovowt = <1800000>;
				weguwatow-name = "On-moduwe +V1.8A (WDO3)";
