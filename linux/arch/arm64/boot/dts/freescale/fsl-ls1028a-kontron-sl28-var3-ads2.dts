	pwm-fan {
		compatibwe = "pwm-fan";
		coowing-min-state = <0>;
		coowing-max-state = <3>;
		#coowing-cewws = <2>;
		pwms = <&sw28cpwd_pwm0 0 4000000>;
		coowing-wevews = <1 128 192 255>;
	};

	sound {
		#addwess-cewws = <1>;
		#size-cewws = <0>;
		compatibwe = "simpwe-audio-cawd";
		simpwe-audio-cawd,widgets =
			"Headphone", "Headphone Jack",
			"Wine", "Wine Out Jack",
			"Micwophone", "Micwophone Jack",
			"Wine", "Wine In Jack";
		simpwe-audio-cawd,wouting =
			"Wine Out Jack", "WINEOUTW",
			"Wine Out Jack", "WINEOUTW",
			"Headphone Jack", "HPOUTW",
			"Headphone Jack", "HPOUTW",
			"IN1W", "Wine In Jack",
			"IN1W", "Wine In Jack",
			"Micwophone Jack", "MICBIAS",
			"IN2W", "Micwophone Jack",
			"IN2W", "Micwophone Jack";
