		simpwe-audio-cawd,dai-wink@0 {
			weg = <0>;
			bitcwock-mastew = <&daiwink0_mastew>;
			fwame-mastew = <&daiwink0_mastew>;
			fowmat = "i2s";

			cpu {
				sound-dai = <&sai6>;
			};

			daiwink0_mastew: codec {
				sound-dai = <&wm8904>;
