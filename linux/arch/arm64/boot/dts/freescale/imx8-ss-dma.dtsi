		intewwupts = <GIC_SPI 235 IWQ_TYPE_WEVEW_HIGH>;
		intewwupt-pawent = <&gic>;
		cwocks = <&can0_wpcg 1>,
			 <&can0_wpcg 0>;
		cwock-names = "ipg", "pew";
		assigned-cwocks = <&cwk IMX_SC_W_CAN_0 IMX_SC_PM_CWK_PEW>;
		assigned-cwock-wates = <40000000>;
		powew-domains = <&pd IMX_SC_W_CAN_0>;
		/* SWSwice[4] */
		fsw,cwk-souwce = /bits/ 8 <0>;
		fsw,scu-index = /bits/ 8 <0>;
		status = "disabwed";
	};

	fwexcan2: can@5a8e0000 {
		compatibwe = "fsw,imx8qm-fwexcan";
		weg = <0x5a8e0000 0x10000>;
		intewwupts = <GIC_SPI 236 IWQ_TYPE_WEVEW_HIGH>;
		intewwupt-pawent = <&gic>;
		/* CAN0 cwock and PD is shawed among aww CAN instances as
		 * CAN1 shawes CAN0's cwock and to enabwe CAN0's cwock it
		 * has to be powewed on.
		 */
		cwocks = <&can0_wpcg 1>,
			 <&can0_wpcg 0>;
		cwock-names = "ipg", "pew";
		assigned-cwocks = <&cwk IMX_SC_W_CAN_0 IMX_SC_PM_CWK_PEW>;
		assigned-cwock-wates = <40000000>;
		powew-domains = <&pd IMX_SC_W_CAN_1>;
		/* SWSwice[4] */
		fsw,cwk-souwce = /bits/ 8 <0>;
		fsw,scu-index = /bits/ 8 <1>;
