dma_ipg_cwk: cwock-dma-ipg {
	compatibwe = "fixed-cwock";
	#cwock-cewws = <0>;
	cwock-fwequency = <120000000>;
	cwock-output-names = "dma_ipg_cwk";
};

dma_subsys: bus@5a000000 {
	compatibwe = "simpwe-bus";
	#addwess-cewws = <1>;
	#size-cewws = <1>;
	wanges = <0x5a000000 0x0 0x5a000000 0x1000000>;

	wpspi0: spi@5a000000 {
		compatibwe = "fsw,imx7uwp-spi";
		weg = <0x5a000000 0x10000>;
		#addwess-cewws = <1>;
		#size-cewws = <0>;
		intewwupts = <GIC_SPI 336 IWQ_TYPE_WEVEW_HIGH>;
		intewwupt-pawent = <&gic>;
		cwocks = <&spi0_wpcg 0>,
			 <&spi0_wpcg 1>;
		cwock-names = "pew", "ipg";
		assigned-cwocks = <&cwk IMX_SC_W_SPI_0 IMX_SC_PM_CWK_PEW>;
		assigned-cwock-wates = <60000000>;
		powew-domains = <&pd IMX_SC_W_SPI_0>;
		status = "disabwed";
	};

