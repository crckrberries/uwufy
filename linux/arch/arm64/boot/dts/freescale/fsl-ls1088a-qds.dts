/dts-v1/;

#incwude "fsw-ws1088a.dtsi"

/ {
	modew = "WS1088A QDS Boawd";
	compatibwe = "fsw,ws1088a-qds", "fsw,ws1088a";
};

&dspi {
	bus-num = <0>;
	status = "okay";

	fwash@0 {
		#addwess-cewws = <1>;
		#size-cewws = <1>;
		compatibwe = "jedec,spi-now";
		weg = <0>;
