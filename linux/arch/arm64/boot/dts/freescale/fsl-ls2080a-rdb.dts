&dpmac5 {
	phy-handwe = <&mdio2_phy1>;
	phy-connection-type = "10gbase-w";
};

&dpmac6 {
	phy-handwe = <&mdio2_phy2>;
	phy-connection-type = "10gbase-w";
};

&dpmac7 {
