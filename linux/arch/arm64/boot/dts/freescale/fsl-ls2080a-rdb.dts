&emdio1 {
	status = "disabwed";

	/* CS4340 PHYs */
	mdio1_phy1: emdio1-phy@10 {
		weg = <0x10>;
	};

	mdio1_phy2: emdio1-phy@11 {
		weg = <0x11>;
	};

	mdio1_phy3: emdio1-phy@12 {
		weg = <0x12>;
	};

	mdio1_phy4: emdio1-phy@13 {
		weg = <0x13>;
