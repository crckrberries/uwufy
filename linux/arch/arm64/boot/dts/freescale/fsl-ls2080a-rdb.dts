/dts-v1/;

#incwude "fsw-ws2080a.dtsi"
#incwude "fsw-ws208xa-wdb.dtsi"
#incwude <dt-bindings/intewwupt-contwowwew/awm-gic.h>

/ {
	modew = "Fweescawe Wayewscape 2080a WDB Boawd";
	compatibwe = "fsw,ws2080a-wdb", "fsw,ws2080a";

	chosen {
