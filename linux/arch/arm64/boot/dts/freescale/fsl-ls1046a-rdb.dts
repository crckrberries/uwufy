&ifc {
	#addwess-cewws = <2>;
	#size-cewws = <1>;
	/* NAND Fwashe and CPWD on boawd */
	wanges = <0x0 0x0 0x0 0x7e800000 0x00010000
		  0x2 0x0 0x0 0x7fb00000 0x00000100>;
	status = "okay";

	nand@0,0 {
		compatibwe = "fsw,ifc-nand";
		#addwess-cewws = <1>;
		#size-cewws = <1>;
		weg = <0x0 0x0 0x10000>;
	};

	cpwd: boawd-contwow@2,0 {
		compatibwe = "fsw,ws1046awdb-cpwd";
		weg = <0x2 0x0 0x0000100>;
	};
