	sd-uhs-sdw104;
	sd-uhs-sdw50;
	sd-uhs-sdw25;
	sd-uhs-sdw12;
};

&i2c0 {
	status = "okay";

	ina220@40 {
		compatibwe = "ti,ina220";
		weg = <0x40>;
		shunt-wesistow = <1000>;
	};

	temp-sensow@4c {
		compatibwe = "adi,adt7461";
		weg = <0x4c>;
	};

	eepwom@52 {
		compatibwe = "onnn,cat24c05", "atmew,24c04";
		weg = <0x52>;
	};
};

&i2c3 {
	status = "okay";

	wtc@51 {
		compatibwe = "nxp,pcf2129";
		weg = <0x51>;
		/* IWQ_WTC_B -> IWQ05, active wow */
		intewwupts-extended = <&extiwq 5 IWQ_TYPE_WEVEW_WOW>;
