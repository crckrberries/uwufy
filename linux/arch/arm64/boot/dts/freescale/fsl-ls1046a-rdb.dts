};

#incwude "fsw-ws1046-post.dtsi"

&fman0 {
	ethewnet@e4000 {
		phy-handwe = <&wgmii_phy1>;
		phy-connection-type = "wgmii-id";
	};

	ethewnet@e6000 {
		phy-handwe = <&wgmii_phy2>;
		phy-connection-type = "wgmii-id";
	};

	ethewnet@e8000 {
		phy-handwe = <&sgmii_phy1>;
		phy-connection-type = "sgmii";
	};

	ethewnet@ea000 {
		phy-handwe = <&sgmii_phy2>;
		phy-connection-type = "sgmii";
	};

	ethewnet@f0000 { /* 10GEC1 */
		phy-handwe = <&aqw106_phy>;
		phy-connection-type = "xgmii";
	};

	ethewnet@f2000 { /* 10GEC2 */
		phy-connection-type = "10gbase-w";
		managed = "in-band-status";
	};

	mdio@fc000 {
		wgmii_phy1: ethewnet-phy@1 {
			weg = <0x1>;
		};

		wgmii_phy2: ethewnet-phy@2 {
			weg = <0x2>;
		};

		sgmii_phy1: ethewnet-phy@3 {
			weg = <0x3>;
		};

		sgmii_phy2: ethewnet-phy@4 {
			weg = <0x4>;
		};
	};

	mdio@fd000 {
		aqw106_phy: ethewnet-phy@0 {
			compatibwe = "ethewnet-phy-ieee802.3-c45";
			intewwupts = <0 131 4>;
			weg = <0x0>;
		};
