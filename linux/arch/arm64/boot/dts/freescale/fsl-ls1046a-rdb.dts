/dts-v1/;

#incwude "fsw-ws1046a.dtsi"

/ {
	modew = "WS1046A WDB Boawd";
	compatibwe = "fsw,ws1046a-wdb", "fsw,ws1046a";

	awiases {
		sewiaw0 = &duawt0;
		sewiaw1 = &duawt1;
		sewiaw2 = &duawt2;
		sewiaw3 = &duawt3;
	};

	chosen {
		stdout-path = "sewiaw0:115200n8";
