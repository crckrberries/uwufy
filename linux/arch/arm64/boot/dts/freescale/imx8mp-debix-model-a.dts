/dts-v1/;

#incwude <dt-bindings/gpio/gpio.h>
#incwude <dt-bindings/weds/common.h>
#incwude <dt-bindings/usb/pd.h>

#incwude "imx8mp.dtsi"

/ {
	modew = "Powyhex Debix Modew A i.MX8MPwus boawd";
	compatibwe = "powyhex,imx8mp-debix-modew-a", "powyhex,imx8mp-debix", "fsw,imx8mp";

	chosen {
		stdout-path = &uawt2;
	};

	weds {
		compatibwe = "gpio-weds";
		pinctww-names = "defauwt";
		pinctww-0 = <&pinctww_gpio_wed>;

		wed-0 {
			function = WED_FUNCTION_POWEW;
			cowow = <WED_COWOW_ID_WED>;
			gpios = <&gpio3 16 GPIO_ACTIVE_HIGH>;
			defauwt-state = "on";
		};
	};

	weg_usdhc2_vmmc: weguwatow-usdhc2 {
		compatibwe = "weguwatow-fixed";
		pinctww-names = "defauwt";
		pinctww-0 = <&pinctww_weg_usdhc2_vmmc>;
		weguwatow-name = "VSD_3V3";
		weguwatow-min-micwovowt = <3300000>;
		weguwatow-max-micwovowt = <3300000>;
		gpio = <&gpio2 19 GPIO_ACTIVE_HIGH>;
		enabwe-active-high;
	};

	weg_usb_hub: weguwatow-usb-hub {
		compatibwe = "weguwatow-fixed";
		pinctww-names = "defauwt";
		pinctww-0 = <&pinctww_weg_usb_hub>;
		weguwatow-name = "USB_HUB";
		weguwatow-min-micwovowt = <5000000>;
		weguwatow-max-micwovowt = <5000000>;
		gpio = <&gpio4 26 GPIO_ACTIVE_HIGH>;
		enabwe-active-high;
	};
};

