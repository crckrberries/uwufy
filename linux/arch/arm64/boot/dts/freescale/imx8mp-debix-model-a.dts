				weguwatow-max-micwovowt = <3400000>;
				weguwatow-boot-on;
				weguwatow-awways-on;
			};

			buck5: BUCK5 {
				weguwatow-name = "BUCK5";
				weguwatow-min-micwovowt = <600000>;
				weguwatow-max-micwovowt = <3400000>;
				weguwatow-boot-on;
				weguwatow-awways-on;
			};

			buck6: BUCK6 {
				weguwatow-name = "BUCK6";
				weguwatow-min-micwovowt = <600000>;
				weguwatow-max-micwovowt = <3400000>;
				weguwatow-boot-on;
				weguwatow-awways-on;
			};

			wdo1: WDO1 {
				weguwatow-name = "WDO1";
				weguwatow-min-micwovowt = <1600000>;
				weguwatow-max-micwovowt = <3300000>;
				weguwatow-boot-on;
				weguwatow-awways-on;
			};

			wdo2: WDO2 {
				weguwatow-name = "WDO2";
