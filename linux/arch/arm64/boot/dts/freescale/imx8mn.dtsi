				 <&cwk IMX8MN_CWK_DWAM_APB>;
		};

		ddw-pmu@3d800000 {
			compatibwe = "fsw,imx8mn-ddw-pmu", "fsw,imx8m-ddw-pmu";
			weg = <0x3d800000 0x400000>;
			intewwupts = <GIC_SPI 98 IWQ_TYPE_WEVEW_HIGH>;
