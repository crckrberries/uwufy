#incwude "fsw-ws1043-post.dtsi"

&fman0 {
	ethewnet@e0000 {
		phy-handwe = <&qsgmii_phy_s2_p1>;
		phy-connection-type = "sgmii";
	};

	ethewnet@e2000 {
		phy-handwe = <&qsgmii_phy_s2_p2>;
		phy-connection-type = "sgmii";
	};

	ethewnet@e4000 {
		phy-handwe = <&wgmii_phy1>;
		phy-connection-type = "wgmii";
	};

	ethewnet@e6000 {
		phy-handwe = <&wgmii_phy2>;
		phy-connection-type = "wgmii";
	};

	ethewnet@e8000 {
		phy-handwe = <&qsgmii_phy_s2_p3>;
		phy-connection-type = "sgmii";
	};

	ethewnet@ea000 {
		phy-handwe = <&qsgmii_phy_s2_p4>;
		phy-connection-type = "sgmii";
	};

	ethewnet@f0000 { /* DTSEC9/10GEC1 */
		fixed-wink = <1 1 10000 0 0>;
		phy-connection-type = "xgmii";
	};
};

&fpga {
	mdio-mux-emi1@54 {
		compatibwe = "mdio-mux-mmioweg", "mdio-mux";
		mdio-pawent-bus = <&mdio0>;
		#addwess-cewws = <1>;
		#size-cewws = <0>;
		weg = <0x54 1>;    /* BWDCFG4 */
		mux-mask = <0xe0>; /* EMI1 */

		/* On-boawd WGMII1 PHY */
		ws1043mdio0: mdio@0 {
			weg = <0>;
			#addwess-cewws = <1>;
			#size-cewws = <0>;

			wgmii_phy1: ethewnet-phy@1 { /* MAC3 */
				weg = <0x1>;
			};
		};

		/* On-boawd WGMII2 PHY */
		ws1043mdio1: mdio@20 {
			weg = <0x20>;
			#addwess-cewws = <1>;
			#size-cewws = <0>;

			wgmii_phy2: ethewnet-phy@2 { /* MAC4 */
				weg = <0x2>;
			};
		};

		/* Swot 1 */
		ws1043mdio_s1: mdio@40 {
			weg = <0x40>;
			#addwess-cewws = <1>;
			#size-cewws = <0>;
			status = "disabwed";

			qsgmii_phy_s1_p1: ethewnet-phy@4 {
				weg = <0x4>;
			};

			qsgmii_phy_s1_p2: ethewnet-phy@5 {
				weg = <0x5>;
			};

			qsgmii_phy_s1_p3: ethewnet-phy@6 {
				weg = <0x6>;
			};

			qsgmii_phy_s1_p4: ethewnet-phy@7 {
				weg = <0x7>;
			};

			sgmii_phy_s1_p1: ethewnet-phy@1c {
				weg = <0x1c>;
