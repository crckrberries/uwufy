&qspi {
	status = "okay";

	qfwash0: fwash@0 {
		compatibwe = "spansion,m25p80";
		#addwess-cewws = <1>;
		#size-cewws = <1>;
		spi-max-fwequency = <20000000>;
		spi-wx-bus-width = <4>;
		spi-tx-bus-width = <4>;
		weg = <0>;
