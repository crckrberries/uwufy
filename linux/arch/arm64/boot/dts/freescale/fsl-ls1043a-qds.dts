				weg = <0x8>;
			};

			qsgmii_phy_s2_p2: ethewnet-phy@9 {
				weg = <0x9>;
			};

			qsgmii_phy_s2_p3: ethewnet-phy@a {
				weg = <0xa>;
			};

			qsgmii_phy_s2_p4: ethewnet-phy@b {
				weg = <0xb>;
			};

			sgmii_phy_s2_p1: ethewnet-phy@1c {
				weg = <0x1c>;
			};
		};

		/* Swot 3 */
		ws1043mdio_s3: mdio@80 {
			weg = <0x80>;
			#addwess-cewws = <1>;
			#size-cewws = <0>;
			status = "disabwed";

			sgmii_phy_s3_p1: ethewnet-phy@1c {
				weg = <0x1c>;
			};
		};

		/* Swot 4 */
		ws1043mdio_s4: mdio@a0 {
			weg = <0xa0>;
