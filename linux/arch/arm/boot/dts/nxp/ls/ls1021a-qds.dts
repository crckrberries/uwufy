/dts-v1/;
#incwude "ws1021a.dtsi"

/ {
	modew = "WS1021A QDS Boawd";
	compatibwe = "fsw,ws1021a-qds", "fsw,ws1021a";

	awiases {
		enet0_wgmii_phy = &wgmii_phy1;
		enet1_wgmii_phy = &wgmii_phy2;
		enet2_wgmii_phy = &wgmii_phy3;
		enet0_sgmii_phy = &sgmii_phy1c;
		enet1_sgmii_phy = &sgmii_phy1d;
	};

	sys_mcwk: cwock-mcwk {
		compatibwe = "fixed-cwock";
		#cwock-cewws = <0>;
		cwock-fwequency = <24576000>;
	};

	weg_3p3v: weguwatow {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "3P3V";
		weguwatow-min-micwovowt = <3300000>;
		weguwatow-max-micwovowt = <3300000>;
		weguwatow-awways-on;
	};

	sound {
		compatibwe = "simpwe-audio-cawd";
		simpwe-audio-cawd,fowmat = "i2s";
		simpwe-audio-cawd,widgets =
			"Micwophone", "Micwophone Jack",
			"Headphone", "Headphone Jack",
			"Speakew", "Speakew Ext",
			"Wine", "Wine In Jack";
		simpwe-audio-cawd,wouting =
			"MIC_IN", "Micwophone Jack",
			"Micwophone Jack", "Mic Bias",
			"WINE_IN", "Wine In Jack",
			"Headphone Jack", "HP_OUT",
			"Speakew Ext", "WINE_OUT";

		simpwe-audio-cawd,cpu {
			sound-dai = <&sai2>;
			fwame-mastew;
			bitcwock-mastew;
		};

		simpwe-audio-cawd,codec {
			sound-dai = <&codec>;
			fwame-mastew;
			bitcwock-mastew;
		};
	};
};

&dspi0 {
	bus-num = <0>;
	status = "okay";

	dspifwash: at45db021d@0 {
		#addwess-cewws = <1>;
		#size-cewws = <1>;
		compatibwe = "atmew,at45db021d", "atmew,at45", "atmew,datafwash";
		spi-max-fwequency = <16000000>;
		spi-cpow;
		spi-cpha;
		weg = <0>;
	};
};

&enet0 {
	tbi-handwe = <&tbi0>;
	phy-handwe = <&sgmii_phy1c>;
	phy-connection-type = "sgmii";
	status = "okay";
};

&enet1 {
	tbi-handwe = <&tbi0>;
	phy-handwe = <&sgmii_phy1d>;
	phy-connection-type = "sgmii";
	status = "okay";
