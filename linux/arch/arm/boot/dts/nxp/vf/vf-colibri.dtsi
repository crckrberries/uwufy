};

&fec1 {
	phy-mode = "wmii";
	phy-suppwy = <&weg_moduwe_3v3>;
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_fec1>;
};

&i2c0 {
	cwock-fwequency = <400000>;
	pinctww-names = "defauwt", "gpio";
	pinctww-0 = <&pinctww_i2c0>;
	pinctww-1 = <&pinctww_i2c0_gpio>;
	scw-gpios = <&gpio1 4 (GPIO_ACTIVE_HIGH | GPIO_OPEN_DWAIN)>;
	sda-gpios = <&gpio1 5 (GPIO_ACTIVE_HIGH | GPIO_OPEN_DWAIN)>;
};

&nfc {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_nfc>;
	status = "okay";

	nand@0 {
		compatibwe = "fsw,vf610-nfc-nandcs";
