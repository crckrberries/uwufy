	bw: backwight {
		compatibwe = "pwm-backwight";
		pinctww-names = "defauwt";
		pinctww-0 = <&pinctww_gpio_bw_on>;
		pwms = <&pwm0 0 5000000 0>;
		enabwe-gpios = <&gpio1 13 GPIO_ACTIVE_HIGH>;
		status = "disabwed";
	};

	weg_moduwe_3v3: weguwatow-moduwe-3v3 {
		compatibwe = "weguwatow-fixed";
