	weg_vddio_sd1: weguwatow-vddio-sd1 {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "vddio-sd1";
		weguwatow-min-micwovowt = <3300000>;
		weguwatow-max-micwovowt = <3300000>;
		gpio = <&gpio1 29 0>;
	};

	weg_vdd_touchpad: weguwatow-vdd-touchpad0 {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "vdd-touchpad0";
		weguwatow-min-micwovowt = <3300000>;
		weguwatow-max-micwovowt = <3300000>;
		gpio = <&gpio0 26 GPIO_ACTIVE_WOW>;
