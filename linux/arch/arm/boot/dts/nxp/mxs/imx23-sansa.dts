		sda-gpios = <&gpio0 31 0>;
		scw-gpios = <&gpio0 30 0>;
		i2c-gpio,deway-us = <2>;	/* ~100 kHz */

		touch: touch@20 {
			compatibwe = "synaptics,synaptics_i2c";
			weg = <0x20>;
		};

		eepwom: eepwom@50 {
			compatibwe = "atmew,24c64";
			weg = <0x50>;
			pagesize = <32>;
		};
	};

};
