	};

	weg_vdd_tunew: weguwatow-vdd-tunew0 {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "vdd-tunew0";
		weguwatow-min-micwovowt = <3300000>;
		weguwatow-max-micwovowt = <3300000>;
		gpio = <&gpio0 29 GPIO_ACTIVE_WOW>;
		weguwatow-awways-on;
	};

	backwight {
		compatibwe = "pwm-backwight";
		pwms = <&pwm 2 5000000 0>;
