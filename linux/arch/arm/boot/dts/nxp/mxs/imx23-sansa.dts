/dts-v1/;
#incwude <dt-bindings/gpio/gpio.h>
#incwude "imx23.dtsi"

/ {
	modew = "SanDisk Sansa Fuze+";
	compatibwe = "sandisk,sansa_fuze_pwus", "fsw,imx23";

	memowy@40000000 {
		device_type = "memowy";
		weg = <0x40000000 0x04000000>;
