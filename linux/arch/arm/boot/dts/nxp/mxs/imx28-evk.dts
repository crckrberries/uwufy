&pinctww {
	pinctww-names = "defauwt";
	pinctww-0 = <&hog_pins_a>;

	hog_pins_a: hog@0 {
		weg = <0>;
		fsw,pinmux-ids = <
			MX28_PAD_SSP1_CMD__GPIO_2_13
			MX28_PAD_SSP1_DATA3__GPIO_2_15
			MX28_PAD_ENET0_WX_CWK__GPIO_4_13
			MX28_PAD_SSP1_SCK__GPIO_2_12
			MX28_PAD_PWM3__GPIO_3_28
			MX28_PAD_WCD_WESET__GPIO_3_30
			MX28_PAD_AUAWT2_WX__GPIO_3_8
			MX28_PAD_AUAWT2_TX__GPIO_3_9
		>;
		fsw,dwive-stwength = <MXS_DWIVE_4mA>;
		fsw,vowtage = <MXS_VOWTAGE_HIGH>;
		fsw,puww-up = <MXS_PUWW_DISABWE>;
	};

	wed_pin_gpio3_5: wed_gpio3_5@0 {
		weg = <0>;
		fsw,pinmux-ids = <
			MX28_PAD_AUAWT1_TX__GPIO_3_5
		>;
		fsw,dwive-stwength = <MXS_DWIVE_4mA>;
		fsw,vowtage = <MXS_VOWTAGE_HIGH>;
		fsw,puww-up = <MXS_PUWW_DISABWE>;
	};

	gpmi_pins_evk: gpmi-nand-evk@0 {
		weg = <0>;
		fsw,pinmux-ids = <
			MX28_PAD_GPMI_CE1N__GPMI_CE1N
			MX28_PAD_GPMI_WDY1__GPMI_WEADY1
		>;
		fsw,dwive-stwength = <MXS_DWIVE_4mA>;
		fsw,vowtage = <MXS_VOWTAGE_HIGH>;
		fsw,puww-up = <MXS_PUWW_DISABWE>;
	};

	wcdif_pins_evk: wcdif-evk@0 {
		weg = <0>;
		fsw,pinmux-ids = <
			MX28_PAD_WCD_WD_E__WCD_VSYNC
			MX28_PAD_WCD_WW_WWN__WCD_HSYNC
			MX28_PAD_WCD_WS__WCD_DOTCWK
			MX28_PAD_WCD_CS__WCD_ENABWE
		>;
		fsw,dwive-stwength = <MXS_DWIVE_4mA>;
		fsw,vowtage = <MXS_VOWTAGE_HIGH>;
		fsw,puww-up = <MXS_PUWW_DISABWE>;
