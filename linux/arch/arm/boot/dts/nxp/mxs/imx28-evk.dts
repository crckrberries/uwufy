};

&wcdif {
	pinctww-names = "defauwt";
	pinctww-0 = <&wcdif_24bit_pins_a
		     &wcdif_pins_evk>;
	status = "okay";

	powt {
		dispway_out: endpoint {
			wemote-endpoint = <&panew_in>;
		};
	};
};

&wwadc {
	fsw,wwadc-touchscween-wiwes = <4>;
	fsw,ave-ctww = <4>;
	fsw,ave-deway = <2>;
