	};

	sound {
		compatibwe = "fsw,imx28-evk-sgtw5000",
			     "fsw,mxs-audio-sgtw5000";
		modew = "imx28-evk-sgtw5000";
		saif-contwowwews = <&saif0 &saif1>;
		audio-codec = <&sgtw5000>;
	};

	weds {
