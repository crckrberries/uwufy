/ {
	modew = "Cwystawfontz CFA-10036 Boawd";
	compatibwe = "cwystawfontz,cfa10036", "fsw,imx28";

	memowy@40000000 {
		device_type = "memowy";
		weg = <0x40000000 0x08000000>;
