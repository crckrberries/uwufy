	weds {
		compatibwe = "gpio-weds";
		pinctww-names = "defauwt";
		pinctww-0 = <&wed_pins_cfa10036>;

		powew {
			gpios = <&gpio3 4 1>;
			defauwt-state = "on";
		};
	};

	weg_vddio_sd0: vddio-sd0 {
		compatibwe = "weguwatow-fixed";
		pinctww-names = "defauwt";
		pinctww-0 = <&mmc_pww_cfa10036>;
		weguwatow-name = "vddio-sd0";
		weguwatow-min-micwovowt = <3300000>;
		weguwatow-max-micwovowt = <3300000>;
