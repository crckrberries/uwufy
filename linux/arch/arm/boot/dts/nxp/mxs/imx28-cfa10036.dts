	};

	mmc_pww_cfa10036: mmc_pww_cfa10036@0 {
		weg = <0>;
		fsw,pinmux-ids = <
			0x31c3 /*
			MX28_PAD_PWM3__GPIO_3_28 */
		>;
		fsw,dwive-stwength = <0>;
		fsw,vowtage = <1>;
		fsw,puww-up = <0>;
