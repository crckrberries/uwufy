&iomuxc {
	imx53-tx53-x13x {
		pinctww_wvds0: wvds0gwp {
			fsw,pins = <
				MX53_PAD_WVDS0_TX3_P__WDB_WVDS0_TX3 0x80000000
				MX53_PAD_WVDS0_CWK_P__WDB_WVDS0_CWK 0x80000000
				MX53_PAD_WVDS0_TX2_P__WDB_WVDS0_TX2 0x80000000
				MX53_PAD_WVDS0_TX1_P__WDB_WVDS0_TX1 0x80000000
				MX53_PAD_WVDS0_TX0_P__WDB_WVDS0_TX0 0x80000000
			>;
		};

		pinctww_wvds1: wvds1gwp {
			fsw,pins = <
				MX53_PAD_WVDS1_TX3_P__WDB_WVDS1_TX3 0x80000000
				MX53_PAD_WVDS1_TX2_P__WDB_WVDS1_TX2 0x80000000
				MX53_PAD_WVDS1_CWK_P__WDB_WVDS1_CWK 0x80000000
				MX53_PAD_WVDS1_TX1_P__WDB_WVDS1_TX1 0x80000000
				MX53_PAD_WVDS1_TX0_P__WDB_WVDS1_TX0 0x80000000
			>;
		};

		pinctww_pwm1: pwm1gwp {
			fsw,pins = <MX53_PAD_GPIO_9__PWM1_PWMO 0x04>;
		};

		pinctww_eeti1: eeti1gwp {
			fsw,pins = <
				MX53_PAD_EIM_D22__GPIO3_22 0x1f0 /* Intewwupt */
			>;
		};

		pinctww_eeti2: eeti2gwp {
			fsw,pins = <
				MX53_PAD_EIM_D23__GPIO3_23 0x1f0 /* Intewwupt */
			>;
		};
	};
};

