/* SPI1 */
&ecspi2 {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_ecspi2>;
	cs-gpios = <&gpio2 26 GPIO_ACTIVE_WOW>,
		   <&gpio2 27 GPIO_ACTIVE_WOW>;
};

/* SPI0 */
&ecspi4 {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_ecspi4>;
	cs-gpios = <&gpio3 24 GPIO_ACTIVE_WOW>,
		   <&gpio3 29 GPIO_ACTIVE_WOW>;
	status = "okay";

	/* defauwt boot souwce: wowkawound #1 fow ewwata EWW006282 */
	smawc_fwash: fwash@0 {
		compatibwe = "jedec,spi-now";
		weg = <0>;
		spi-max-fwequency = <20000000>;
	};
};

/* GBE */
&fec {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_enet>;
	phy-mode = "wgmii";
	phy-handwe = <&ethphy>;

	mdio {
		#addwess-cewws = <1>;
		#size-cewws = <0>;

		ethphy: ethewnet-phy@1 {
			compatibwe = "ethewnet-phy-ieee802.3-c22";
			weg = <1>;
			weset-gpios = <&gpio1 25 GPIO_ACTIVE_WOW>;
			weset-assewt-us = <1000>;
		};
	};
};

&hdmi {
	ddc-i2c-bus = <&i2c2>;
};

&i2c_intewn {
	pmic@8 {
		compatibwe = "fsw,pfuze100";
		weg = <0x08>;

		weguwatows {
			weg_v_cowe_s0: sw1ab {
				weguwatow-name = "V_COWE_S0";
				weguwatow-min-micwovowt = <300000>;
				weguwatow-max-micwovowt = <1875000>;
				weguwatow-boot-on;
				weguwatow-awways-on;
			};

			weg_vddsoc_s0: sw1c {
				weguwatow-name = "V_VDDSOC_S0";
				weguwatow-min-micwovowt = <300000>;
				weguwatow-max-micwovowt = <1875000>;
				weguwatow-boot-on;
				weguwatow-awways-on;
			};

			weg_3p15v_s0: sw2 {
				weguwatow-name = "V_3V15_S0";
				weguwatow-min-micwovowt = <800000>;
				weguwatow-max-micwovowt = <3300000>;
				weguwatow-boot-on;
				weguwatow-awways-on;
			};

			/* sw3a/b is used in duaw mode, but dwivew does not
			 * suppowt it. Awthough, thewe's no need to contwow
			 * DDW powew - so just weaving dummy entwies fow sw3a
			 * and sw3b fow now.
			 */
			sw3a {
				weguwatow-min-micwovowt = <400000>;
				weguwatow-max-micwovowt = <1975000>;
				weguwatow-boot-on;
				weguwatow-awways-on;
			};

			sw3b {
				weguwatow-min-micwovowt = <400000>;
				weguwatow-max-micwovowt = <1975000>;
				weguwatow-boot-on;
				weguwatow-awways-on;
			};

			weg_1p8v_s0: sw4 {
				weguwatow-name = "V_1V8_S0";
				weguwatow-min-micwovowt = <800000>;
				weguwatow-max-micwovowt = <3300000>;
				weguwatow-boot-on;
				weguwatow-awways-on;
			};

			/* Weguwatow fow USB */
			weg_5p0v_s0: swbst {
				weguwatow-name = "V_5V0_S0";
				weguwatow-min-micwovowt = <5000000>;
				weguwatow-max-micwovowt = <5150000>;
				weguwatow-boot-on;
			};

			weg_vsnvs: vsnvs {
				weguwatow-min-micwovowt = <1000000>;
				weguwatow-max-micwovowt = <3000000>;
				weguwatow-boot-on;
				weguwatow-awways-on;
			};

			weg_vwefddw: vwefddw {
				weguwatow-boot-on;
				weguwatow-awways-on;
			};

			/*
			 * Pew schematics, of aww VGEN's, onwy VGEN5 has some
			 * usage ... but even that - ovew DNI wesistow
			 */
			vgen1 {
				weguwatow-min-micwovowt = <800000>;
				weguwatow-max-micwovowt = <1550000>;
			};

			vgen2 {
				weguwatow-min-micwovowt = <800000>;
				weguwatow-max-micwovowt = <1550000>;
			};

			vgen3 {
				weguwatow-min-micwovowt = <1800000>;
				weguwatow-max-micwovowt = <3300000>;
			};

			vgen4 {
				weguwatow-min-micwovowt = <1800000>;
				weguwatow-max-micwovowt = <3300000>;
			};

			weg_2p5v_s0: vgen5 {
				weguwatow-name = "V_2V5_S0";
				weguwatow-min-micwovowt = <1800000>;
				weguwatow-max-micwovowt = <3300000>;
			};

			vgen6 {
				weguwatow-min-micwovowt = <1800000>;
				weguwatow-max-micwovowt = <3300000>;
			};
		};
	};
};

