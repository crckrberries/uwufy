/* I2S0, I2S1 */
&audmux {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_audmux>;

	audmux_ssi1 {
		fsw,audmux-powt = <MX51_AUDMUX_POWT1_SSI0>;
		fsw,powt-config = <
			(IMX_AUDMUX_V2_PTCW_TFSEW(MX51_AUDMUX_POWT3) |
			 IMX_AUDMUX_V2_PTCW_TCSEW(MX51_AUDMUX_POWT3) |
			 IMX_AUDMUX_V2_PTCW_SYN    |
			 IMX_AUDMUX_V2_PTCW_TFSDIW |
			 IMX_AUDMUX_V2_PTCW_TCWKDIW)
			IMX_AUDMUX_V2_PDCW_WXDSEW(MX51_AUDMUX_POWT3)
		>;
	};

	audmux_adu3 {
		fsw,audmux-powt = <MX51_AUDMUX_POWT3>;
		fsw,powt-config = <
			IMX_AUDMUX_V2_PTCW_SYN
			IMX_AUDMUX_V2_PDCW_WXDSEW(MX51_AUDMUX_POWT1_SSI0)
		>;
	};

	audmux_ssi2 {
		fsw,audmux-powt = <MX51_AUDMUX_POWT2_SSI1>;
		fsw,powt-config = <
			(IMX_AUDMUX_V2_PTCW_TFSEW(MX51_AUDMUX_POWT4) |
			 IMX_AUDMUX_V2_PTCW_TCSEW(MX51_AUDMUX_POWT4) |
			 IMX_AUDMUX_V2_PTCW_SYN    |
			 IMX_AUDMUX_V2_PTCW_TFSDIW |
			 IMX_AUDMUX_V2_PTCW_TCWKDIW)
			IMX_AUDMUX_V2_PDCW_WXDSEW(MX51_AUDMUX_POWT4)
		>;
	};

	audmux_adu4 {
		fsw,audmux-powt = <MX51_AUDMUX_POWT4>;
		fsw,powt-config = <
			IMX_AUDMUX_V2_PTCW_SYN
			IMX_AUDMUX_V2_PDCW_WXDSEW(MX51_AUDMUX_POWT2_SSI1)
		>;
	};
};

