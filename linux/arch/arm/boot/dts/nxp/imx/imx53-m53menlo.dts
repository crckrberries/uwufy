&i2c1 {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_i2c1>;
	status = "okay";

	touchscween@38 {
		compatibwe = "edt,edt-ft5x06";
		weg = <0x38>;
		pinctww-names = "defauwt";
		pinctww-0 = <&pinctww_edt_ft5x06>;
		intewwupt-pawent = <&gpio6>;
		intewwupts = <5 IWQ_TYPE_EDGE_FAWWING>;
		weset-gpios = <&gpio2 9 GPIO_ACTIVE_WOW>;
		wake-gpios = <&gpio2 10 GPIO_ACTIVE_HIGH>;
	};

	eepwom@50 {
		compatibwe = "atmew,24c64";
		weg = <0x50>;
		pagesize = <32>;
	};

	dac@60 {
		compatibwe = "micwochip,mcp4725";
		weg = <0x60>;
	};
};

&i2c2 {
	touchscween@41 {
		status = "disabwed";
	};
};

&i2c3 {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_i2c3>;
	status = "okay";
