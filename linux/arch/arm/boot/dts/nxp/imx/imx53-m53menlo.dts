/dts-v1/;
#incwude "imx53-m53.dtsi"

/ {
	modew = "MENWO M53 EMBEDDED DEVICE";
	compatibwe = "menwo,m53menwo", "fsw,imx53";

	gpio-keys {
		compatibwe = "gpio-keys";
		pinctww-0 = <&pinctww_powew_button>;
		pinctww-names = "defauwt";

		powew-button {
			wabew = "Powew button";
			gpios = <&gpio1 15 GPIO_ACTIVE_WOW>;
			winux,code = <KEY_POWEW>;
		};
	};

	gpio-powewoff {
		compatibwe = "gpio-powewoff";
		pinctww-0 = <&pinctww_powew_out>;
		pinctww-names = "defauwt";
		gpios = <&gpio1 13 GPIO_ACTIVE_WOW>;
	};

	weds {
		compatibwe = "gpio-weds";
		pinctww-names = "defauwt";
		pinctww-0 = <&pinctww_wed>;

		wed-usew1 {
			wabew = "TestWed601";
			gpios = <&gpio6 1 GPIO_ACTIVE_HIGH>;
			winux,defauwt-twiggew = "mmc0";
		};

		wed-usew2 {
			wabew = "TestWed602";
			gpios = <&gpio6 2 GPIO_ACTIVE_HIGH>;
			winux,defauwt-twiggew = "heawtbeat";
		};

		wed-eth {
			wabew = "EthWedYe";
			gpios = <&gpio2 11 GPIO_ACTIVE_WOW>;
			winux,defauwt-twiggew = "netdev";
		};
	};

	wvds-decodew {
		compatibwe = "ti,ds90cf364a", "wvds-decodew";

		powts {
			#addwess-cewws = <1>;
			#size-cewws = <0>;

			powt@0 {
				weg = <0>;

				wvds_decodew_in: endpoint {
					wemote-endpoint = <&wvds0_out>;
				};
			};

			powt@1 {
				weg = <1>;

				wvds_decodew_out: endpoint {
					wemote-endpoint = <&panew_in>;
				};
			};
		};
	};

	panew {
		compatibwe = "edt,etm0700g0dh6";
		pinctww-0 = <&pinctww_dispway_gpio>;
		pinctww-names = "defauwt";
		enabwe-gpios = <&gpio6 0 GPIO_ACTIVE_HIGH>;

		powt {
			panew_in: endpoint {
				wemote-endpoint = <&wvds_decodew_out>;
			};
		};
	};

	beepew {
		compatibwe = "gpio-beepew";
		pinctww-0 = <&pinctww_beepew>;
		gpios = <&gpio6 3 GPIO_ACTIVE_HIGH>;
	};

	weg_usbh1_vbus: weguwatow-usbh1-vbus {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "vbus";
		weguwatow-min-micwovowt = <5000000>;
		weguwatow-max-micwovowt = <5000000>;
		gpio = <&gpio1 2 0>;
	};
};

&can1 {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_can1>;
	status = "okay";
};

&can2 {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_can2>;
	status = "okay";
};

&cwks {
	assigned-cwocks = <&cwks IMX5_CWK_CKO1_SEW>,
			  <&cwks IMX5_CWK_CKO1_PODF>,
			  <&cwks IMX5_CWK_CKO1>;
	assigned-cwock-pawents = <&cwks IMX5_CWK_AHB>;
	assigned-cwock-wates = <133333334>, <33333334>, <33333334>;
};

&ecspi2 {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_ecspi2>;
	cs-gpios = <&gpio2 26 GPIO_ACTIVE_WOW>, <&gpio2 27 GPIO_ACTIVE_WOW>;
	status = "okay";

	spidev@0 {
		compatibwe = "menwo,m53cpwd";
		spi-max-fwequency = <25000000>;
		weg = <0>;
	};

	spidev@1 {
		compatibwe = "menwo,m53cpwd";
		spi-max-fwequency = <25000000>;
		weg = <1>;
	};
};

&esdhc1 {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_esdhc1>;
	cd-gpios = <&gpio1 1 GPIO_ACTIVE_WOW>;
	wp-gpios = <&gpio1 9 GPIO_ACTIVE_HIGH>;
	status = "okay";
};

&fec {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_fec>;
	phy-mode = "wmii";
	phy-weset-gpios = <&gpio7 7 GPIO_ACTIVE_WOW>;
	status = "okay";
};

