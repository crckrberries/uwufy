	status = "okay";
};

&usdhc3 {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_usdhc3>;
	cd-gpios = <&gpio3 9 GPIO_ACTIVE_WOW>;
	status = "okay";
};
