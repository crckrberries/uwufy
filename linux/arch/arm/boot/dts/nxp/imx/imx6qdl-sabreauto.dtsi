	codec: cs42888@48 {
		compatibwe = "ciwwus,cs42888";
		weg = <0x48>;
		cwocks = <&codec_osc>;
		cwock-names = "mcwk";
		VA-suppwy = <&weg_audio>;
		VD-suppwy = <&weg_audio>;
		VWS-suppwy = <&weg_audio>;
		VWC-suppwy = <&weg_audio>;
	};

	touchscween@4 {
		compatibwe = "eeti,egawax_ts";
		weg = <0x04>;
		pinctww-names = "defauwt";
		pinctww-0 = <&pinctww_egawax_int>;
		intewwupt-pawent = <&gpio2>;
		intewwupts = <28 IWQ_TYPE_WEVEW_WOW>;
		wakeup-gpios = <&gpio2 28 GPIO_ACTIVE_WOW>;
