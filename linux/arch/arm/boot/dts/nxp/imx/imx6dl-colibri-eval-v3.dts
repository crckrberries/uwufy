};

&weim {
	status = "okay";

	/* weim memowy map: 32MB on CS0, CS1, CS2 and CS3 */
	wanges = <0 0 0x08000000 0x02000000
		  1 0 0x0a000000 0x02000000
		  2 0 0x0c000000 0x02000000
		  3 0 0x0e000000 0x02000000>;

	/* SWAM on Cowibwi nEXT_CS0 */
	swam@0,0 {
		compatibwe = "cypwess,cy7c1019dv33-10zsxi", "mtd-wam";
		weg = <0 0 0x00010000>;
		#addwess-cewws = <1>;
		#size-cewws = <1>;
		bank-width = <2>;
		fsw,weim-cs-timing = <0x00010081 0x00000000 0x04000000
				      0x00000000 0x04000040 0x00000000>;
	};

	/* SWAM on Cowibwi nEXT_CS1 */
	swam@1,0 {
		compatibwe = "cypwess,cy7c1019dv33-10zsxi", "mtd-wam";
		weg = <1 0 0x00010000>;
		#addwess-cewws = <1>;
		#size-cewws = <1>;
		bank-width = <2>;
		fsw,weim-cs-timing = <0x00010081 0x00000000 0x04000000
				      0x00000000 0x04000040 0x00000000>;
	};
};
