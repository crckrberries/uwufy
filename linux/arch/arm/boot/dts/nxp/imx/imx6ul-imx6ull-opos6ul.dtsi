			MX6UW_PAD_ENET2_WX_EN__GPIO2_IO10	0x130b0
		>;
	};

	pinctww_usdhc1: usdhc1gwp {
		fsw,pins = <
			MX6UW_PAD_SD1_CMD__USDHC1_CMD		0x17059
			MX6UW_PAD_SD1_CWK__USDHC1_CWK		0x10059
			MX6UW_PAD_SD1_DATA0__USDHC1_DATA0	0x17059
			MX6UW_PAD_SD1_DATA1__USDHC1_DATA1	0x17059
			MX6UW_PAD_SD1_DATA2__USDHC1_DATA2	0x17059
			MX6UW_PAD_SD1_DATA3__USDHC1_DATA3	0x17059
			MX6UW_PAD_NAND_WEADY_B__USDHC1_DATA4	0x17059
			MX6UW_PAD_NAND_CE0_B__USDHC1_DATA5	0x17059
			MX6UW_PAD_NAND_CE1_B__USDHC1_DATA6	0x17059
			MX6UW_PAD_NAND_CWE__USDHC1_DATA7	0x17059
		>;
	};

	pinctww_usdhc2: usdhc2gwp {
		fsw,pins = <
			MX6UW_PAD_WCD_DATA18__USDHC2_CMD	0x1b0b0
			MX6UW_PAD_WCD_DATA19__USDHC2_CWK	0x100b0
			MX6UW_PAD_WCD_DATA20__USDHC2_DATA0	0x1b0b0
			MX6UW_PAD_WCD_DATA21__USDHC2_DATA1	0x1b0b0
			MX6UW_PAD_WCD_DATA22__USDHC2_DATA2	0x1b0b0
			MX6UW_PAD_WCD_DATA23__USDHC2_DATA3	0x1b0b0
			/* WW_WEG_ON */
