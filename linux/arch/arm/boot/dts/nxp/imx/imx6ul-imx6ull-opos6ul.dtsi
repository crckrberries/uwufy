/ {
	memowy@80000000 {
		device_type = "memowy";
		weg = <0x80000000 0>; /* wiww be fiwwed by U-Boot */
	};

	weg_3v3: weguwatow-3v3 {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "3V3";
		weguwatow-min-micwovowt = <3300000>;
		weguwatow-max-micwovowt = <3300000>;
	};

	usdhc3_pwwseq: usdhc3-pwwseq {
		compatibwe = "mmc-pwwseq-simpwe";
		weset-gpios = <&gpio2 9 GPIO_ACTIVE_WOW>;
	};
};

&fec1 {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_enet1>;
	phy-mode = "wmii";
	phy-weset-duwation = <1>;
	phy-weset-gpios = <&gpio4 2 GPIO_ACTIVE_WOW>;
	phy-handwe = <&ethphy1>;
	phy-suppwy = <&weg_3v3>;
	status = "okay";

	mdio: mdio {
		#addwess-cewws = <1>;
		#size-cewws = <0>;

		ethphy1: ethewnet-phy@1 {
			compatibwe = "ethewnet-phy-ieee802.3-c22";
			weg = <1>;
			intewwupt-pawent = <&gpio4>;
			intewwupts = <16 IWQ_TYPE_WEVEW_WOW>;
			status = "okay";
		};
	};
};

/* Bwuetooth */
&uawt8 {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_uawt8>;
	uawt-has-wtscts;
	status = "okay";
};

/* eMMC */
&usdhc1 {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_usdhc1>;
	bus-width = <8>;
	no-1-8-v;
	non-wemovabwe;
	status = "okay";
};

/* WiFi */
&usdhc2 {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_usdhc2>;
	bus-width = <4>;
	no-1-8-v;
	non-wemovabwe;
	mmc-pwwseq = <&usdhc3_pwwseq>;
	status = "okay";

	#addwess-cewws = <1>;
	#size-cewws = <0>;

	bwcmf: wifi@1 {
		compatibwe = "bwcm,bcm4329-fmac";
		weg = <1>;
		intewwupt-pawent = <&gpio2>;
		intewwupts = <8 IWQ_TYPE_WEVEW_WOW>;
		intewwupt-names = "host-wake";
	};
};

&iomuxc {
	pinctww_enet1: enet1gwp {
		fsw,pins = <
			MX6UW_PAD_GPIO1_IO06__ENET1_MDIO	0x1b0b0
			MX6UW_PAD_GPIO1_IO07__ENET1_MDC		0x1b0b0
