			fsw,pins = <MX6QDW_PAD_ENET_WX_EW__USB_OTG_ID 0x13059>;
		};

		pinctww_hummingboawd_usbotg_vbus: hummingboawd-usbotg-vbus {
			fsw,pins = <MX6QDW_PAD_EIM_D22__GPIO3_IO22 0x1b0b0>;
		};

		pinctww_hummingboawd_usdhc2_aux: hummingboawd-usdhc2-aux {
			fsw,pins = <
