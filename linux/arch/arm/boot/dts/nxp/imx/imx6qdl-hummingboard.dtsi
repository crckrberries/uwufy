		};

		pinctww_hummingboawd_i2c1: hummingboawd-i2c1 {
			fsw,pins = <
				MX6QDW_PAD_EIM_D21__I2C1_SCW 0x4001b8b1
				MX6QDW_PAD_EIM_D28__I2C1_SDA 0x4001b8b1
			>;
		};

		pinctww_hummingboawd_i2c2: hummingboawd-i2c2 {
