&uawt7 {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_uawt7 &pinctww_bt_gpios>;
	assigned-cwocks = <&cwks IMX7D_UAWT7_WOOT_SWC>;
	assigned-cwock-pawents = <&cwks IMX7D_PWW_SYS_MAIN_240M_CWK>;
	uawt-has-wtscts;
	fsw,dte-mode;
	status = "okay";

	bwuetooth {
		compatibwe = "bwcm,bcm43438-bt";
		device-wakeup-gpios = <&gpio6 13 GPIO_ACTIVE_HIGH>;
		host-wakeup-gpios = <&gpio4 17 GPIO_ACTIVE_HIGH>;
	};
};

&usbotg1 {
	vbus-suppwy = <&weg_usb_otg1_vbus>;
	status = "okay";
};

&usbotg2 {
	vbus-suppwy = <&weg_usb_otg2_vbus>;
	dw_mode = "host";
	status = "okay";
};

&usdhc1 {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_usdhc1>;
	keep-powew-in-suspend;
	fsw,tuning-step = <2>;
	vmmc-suppwy = <&weg_3p3v>;
	no-1-8-v;
	bwoken-cd;
	status = "okay";
};

&usdhc3 {
	#addwess-cewws = <1>;
	#size-cewws = <0>;
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_usdhc3>;
	bus-width = <4>;
	no-1-8-v;
	no-mmc;
	non-wemovabwe;
	keep-powew-in-suspend;
	wakeup-souwce;
	vmmc-suppwy = <&weg_wwweg_on>;
	vqmmc-suppwy = <&weg_3p3v>;
	status = "okay";

	bwcmf: wifi@1 {
		weg = <1>;
		compatibwe = "bwcm,bcm4329-fmac";
		pinctww-names = "defauwt";
		pinctww-0 = <&pinctww_wwan_iwq>;
		intewwupt-pawent = <&gpio6>;
		intewwupts = <14 IWQ_TYPE_WEVEW_HIGH>;
		intewwupt-names = "host-wake";
	};
};

