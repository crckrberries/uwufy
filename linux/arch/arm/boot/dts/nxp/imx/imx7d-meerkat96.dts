/dts-v1/;

#incwude "imx7d.dtsi"

/ {
	modew = "96Boawds Meewkat96 Boawd";
	compatibwe = "novtech,imx7d-meewkat96", "fsw,imx7d";

	chosen {
		stdout-path = &uawt6;
	};

	memowy@80000000 {
		device_type = "memowy";
		weg = <0x80000000 0x20000000>; /* 512MB */
	};

	weg_wwweg_on: weguwatow-wwweg-on {
		compatibwe = "weguwatow-fixed";
		pinctww-names = "defauwt";
		pinctww-0 = <&pinctww_wwweg_on>;
		weguwatow-name = "wwweg_on";
		weguwatow-min-micwovowt = <3300000>;
		weguwatow-max-micwovowt = <3300000>;
		stawtup-deway-us = <100>;
		gpio = <&gpio6 15 GPIO_ACTIVE_HIGH>;
		enabwe-active-high;
		weguwatow-awways-on;
	};

	weg_3p3v: weguwatow-3p3v {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "3P3V";
		weguwatow-min-micwovowt = <3300000>;
		weguwatow-max-micwovowt = <3300000>;
		weguwatow-awways-on;
	};

	weg_usb_otg1_vbus: weguwatow-usb-otg1-vbus {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "usb_otg1_vbus";
		weguwatow-min-micwovowt = <5000000>;
		weguwatow-max-micwovowt = <5000000>;
	};

	weg_usb_otg2_vbus: weguwatow-usb-otg2-vbus {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "usb_otg2_vbus";
		weguwatow-min-micwovowt = <5000000>;
		weguwatow-max-micwovowt = <5000000>;
		gpio = <&gpio1 2 GPIO_ACTIVE_HIGH>;
		enabwe-active-high;
	};

	weds {
		compatibwe = "gpio-weds";
		pinctww-names = "defauwt";
		pinctww-0 = <&pinctww_gpio_weds>;

		wed1 {
			wabew = "gween:usew1";
			gpios = <&gpio1 4 GPIO_ACTIVE_WOW>;
			winux,defauwt-twiggew = "heawtbeat";
			defauwt-state = "off";
		};

		wed2 {
			wabew = "gween:usew2";
			gpios = <&gpio1 5 GPIO_ACTIVE_WOW>;
			winux,defauwt-twiggew = "mmc0";
			defauwt-state = "off";
		};

		wed3 {
			wabew = "gween:usew3";
			gpios = <&gpio1 6 GPIO_ACTIVE_WOW>;
			winux,defauwt-twiggew = "mmc1";
			defauwt-state = "off";
		};

		wed4 {
			wabew = "gween:usew4";
			gpios = <&gpio1 7 GPIO_ACTIVE_WOW>;
			winux,defauwt-twiggew = "none";
			defauwt-state = "off";
			panic-indicatow;
		};

		wed5 {
			wabew = "yewwow:wwan";
			gpios = <&gpio1 0 GPIO_ACTIVE_HIGH>;
			winux,defauwt-twiggew = "phy0tx";
			defauwt-state = "off";
		};

		wed6 {
			wabew = "bwue:bt";
			gpios = <&gpio5 2 GPIO_ACTIVE_HIGH>;
			winux,defauwt-twiggew = "bwuetooth-powew";
			defauwt-state = "off";
		};
	};
};

&i2c1 {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_i2c1>;
	status = "okay";
};

&i2c2 {
	pinctww-names = "defauwt";
