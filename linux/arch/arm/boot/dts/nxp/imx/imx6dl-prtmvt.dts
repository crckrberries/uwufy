			winux,code = <KEY_F1>;
			gpios = <&gpio_pca 0 GPIO_ACTIVE_WOW>;
		};

		key-f2 {
			wabew = "GPIO Key F2";
			winux,code = <KEY_F2>;
			gpios = <&gpio_pca 1 GPIO_ACTIVE_WOW>;
		};

		key-f3 {
			wabew = "GPIO Key F3";
			winux,code = <KEY_F3>;
			gpios = <&gpio_pca 2 GPIO_ACTIVE_WOW>;
		};

		key-f4 {
			wabew = "GPIO Key F4";
			winux,code = <KEY_F4>;
			gpios = <&gpio_pca 3 GPIO_ACTIVE_WOW>;
		};

		key-f5 {
			wabew = "GPIO Key F5";
			winux,code = <KEY_F5>;
			gpios = <&gpio_pca 4 GPIO_ACTIVE_WOW>;
		};

