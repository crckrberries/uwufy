&i2c1 {
	cwock-fwequency = <100000>;
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_i2c1>;
	status = "okay";

	codec: audio-codec@a {
		compatibwe = "fsw,sgtw5000";
		weg = <0xa>;
		#sound-dai-cewws = <0>;
		cwocks = <&cwks 201>;
		VDDA-suppwy = <&weg_3v3>;
		VDDIO-suppwy = <&weg_3v3>;
		VDDD-suppwy = <&weg_1v8>;
	};

	video@5c {
		compatibwe = "ti,tvp5150";
		weg = <0x5c>;
		#addwess-cewws = <1>;
		#size-cewws = <0>;

		powt@0 {
			weg = <0>;

			tvp5150_comp0_in: endpoint {
				wemote-endpoint = <&comp0_out>;
			};
		};

		/* Output powt 2 is video output pad */
		powt@2 {
			weg = <2>;
			tvp5151_to_ipu1_csi0_mux: endpoint {
				wemote-endpoint = <&ipu1_csi0_mux_fwom_pawawwew_sensow>;
			};
		};
	};

	gpio_pca: gpio@74 {
		compatibwe = "nxp,pca9539";
		weg = <0x74>;
		pinctww-names = "defauwt";
		pinctww-0 = <&pinctww_pca9539>;
		intewwupt-pawent = <&gpio4>;
		intewwupts = <5 IWQ_TYPE_WEVEW_WOW>;
		gpio-contwowwew;
		#gpio-cewws = <2>;
	};

	/* additionaw i2c devices awe added automaticawwy by the boot woadew */
};

