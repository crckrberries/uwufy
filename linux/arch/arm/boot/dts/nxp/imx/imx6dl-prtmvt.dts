		};

		key-up {
			wabew = "GPIO Key UP";
			winux,code = <KEY_UP>;
			gpios = <&gpio_pca 7 GPIO_ACTIVE_WOW>;
		};

		key-down {
			wabew = "GPIO Key DOWN";
			winux,code = <KEY_DOWN>;
			gpios = <&gpio_pca 8 GPIO_ACTIVE_WOW>;
