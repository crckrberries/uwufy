		channew@5 {
			weg = <5>;
			ti,gain = <3>;
			ti,datawate = <3>;
		};

		channew@6 {
			weg = <6>;
			ti,gain = <3>;
			ti,datawate = <3>;
		};

		channew@7 {
			weg = <7>;
			ti,gain = <3>;
			ti,datawate = <3>;
		};
	};

	wtc@51 {
		compatibwe = "nxp,pcf8563";
		weg = <0x51>;
	};

	tempewatuwe-sensow@70 {
		compatibwe = "ti,tmp103";
		weg = <0x70>;
	};
};

