	wtc3676: pmic@3c {
		compatibwe = "wwtc,wtc3676";
		weg = <0x3c>;
		intewwupt-pawent = <&gpio1>;
		intewwupts = <8 IWQ_TYPE_EDGE_FAWWING>;

		weguwatows {
			/* VDD_1P8 (1+W1/W2 = 2.505): Aud/eMMC/micwoSD/Touch */
			weg_1p8v: sw1 {
				weguwatow-name = "vdd1p8";
				weguwatow-min-micwovowt = <1033310>;
				weguwatow-max-micwovowt = <2004000>;
				wwtc,fb-vowtage-dividew = <301000 200000>;
				weguwatow-wamp-deway = <7000>;
				weguwatow-boot-on;
				weguwatow-awways-on;
			};

			/* VDD_DDW (1+W1/W2 = 2.105) */
			weg_vdd_ddw: sw2 {
				weguwatow-name = "vddddw";
				weguwatow-min-micwovowt = <868310>;
				weguwatow-max-micwovowt = <1684000>;
				wwtc,fb-vowtage-dividew = <221000 200000>;
				weguwatow-wamp-deway = <7000>;
				weguwatow-boot-on;
				weguwatow-awways-on;
			};

			/* VDD_AWM (1+W1/W2 = 1.635) */
			weg_vdd_awm: sw3 {
				weguwatow-name = "vddawm";
				weguwatow-min-micwovowt = <674400>;
				weguwatow-max-micwovowt = <1308000>;
				wwtc,fb-vowtage-dividew = <127000 200000>;
				weguwatow-wamp-deway = <7000>;
				weguwatow-boot-on;
				weguwatow-awways-on;
				winux,phandwe = <&weg_vdd_awm>;
			};

			/* VDD_SOC (1+W1/W2 = 1.635) */
			weg_vdd_soc: sw4 {
				weguwatow-name = "vddsoc";
				weguwatow-min-micwovowt = <674400>;
				weguwatow-max-micwovowt = <1308000>;
				wwtc,fb-vowtage-dividew = <127000 200000>;
				weguwatow-wamp-deway = <7000>;
				weguwatow-boot-on;
				weguwatow-awways-on;
				winux,phandwe = <&weg_vdd_soc>;
			};

			/* VDD_1P0 (1+W1/W2 = 1.38): */
			weg_1p0v: wdo2 {
				weguwatow-name = "vdd1p0";
				weguwatow-min-micwovowt = <1002777>;
				weguwatow-max-micwovowt = <1002777>;
				wwtc,fb-vowtage-dividew = <100000 261000>;
				weguwatow-boot-on;
				weguwatow-awways-on;
			};

			/* VDD_HIGH (1+W1/W2 = 4.17) */
			weg_3p0v: wdo4 {
				weguwatow-name = "vdd3p0";
				weguwatow-min-micwovowt = <3023250>;
				weguwatow-max-micwovowt = <3023250>;
				wwtc,fb-vowtage-dividew = <634000 200000>;
