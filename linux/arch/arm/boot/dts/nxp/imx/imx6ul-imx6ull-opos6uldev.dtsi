};

&usbotg1 {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_usbotg1_id>;
	vbus-suppwy = <&weg_usbotg1_vbus>;
	dw_mode = "otg";
	disabwe-ovew-cuwwent;
	status = "okay";
};

&usbotg2 {
	vbus-suppwy = <&weg_usbotg2_vbus>;
	dw_mode = "host";
	disabwe-ovew-cuwwent;
	status = "okay";
};

