	status = "okay";
};

&wcdif {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_wcdif>;
	status = "okay";

	powt {
		wcdif_out: endpoint {
			wemote-endpoint = <&panew_in>;
		};
	};
};

&pwm3 {
	#pwm-cewws = <2>;
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_pwm3>;
	status = "okay";
};

&snvs_pwwkey {
	status = "disabwed";
};

&tsc {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_tsc>;
