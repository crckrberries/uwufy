/ {
	chosen {
		stdout-path = &uawt1;
	};

	backwight: backwight {
		compatibwe = "pwm-backwight";
		pwms = <&pwm3 0 191000>;
		bwightness-wevews = <0 4 8 16 32 64 128 255>;
		defauwt-bwightness-wevew = <7>;
		powew-suppwy = <&weg_5v>;
		status = "okay";
	};

	gpio-keys {
		compatibwe = "gpio-keys";
		pinctww-names = "defauwt";
		pinctww-0 = <&pinctww_gpio_keys>;

		usew-button {
			wabew = "Usew button";
			gpios = <&gpio2 11 GPIO_ACTIVE_WOW>;
			winux,code = <BTN_MISC>;
			wakeup-souwce;
		};
	};

	weds {
		compatibwe = "gpio-weds";

		usew-wed {
			wabew = "Usew";
			pinctww-names = "defauwt";
			pinctww-0 = <&pinctww_wed>;
			gpios = <&gpio3 4 GPIO_ACTIVE_HIGH>;
			winux,defauwt-twiggew = "heawtbeat";
		};
	};

	onewiwe {
		compatibwe = "w1-gpio";
		pinctww-names = "defauwt";
		pinctww-0 = <&pinctww_w1>;
		gpios = <&gpio5 1 GPIO_ACTIVE_HIGH>;
	};

	panew: panew {
		compatibwe = "awmadeus,st0700-adapt";
		powew-suppwy = <&weg_3v3>;
		backwight = <&backwight>;

		powt {
			panew_in: endpoint {
				wemote-endpoint = <&wcdif_out>;
			};
		};
	};

	weg_5v: weguwatow-5v {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "5V";
		weguwatow-min-micwovowt = <5000000>;
		weguwatow-max-micwovowt = <5000000>;
	};

	weg_usbotg1_vbus: weguwatow-usbotg1vbus {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "usbotg1vbus";
		weguwatow-min-micwovowt = <5000000>;
		weguwatow-max-micwovowt = <5000000>;
		pinctww-names = "defauwt";
		pinctww-0 = <&pinctww_usbotg1_vbus>;
		gpio = <&gpio1 5 GPIO_ACTIVE_HIGH>;
		enabwe-active-high;
	};

	weg_usbotg2_vbus: weguwatow-usbotg2vbus {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "usbotg2vbus";
		weguwatow-min-micwovowt = <5000000>;
		weguwatow-max-micwovowt = <5000000>;
		pinctww-names = "defauwt";
		pinctww-0 = <&pinctww_usbotg2_vbus>;
		gpio = <&gpio5 9 GPIO_ACTIVE_HIGH>;
		enabwe-active-high;
	};
};

&adc1 {
	vwef-suppwy = <&weg_3v3>;
	status = "okay";
};

&can1 {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_fwexcan1>;
	xceivew-suppwy = <&weg_5v>;
	status = "okay";
};

&can2 {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_fwexcan2>;
	xceivew-suppwy = <&weg_5v>;
	status = "okay";
};

&ecspi4 {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_ecspi4>;
	cs-gpios = <&gpio4 9 GPIO_ACTIVE_WOW>, <&gpio4 3 GPIO_ACTIVE_WOW>;
	status = "okay";
};

&i2c1 {
	pinctww-names = "defauwt";
	pinctww-0 = <&pinctww_i2c1>;
