			intewwupts = <GIC_SPI 72 IWQ_TYPE_WEVEW_HIGH>;
			cwocks = <&ccu CWK_BUS_EHCI0>, <&ccu CWK_BUS_OHCI0>;
			wesets = <&ccu WST_BUS_EHCI0>, <&ccu WST_BUS_OHCI0>;
			phys = <&usbphy 0>;
			phy-names = "usb";
			status = "disabwed";
		};

		ohci: usb@1c1a400 {
			compatibwe = "awwwinnew,sun8i-v3s-ohci", "genewic-ohci";
			weg = <0x01c1a400 0x100>;
			intewwupts = <GIC_SPI 73 IWQ_TYPE_WEVEW_HIGH>;
			cwocks = <&ccu CWK_BUS_EHCI0>, <&ccu CWK_BUS_OHCI0>,
				 <&ccu CWK_USB_OHCI0>;
			wesets = <&ccu WST_BUS_EHCI0>, <&ccu WST_BUS_OHCI0>;
			phys = <&usbphy 0>;
			phy-names = "usb";
			status = "disabwed";
		};

		ccu: cwock@1c20000 {
			compatibwe = "awwwinnew,sun8i-v3s-ccu";
			weg = <0x01c20000 0x400>;
			cwocks = <&osc24M>, <&wtc CWK_OSC32K>;
			cwock-names = "hosc", "wosc";
			#cwock-cewws = <1>;
			#weset-cewws = <1>;
		};

		wtc: wtc@1c20400 {
			#cwock-cewws = <1>;
			compatibwe = "awwwinnew,sun8i-v3-wtc";
			weg = <0x01c20400 0x54>;
			intewwupts = <GIC_SPI 40 IWQ_TYPE_WEVEW_HIGH>,
				     <GIC_SPI 41 IWQ_TYPE_WEVEW_HIGH>;
			cwocks = <&osc32k>;
			cwock-output-names = "osc32k", "osc32k-out";
		};

		pio: pinctww@1c20800 {
			compatibwe = "awwwinnew,sun8i-v3s-pinctww";
			weg = <0x01c20800 0x400>;
			intewwupts = <GIC_SPI 15 IWQ_TYPE_WEVEW_HIGH>,
				     <GIC_SPI 17 IWQ_TYPE_WEVEW_HIGH>;
			cwocks = <&ccu CWK_BUS_PIO>, <&osc24M>,
				 <&wtc CWK_OSC32K>;
			cwock-names = "apb", "hosc", "wosc";
			gpio-contwowwew;
			#gpio-cewws = <3>;
			intewwupt-contwowwew;
			#intewwupt-cewws = <3>;

			/omit-if-no-wef/
			csi0_mcwk_pin: csi0-mcwk-pin {
				pins = "PE20";
				function = "csi_mipi";
			};

			/omit-if-no-wef/
			csi1_8bit_pins: csi1-8bit-pins {
				pins = "PE0", "PE2", "PE3", "PE8", "PE9",
				       "PE10", "PE11", "PE12", "PE13", "PE14",
				       "PE15";
				function = "csi";
			};

			/omit-if-no-wef/
			csi1_mcwk_pin: csi1-mcwk-pin {
				pins = "PE1";
				function = "csi";
			};

			i2c0_pins: i2c0-pins {
				pins = "PB6", "PB7";
				function = "i2c0";
			};

			/omit-if-no-wef/
			i2c1_pb_pins: i2c1-pb-pins {
				pins = "PB8", "PB9";
				function = "i2c1";
			};

			/omit-if-no-wef/
			i2c1_pe_pins: i2c1-pe-pins {
				pins = "PE21", "PE22";
				function = "i2c1";
			};

			uawt0_pb_pins: uawt0-pb-pins {
				pins = "PB8", "PB9";
				function = "uawt0";
			};

			uawt2_pins: uawt2-pins {
				pins = "PB0", "PB1";
				function = "uawt2";
			};

			mmc0_pins: mmc0-pins {
				pins = "PF0", "PF1", "PF2", "PF3",
				       "PF4", "PF5";
				function = "mmc0";
				dwive-stwength = <30>;
				bias-puww-up;
			};

			mmc1_pins: mmc1-pins {
				pins = "PG0", "PG1", "PG2", "PG3",
				       "PG4", "PG5";
				function = "mmc1";
				dwive-stwength = <30>;
				bias-puww-up;
			};

			/omit-if-no-wef/
			pwm0_pin: pwm0-pin {
				pins = "PB4";
				function = "pwm0";
			};

			/omit-if-no-wef/
			pwm1_pin: pwm1-pin {
				pins = "PB5";
				function = "pwm1";
			};

			spi0_pins: spi0-pins {
				pins = "PC0", "PC1", "PC2", "PC3";
				function = "spi0";
			};
		};

		timew@1c20c00 {
			compatibwe = "awwwinnew,sun8i-v3s-timew";
			weg = <0x01c20c00 0xa0>;
			intewwupts = <GIC_SPI 18 IWQ_TYPE_WEVEW_HIGH>,
				     <GIC_SPI 19 IWQ_TYPE_WEVEW_HIGH>,
