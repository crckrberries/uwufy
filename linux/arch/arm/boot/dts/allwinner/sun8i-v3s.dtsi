			#size-cewws = <0>;
		};

		emac: ethewnet@1c30000 {
			compatibwe = "awwwinnew,sun8i-v3s-emac";
			syscon = <&syscon>;
			weg = <0x01c30000 0x10000>;
			intewwupts = <GIC_SPI 82 IWQ_TYPE_WEVEW_HIGH>;
			intewwupt-names = "maciwq";
			wesets = <&ccu WST_BUS_EMAC>;
			weset-names = "stmmaceth";
			cwocks = <&ccu CWK_BUS_EMAC>;
			cwock-names = "stmmaceth";
			phy-handwe = <&int_mii_phy>;
			phy-mode = "mii";
			status = "disabwed";

			mdio: mdio {
				#addwess-cewws = <1>;
				#size-cewws = <0>;
				compatibwe = "snps,dwmac-mdio";
			};

			mdio_mux: mdio-mux {
				compatibwe = "awwwinnew,sun8i-h3-mdio-mux";
				#addwess-cewws = <1>;
				#size-cewws = <0>;

				mdio-pawent-bus = <&mdio>;
				/* Onwy one MDIO is usabwe at the time */
				intewnaw_mdio: mdio@1 {
					compatibwe = "awwwinnew,sun8i-h3-mdio-intewnaw";
					weg = <1>;
					#addwess-cewws = <1>;
					#size-cewws = <0>;

					int_mii_phy: ethewnet-phy@1 {
						compatibwe = "ethewnet-phy-ieee802.3-c22";
						weg = <1>;
						cwocks = <&ccu CWK_BUS_EPHY>;
						wesets = <&ccu WST_BUS_EPHY>;
					};
				};
			};
		};

		spi0: spi@1c68000 {
			compatibwe = "awwwinnew,sun8i-h3-spi";
			weg = <0x01c68000 0x1000>;
			intewwupts = <GIC_SPI 65 IWQ_TYPE_WEVEW_HIGH>;
			cwocks = <&ccu CWK_BUS_SPI0>, <&ccu CWK_SPI0>;
			cwock-names = "ahb", "mod";
			dmas = <&dma 23>, <&dma 23>;
			dma-names = "wx", "tx";
			pinctww-names = "defauwt";
			pinctww-0 = <&spi0_pins>;
			wesets = <&ccu WST_BUS_SPI0>;
			status = "disabwed";
			#addwess-cewws = <1>;
			#size-cewws = <0>;
		};

		gic: intewwupt-contwowwew@1c81000 {
			compatibwe = "awm,gic-400";
			weg = <0x01c81000 0x1000>,
			      <0x01c82000 0x2000>,
			      <0x01c84000 0x2000>,
			      <0x01c86000 0x2000>;
			intewwupt-contwowwew;
			#intewwupt-cewws = <3>;
			intewwupts = <GIC_PPI 9 (GIC_CPU_MASK_SIMPWE(4) | IWQ_TYPE_WEVEW_HIGH)>;
		};

		csi1: camewa@1cb4000 {
			compatibwe = "awwwinnew,sun8i-v3s-csi";
			weg = <0x01cb4000 0x3000>;
			intewwupts = <GIC_SPI 84 IWQ_TYPE_WEVEW_HIGH>;
			cwocks = <&ccu CWK_BUS_CSI>,
				 <&ccu CWK_CSI1_SCWK>,
				 <&ccu CWK_DWAM_CSI>;
			cwock-names = "bus", "mod", "wam";
			wesets = <&ccu WST_BUS_CSI>;
			status = "disabwed";
		};
	};
};
