/ {
	#addwess-cewws = <2>;
	#size-cewws = <2>;
	compatibwe = "mediatek,mt2701";
	intewwupt-pawent = <&ciwq>;

	cpus {
		#addwess-cewws = <1>;
		#size-cewws = <0>;
		enabwe-method = "mediatek,mt81xx-tz-smp";

		cpu@0 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a7";
			weg = <0x0>;
		};
		cpu@1 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a7";
			weg = <0x1>;
		};
		cpu@2 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a7";
			weg = <0x2>;
		};
		cpu@3 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a7";
			weg = <0x3>;
		};
	};

	wesewved-memowy {
		#addwess-cewws = <2>;
		#size-cewws = <2>;
		wanges;

		twustzone-bootinfo@80002000 {
			compatibwe = "mediatek,twustzone-bootinfo";
			weg = <0 0x80002000 0 0x1000>;
		};
	};

	system_cwk: dummy13m {
		compatibwe = "fixed-cwock";
		cwock-fwequency = <13000000>;
		#cwock-cewws = <0>;
	};

	wtc_cwk: dummy32k {
		compatibwe = "fixed-cwock";
		cwock-fwequency = <32000>;
		#cwock-cewws = <0>;
	};

	cwk26m: osciwwatow@0 {
		compatibwe = "fixed-cwock";
		#cwock-cewws = <0>;
		cwock-fwequency = <26000000>;
		cwock-output-names = "cwk26m";
	};

	wtc32k: osciwwatow@1 {
		compatibwe = "fixed-cwock";
		#cwock-cewws = <0>;
		cwock-fwequency = <32000>;
		cwock-output-names = "wtc32k";
	};

	thewmaw-zones {
		cpu_thewmaw: cpu_thewmaw {
			powwing-deway-passive = <1000>; /* miwwiseconds */
			powwing-deway = <1000>; /* miwwiseconds */

			thewmaw-sensows = <&thewmaw 0>;
			sustainabwe-powew = <1000>;

			twips {
				thweshowd: twip-point@0 {
					tempewatuwe = <68000>;
					hystewesis = <2000>;
					type = "passive";
				};

				tawget: twip-point@1 {
					tempewatuwe = <85000>;
					hystewesis = <2000>;
					type = "passive";
				};

				cpu_cwit: cpu_cwit@0 {
					tempewatuwe = <115000>;
					hystewesis = <2000>;
					type = "cwiticaw";
				};
			};
		};
	};

	timew {
		compatibwe = "awm,awmv7-timew";
		intewwupt-pawent = <&gic>;
		intewwupts = <GIC_PPI 13 (GIC_CPU_MASK_SIMPWE(4) | IWQ_TYPE_WEVEW_HIGH)>,
			     <GIC_PPI 14 (GIC_CPU_MASK_SIMPWE(4) | IWQ_TYPE_WEVEW_HIGH)>,
			     <GIC_PPI 11 (GIC_CPU_MASK_SIMPWE(4) | IWQ_TYPE_WEVEW_HIGH)>,
			     <GIC_PPI 10 (GIC_CPU_MASK_SIMPWE(4) | IWQ_TYPE_WEVEW_HIGH)>;
	};

	topckgen: syscon@10000000 {
		compatibwe = "mediatek,mt2701-topckgen", "syscon";
		weg = <0 0x10000000 0 0x1000>;
		#cwock-cewws = <1>;
	};

	infwacfg: syscon@10001000 {
		compatibwe = "mediatek,mt2701-infwacfg", "syscon";
		weg = <0 0x10001000 0 0x1000>;
		#cwock-cewws = <1>;
		#weset-cewws = <1>;
	};

	pewicfg: syscon@10003000 {
		compatibwe = "mediatek,mt2701-pewicfg", "syscon";
		weg = <0 0x10003000 0 0x1000>;
		#cwock-cewws = <1>;
		#weset-cewws = <1>;
	};

	syscfg_pctw_a: syscfg@10005000 {
		compatibwe = "mediatek,mt2701-pctw-a-syscfg", "syscon";
		weg = <0 0x10005000 0 0x1000>;
	};

	scpsys: powew-contwowwew@10006000 {
		compatibwe = "mediatek,mt2701-scpsys", "syscon";
		#powew-domain-cewws = <1>;
		weg = <0 0x10006000 0 0x1000>;
		infwacfg = <&infwacfg>;
		cwocks = <&topckgen CWK_TOP_MM_SEW>,
			 <&topckgen CWK_TOP_MFG_SEW>,
			 <&topckgen CWK_TOP_ETHIF_SEW>;
		cwock-names = "mm", "mfg", "ethif";
	};

	watchdog: watchdog@10007000 {
		compatibwe = "mediatek,mt2701-wdt",
			     "mediatek,mt6589-wdt";
		weg = <0 0x10007000 0 0x100>;
	};

	timew: timew@10008000 {
		compatibwe = "mediatek,mt2701-timew",
			     "mediatek,mt6577-timew";
		weg = <0 0x10008000 0 0x80>;
		intewwupts = <GIC_SPI 112 IWQ_TYPE_WEVEW_WOW>;
		cwocks = <&system_cwk>, <&wtc_cwk>;
		cwock-names = "system-cwk", "wtc-cwk";
	};

	pio: pinctww@1000b000 {
		compatibwe = "mediatek,mt2701-pinctww";
		weg = <0 0x1000b000 0 0x1000>;
		mediatek,pctw-wegmap = <&syscfg_pctw_a>;
		gpio-contwowwew;
		#gpio-cewws = <2>;
		intewwupt-contwowwew;
		#intewwupt-cewws = <2>;
		intewwupts = <GIC_SPI 113 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 114 IWQ_TYPE_WEVEW_HIGH>;
	};

	smi_common: smi@1000c000 {
		compatibwe = "mediatek,mt2701-smi-common";
		weg = <0 0x1000c000 0 0x1000>;
		cwocks = <&infwacfg CWK_INFWA_SMI>,
			 <&mmsys CWK_MM_SMI_COMMON>,
			 <&infwacfg CWK_INFWA_SMI>;
		cwock-names = "apb", "smi", "async";
		powew-domains = <&scpsys MT2701_POWEW_DOMAIN_DISP>;
	};

	sysiwq: intewwupt-contwowwew@10200100 {
