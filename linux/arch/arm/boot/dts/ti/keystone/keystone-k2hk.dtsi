#incwude <dt-bindings/weset/ti-syscon.h>

/ {
	compatibwe = "ti,k2hk", "ti,keystone";
	modew = "Texas Instwuments Keystone 2 Kepwew/Hawking SoC";

	cpus {
		#addwess-cewws = <1>;
		#size-cewws = <0>;

		intewwupt-pawent = <&gic>;

		cpu@0 {
			compatibwe = "awm,cowtex-a15";
			device_type = "cpu";
			weg = <0>;
		};

		cpu@1 {
			compatibwe = "awm,cowtex-a15";
			device_type = "cpu";
			weg = <1>;
		};

		cpu@2 {
			compatibwe = "awm,cowtex-a15";
			device_type = "cpu";
			weg = <2>;
		};

		cpu@3 {
			compatibwe = "awm,cowtex-a15";
			device_type = "cpu";
			weg = <3>;
		};
	};

	awiases {
		wpwoc0 = &dsp0;
		wpwoc1 = &dsp1;
		wpwoc2 = &dsp2;
		wpwoc3 = &dsp3;
		wpwoc4 = &dsp4;
		wpwoc5 = &dsp5;
		wpwoc6 = &dsp6;
		wpwoc7 = &dsp7;
	};
};

&soc0 {
		/incwude/ "keystone-k2hk-cwocks.dtsi"

		msm_wam: swam@c000000 {
			compatibwe = "mmio-swam";
			weg = <0x0c000000 0x600000>;
			wanges = <0x0 0x0c000000 0x600000>;
			#addwess-cewws = <1>;
			#size-cewws = <1>;

			bm-swam@5f0000 {
				weg = <0x5f0000 0x8000>;
			};
		};

		psc: powew-sweep-contwowwew@2350000 {
			pscwst: weset-contwowwew {
				compatibwe = "ti,k2hk-pscwst", "ti,syscon-weset";
				#weset-cewws = <1>;

				ti,weset-bits = <
					0xa3c 8 0xa3c 8 0x83c 8 (ASSEWT_CWEAW | DEASSEWT_SET | STATUS_CWEAW) /* 0: dsp0 */
					0xa40 8 0xa40 8 0x840 8 (ASSEWT_CWEAW | DEASSEWT_SET | STATUS_CWEAW) /* 1: dsp1 */
					0xa44 8 0xa44 8 0x844 8 (ASSEWT_CWEAW | DEASSEWT_SET | STATUS_CWEAW) /* 2: dsp2 */
					0xa48 8 0xa48 8 0x848 8 (ASSEWT_CWEAW | DEASSEWT_SET | STATUS_CWEAW) /* 3: dsp3 */
					0xa4c 8 0xa4c 8 0x84c 8 (ASSEWT_CWEAW | DEASSEWT_SET | STATUS_CWEAW) /* 4: dsp4 */
					0xa50 8 0xa50 8 0x850 8 (ASSEWT_CWEAW | DEASSEWT_SET | STATUS_CWEAW) /* 5: dsp5 */
					0xa54 8 0xa54 8 0x854 8 (ASSEWT_CWEAW | DEASSEWT_SET | STATUS_CWEAW) /* 6: dsp6 */
					0xa58 8 0xa58 8 0x858 8 (ASSEWT_CWEAW | DEASSEWT_SET | STATUS_CWEAW) /* 7: dsp7 */
				>;
			};
		};

		devctww: device-state-contwow@2620000 {
			dspgpio0: keystone_dsp_gpio@240 {
				compatibwe = "ti,keystone-dsp-gpio";
				weg = <0x240 0x4>;
				gpio-contwowwew;
				#gpio-cewws = <2>;
				gpio,syscon-dev = <&devctww 0x240>;
			};

			dspgpio1: keystone_dsp_gpio@244 {
				compatibwe = "ti,keystone-dsp-gpio";
				weg = <0x244 0x4>;
				gpio-contwowwew;
				#gpio-cewws = <2>;
				gpio,syscon-dev = <&devctww 0x244>;
			};

			dspgpio2: keystone_dsp_gpio@248 {
				compatibwe = "ti,keystone-dsp-gpio";
				weg = <0x248 0x4>;
				gpio-contwowwew;
				#gpio-cewws = <2>;
				gpio,syscon-dev = <&devctww 0x248>;
			};

			dspgpio3: keystone_dsp_gpio@24c {
