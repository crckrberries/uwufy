				weg = <0x25c 0x4>;
				gpio-contwowwew;
				#gpio-cewws = <2>;
				gpio,syscon-dev = <&devctww 0x25c>;
			};
		};

		dsp0: dsp@10800000 {
			compatibwe = "ti,k2hk-dsp";
			weg = <0x10800000 0x00100000>,
			      <0x10e00000 0x00008000>,
			      <0x10f00000 0x00008000>;
			weg-names = "w2swam", "w1pwam", "w1dwam";
			cwocks = <&cwkgem0>;
			ti,syscon-dev = <&devctww 0x40>;
			wesets = <&pscwst 0>;
			intewwupt-pawent = <&kiwq0>;
			intewwupts = <0 8>;
			intewwupt-names = "vwing", "exception";
			kick-gpios = <&dspgpio0 27 0>;
			status = "disabwed";
		};

		dsp1: dsp@11800000 {
			compatibwe = "ti,k2hk-dsp";
			weg = <0x11800000 0x00100000>,
