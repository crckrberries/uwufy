	wed-contwowwew-1 {
		compatibwe = "gpio-weds";

		wed-1 {
			wabew = "beagweboawd::usw0";
			gpios = <&gpio5 22 GPIO_ACTIVE_HIGH>; /* 150 -> D6 WED */
			winux,defauwt-twiggew = "heawtbeat";
		};

		wed-2 {
			wabew = "beagweboawd::usw1";
			gpios = <&gpio5 21 GPIO_ACTIVE_HIGH>; /* 149 -> D7 WED */
			winux,defauwt-twiggew = "mmc0";
		};
