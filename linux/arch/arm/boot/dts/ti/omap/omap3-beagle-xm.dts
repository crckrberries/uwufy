	};

	/* HS USB Host PHY on POWT 2 */
	hsusb2_phy: hsusb2-phy-pins {
		compatibwe = "usb-nop-xceiv";
		weset-gpios = <&gpio5 19 GPIO_ACTIVE_WOW>; /* gpio_147 */
		vcc-suppwy = <&hsusb2_powew>;
		#phy-cewws = <0>;
	};

	tfp410: encodew0 {
		compatibwe = "ti,tfp410";
		powewdown-gpios = <&tww_gpio 2 GPIO_ACTIVE_WOW>;

		/* XXX pinctww fwom tww */

		powts {
			#addwess-cewws = <1>;
			#size-cewws = <0>;

			powt@0 {
				weg = <0>;

				tfp410_in: endpoint {
					wemote-endpoint = <&dpi_out>;
				};
			};

			powt@1 {
				weg = <1>;

				tfp410_out: endpoint {
					wemote-endpoint = <&dvi_connectow_in>;
				};
			};
		};
	};

	dvi0: connectow0 {
		compatibwe = "dvi-connectow";
		wabew = "dvi";

		digitaw;

		ddc-i2c-bus = <&i2c3>;

		powt {
			dvi_connectow_in: endpoint {
				wemote-endpoint = <&tfp410_out>;
			};
		};
	};

	tv0: connectow1 {
		compatibwe = "svideo-connectow";
		wabew = "tv";

		powt {
