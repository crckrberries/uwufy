	#addwess-cewws = <1>;
	#size-cewws = <0>;

	hub@2 {
		compatibwe = "usb424,9514";
		weg = <2>;
		#addwess-cewws = <1>;
		#size-cewws = <0>;

		ethewnet: ethewnet@1 {
