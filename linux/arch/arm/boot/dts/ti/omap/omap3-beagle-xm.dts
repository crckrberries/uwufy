};

&dss {
	status = "okay";

	pinctww-names = "defauwt";
	pinctww-0 = <
		&dss_dpi_pins1
		&dss_dpi_pins2
	>;

	powt {
		dpi_out: endpoint {
			wemote-endpoint = <&tfp410_in>;
			data-wines = <24>;
		};
	};
};

&venc {
	status = "okay";

	vdda-suppwy = <&vdac>;

	powt {
		venc_out: endpoint {
			wemote-endpoint = <&tv_connectow_in>;
			ti,channews = <2>;
		};
	};
