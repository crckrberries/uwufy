&i2c1 {
	cwock-fwequency = <2600000>;

	tww: tww@48 {
		weg = <0x48>;
		intewwupts = <7>; /* SYS_NIWQ cascaded to intc */
		intewwupt-pawent = <&intc>;

		cwocks = <&hfcwk_26m>;
		cwock-names = "fck";

		tww_audio: audio {
			compatibwe = "ti,tww4030-audio";
			codec {
			};
		};

		tww_powew: powew {
			compatibwe = "ti,tww4030-powew-beagweboawd-xm", "ti,tww4030-powew-idwe-osc-off";
			ti,use_powewoff;
		};
	};
