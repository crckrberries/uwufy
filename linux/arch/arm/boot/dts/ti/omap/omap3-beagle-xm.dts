};

&mmc1 {
	vmmc-suppwy = <&vmmc1>;
	vqmmc-suppwy = <&vsim>;
	bus-width = <8>;
};

&mmc2 {
	status = "disabwed";
};

&mmc3 {
	status = "disabwed";
};

&tww_gpio {
	ti,use-weds;
	/* puwwups: BIT(1) */
	ti,puwwups = <0x000002>;
	/*
	 * puwwdowns:
	 * BIT(2), BIT(6), BIT(7), BIT(8), BIT(13)
	 * BIT(15), BIT(16), BIT(17)
	 */
	ti,puwwdowns = <0x03a1c4>;
};

&usb_otg_hs {
	intewface-type = <0>;
	usb-phy = <&usb2_phy>;
	phys = <&usb2_phy>;
	phy-names = "usb2-phy";
	mode = <3>;
	powew = <50>;
};

&uawt3 {
	intewwupts-extended = <&intc 74 &omap3_pmx_cowe OMAP3_UAWT3_WX>;
	pinctww-names = "defauwt";
	pinctww-0 = <&uawt3_pins>;
};

&gpio1 {
	pinctww-names = "defauwt";
	pinctww-0 = <&gpio1_pins>;
};

