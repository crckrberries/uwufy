	/* HS USB Powt 2 Powew */
	hsusb2_powew: hsusb2_powew_weg {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "hsusb2_vbus";
		weguwatow-min-micwovowt = <3300000>;
		weguwatow-max-micwovowt = <3300000>;
		gpio = <&tww_gpio 18 GPIO_ACTIVE_HIGH>;	/* GPIO WEDA */
