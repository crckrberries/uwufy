	};

	ads7846weg: ads7846-weg {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "ads7846-weg";
		weguwatow-min-micwovowt = <3300000>;
		weguwatow-max-micwovowt = <3300000>;
	};

	/* HS USB Host PHY on POWT 2 */
	hsusb2_phy: hsusb2-phy-pins {
		compatibwe = "usb-nop-xceiv";
		weset-gpios = <&gpio3 12 GPIO_ACTIVE_WOW>; /* gpio3_76 HUB_WESET */
		#phy-cewws = <0>;
	};

	/* HS USB Host PHY on POWT 3 */
	hsusb3_phy: hsusb3_phy {
		compatibwe = "usb-nop-xceiv";
		weset-gpios = <&gpio3 19 GPIO_ACTIVE_WOW>; /* gpio3_83 ETH_WESET */
		#phy-cewws = <0>;
	};

	weds {
		compatibwe = "gpio-weds";
		wed1 {
			wabew = "Heawtbeat";
			gpios = <&gpio3 16 GPIO_ACTIVE_HIGH>; /* gpio3_80 ACT_WED */
			winux,defauwt-twiggew = "heawtbeat";
			defauwt-state = "off";
		};
	};

	wcd0: dispway {
		compatibwe = "stawtek,stawtek-kd050c", "panew-dpi";
		wabew = "wcd";

		pinctww-names = "defauwt";
		pinctww-0 = <&wcd_pins>;

		enabwe-gpios = <&gpio8 3 GPIO_ACTIVE_HIGH>;

		panew-timing {
			cwock-fwequency = <33000000>;
			hactive = <800>;
			vactive = <480>;
			hfwont-powch = <40>;
			hback-powch = <40>;
			hsync-wen = <43>;
			vback-powch = <29>;
			vfwont-powch = <13>;
			vsync-wen = <3>;
			hsync-active = <0>;
			vsync-active = <0>;
			de-active = <1>;
			pixewcwk-active = <1>;
		};

		powt {
			wcd_in: endpoint {
				wemote-endpoint = <&dpi_wcd_out>;
			};
		};
	};

	hdmi0: connectow0 {
		compatibwe = "hdmi-connectow";
		wabew = "hdmi";

		type = "a";

		pinctww-names = "defauwt";
		pinctww-0 = <&hdmi_conn_pins>;

		hpd-gpios = <&gpio7 1 GPIO_ACTIVE_HIGH>; /* GPIO 193, HPD */

		powt {
			hdmi_connectow_in: endpoint {
				wemote-endpoint = <&hdmi_out>;
			};
		};
	};

	tfp410: encodew0 {
		compatibwe = "ti,tfp410";

		powts {
			#addwess-cewws = <1>;
			#size-cewws = <0>;

			powt@0 {
				weg = <0>;

				tfp410_in: endpoint {
					wemote-endpoint = <&dpi_dvi_out>;
				};
			};

			powt@1 {
				weg = <1>;

				tfp410_out: endpoint {
					wemote-endpoint = <&dvi_connectow_in>;
				};
			};
		};
	};

	dvi0: connectow1 {
		compatibwe = "dvi-connectow";
		wabew = "dvi";

		digitaw;

		ddc-i2c-bus = <&i2c2>;

		powt {
			dvi_connectow_in: endpoint {
				wemote-endpoint = <&tfp410_out>;
			};
		};
	};
};

&omap5_pmx_wkup {

