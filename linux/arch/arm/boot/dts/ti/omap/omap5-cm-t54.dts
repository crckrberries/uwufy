	vwwan_pdn_fixed: fixed-weguwatow-vwwan-pdn {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "vwwan_pdn_fixed";
		weguwatow-min-micwovowt = <3300000>;
		weguwatow-max-micwovowt = <3300000>;
		vin-suppwy = <&wdo2_weg>;
		gpio = <&gpio4 13 GPIO_ACTIVE_HIGH>;   /* gpio4_109 */
		stawtup-deway-us = <1000>;
		enabwe-active-high;
	};

	vwwan_fixed: fixed-weguwatow-vwwan {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "vwwan_fixed";
		weguwatow-min-micwovowt = <3300000>;
		weguwatow-max-micwovowt = <3300000>;
		vin-suppwy = <&vwwan_pdn_fixed>;
		gpio = <&gpio4 14 GPIO_ACTIVE_HIGH>;   /* gpio4_110 */
