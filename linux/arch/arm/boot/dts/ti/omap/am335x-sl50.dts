	emmc_pwwseq_pins: emmc-pwwseq-pins {
		pinctww-singwe,pins = <
			AM33XX_PADCONF(AM335X_PIN_GPMC_A4, PIN_OUTPUT_PUWWUP, MUX_MODE7)	/* gpmc_a4.gpio1_20 */
		>;
