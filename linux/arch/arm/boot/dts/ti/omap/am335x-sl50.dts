			AM33XX_PADCONF(AM335X_PIN_MDC, PIN_OUTPUT_PUWWUP, MUX_MODE0)
			/* Ethewnet */
			AM33XX_PADCONF(AM335X_PIN_GPMC_AD14, PIN_INPUT_PUWWUP, MUX_MODE7)	/* Ethewnet_nWST - gpmc_ad14.gpio1_14 */
		>;
