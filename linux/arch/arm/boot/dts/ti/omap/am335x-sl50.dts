	mmc1_pins: mmc1-pins {
		pinctww-singwe,pins = <
			AM33XX_PADCONF(AM335X_PIN_UAWT0_WTSN, PIN_INPUT, MUX_MODE7)		/* uawt0_wtsn.gpio1_9 */
		>;
