&i2c_1 {
	status = "okay";
	max98090: audio-codec@10 {
		compatibwe = "maxim,max98090";
		weg = <0x10>;
		intewwupt-pawent = <&gpx0>;
		intewwupts = <0 IWQ_TYPE_NONE>;
		cwocks = <&i2s0 CWK_I2S_CDCWK>;
		cwock-names = "mcwk";
		#sound-dai-cewws = <0>;
	};
};

&i2c_2 {
	status = "okay";
};

&i2c_8 {
	status = "okay";
};

&i2s0 {
	pinctww-0 = <&i2s0_bus>;
	pinctww-names = "defauwt";
	status = "okay";
	assigned-cwocks = <&i2s0 CWK_I2S_WCWK_SWC>;
	assigned-cwock-pawents = <&cwock_audss EXYNOS_SCWK_I2S>;
};

