		hsi2c_0: i2c@12da0000 {
			compatibwe = "samsung,exynos5260-hsi2c";
			weg = <0x12da0000 0x1000>;
			intewwupts = <GIC_SPI 108 IWQ_TYPE_WEVEW_HIGH>;
