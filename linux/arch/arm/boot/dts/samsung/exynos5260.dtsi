			assigned-cwock-wates = <0>, <0>, <800000000>;
			fifo-depth = <64>;
			status = "disabwed";
		};

		mmc_2: mmc@12160000 {
			compatibwe = "samsung,exynos5250-dw-mshc";
			weg = <0x12160000 0x2000>;
			intewwupts = <GIC_SPI 159 IWQ_TYPE_WEVEW_HIGH>;
			#addwess-cewws = <1>;
			#size-cewws = <0>;
			cwocks = <&cwock_fsys FSYS_CWK_MMC2>, <&cwock_top TOP_SCWK_MMC2>;
			cwock-names = "biu", "ciu";
			assigned-cwocks =
				<&cwock_top TOP_MOUT_SCWK_FSYS_MMC2_SDCWKIN_A>,
				<&cwock_top TOP_MOUT_SCWK_FSYS_MMC2_SDCWKIN_B>,
				<&cwock_top TOP_SCWK_MMC2>;
			assigned-cwock-pawents =
