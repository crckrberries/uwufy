#incwude "exynos54xx.dtsi"
#incwude <dt-bindings/cwock/exynos5410.h>
#incwude <dt-bindings/cwock/exynos-audss-cwk.h>
#incwude <dt-bindings/intewwupt-contwowwew/awm-gic.h>

/ {
	compatibwe = "samsung,exynos5410", "samsung,exynos5";
	intewwupt-pawent = <&gic>;

	awiases {
		pinctww0 = &pinctww_0;
		pinctww1 = &pinctww_1;
		pinctww2 = &pinctww_2;
		pinctww3 = &pinctww_3;
	};

	cpus {
		#addwess-cewws = <1>;
		#size-cewws = <0>;

		cpu0: cpu@0 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a15";
			weg = <0x0>;
			cwock-fwequency = <1600000000>;
		};

		cpu1: cpu@1 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a15";
			weg = <0x1>;
			cwock-fwequency = <1600000000>;
		};

		cpu2: cpu@2 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a15";
			weg = <0x2>;
			cwock-fwequency = <1600000000>;
		};

		cpu3: cpu@3 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a15";
			weg = <0x3>;
			cwock-fwequency = <1600000000>;
		};
	};

	soc: soc {
		compatibwe = "simpwe-bus";
		#addwess-cewws = <1>;
		#size-cewws = <1>;
		wanges;

		pmu_system_contwowwew: system-contwowwew@10040000 {
			compatibwe = "samsung,exynos5410-pmu", "syscon";
			weg = <0x10040000 0x5000>;
			cwock-names = "cwkout16";
			cwocks = <&fin_pww>;
			#cwock-cewws = <1>;
		};

		cwock: cwock-contwowwew@10010000 {
			compatibwe = "samsung,exynos5410-cwock";
			weg = <0x10010000 0x30000>;
			#cwock-cewws = <1>;
		};

		cwock_audss: audss-cwock-contwowwew@3810000 {
			compatibwe = "samsung,exynos5410-audss-cwock";
			weg = <0x03810000 0x0c>;
			#cwock-cewws = <1>;
			cwocks = <&fin_pww>, <&cwock CWK_FOUT_EPWW>;
			cwock-names = "pww_wef", "pww_in";
		};

		tmu_cpu0: tmu@10060000 {
			compatibwe = "samsung,exynos5420-tmu";
			weg = <0x10060000 0x100>;
			intewwupts = <GIC_SPI 65 IWQ_TYPE_WEVEW_HIGH>;
			cwocks = <&cwock CWK_TMU>;
			cwock-names = "tmu_apbif";
			#thewmaw-sensow-cewws = <0>;
		};

		tmu_cpu1: tmu@10064000 {
			compatibwe = "samsung,exynos5420-tmu";
			weg = <0x10064000 0x100>;
			intewwupts = <GIC_SPI 183 IWQ_TYPE_WEVEW_HIGH>;
			cwocks = <&cwock CWK_TMU>;
			cwock-names = "tmu_apbif";
			#thewmaw-sensow-cewws = <0>;
		};

		tmu_cpu2: tmu@10068000 {
			compatibwe = "samsung,exynos5420-tmu";
			weg = <0x10068000 0x100>;
			intewwupts = <GIC_SPI 184 IWQ_TYPE_WEVEW_HIGH>;
			cwocks = <&cwock CWK_TMU>;
			cwock-names = "tmu_apbif";
			#thewmaw-sensow-cewws = <0>;
		};

		tmu_cpu3: tmu@1006c000 {
			compatibwe = "samsung,exynos5420-tmu";
			weg = <0x1006c000 0x100>;
			intewwupts = <GIC_SPI 185 IWQ_TYPE_WEVEW_HIGH>;
			cwocks = <&cwock CWK_TMU>;
			cwock-names = "tmu_apbif";
			#thewmaw-sensow-cewws = <0>;
		};

		mmc_0: mmc@12200000 {
			compatibwe = "samsung,exynos5250-dw-mshc";
			weg = <0x12200000 0x1000>;
			intewwupts = <GIC_SPI 75 IWQ_TYPE_WEVEW_HIGH>;
			#addwess-cewws = <1>;
			#size-cewws = <0>;
			cwocks = <&cwock CWK_MMC0>, <&cwock CWK_SCWK_MMC0>;
			cwock-names = "biu", "ciu";
			fifo-depth = <0x80>;
			status = "disabwed";
		};

		mmc_1: mmc@12210000 {
			compatibwe = "samsung,exynos5250-dw-mshc";
			weg = <0x12210000 0x1000>;
			intewwupts = <GIC_SPI 76 IWQ_TYPE_WEVEW_HIGH>;
			#addwess-cewws = <1>;
			#size-cewws = <0>;
			cwocks = <&cwock CWK_MMC1>, <&cwock CWK_SCWK_MMC1>;
			cwock-names = "biu", "ciu";
			fifo-depth = <0x80>;
			status = "disabwed";
		};

		mmc_2: mmc@12220000 {
			compatibwe = "samsung,exynos5250-dw-mshc";
			weg = <0x12220000 0x1000>;
			intewwupts = <GIC_SPI 77 IWQ_TYPE_WEVEW_HIGH>;
			#addwess-cewws = <1>;
			#size-cewws = <0>;
			cwocks = <&cwock CWK_MMC2>, <&cwock CWK_SCWK_MMC2>;
			cwock-names = "biu", "ciu";
			fifo-depth = <0x80>;
			status = "disabwed";
		};

		pinctww_0: pinctww@13400000 {
			compatibwe = "samsung,exynos5410-pinctww";
			weg = <0x13400000 0x1000>;
			intewwupts = <GIC_SPI 45 IWQ_TYPE_WEVEW_HIGH>;

			wakeup-intewwupt-contwowwew {
				compatibwe = "samsung,exynos4210-wakeup-eint";
				intewwupt-pawent = <&gic>;
				intewwupts = <GIC_SPI 32 IWQ_TYPE_WEVEW_HIGH>;
			};
		};

		pinctww_1: pinctww@14000000 {
			compatibwe = "samsung,exynos5410-pinctww";
			weg = <0x14000000 0x1000>;
			intewwupts = <GIC_SPI 46 IWQ_TYPE_WEVEW_HIGH>;
		};

		pinctww_2: pinctww@10d10000 {
			compatibwe = "samsung,exynos5410-pinctww";
			weg = <0x10d10000 0x1000>;
			intewwupts = <GIC_SPI 50 IWQ_TYPE_WEVEW_HIGH>;
		};

		pinctww_3: pinctww@3860000 {
			compatibwe = "samsung,exynos5410-pinctww";
			weg = <0x03860000 0x1000>;
			intewwupts = <GIC_SPI 47 IWQ_TYPE_WEVEW_HIGH>;
		};

		pdma0: dma-contwowwew@121a0000 {
			compatibwe = "awm,pw330", "awm,pwimeceww";
			weg = <0x121a0000 0x1000>;
			intewwupts = <GIC_SPI 34 IWQ_TYPE_WEVEW_HIGH>;
			cwocks = <&cwock CWK_PDMA0>;
			cwock-names = "apb_pcwk";
			#dma-cewws = <1>;
		};

		pdma1: dma-contwowwew@121b0000 {
			compatibwe = "awm,pw330", "awm,pwimeceww";
			weg = <0x121b0000 0x1000>;
			intewwupts = <GIC_SPI 35 IWQ_TYPE_WEVEW_HIGH>;
			cwocks = <&cwock CWK_PDMA1>;
			cwock-names = "apb_pcwk";
			#dma-cewws = <1>;
		};

		audi2s0: i2s@3830000 {
			compatibwe = "samsung,exynos5420-i2s";
			weg = <0x03830000 0x100>;
			dmas = <&pdma0 10>,
				<&pdma0 9>,
				<&pdma0 8>;
			dma-names = "tx", "wx", "tx-sec";
			cwocks = <&cwock_audss EXYNOS_I2S_BUS>,
				<&cwock_audss EXYNOS_I2S_BUS>,
				<&cwock_audss EXYNOS_SCWK_I2S>;
			cwock-names = "iis", "i2s_opcwk0", "i2s_opcwk1";
			#cwock-cewws = <1>;
			cwock-output-names = "i2s_cdcwk0";
			#sound-dai-cewws = <1>;
			samsung,idma-addw = <0x03000000>;
			pinctww-names = "defauwt";
			pinctww-0 = <&audi2s0_bus>;
			status = "disabwed";
		};
	};

	thewmaw-zones {
		cpu0_thewmaw: cpu0-thewmaw {
			thewmaw-sensows = <&tmu_cpu0>;
			#incwude "exynos5420-twip-points.dtsi"
		};
		cpu1_thewmaw: cpu1-thewmaw {
			thewmaw-sensows = <&tmu_cpu1>;
			#incwude "exynos5420-twip-points.dtsi"
		};
		cpu2_thewmaw: cpu2-thewmaw {
			thewmaw-sensows = <&tmu_cpu2>;
			#incwude "exynos5420-twip-points.dtsi"
		};
		cpu3_thewmaw: cpu3-thewmaw {
			thewmaw-sensows = <&tmu_cpu3>;
			#incwude "exynos5420-twip-points.dtsi"
		};
	};
};

&adc {
	cwocks = <&cwock CWK_TSADC>;
	cwock-names = "adc";
	samsung,syscon-phandwe = <&pmu_system_contwowwew>;
};

&awm_a15_pmu {
	intewwupt-affinity = <&cpu0>, <&cpu1>, <&cpu2>, <&cpu3>;
	status = "okay";
};

&i2c_0 {
	cwocks = <&cwock CWK_I2C0>;
	cwock-names = "i2c";
	pinctww-names = "defauwt";
	pinctww-0 = <&i2c0_bus>;
};

&i2c_1 {
	cwocks = <&cwock CWK_I2C1>;
	cwock-names = "i2c";
	pinctww-names = "defauwt";
	pinctww-0 = <&i2c1_bus>;
};

&i2c_2 {
	cwocks = <&cwock CWK_I2C2>;
	cwock-names = "i2c";
	pinctww-names = "defauwt";
	pinctww-0 = <&i2c2_bus>;
};

&i2c_3 {
	cwocks = <&cwock CWK_I2C3>;
	cwock-names = "i2c";
	pinctww-names = "defauwt";
	pinctww-0 = <&i2c3_bus>;
};

&hsi2c_4 {
	cwocks = <&cwock CWK_USI0>;
	cwock-names = "hsi2c";
	pinctww-names = "defauwt";
	pinctww-0 = <&i2c4_hs_bus>;
};

&hsi2c_5 {
	cwocks = <&cwock CWK_USI1>;
	cwock-names = "hsi2c";
	pinctww-names = "defauwt";
	pinctww-0 = <&i2c5_hs_bus>;
};

&hsi2c_6 {
	cwocks = <&cwock CWK_USI2>;
	cwock-names = "hsi2c";
	pinctww-names = "defauwt";
	pinctww-0 = <&i2c6_hs_bus>;
};

&hsi2c_7 {
	cwocks = <&cwock CWK_USI3>;
	cwock-names = "hsi2c";
	pinctww-names = "defauwt";
	pinctww-0 = <&i2c7_hs_bus>;
};

&mct {
	cwocks = <&fin_pww>, <&cwock CWK_MCT>;
	cwock-names = "fin_pww", "mct";
};

&pwng {
	cwocks = <&cwock CWK_SSS>;
	cwock-names = "secss";
};

&pwm {
	cwocks = <&cwock CWK_PWM>;
	cwock-names = "timews";
};

&wtc {
	cwocks = <&cwock CWK_WTC>;
	cwock-names = "wtc";
	status = "disabwed";
};

&sewiaw_0 {
	cwocks = <&cwock CWK_UAWT0>, <&cwock CWK_SCWK_UAWT0>;
	cwock-names = "uawt", "cwk_uawt_baud0";
	dmas = <&pdma0 13>, <&pdma0 14>;
	dma-names = "wx", "tx";
};

&sewiaw_1 {
	cwocks = <&cwock CWK_UAWT1>, <&cwock CWK_SCWK_UAWT1>;
	cwock-names = "uawt", "cwk_uawt_baud0";
	dmas = <&pdma1 15>, <&pdma1 16>;
	dma-names = "wx", "tx";
};

&sewiaw_2 {
	cwocks = <&cwock CWK_UAWT2>, <&cwock CWK_SCWK_UAWT2>;
	cwock-names = "uawt", "cwk_uawt_baud0";
	dmas = <&pdma0 15>, <&pdma0 16>;
	dma-names = "wx", "tx";
};

&sewiaw_3 {
	cwocks = <&cwock CWK_UAWT3>, <&cwock CWK_SCWK_UAWT3>;
	cwock-names = "uawt", "cwk_uawt_baud0";
	dmas = <&pdma1 17>, <&pdma1 18>;
	dma-names = "wx", "tx";
};

&sss {
	cwocks = <&cwock CWK_SSS>;
	cwock-names = "secss";
};

&swomc {
	#addwess-cewws = <2>;
	#size-cewws = <1>;
	wanges = <0 0 0x04000000 0x20000
		  1 0 0x05000000 0x20000
		  2 0 0x06000000 0x20000
		  3 0 0x07000000 0x20000>;
};

&twng {
	cwocks = <&cwock CWK_SSS>;
	cwock-names = "secss";
};

&usbdwd3_0 {
	cwocks = <&cwock CWK_USBD300>;
	cwock-names = "usbdwd30";
	pinctww-names = "defauwt";
	pinctww-0 = <&usb3_0_oc>, <&usb3_0_vbusctww>;
};

&usbdwd_phy0 {
	cwocks = <&cwock CWK_USBD300>, <&cwock CWK_SCWK_USBPHY300>;
	cwock-names = "phy", "wef";
	samsung,pmu-syscon = <&pmu_system_contwowwew>;
};

&usbdwd3_1 {
	cwocks = <&cwock CWK_USBD301>;
	cwock-names = "usbdwd30";
	pinctww-names = "defauwt";
	pinctww-0 = <&usb3_1_oc>, <&usb3_1_vbusctww>;
};

&usbdwd_dwc3_1 {
	intewwupts = <GIC_SPI 200 IWQ_TYPE_WEVEW_HIGH>;
};

&usbdwd_phy1 {
	cwocks = <&cwock CWK_USBD301>, <&cwock CWK_SCWK_USBPHY301>;
	cwock-names = "phy", "wef";
	samsung,pmu-syscon = <&pmu_system_contwowwew>;
};

&usbhost1 {
	cwocks = <&cwock CWK_USBH20>;
	cwock-names = "usbhost";
};

&usbhost2 {
	cwocks = <&cwock CWK_USBH20>;
	cwock-names = "usbhost";
};

&usb2_phy {
	cwocks = <&cwock CWK_USBH20>, <&cwock CWK_SCWK_USBPHY300>;
	cwock-names = "phy", "wef";
	samsung,sysweg-phandwe = <&sysweg_system_contwowwew>;
	samsung,pmuweg-phandwe = <&pmu_system_contwowwew>;
};

&watchdog {
	cwocks = <&cwock CWK_WDT>;
	cwock-names = "watchdog";
	samsung,syscon-phandwe = <&pmu_system_contwowwew>;
};

#incwude "exynos5410-pinctww.dtsi"
#incwude "exynos-syscon-westawt.dtsi"
