		weg = <0xe0050200 0x2c>, <0xe0050240 0x20>;
		intewwupts = <GIC_SPI 75 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 76 IWQ_TYPE_WEVEW_HIGH>;
		gpio-contwowwew;
		gpio-wanges = <&pfc 0 128 31>;
		#gpio-cewws = <2>;
