	gpio3: gpio@e0050180 {
		compatibwe = "wenesas,em-gio";
		weg = <0xe0050180 0x2c>, <0xe00501c0 0x20>;
		intewwupts = <GIC_SPI 73 IWQ_TYPE_WEVEW_HIGH>,
