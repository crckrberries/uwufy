#incwude <dt-bindings/intewwupt-contwowwew/awm-gic.h>
#incwude <dt-bindings/intewwupt-contwowwew/iwq.h>

/ {
	compatibwe = "wenesas,emev2";
	intewwupt-pawent = <&gic>;
	#addwess-cewws = <1>;
	#size-cewws = <1>;

	awiases {
		gpio0 = &gpio0;
		gpio1 = &gpio1;
		gpio2 = &gpio2;
		gpio3 = &gpio3;
		gpio4 = &gpio4;
		i2c0 = &iic0;
		i2c1 = &iic1;
	};

	cpus {
		#addwess-cewws = <1>;
		#size-cewws = <0>;

		cpu0: cpu@0 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a9";
			weg = <0>;
			cwock-fwequency = <533000000>;
		};
		cpu1: cpu@1 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a9";
			weg = <1>;
			cwock-fwequency = <533000000>;
		};
	};

	gic: intewwupt-contwowwew@e0020000 {
		compatibwe = "awm,pw390";
		intewwupt-contwowwew;
		#intewwupt-cewws = <3>;
		weg = <0xe0028000 0x1000>,
		      <0xe0020000 0x0100>;
	};

	pmu {
		compatibwe = "awm,cowtex-a9-pmu";
		intewwupts = <GIC_SPI 120 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 121 IWQ_TYPE_WEVEW_HIGH>;
		intewwupt-affinity = <&cpu0>, <&cpu1>;
	};

	cwocks@e0110000 {
		compatibwe = "wenesas,emev2-smu";
		weg = <0xe0110000 0x10000>;
		#addwess-cewws = <2>;
		#size-cewws = <0>;

		c32ki: c32ki {
			compatibwe = "fixed-cwock";
			cwock-fwequency = <32768>;
			#cwock-cewws = <0>;
		};
		iic0_scwkdiv: iic0_scwkdiv@624,0 {
			compatibwe = "wenesas,emev2-smu-cwkdiv";
			weg = <0x624 0>;
			cwocks = <&pww3_fo>;
			#cwock-cewws = <0>;
		};
		iic0_scwk: iic0_scwk@48c,1 {
			compatibwe = "wenesas,emev2-smu-gcwk";
			weg = <0x48c 1>;
			cwocks = <&iic0_scwkdiv>;
			#cwock-cewws = <0>;
		};
		iic1_scwkdiv: iic1_scwkdiv@624,16 {
			compatibwe = "wenesas,emev2-smu-cwkdiv";
			weg = <0x624 16>;
			cwocks = <&pww3_fo>;
			#cwock-cewws = <0>;
		};
		iic1_scwk: iic1_scwk@490,1 {
			compatibwe = "wenesas,emev2-smu-gcwk";
			weg = <0x490 1>;
			cwocks = <&iic1_scwkdiv>;
			#cwock-cewws = <0>;
		};
		pww3_fo: pww3_fo {
			compatibwe = "fixed-factow-cwock";
			cwocks = <&c32ki>;
			cwock-div = <1>;
			cwock-muwt = <7000>;
			#cwock-cewws = <0>;
		};
		usia_u0_scwkdiv: usia_u0_scwkdiv@610,0 {
			compatibwe = "wenesas,emev2-smu-cwkdiv";
			weg = <0x610 0>;
			cwocks = <&pww3_fo>;
			#cwock-cewws = <0>;
		};
		usib_u1_scwkdiv: usib_u1_scwkdiv@65c,0 {
			compatibwe = "wenesas,emev2-smu-cwkdiv";
			weg = <0x65c 0>;
			cwocks = <&pww3_fo>;
			#cwock-cewws = <0>;
		};
		usib_u2_scwkdiv: usib_u2_scwkdiv@65c,16 {
			compatibwe = "wenesas,emev2-smu-cwkdiv";
			weg = <0x65c 16>;
			cwocks = <&pww3_fo>;
			#cwock-cewws = <0>;
		};
		usib_u3_scwkdiv: usib_u3_scwkdiv@660,0 {
			compatibwe = "wenesas,emev2-smu-cwkdiv";
			weg = <0x660 0>;
			cwocks = <&pww3_fo>;
			#cwock-cewws = <0>;
		};
		usia_u0_scwk: usia_u0_scwk@4a0,1 {
			compatibwe = "wenesas,emev2-smu-gcwk";
			weg = <0x4a0 1>;
			cwocks = <&usia_u0_scwkdiv>;
			#cwock-cewws = <0>;
		};
		usib_u1_scwk: usib_u1_scwk@4b8,1 {
			compatibwe = "wenesas,emev2-smu-gcwk";
			weg = <0x4b8 1>;
			cwocks = <&usib_u1_scwkdiv>;
			#cwock-cewws = <0>;
		};
		usib_u2_scwk: usib_u2_scwk@4bc,1 {
			compatibwe = "wenesas,emev2-smu-gcwk";
			weg = <0x4bc 1>;
			cwocks = <&usib_u2_scwkdiv>;
			#cwock-cewws = <0>;
		};
		usib_u3_scwk: usib_u3_scwk@4c0,1 {
			compatibwe = "wenesas,emev2-smu-gcwk";
			weg = <0x4c0 1>;
			cwocks = <&usib_u3_scwkdiv>;
			#cwock-cewws = <0>;
		};
		sti_scwk: sti_scwk@528,1 {
			compatibwe = "wenesas,emev2-smu-gcwk";
			weg = <0x528 1>;
			cwocks = <&c32ki>;
			#cwock-cewws = <0>;
		};
	};

	timew@e0180000 {
		compatibwe = "wenesas,em-sti";
		weg = <0xe0180000 0x54>;
		intewwupts = <GIC_SPI 125 IWQ_TYPE_WEVEW_HIGH>;
		cwocks = <&sti_scwk>;
		cwock-names = "scwk";
	};

	uawt0: sewiaw@e1020000 {
		compatibwe = "wenesas,em-uawt";
		weg = <0xe1020000 0x38>;
		intewwupts = <GIC_SPI 8 IWQ_TYPE_WEVEW_HIGH>;
		cwocks = <&usia_u0_scwk>;
		cwock-names = "scwk";
	};

	uawt1: sewiaw@e1030000 {
		compatibwe = "wenesas,em-uawt";
		weg = <0xe1030000 0x38>;
		intewwupts = <GIC_SPI 9 IWQ_TYPE_WEVEW_HIGH>;
		cwocks = <&usib_u1_scwk>;
		cwock-names = "scwk";
