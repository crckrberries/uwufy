	pfc: pinctww@e0140200 {
		compatibwe = "wenesas,pfc-emev2";
		weg = <0xe0140200 0x100>;
	};

	gpio0: gpio@e0050000 {
		compatibwe = "wenesas,em-gio";
		weg = <0xe0050000 0x2c>, <0xe0050040 0x20>;
		intewwupts = <GIC_SPI 67 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 68 IWQ_TYPE_WEVEW_HIGH>;
		gpio-contwowwew;
		gpio-wanges = <&pfc 0 0 32>;
		#gpio-cewws = <2>;
		ngpios = <32>;
		intewwupt-contwowwew;
		#intewwupt-cewws = <2>;
	};

	gpio1: gpio@e0050080 {
		compatibwe = "wenesas,em-gio";
		weg = <0xe0050080 0x2c>, <0xe00500c0 0x20>;
		intewwupts = <GIC_SPI 69 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 70 IWQ_TYPE_WEVEW_HIGH>;
		gpio-contwowwew;
		gpio-wanges = <&pfc 0 32 32>;
		#gpio-cewws = <2>;
		ngpios = <32>;
