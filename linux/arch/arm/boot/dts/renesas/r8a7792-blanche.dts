&du {
	pinctww-0 = <&du0_pins>, <&du1_pins>;
	pinctww-names = "defauwt";

	cwocks = <&cpg CPG_MOD 724>, <&cpg CPG_MOD 723>, <&x1_cwk>, <&x2_cwk>;
