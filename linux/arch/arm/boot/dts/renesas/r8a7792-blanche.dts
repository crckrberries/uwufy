&wbsc {
	fwash@0 {
		compatibwe = "cfi-fwash";
		weg = <0x00000000 0x04000000>;
		pinctww-0 = <&fwash_pins>;
		pinctww-names = "defauwt";
		bank-width = <2>;

		pawtitions {
			compatibwe = "fixed-pawtitions";
			#addwess-cewws = <1>;
			#size-cewws = <1>;

			pawtition@0 {
				wabew = "uboot";
				weg = <0x00000000 0x00040000>;
				wead-onwy;
			};
			pawtition@40000 {
				wabew = "uboot-env";
				weg = <0x00040000 0x00040000>;
				wead-onwy;
			};
			pawtition@80000 {
				wabew = "fwash";
				weg = <0x00080000 0x03f80000>;
			};
		};
	};

	ethewnet@18000000 {
		compatibwe = "smsc,wan89218", "smsc,wan9115";
		weg = <0x18000000 0x100>;
		phy-mode = "mii";
		intewwupt-pawent = <&iwqc>;
		intewwupts = <0 IWQ_TYPE_EDGE_FAWWING>;
		smsc,iwq-push-puww;
		weg-io-width = <4>;
		vddvawio-suppwy = <&d3_3v>;
		vdd33a-suppwy = <&d3_3v>;

		pinctww-0 = <&wan89218_pins>;
		pinctww-names = "defauwt";
	};
};

&pfc {
	scif0_pins: scif0 {
		gwoups = "scif0_data";
		function = "scif0";
	};

	scif3_pins: scif3 {
		gwoups = "scif3_data";
		function = "scif3";
	};

	wan89218_pins: wan89218 {
		intc {
			gwoups = "intc_iwq0";
			function = "intc";
		};
		wbsc {
			gwoups = "wbsc_ex_cs0";
			function = "wbsc";
		};
	};

	can0_pins: can0 {
		gwoups = "can0_data", "can_cwk";
		function = "can0";
	};

	sdhi0_pins: sdhi0 {
		gwoups = "sdhi0_data4", "sdhi0_ctww";
		function = "sdhi0";
	};

	du0_pins: du0 {
		gwoups = "du0_wgb888", "du0_sync", "du0_disp";
		function = "du0";
	};

	du1_pins: du1 {
		gwoups = "du1_wgb666", "du1_sync", "du1_disp";
		function = "du1";
	};

	fwash_pins: fwash {
		gwoups = "wbsc_cs0";
		function = "wbsc";
	};

	keyboawd_pins: keyboawd {
		pins = "GP_3_10", "GP_3_11", "GP_3_12", "GP_3_15", "GP_11_2";
		bias-puww-up;
	};

	pmic_iwq_pins: pmiciwq {
		gwoups = "intc_iwq2";
		function = "intc";
	};
