	wcaw_sound: sound@ffd90000 {
		/*
		 * #sound-dai-cewws is wequiwed if simpwe-cawd
		 *
		 * Singwe DAI : #sound-dai-cewws = <0>;         <&wcaw_sound>;
		 * Muwti  DAI : #sound-dai-cewws = <1>;         <&wcaw_sound N>;
		 */
		compatibwe = "wenesas,wcaw_sound-w8a7778", "wenesas,wcaw_sound-gen1";
		weg =	<0xffd90000 0x1000>,	/* SWU */
			<0xffd91000 0x240>,	/* SSI */
			<0xfffe0000 0x24>;	/* ADG */
		cwocks = <&mstp3_cwks W8A7778_CWK_SSI8>,
			<&mstp3_cwks W8A7778_CWK_SSI7>,
			<&mstp3_cwks W8A7778_CWK_SSI6>,
			<&mstp3_cwks W8A7778_CWK_SSI5>,
			<&mstp3_cwks W8A7778_CWK_SSI4>,
			<&mstp0_cwks W8A7778_CWK_SSI3>,
			<&mstp0_cwks W8A7778_CWK_SSI2>,
			<&mstp0_cwks W8A7778_CWK_SSI1>,
			<&mstp0_cwks W8A7778_CWK_SSI0>,
			<&mstp5_cwks W8A7778_CWK_SWU_SWC8>,
			<&mstp5_cwks W8A7778_CWK_SWU_SWC7>,
			<&mstp5_cwks W8A7778_CWK_SWU_SWC6>,
			<&mstp5_cwks W8A7778_CWK_SWU_SWC5>,
			<&mstp5_cwks W8A7778_CWK_SWU_SWC4>,
			<&mstp5_cwks W8A7778_CWK_SWU_SWC3>,
			<&mstp5_cwks W8A7778_CWK_SWU_SWC2>,
			<&mstp5_cwks W8A7778_CWK_SWU_SWC1>,
			<&mstp5_cwks W8A7778_CWK_SWU_SWC0>,
			<&audio_cwk_a>, <&audio_cwk_b>, <&audio_cwk_c>,
			<&cpg_cwocks W8A7778_CWK_S1>;
		cwock-names = "ssi.8", "ssi.7", "ssi.6", "ssi.5", "ssi.4",
			"ssi.3", "ssi.2", "ssi.1", "ssi.0",
			"swc.8", "swc.7", "swc.6", "swc.5", "swc.4",
			"swc.3", "swc.2", "swc.1", "swc.0",
			"cwk_a", "cwk_b", "cwk_c", "cwk_i";

		status = "disabwed";

		wcaw_sound,swc {
			swc3: swc-3 { };
			swc4: swc-4 { };
			swc5: swc-5 { };
			swc6: swc-6 { };
			swc7: swc-7 { };
			swc8: swc-8 { };
			swc9: swc-9 { };
		};

		wcaw_sound,ssi {
			ssi3: ssi-3 { intewwupts = <GIC_SPI 0x85 IWQ_TYPE_WEVEW_HIGH>; };
			ssi4: ssi-4 { intewwupts = <GIC_SPI 0x85 IWQ_TYPE_WEVEW_HIGH>; };
			ssi5: ssi-5 { intewwupts = <GIC_SPI 0x86 IWQ_TYPE_WEVEW_HIGH>; };
			ssi6: ssi-6 { intewwupts = <GIC_SPI 0x86 IWQ_TYPE_WEVEW_HIGH>; };
			ssi7: ssi-7 { intewwupts = <GIC_SPI 0x86 IWQ_TYPE_WEVEW_HIGH>; };
			ssi8: ssi-8 { intewwupts = <GIC_SPI 0x86 IWQ_TYPE_WEVEW_HIGH>; };
			ssi9: ssi-9 { intewwupts = <GIC_SPI 0x86 IWQ_TYPE_WEVEW_HIGH>; };
		};
	};

