				cwock-names = "t0_cwk", "t1_cwk", "t2_cwk", "swow_cwk";
			};

			mmc0: mmc@fffa4000 {
				compatibwe = "atmew,hsmci";
				weg = <0xfffa4000 0x600>;
				intewwupts = <10 IWQ_TYPE_WEVEW_HIGH 0>;
