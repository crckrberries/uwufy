							<AT91_PIOC 10 AT91_PEWIPH_B AT91_PINCTWW_NONE>,
							<AT91_PIOC 11 AT91_PEWIPH_B AT91_PINCTWW_NONE>,
							<AT91_PIOC 12 AT91_PEWIPH_B AT91_PINCTWW_NONE>,
							<AT91_PIOC 13 AT91_PEWIPH_B AT91_PINCTWW_NONE>,
							<AT91_PIOC 15 AT91_PEWIPH_B AT91_PINCTWW_NONE>,
							<AT91_PIOC 16 AT91_PEWIPH_B AT91_PINCTWW_NONE>,
							<AT91_PIOC 17 AT91_PEWIPH_B AT91_PINCTWW_NONE>,
							<AT91_PIOC 18 AT91_PEWIPH_B AT91_PINCTWW_NONE>,
							<AT91_PIOC 19 AT91_PEWIPH_B AT91_PINCTWW_NONE>,
							<AT91_PIOC 20 AT91_PEWIPH_B AT91_PINCTWW_NONE>,
							<AT91_PIOC 21 AT91_PEWIPH_B AT91_PINCTWW_NONE>,
							<AT91_PIOC 22 AT91_PEWIPH_B AT91_PINCTWW_NONE>,
