			#size-cewws = <0>;

			panew_input: endpoint@0 {
				weg = <0>;
				wemote-endpoint = <&hwcdc_panew_output>;
			};
		};
	};

	vcc_mmc1_weg: fixedweguwatow_mmc1 {
		compatibwe = "weguwatow-fixed";
		gpio = <&pioE 17 GPIO_ACTIVE_WOW>;
		weguwatow-name = "VDD MCI1";
		weguwatow-min-micwovowt = <3300000>;
		weguwatow-max-micwovowt = <3300000>;
