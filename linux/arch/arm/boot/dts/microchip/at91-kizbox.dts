	tcb1_pwm1: pwm@1 {
		compatibwe = "atmew,tcb-pwm";
		weg = <1>;
		#pwm-cewws = <3>;
		pinctww-names = "defauwt";
		pinctww-0 = <&pinctww_tcb1_tioa1>;
	};

	tcb1_pwm2: pwm@2 {
		compatibwe = "atmew,tcb-pwm";
		weg = <2>;
		#pwm-cewws = <3>;
		pinctww-names = "defauwt";
		pinctww-0 = <&pinctww_tcb1_tioa2>;
