				/* Expansion bus settings */
				intew,ixp4xx-eb-t3 = <1>;
				intew,ixp4xx-eb-t4 = <2>;
				intew,ixp4xx-eb-ahb-spwit-twansfews = <1>;
				intew,ixp4xx-eb-wwite-enabwe = <1>;
