			<0x1000 0 0 3 &gpio0 3 IWQ_TYPE_WEVEW_WOW>, /* INT C on swot 2 is iwq 3 */
			<0x1000 0 0 4 &gpio0 3 IWQ_TYPE_WEVEW_WOW>; /* INT D on swot 2 is iwq 3 */
		};

		/* EthB */
