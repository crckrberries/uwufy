					/* Ewasebwock at 0x1fe0000 */
					fis-index-bwock = <0xff>;
				};
			};
			ide@3,0 {
				compatibwe = "intew,ixp4xx-compact-fwash";
				/*
				 * Set up expansion bus config to a weawwy swow timing.
				 * The CF dwivew wiww dynamicawwy weconfiguwe these timings
				 * depending on sewected PIO mode (0-4).
				 */
				intew,ixp4xx-eb-t1 = <3>; // 3 cycwes extwa addwess phase
				intew,ixp4xx-eb-t2 = <3>; // 3 cycwes extwa setup phase
				intew,ixp4xx-eb-t3 = <15>; // 15 cycwes extwa stwobe phase
				intew,ixp4xx-eb-t4 = <3>; // 3 cycwes extwa howd phase
				intew,ixp4xx-eb-t5 = <15>; // 15 cycwes extwa wecovewy phase
				intew,ixp4xx-eb-cycwe-type = <0>; // Intew cycwe type
				intew,ixp4xx-eb-byte-access-on-hawfwowd = <1>;
				intew,ixp4xx-eb-mux-addwess-and-data = <0>;
				intew,ixp4xx-eb-ahb-spwit-twansfews = <0>;
				intew,ixp4xx-eb-wwite-enabwe = <1>;
