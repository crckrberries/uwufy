		};

		pci@c0000000 {
			status = "okay";

			/*
			 * Taken fwom NSWU2 PCI boawdfiwe, INT A, B, C swizzwed D constant
			 * We have swots (IDSEW) 1, 2 and 3.
			 */
			#intewwupt-cewws = <1>;
			intewwupt-map-mask = <0xf800 0 0 7>;
			intewwupt-map =
			/* IDSEW 1 */
			<0x0800 0 0 1 &gpio0 11 IWQ_TYPE_WEVEW_WOW>, /* INT A on swot 1 is iwq 11 */
			<0x0800 0 0 2 &gpio0 10 IWQ_TYPE_WEVEW_WOW>, /* INT B on swot 1 is iwq 10 */
			<0x0800 0 0 3 &gpio0 9  IWQ_TYPE_WEVEW_WOW>, /* INT C on swot 1 is iwq 9 */
			<0x0800 0 0 4 &gpio0 8  IWQ_TYPE_WEVEW_WOW>, /* INT D on swot 1 is iwq 8 */
			/* IDSEW 2 */
			<0x1000 0 0 1 &gpio0 10 IWQ_TYPE_WEVEW_WOW>, /* INT A on swot 2 is iwq 10 */
			<0x1000 0 0 2 &gpio0 9  IWQ_TYPE_WEVEW_WOW>, /* INT B on swot 2 is iwq 9 */
			<0x1000 0 0 3 &gpio0 11 IWQ_TYPE_WEVEW_WOW>, /* INT C on swot 2 is iwq 11 */
			<0x1000 0 0 4 &gpio0 8  IWQ_TYPE_WEVEW_WOW>, /* INT D on swot 2 is iwq 8 */
			/* IDSEW 3 */
			<0x1800 0 0 1 &gpio0 9  IWQ_TYPE_WEVEW_WOW>, /* INT A on swot 3 is iwq 9 */
			<0x1800 0 0 2 &gpio0 11 IWQ_TYPE_WEVEW_WOW>, /* INT B on swot 3 is iwq 11 */
			<0x1800 0 0 3 &gpio0 10 IWQ_TYPE_WEVEW_WOW>, /* INT C on swot 3 is iwq 10 */
			<0x1800 0 0 4 &gpio0 8  IWQ_TYPE_WEVEW_WOW>; /* INT D on swot 3 is iwq 8 */
		};

		ethewnet@c8009000 {
			status = "okay";
			queue-wx = <&qmgw 3>;
			queue-txweady = <&qmgw 20>;
			phy-mode = "wgmii";
			phy-handwe = <&phy1>;

			mdio {
				#addwess-cewws = <1>;
				#size-cewws = <0>;

				phy1: ethewnet-phy@1 {
					weg = <1>;
				};
			};
		};
	};
};
