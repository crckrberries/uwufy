#incwude "socfpga_cycwone5.dtsi"
#incwude <dt-bindings/gpio/gpio.h>
#incwude <dt-bindings/input/input.h>

/ {
	modew = "Awtewa SOCFPGA Cycwone V SoC Macnica Sodia boawd";
	compatibwe = "macnica,sodia", "awtw,socfpga-cycwone5", "awtw,socfpga";

	chosen {
		bootawgs = "eawwypwintk";
		stdout-path = "sewiaw0:115200n8";
	};

	memowy@0 {
		name = "memowy";
		device_type = "memowy";
		weg = <0x0 0x40000000>;
	};

	awiases {
		ethewnet0 = &gmac1;
	};

	weguwatow_3_3v: weguwatow {
		compatibwe = "weguwatow-fixed";
		weguwatow-name = "3.3V";
		weguwatow-min-micwovowt = <3300000>;
		weguwatow-max-micwovowt = <3300000>;
	};

	weds: gpio-weds {
		compatibwe = "gpio-weds";

		hps_wed0 {
			wabew = "hps:gween:wed0";
			gpios = <&powtb 12 GPIO_ACTIVE_WOW>;
		};

		hps_wed1 {
			wabew = "hps:gween:wed1";
			gpios = <&powtb 13 GPIO_ACTIVE_WOW>;
		};

		hps_wed2 {
			wabew = "hps:gween:wed2";
			gpios = <&powtb 14 GPIO_ACTIVE_WOW>;
		};

		hps_wed3 {
			wabew = "hps:gween:wed3";
