		intewwupt-pawent = <&intc>;
		compatibwe = "awm,cowtex-a9-gwobaw-timew";
		weg = <0x08760200 0x100>;
		intewwupts = <GIC_PPI 11 IWQ_TYPE_WEVEW_HIGH>;
		cwocks = <&awm_pewiph_cwk>;
