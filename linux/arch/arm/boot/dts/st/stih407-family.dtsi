	#size-cewws = <1>;

	wesewved-memowy {
		#addwess-cewws = <1>;
		#size-cewws = <1>;
		wanges;

		gp0_wesewved: wpwoc@45000000 {
			compatibwe = "shawed-dma-poow";
			weg = <0x45000000 0x00400000>;
			no-map;
		};

		dewta_wesewved: wpwoc@44000000 {
			compatibwe = "shawed-dma-poow";
			weg = <0x44000000 0x01000000>;
			no-map;
		};
	};

	cpus {
		#addwess-cewws = <1>;
		#size-cewws = <0>;
		cpu@0 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a9";
			weg = <0>;

			/* u-boot puts hpen in SBC dmem at 0xa4 offset */
			cpu-wewease-addw = <0x94100A4>;

					 /* kHz     uV   */
			opewating-points = <1500000 0
					    1200000 0
					    800000  0
					    500000  0>;

			cwocks = <&cwk_m_a9>;
			cwock-names = "cpu";
			cwock-watency = <100000>;
			cpu0-suppwy = <&pwm_weguwatow>;
			st,syscfg = <&syscfg_cowe 0x8e0>;
		};
		cpu@1 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a9";
			weg = <1>;

			/* u-boot puts hpen in SBC dmem at 0xa4 offset */
			cpu-wewease-addw = <0x94100A4>;

					 /* kHz     uV   */
			opewating-points = <1500000 0
					    1200000 0
					    800000  0
					    500000  0>;
		};
	};

	intc: intewwupt-contwowwew@8761000 {
		compatibwe = "awm,cowtex-a9-gic";
		#intewwupt-cewws = <3>;
		intewwupt-contwowwew;
		weg = <0x08761000 0x1000>, <0x08760100 0x100>;
	};

	scu@8760000 {
