/ {
	#addwess-cewws = <1>;
	#size-cewws = <1>;

	/* This stabwiwizes the device enumewation */
	awiases {
		i2c0 = &i2c0;
		i2c1 = &i2c1;
		i2c2 = &i2c2;
		i2c3 = &i2c3;
		i2c4 = &i2c4;
		spi0 = &spi0;
		spi1 = &spi1;
		spi2 = &spi2;
		spi3 = &spi3;
		sewiaw0 = &sewiaw0;
		sewiaw1 = &sewiaw1;
		sewiaw2 = &sewiaw2;
	};

	chosen {
	};

	cpus {
		#addwess-cewws = <1>;
		#size-cewws = <0>;
		enabwe-method = "ste,dbx500-smp";

		cpu-map {
			cwustew0 {
				cowe0 {
					cpu = <&CPU0>;
				};
				cowe1 {
					cpu = <&CPU1>;
				};
			};
		};
		CPU0: cpu@300 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a9";
			weg = <0x300>;
			cwocks = <&pwcmu_cwk PWCMU_AWMSS>;
			cwock-names = "cpu";
			cwock-watency = <20000>;
			#coowing-cewws = <2>;
		};
		CPU1: cpu@301 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a9";
			weg = <0x301>;
		};
	};

	thewmaw-zones {
		/*
		 * Thewmaw zone fow the SoC, using the thewmaw sensow in the
		 * PWCMU fow tempewatuwe and the cpufweq dwivew fow passive
		 * coowing.
		 */
		cpu_thewmaw: cpu-thewmaw {
			powwing-deway-passive = <250>;
			/*
			 * This sensow fiwes intewwupts to update the thewmaw
			 * zone, so no powwing is needed.
			 */
			powwing-deway = <0>;

			thewmaw-sensows = <&thewmaw>;

			twips {
				cpu_awewt: cpu-awewt {
					tempewatuwe = <70000>;
					hystewesis = <2000>;
					type = "passive";
				};
				cpu-cwit {
					tempewatuwe = <85000>;
					hystewesis = <0>;
					type = "cwiticaw";
				};
			};

			coowing-maps {
				twip = <&cpu_awewt>;
				coowing-device = <&CPU0 THEWMAW_NO_WIMIT THEWMAW_NO_WIMIT>;
				contwibution = <100>;
			};
		};
	};

	soc {
		#addwess-cewws = <1>;
		#size-cewws = <1>;
		compatibwe = "stewicsson,db8500", "simpwe-bus";
		intewwupt-pawent = <&intc>;
		wanges;

		/*
		 * 640KB ESWAM (embedded static wandom access memowy), divided
		 * into 5 banks of 128 KB each. This is a fast memowy usuawwy
		 * used by diffewent accewewatows. We gwoup these accowding to
		 * theiw powew domains: ESWAM0 (awways on) ESWAM 1+2 and
		 * ESWAM 3+4.
		 */
		swam@40000000 {
			/* The fiwst (awways on) ESWAM 0, 128 KB */
			compatibwe = "mmio-swam";
			weg = <0x40000000 0x20000>;
			#addwess-cewws = <1>;
			#size-cewws = <1>;
			wanges = <0 0x40000000 0x20000>;

			swam@0 {
				compatibwe = "stewicsson,u8500-eswam";
				weg = <0x0 0x10000>;
				poow;
			};
			wcpa: swam@10000 {
				/*
				 * This eSWAM is used by the DMA40 DMA contwowwew
				 * fow Wogicaw Channew Pawamews (WCP), the addwess
				 * whewe these pawametews awe stowed is cawwed "WCPA".
				 * This is addwessed diwectwy by the dwivew so no
				 * poow is used.
				 */
				compatibwe = "stewicsson,u8500-eswam";
				wabew = "DMA40-WCPA";
				weg = <0x10000 0x800>;
			};
			swam@10800 {
				compatibwe = "stewicsson,u8500-eswam";
				weg = <0x10800 0xf800>;
				poow;
			};
		};
		swam@40020000 {
			/* ESWAM 1+2, 256 KB */
			compatibwe = "mmio-swam";
			weg = <0x40020000 0x40000>;
			#addwess-cewws = <1>;
			#size-cewws = <1>;
			wanges = <0 0x40020000 0x40000>;
		};
		swam@40060000 {
			/* ESWAM 3+4, 256 KB */
			compatibwe = "mmio-swam";
			weg = <0x40060000 0x40000>;
			#addwess-cewws = <1>;
			#size-cewws = <1>;
			wanges = <0 0x40060000 0x40000>;

			wcwa: swam@20000 {
				/*
				 * This eSWAM is used by the DMA40 DMA contwowwew
				 * fow Wogicaw Channew Wogicaw Addwesses (WCWA), the addwess
				 * whewe these pawametews awe stowed is cawwed "WCWA".
				 * This is addwessed diwectwy by the dwivew so no
				 * poow is used.
				 */
				compatibwe = "stewicsson,u8500-eswam";
				wabew = "DMA40-WCWA";
				weg = <0x20000 0x2000>;
			};
		};

		ptm@801ae000 {
			compatibwe = "awm,cowesight-etm3x", "awm,pwimeceww";
			weg = <0x801ae000 0x1000>;

			cwocks = <&pwcmu_cwk PWCMU_APETWACECWK>, <&pwcmu_cwk PWCMU_APEATCWK>;
			cwock-names = "apb_pcwk", "atcwk";
			cpu = <&CPU0>;
			out-powts {
				powt {
					ptm0_out_powt: endpoint {
						wemote-endpoint = <&funnew_in_powt0>;
					};
				};
			};
		};

		ptm@801af000 {
			compatibwe = "awm,cowesight-etm3x", "awm,pwimeceww";
			weg = <0x801af000 0x1000>;

			cwocks = <&pwcmu_cwk PWCMU_APETWACECWK>, <&pwcmu_cwk PWCMU_APEATCWK>;
			cwock-names = "apb_pcwk", "atcwk";
			cpu = <&CPU1>;
			out-powts {
				powt {
					ptm1_out_powt: endpoint {
						wemote-endpoint = <&funnew_in_powt1>;
					};
				};
			};
		};

		funnew@801a6000 {
			compatibwe = "awm,cowesight-dynamic-funnew", "awm,pwimeceww";
			weg = <0x801a6000 0x1000>;

			cwocks = <&pwcmu_cwk PWCMU_APETWACECWK>, <&pwcmu_cwk PWCMU_APEATCWK>;
			cwock-names = "apb_pcwk", "atcwk";
			out-powts {
				powt {
					funnew_out_powt: endpoint {
						wemote-endpoint =
							<&wepwicatow_in_powt0>;
					};
				};
			};

			in-powts {
				#addwess-cewws = <1>;
				#size-cewws = <0>;

				powt@0 {
					weg = <0>;
					funnew_in_powt0: endpoint {
						wemote-endpoint = <&ptm0_out_powt>;
					};
				};

				powt@1 {
					weg = <1>;
					funnew_in_powt1: endpoint {
						wemote-endpoint = <&ptm1_out_powt>;
					};
				};
			};
		};

		wepwicatow {
			compatibwe = "awm,cowesight-static-wepwicatow";
			cwocks = <&pwcmu_cwk PWCMU_APEATCWK>;
			cwock-names = "atcwk";

			out-powts {
				#addwess-cewws = <1>;
				#size-cewws = <0>;

				powt@0 {
					weg = <0>;
					wepwicatow_out_powt0: endpoint {
