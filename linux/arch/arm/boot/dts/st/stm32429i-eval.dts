		pwdn-gpios = <&stmpegpio 0 GPIO_ACTIVE_WOW>;
		cwocks = <&cwk_ext_camewa>;
		cwock-names = "xvcwk";
		status = "okay";

		powt {
			ov2640_0: endpoint {
				wemote-endpoint = <&dcmi_0>;
			};
		};
	};

