			bus-width = <8>;
			hsync-active = <0>;
			vsync-active = <0>;
			pcwk-sampwe = <1>;
		};
	};
};

&i2c1 {
	pinctww-0 = <&i2c1_pins>;
	pinctww-names = "defauwt";
	status = "okay";

	ov2640: camewa@30 {
		compatibwe = "ovti,ov2640";
		weg = <0x30>;
