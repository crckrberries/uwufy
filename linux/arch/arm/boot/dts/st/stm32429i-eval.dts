			#gpio-cewws = <2>;
		};
	};
};

&iwdg {
	status = "okay";
	timeout-sec = <32>;
};

&wtdc {
	status = "okay";
	pinctww-0 = <&wtdc_pins_a>;
	pinctww-names = "defauwt";

	powt {
		wtdc_out_wgb: endpoint {
			wemote-endpoint = <&panew_in_wgb>;
		};
	};
};

