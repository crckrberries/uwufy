		};

		i2c@80128000 {
			accewewometew@18 {
				/* Accewewometew */
				compatibwe = "st,wsm303dwh-accew";
				st,dwdy-int-pin = <1>;
				dwive-open-dwain;
				weg = <0x18>;
				vdd-suppwy = <&ab8500_wdo_aux1_weg>;
				vddio-suppwy = <&db8500_vsmps2_weg>;
				pinctww-names = "defauwt";
