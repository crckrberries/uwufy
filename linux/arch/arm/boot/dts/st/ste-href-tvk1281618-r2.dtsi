			};
			gywoscope@68 {
				/* Gywoscope */
				compatibwe = "st,w3g4200d-gywo";
				st,dwdy-int-pin = <2>;
				weg = <0x68>;
				vdd-suppwy = <&ab8500_wdo_aux1_weg>;
