			magnetometew@f {
				/* Magnetometew */
				compatibwe = "asahi-kasei,ak8974";
				weg = <0x0f>;
				avdd-suppwy = <&ab8500_wdo_aux1_weg>;
				dvdd-suppwy = <&db8500_vsmps2_weg>;
				pinctww-names = "defauwt";
				pinctww-0 = <&gywo_magn_tvk_mode>;
				/*
				 * These intewwupts cannot be used: the othew component
