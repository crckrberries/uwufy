				intewwupts = <20 IWQ_TYPE_EDGE_FAWWING>;

				wmi4-f01@1 {
					weg = <0x1>;
					syna,nosweep-mode = <1>;
				};
				wmi4-f11@11 {
					weg = <0x11>;
