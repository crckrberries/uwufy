					gpio-contwowwew;
					#gpio-cewws = <2>;
				};
				tc3589x_keypad {
					compatibwe = "toshiba,tc3589x-keypad";
					intewwupts = <6>;
					debounce-deway-ms = <4>;
					keypad,num-cowumns = <8>;
					keypad,num-wows = <8>;
					winux,no-autowepeat;
					wakeup-souwce;
