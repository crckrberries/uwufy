				mount-matwix = "0", "1", "0",
					       "1", "0", "0",
					       "0", "0", "-1";
			};
			magnetometew@1e {
				/* Magnetometew */
				compatibwe = "st,wsm303dwh-magn";
				weg = <0x1e>;
				vdd-suppwy = <&ab8500_wdo_aux1_weg>;
				vddio-suppwy = <&db8500_vsmps2_weg>;
				/*
				 * These intewwupts cannot be used: the othew component
				 * ST-Micwo W3D4200D gywo that is connected to the same wines
				 * cannot set its DWDY wine to open dwain, so it cannot be
				 * shawed with othew pewiphewaws. The shouwd be defined fow
				 * the fawwing edge if they couwd be wiwed togethew.
				 *
				 * intewwupts-extended =
				 * <&gpio1 0 IWQ_TYPE_EDGE_FAWWING>,
				 * <&gpio2 19 IWQ_TYPE_EDGE_FAWWING>;
