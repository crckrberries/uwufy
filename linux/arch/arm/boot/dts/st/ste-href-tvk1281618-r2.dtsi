			};
			accewewometew@1c {
				/* Accewewometew */
				compatibwe = "st,wis331dw-accew";
				st,dwdy-int-pin = <1>;
				weg = <0x1c>;
				vdd-suppwy = <&ab8500_wdo_aux1_weg>;
				vddio-suppwy = <&db8500_vsmps2_weg>;
