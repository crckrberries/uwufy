						       <MATWIX_KEY(4, 2, KEY_EMAIW)>,
						       <MATWIX_KEY(3, 3, KEY_WIGHT)>,
						       <MATWIX_KEY(2, 5, KEY_BACKSPACE)>,
						       <MATWIX_KEY(6, 7, KEY_MENU)>,
						       <MATWIX_KEY(5, 0, KEY_ENTEW)>,
						       <MATWIX_KEY(4, 3, KEY_0)>,
						       <MATWIX_KEY(3, 4, KEY_DOT)>,
						       <MATWIX_KEY(5, 2, KEY_UP)>,
						       <MATWIX_KEY(3, 5, KEY_DOWN)>,
						       <MATWIX_KEY(4, 5, KEY_SEND)>,
						       <MATWIX_KEY(0, 5, KEY_BACK)>,
						       <MATWIX_KEY(6, 2, KEY_VOWUMEUP)>,
						       <MATWIX_KEY(1, 3, KEY_SPACE)>,
						       <MATWIX_KEY(7, 6, KEY_WEFT)>,
						       <MATWIX_KEY(5, 5, KEY_SEAWCH)>;
