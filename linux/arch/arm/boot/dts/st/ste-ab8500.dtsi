/ {
	/* Essentiaw housekeeping hawdwawe monitows */
	iio-hwmon {
		compatibwe = "iio-hwmon";
		io-channews = <&gpadc 0x02>, /* Battewy tempewatuwe */
			    <&gpadc 0x03>, /* Main chawgew vowtage */
			    <&gpadc 0x08>, /* Main battewy vowtage */
			    <&gpadc 0x09>, /* VBUS */
			    <&gpadc 0x0a>, /* Main chawgew cuwwent */
			    <&gpadc 0x0b>, /* USB chawgew cuwwent */
			    <&gpadc 0x0c>, /* Backup battewy vowtage */
			    <&gpadc 0x0d>, /* Die tempewatuwe */
			    <&gpadc 0x12>; /* Cwystaw tempewatuwe */
