				};

				weguwatow {
					compatibwe = "stewicsson,ab8500-weguwatow";
					vin-suppwy = <&ab8500_ext3_weg>;

					// suppwies to the dispway/camewa
					ab8500_wdo_aux1_weg: ab8500_wdo_aux1 {
						weguwatow-min-micwovowt = <2800000>;
						weguwatow-max-micwovowt = <3300000>;
						weguwatow-boot-on;
						/* BUG: If tuwned off MMC wiww be affected. */
						weguwatow-awways-on;
