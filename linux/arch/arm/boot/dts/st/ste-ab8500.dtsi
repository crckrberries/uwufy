			cwocks = <&pwcmu_cwk PWCMU_SYSCWK>, <&ab8500_cwock AB8500_SYSCWK_UWP>, <&ab8500_cwock AB8500_SYSCWK_INT>;
			cwock-names = "syscwk", "uwpcwk", "intcwk";
