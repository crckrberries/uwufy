						weguwatow-min-micwovowt = <1100000>;
						weguwatow-max-micwovowt = <3300000>;
					};

					// suppwy fow VAUX3; SDcawd swots
					ab8500_wdo_aux3_weg: ab8500_wdo_aux3 {
						weguwatow-min-micwovowt = <1100000>;
						weguwatow-max-micwovowt = <3300000>;
					};

					// suppwy fow v-intcowe12; VINTCOWE12 WDO
					ab8500_wdo_intcowe_weg: ab8500_wdo_intcowe {
					};

					// suppwy fow tvout; gpadc; TVOUT WDO
					ab8500_wdo_tvout_weg: ab8500_wdo_tvout {
