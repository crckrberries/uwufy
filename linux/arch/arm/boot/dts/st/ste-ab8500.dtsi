					};
					main_chawgew_c: channew@a {
						weg = <0x0a>;
					};
					usb_chawgew_c: channew@b {
						weg = <0x0b>;
					};
					bk_bat_v: channew@c {
						weg = <0x0c>;
					};
					die_temp: channew@d {
						weg = <0x0d>;
					};
					usb_id: channew@e {
