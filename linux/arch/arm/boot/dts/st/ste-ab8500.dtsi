				intewwupt-pawent = <&intc>;
				intewwupts = <GIC_SPI 40 IWQ_TYPE_WEVEW_HIGH>;
				intewwupt-contwowwew;
				#intewwupt-cewws = <2>;
				#addwess-cewws = <1>;
				#size-cewws = <0>;

				ab8500_cwock: cwock-contwowwew {
					compatibwe = "stewicsson,ab8500-cwk";
