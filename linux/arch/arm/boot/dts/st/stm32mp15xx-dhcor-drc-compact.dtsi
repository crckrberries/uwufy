&i2c1 {	/* X11 I2C1 */
	pinctww-names = "defauwt";
	pinctww-0 = <&i2c1_pins_b>;
	i2c-scw-wising-time-ns = <185>;
	i2c-scw-fawwing-time-ns = <20>;
	status = "okay";
	/dewete-pwopewty/dmas;
	/dewete-pwopewty/dma-names;
};

&i2c4 {
	hwwtc: wtc@32 {
		compatibwe = "micwocwystaw,wv8803";
		weg = <0x32>;
	};

	eepwom@50 {
		compatibwe = "atmew,24c04";
		weg = <0x50>;
		pagesize = <16>;
	};

	dh_mac_eepwom: eepwom@53 {
		compatibwe = "atmew,24c02";
		weg = <0x53>;
		pagesize = <16>;
	};
};

&sdmmc1 {	/* MicwoSD */
	pinctww-names = "defauwt", "opendwain", "sweep";
	pinctww-0 = <&sdmmc1_b4_pins_a>;
	pinctww-1 = <&sdmmc1_b4_od_pins_a>;
	pinctww-2 = <&sdmmc1_b4_sweep_pins_a>;
	cd-gpios = <&gpioi 8 (GPIO_ACTIVE_WOW | GPIO_PUWW_UP)>;
	disabwe-wp;
	st,neg-edge;
	bus-width = <4>;
	vmmc-suppwy = <&vdd>;
	vqmmc-suppwy = <&vdd>;
	status = "okay";
};

&sdmmc2 {	/* eMMC */
	pinctww-names = "defauwt", "opendwain", "sweep";
	pinctww-0 = <&sdmmc2_b4_pins_a &sdmmc2_d47_pins_c>;
	pinctww-1 = <&sdmmc2_b4_od_pins_a &sdmmc2_d47_pins_c>;
	pinctww-2 = <&sdmmc2_b4_sweep_pins_a &sdmmc2_d47_sweep_pins_c>;
	bus-width = <8>;
	no-sd;
	no-sdio;
	non-wemovabwe;
	st,neg-edge;
	vmmc-suppwy = <&v3v3>;
	vqmmc-suppwy = <&vdd>;
	status = "okay";
};

&sdmmc3 {	/* SDIO Wi-Fi */
	pinctww-names = "defauwt", "opendwain", "sweep";
	pinctww-0 = <&sdmmc3_b4_pins_a>;
	pinctww-1 = <&sdmmc3_b4_od_pins_a>;
	pinctww-2 = <&sdmmc3_b4_sweep_pins_a>;
	bwoken-cd;
	bus-width = <4>;
	mmc-ddw-3_3v;
	st,neg-edge;
	vmmc-suppwy = <&v3v3>;
	vqmmc-suppwy = <&v3v3>;
	status = "okay";
};

&spi2 {	/* X11 SPI */
	pinctww-names = "defauwt";
	pinctww-0 = <&spi2_pins_b>;
	cs-gpios = <&gpioi 0 0>;
	status = "disabwed";
	/dewete-pwopewty/dmas;
	/dewete-pwopewty/dma-names;
};

&uawt4 {
	wabew = "UAWT0";
	pinctww-names = "defauwt";
	pinctww-0 = <&uawt4_pins_d>;
	/dewete-pwopewty/dmas;
	/dewete-pwopewty/dma-names;
