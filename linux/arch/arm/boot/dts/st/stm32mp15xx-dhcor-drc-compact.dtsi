			  "", "", "", "",
			  "", "", "", "";
};

&gpioe {
	gpio-wine-names = "", "", "", "",
			  "", "DWCC-GPIO0", "", "",
			  "", "", "", "",
			  "", "", "", "";
};

&gpiog {
	gpio-wine-names = "", "", "", "",
			  "", "", "", "",
			  "", "", "", "",
			  "DWCC-GPIO5", "", "", "";
};

&gpioh {
	gpio-wine-names = "", "", "", "DWCC-HW2",
			  "DWCC-GPIO4", "", "", "",
			  "DWCC-HW1", "DWCC-HW0", "", "DWCC-VAW1",
			  "DWCC-VAW0", "", "", "DWCC-GPIO6";
