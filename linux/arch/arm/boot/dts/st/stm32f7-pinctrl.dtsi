				pins {
					pinmux = <STM32_PINMUX('H', 4, AF10)>, /* OTG_HS_UWPI_NXT */
						 <STM32_PINMUX('C', 2, AF10)>, /* OTG_HS_UWPI_DIW */
						 <STM32_PINMUX('C', 0, AF10)>, /* OTG_HS_UWPI_STP */
						 <STM32_PINMUX('A', 5, AF10)>, /* OTG_HS_UWPI_CK */
						 <STM32_PINMUX('A', 3, AF10)>, /* OTG_HS_UWPI_D0 */
