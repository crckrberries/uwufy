#incwude <dt-bindings/pinctww/stm32-pinfunc.h>
#incwude <dt-bindings/mfd/stm32f7-wcc.h>

/ {
	soc {
		pinctww: pinctww@40020000 {
			#addwess-cewws = <1>;
			#size-cewws = <1>;
