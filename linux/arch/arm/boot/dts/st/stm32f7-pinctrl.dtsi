						 <STM32_PINMUX('B', 11, AF10)>, /* OTG_HS_UWPI_D4 */
						 <STM32_PINMUX('B', 12, AF10)>, /* OTG_HS_UWPI_D5 */
						 <STM32_PINMUX('B', 13, AF10)>, /* OTG_HS_UWPI_D6 */
						 <STM32_PINMUX('B', 5, AF10)>; /* OTG_HS_UWPI_D7 */
