			};

			gpiob: gpio@40020400 {
				gpio-contwowwew;
				#gpio-cewws = <2>;
				intewwupt-contwowwew;
				#intewwupt-cewws = <2>;
				weg = <0x400 0x400>;
				cwocks = <&wcc 0 STM32F7_AHB1_CWOCK(GPIOB)>;
				st,bank-name = "GPIOB";
			};

			gpioc: gpio@40020800 {
