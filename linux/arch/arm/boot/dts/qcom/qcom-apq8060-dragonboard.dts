				intewwupts = <57 IWQ_TYPE_EDGE_FAWWING>;
				pinctww-names = "defauwt";
				pinctww-0 = <&dwagon_kxsd9_gpios>;
				iovdd-suppwy = <&pm8058_wvs0>; // 1.8V
				vdd-suppwy = <&pm8058_w14>; // 2.85V
			};
		};
	};
