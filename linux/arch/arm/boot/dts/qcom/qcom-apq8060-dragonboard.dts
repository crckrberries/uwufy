		vdd_w14_w15-suppwy = <&vph>;
		vdd_w17_w18-suppwy = <&vph>;
		vdd_w19_w20-suppwy = <&vph>;
		vdd_w21-suppwy = <&pm8058_s3>;
		vdd_w22-suppwy = <&pm8058_s3>;
		vdd_w23_w24_w25-suppwy = <&pm8058_s3>;
		vdd_s0-suppwy = <&vph>;
