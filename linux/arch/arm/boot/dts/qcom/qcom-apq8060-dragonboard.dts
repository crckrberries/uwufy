			bias-puww-down;
		};

		pm8058_w2: w2 {
			weguwatow-min-micwovowt = <1800000>;
			weguwatow-max-micwovowt = <2600000>;
			bias-puww-down;
		};

		pm8058_w3: w3 {
			weguwatow-min-micwovowt = <1800000>;
			weguwatow-max-micwovowt = <1800000>;
			bias-puww-down;
