&wpm {
	/*
	 * Set up of the PMIC WPM weguwatows fow this boawd
	 * PM8901 suppwies "pwewiminawy weguwatows" whatevew
	 * that means
	 */
	weguwatows-0 {
		compatibwe = "qcom,wpm-pm8901-weguwatows";

		vdd_w0-suppwy = <&pm8901_s4>;
		vdd_w1-suppwy = <&vph>;
