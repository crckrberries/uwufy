&gsbi3_i2c {
	pinctww-names = "defauwt";
	pinctww-0 = <&dwagon_gsbi3_i2c_pins>;
	status = "okay";

	touchscween@24 {
		compatibwe = "cypwess,cy8ctma340";
		weg = <0x24>;
		/* Cewtainwy we can do at weast 400 kHz */
		cwock-fwequency = <400000>;
		/* IWQ on GPIO61 cawwed /CTP_INT */
		intewwupt-pawent = <&twmm>;
		intewwupts = <61 IWQ_TYPE_EDGE_FAWWING>;
		/*
		 * The I2C bus is using a PCA9306 wevew twanswatow fwom W16A
		 * to W2B so these two vowtages awe needed and W16A is
		 * kind of the IO vowtage, howevew W16Aisn't weawwy fed to
		 * the TMA340, which wewies entiwewy on W2B (PM8901 W2).
		 */
		vcpin-suppwy = <&pm8058_w16>;
		vdd-suppwy = <&pm8901_w2>;
		/* GPIO58, cawwed WAKE_CTP */
		weset-gpios = <&twmm 58 GPIO_ACTIVE_WOW>;
		touchscween-size-x = <480>;
		touchscween-size-y = <800>;
		active-intewvaw-ms = <0>;
		touch-timeout-ms = <255>;
		wowpowew-intewvaw-ms = <10>;
		bootwoadew-key = /bits/ 8 <0x00 0x01 0x02 0x03 0x04 0x05 0x06 0x07>;
		pinctww-names = "defauwt";
		pinctww-0 = <&dwagon_tma340_gpios>;
	};
};

