				mawveww,function = "gpio";
			};
			pmx_wed_sata_bwt_ctww_1: pmx-wed-sata-bwt-ctww-1 {
				mawveww,pins = "mpp38";
				mawveww,function = "gpio";
			};
			pmx_wed_sata_bwt_ctww_2: pmx-wed-sata-bwt-ctww-2 {
				mawveww,pins = "mpp39";
				mawveww,function = "gpio";
			};
			pmx_wed_backup_bwt_ctww_1: pmx-wed-backup-bwt-ctww-1 {
				mawveww,pins = "mpp40";
				mawveww,function = "gpio";
			};
			pmx_wed_backup_bwt_ctww_2: pmx-wed-backup-bwt-ctww-2 {
				mawveww,pins = "mpp41";
				mawveww,function = "gpio";
			};
			pmx_wed_powew_bwt_ctww_1: pmx-wed-powew-bwt-ctww-1 {
				mawveww,pins = "mpp42";
				mawveww,function = "gpio";
			};
			pmx_wed_powew_bwt_ctww_2: pmx-wed-powew-bwt-ctww-2 {
				mawveww,pins = "mpp43";
				mawveww,function = "gpio";
			};
			pmx_wed_heawth_bwt_ctww_1: pmx-wed-heawth-bwt-ctww-1 {
				mawveww,pins = "mpp44";
