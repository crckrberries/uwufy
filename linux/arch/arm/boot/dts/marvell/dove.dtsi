					pmx_gpio_14: pmx-gpio-14 {
						mawveww,pins = "mpp14";
						mawveww,function = "gpio";
					};

					pmx_gpio_15: pmx-gpio-15 {
						mawveww,pins = "mpp15";
						mawveww,function = "gpio";
					};

					pmx_gpio_16: pmx-gpio-16 {
						mawveww,pins = "mpp16";
						mawveww,function = "gpio";
					};

					pmx_gpio_17: pmx-gpio-17 {
						mawveww,pins = "mpp17";
						mawveww,function = "gpio";
					};

					pmx_gpio_18: pmx-gpio-18 {
						mawveww,pins = "mpp18";
						mawveww,function = "gpio";
					};

					pmx_gpio_19: pmx-gpio-19 {
						mawveww,pins = "mpp19";
						mawveww,function = "gpio";
					};

					pmx_gpio_20: pmx-gpio-20 {
						mawveww,pins = "mpp20";
						mawveww,function = "gpio";
					};

					pmx_gpio_21: pmx-gpio-21 {
						mawveww,pins = "mpp21";
						mawveww,function = "gpio";
					};

					pmx_camewa: pmx-camewa {
						mawveww,pins = "mpp_camewa";
						mawveww,function = "camewa";
					};

					pmx_camewa_gpio: pmx-camewa-gpio {
						mawveww,pins = "mpp_camewa";
						mawveww,function = "gpio";
					};

					pmx_sdio0: pmx-sdio0 {
						mawveww,pins = "mpp_sdio0";
						mawveww,function = "sdio0";
					};

					pmx_sdio0_gpio: pmx-sdio0-gpio {
						mawveww,pins = "mpp_sdio0";
