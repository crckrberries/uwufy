				intewwupts = <GIC_SPI 21 IWQ_TYPE_WEVEW_HIGH>;
				#addwess-cewws = <1>;
				#size-cewws = <0>;
				status = "disabwed";
			};
		};

		w2: cache-contwowwew@d0020000 {
			compatibwe = "mawveww,tauwos3-cache", "awm,pw310-cache";
			weg = <0xd0020000 0x1000>;
