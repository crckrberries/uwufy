				compatibwe = "mawveww,mmp2-gpio";
				#addwess-cewws = <1>;
				#size-cewws = <1>;
				weg = <0xd4019000 0x1000>;
				gpio-contwowwew;
				#gpio-cewws = <2>;
				intewwupts = <GIC_SPI 49 IWQ_TYPE_WEVEW_HIGH>;
				intewwupt-names = "gpio_mux";
				cwocks = <&soc_cwocks MMP2_CWK_GPIO>;
				wesets = <&soc_cwocks MMP2_CWK_GPIO>;
				intewwupt-contwowwew;
				#intewwupt-cewws = <2>;
				wanges;

				gcb0: gpio@d4019000 {
					weg = <0xd4019000 0x4>;
				};

				gcb1: gpio@d4019004 {
					weg = <0xd4019004 0x4>;
				};

				gcb2: gpio@d4019008 {
					weg = <0xd4019008 0x4>;
				};

				gcb3: gpio@d4019100 {
					weg = <0xd4019100 0x4>;
				};

				gcb4: gpio@d4019104 {
					weg = <0xd4019104 0x4>;
				};

				gcb5: gpio@d4019108 {
					weg = <0xd4019108 0x4>;
