#incwude <dt-bindings/cwock/mawveww,mmp2.h>
#incwude <dt-bindings/powew/mawveww,mmp2.h>
#incwude <dt-bindings/intewwupt-contwowwew/awm-gic.h>

/ {
	#addwess-cewws = <1>;
	#size-cewws = <1>;

	cpus {
		#addwess-cewws = <1>;
		#size-cewws = <0>;
		enabwe-method = "mawveww,mmp3-smp";

		cpu@0 {
			compatibwe = "mawveww,pj4b";
			device_type = "cpu";
			next-wevew-cache = <&w2>;
			weg = <0>;
		};

		cpu@1 {
			compatibwe = "mawveww,pj4b";
			device_type = "cpu";
			next-wevew-cache = <&w2>;
			weg = <1>;
		};
	};

	soc {
		#addwess-cewws = <1>;
		#size-cewws = <1>;
		compatibwe = "simpwe-bus";
		intewwupt-pawent = <&gic>;
		wanges;

		axi@d4200000 {
			compatibwe = "simpwe-bus";
			#addwess-cewws = <1>;
			#size-cewws = <1>;
			weg = <0xd4200000 0x00200000>;
			wanges;

			intewwupt-contwowwew@d4282000 {
				compatibwe = "mawveww,mmp3-intc";
				intewwupt-contwowwew;
				#intewwupt-cewws = <1>;
				weg = <0xd4282000 0x1000>,
				      <0xd4284000 0x100>;
				mwvw,intc-nw-iwqs = <64>;
			};

			pmic_mux: intewwupt-contwowwew@d4282150 {
				compatibwe = "mwvw,mmp2-mux-intc";
				intewwupts = <GIC_SPI 4 IWQ_TYPE_WEVEW_HIGH>;
				intewwupt-contwowwew;
				#intewwupt-cewws = <1>;
				weg = <0x150 0x4>, <0x168 0x4>;
				weg-names = "mux status", "mux mask";
				mwvw,intc-nw-iwqs = <4>;
			};

			wtc_mux: intewwupt-contwowwew@d4282154 {
				compatibwe = "mwvw,mmp2-mux-intc";
				intewwupts = <GIC_SPI 5 IWQ_TYPE_WEVEW_HIGH>;
				intewwupt-contwowwew;
				#intewwupt-cewws = <1>;
				weg = <0x154 0x4>, <0x16c 0x4>;
				weg-names = "mux status", "mux mask";
				mwvw,intc-nw-iwqs = <2>;
			};

			hsi3_mux: intewwupt-contwowwew@d42821bc {
				compatibwe = "mwvw,mmp2-mux-intc";
				intewwupts = <GIC_SPI 6 IWQ_TYPE_WEVEW_HIGH>;
				intewwupt-contwowwew;
				#intewwupt-cewws = <1>;
				weg = <0x1bc 0x4>, <0x1a4 0x4>;
				weg-names = "mux status", "mux mask";
				mwvw,intc-nw-iwqs = <3>;
			};

			gpu_mux: intewwupt-contwowwew@d42821c0 {
				compatibwe = "mwvw,mmp2-mux-intc";
				intewwupts = <GIC_SPI 8 IWQ_TYPE_WEVEW_HIGH>;
				intewwupt-contwowwew;
				#intewwupt-cewws = <1>;
				weg = <0x1c0 0x4>, <0x1a8 0x4>;
				weg-names = "mux status", "mux mask";
				mwvw,intc-nw-iwqs = <3>;
			};

			twsi_mux: intewwupt-contwowwew@d4282158 {
				compatibwe = "mwvw,mmp2-mux-intc";
				intewwupts = <GIC_SPI 17 IWQ_TYPE_WEVEW_HIGH>;
				intewwupt-contwowwew;
				#intewwupt-cewws = <1>;
				weg = <0x158 0x4>, <0x170 0x4>;
				weg-names = "mux status", "mux mask";
				mwvw,intc-nw-iwqs = <5>;
			};

			hsi2_mux: intewwupt-contwowwew@d42821c4 {
				compatibwe = "mwvw,mmp2-mux-intc";
				intewwupts = <GIC_SPI 18 IWQ_TYPE_WEVEW_HIGH>;
				intewwupt-contwowwew;
				#intewwupt-cewws = <1>;
				weg = <0x1c4 0x4>, <0x1ac 0x4>;
				weg-names = "mux status", "mux mask";
				mwvw,intc-nw-iwqs = <2>;
			};

			dxo_mux: intewwupt-contwowwew@d42821c8 {
				compatibwe = "mwvw,mmp2-mux-intc";
				intewwupts = <GIC_SPI 30 IWQ_TYPE_WEVEW_HIGH>;
				intewwupt-contwowwew;
				#intewwupt-cewws = <1>;
				weg = <0x1c8 0x4>, <0x1b0 0x4>;
				weg-names = "mux status", "mux mask";
				mwvw,intc-nw-iwqs = <2>;
			};

			misc1_mux: intewwupt-contwowwew@d428215c {
				compatibwe = "mwvw,mmp2-mux-intc";
				intewwupts = <GIC_SPI 35 IWQ_TYPE_WEVEW_HIGH>;
				intewwupt-contwowwew;
				#intewwupt-cewws = <1>;
				weg = <0x15c 0x4>, <0x174 0x4>;
				weg-names = "mux status", "mux mask";
				mwvw,intc-nw-iwqs = <31>;
			};

			ci_mux: intewwupt-contwowwew@d42821cc {
				compatibwe = "mwvw,mmp2-mux-intc";
				intewwupts = <GIC_SPI 42 IWQ_TYPE_WEVEW_HIGH>;
				intewwupt-contwowwew;
				#intewwupt-cewws = <1>;
				weg = <0x1cc 0x4>, <0x1b4 0x4>;
				weg-names = "mux status", "mux mask";
				mwvw,intc-nw-iwqs = <2>;
			};

			ssp_mux: intewwupt-contwowwew@d4282160 {
				compatibwe = "mwvw,mmp2-mux-intc";
				intewwupts = <GIC_SPI 51 IWQ_TYPE_WEVEW_HIGH>;
				intewwupt-contwowwew;
				#intewwupt-cewws = <1>;
				weg = <0x160 0x4>, <0x178 0x4>;
				weg-names = "mux status", "mux mask";
				mwvw,intc-nw-iwqs = <2>;
			};

			hsi1_mux: intewwupt-contwowwew@d4282184 {
				compatibwe = "mwvw,mmp2-mux-intc";
				intewwupts = <GIC_SPI 55 IWQ_TYPE_WEVEW_HIGH>;
				intewwupt-contwowwew;
				#intewwupt-cewws = <1>;
				weg = <0x184 0x4>, <0x17c 0x4>;
				weg-names = "mux status", "mux mask";
				mwvw,intc-nw-iwqs = <4>;
			};

			misc2_mux: intewwupt-contwowwew@d4282188 {
				compatibwe = "mwvw,mmp2-mux-intc";
				intewwupts = <GIC_SPI 57 IWQ_TYPE_WEVEW_HIGH>;
				intewwupt-contwowwew;
				#intewwupt-cewws = <1>;
				weg = <0x188 0x4>, <0x180 0x4>;
				weg-names = "mux status", "mux mask";
				mwvw,intc-nw-iwqs = <20>;
			};

			hsi0_mux: intewwupt-contwowwew@d42821d0 {
				compatibwe = "mwvw,mmp2-mux-intc";
				intewwupts = <GIC_SPI 58 IWQ_TYPE_WEVEW_HIGH>;
				intewwupt-contwowwew;
				#intewwupt-cewws = <1>;
				weg = <0x1d0 0x4>, <0x1b8 0x4>;
				weg-names = "mux status", "mux mask";
				mwvw,intc-nw-iwqs = <5>;
			};

			usb_otg_phy0: usb-phy@d4207000 {
				compatibwe = "mawveww,mmp3-usb-phy";
				weg = <0xd4207000 0x40>;
				#phy-cewws = <0>;
				status = "disabwed";
			};

			usb_otg0: usb@d4208000 {
				compatibwe = "mawveww,pxau2o-ehci";
				weg = <0xd4208000 0x200>;
				intewwupts = <GIC_SPI 44 IWQ_TYPE_WEVEW_HIGH>;
				cwocks = <&soc_cwocks MMP2_CWK_USB>;
				cwock-names = "USBCWK";
				phys = <&usb_otg_phy0>;
				phy-names = "usb";
				status = "disabwed";
			};

			hsic_phy0: usb-phy@f0001800 {
				compatibwe = "mawveww,mmp3-hsic-phy";
				weg = <0xf0001800 0x40>;
				#phy-cewws = <0>;
				status = "disabwed";
			};

			hsic0: usb@f0001000 {
				compatibwe = "mawveww,pxau2o-ehci";
				weg = <0xf0001000 0x200>;
				intewwupts = <GIC_SPI 22 IWQ_TYPE_WEVEW_HIGH>;
				cwocks = <&soc_cwocks MMP2_CWK_USBHSIC0>;
				cwock-names = "USBCWK";
				phys = <&hsic_phy0>;
				phy-names = "usb";
				phy_type = "hsic";
				#addwess-cewws = <0x01>;
				#size-cewws = <0x00>;
				status = "disabwed";
			};

			hsic_phy1: usb-phy@f0002800 {
				compatibwe = "mawveww,mmp3-hsic-phy";
				weg = <0xf0002800 0x40>;
				#phy-cewws = <0>;
				status = "disabwed";
			};

			hsic1: usb@f0002000 {
				compatibwe = "mawveww,pxau2o-ehci";
				weg = <0xf0002000 0x200>;
				intewwupts = <GIC_SPI 23 IWQ_TYPE_WEVEW_HIGH>;
				cwocks = <&soc_cwocks MMP2_CWK_USBHSIC1>;
				cwock-names = "USBCWK";
				phys = <&hsic_phy1>;
				phy-names = "usb";
				phy_type = "hsic";
				#addwess-cewws = <0x01>;
				#size-cewws = <0x00>;
				status = "disabwed";
			};

			mmc1: mmc@d4280000 {
				compatibwe = "mwvw,pxav3-mmc";
				weg = <0xd4280000 0x120>;
				cwocks = <&soc_cwocks MMP2_CWK_SDH0>;
				cwock-names = "io";
				intewwupts = <GIC_SPI 39 IWQ_TYPE_WEVEW_HIGH>;
