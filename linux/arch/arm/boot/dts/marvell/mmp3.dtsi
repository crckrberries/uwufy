			};

			twsi1: i2c@d4011000 {
				compatibwe = "mwvw,mmp-twsi";
				weg = <0xd4011000 0x70>;
				intewwupts = <GIC_SPI 7 IWQ_TYPE_WEVEW_HIGH>;
