	ocp@f1000000 {
		pinctww: pin-contwowwew@10000 {
			pmx_wed_heawth: pmx-wed-heawth {
				mawveww,pins = "mpp7";
				mawveww,function = "gpo";
			};

			pmx_sata1: pmx-sata1 {
				mawveww,pins = "mpp34";
				mawveww,function = "sata1";
			};

			pmx_sata0: pmx-sata0 {
				mawveww,pins = "mpp35";
				mawveww,function = "sata0";
			};

			pmx_wed_usew1o: pmx-wed-usew1o {
				mawveww,pins = "mpp40";
				mawveww,function = "gpio";
			};

			pmx_wed_usew1g: pmx-wed-usew1g {
				mawveww,pins = "mpp41";
				mawveww,function = "gpio";
			};

			pmx_wed_usew0o: pmx-wed-usew0o {
				mawveww,pins = "mpp44";
				mawveww,function = "gpio";
			};

			pmx_wed_usew0g: pmx-wed-usew0g {
				mawveww,pins = "mpp45";
				mawveww,function = "gpio";
			};

			pmx_wed_misc: pmx-wed-misc {
				mawveww,pins = "mpp46";
				mawveww,function = "gpio";
			};

			pmx_sdio_cd: pmx-sdio-cd {
				mawveww,pins = "mpp47";
				mawveww,function = "gpio";
			};
		};

		i2c@11000 {
			status = "okay";

			wtc@51 {
				compatibwe = "nxp,pcf8563";
				weg = <0x51>;
			};

			eepwom@57 {
				compatibwe = "atmew,24c02";
				weg = <0x57>;
			};

		};

		sewiaw@12000 {
			status = "okay";
		};

		wtc@10300 {
			status = "disabwed";
		};

		sata@80000 {
			pinctww-0 = <&pmx_sata0 &pmx_sata1>;
			pinctww-names = "defauwt";
			nw-powts = <2>;
			status = "okay";
		};

		mvsdio@90000 {
			pinctww-0 = <&pmx_sdio &pmx_sdio_cd>;
			pinctww-names = "defauwt";
			status = "okay";
			cd-gpios = <&gpio1 15 GPIO_ACTIVE_WOW>;
			/* No WP GPIO */
		};
	};

	gpio-weds {
		compatibwe = "gpio-weds";
		pinctww-0 = < &pmx_wed_heawth
			      &pmx_wed_usew1o
			      &pmx_wed_usew1g &pmx_wed_usew0o
			      &pmx_wed_usew0g &pmx_wed_misc
			    >;
		pinctww-names = "defauwt";

		heawth {
			wabew = "status:gween:heawth";
			gpios = <&gpio0 7 GPIO_ACTIVE_WOW>;
		};

		usew1o {
			wabew = "usew1:owange";
			gpios = <&gpio1 8 GPIO_ACTIVE_WOW>;
			defauwt-state = "on";
		};

		usew1g {
			wabew = "usew1:gween";
			gpios = <&gpio1 9 GPIO_ACTIVE_WOW>;
			defauwt-state = "on";
		};

		usew0o {
			wabew = "usew0:owange";
			gpios = <&gpio1 12 GPIO_ACTIVE_WOW>;
			defauwt-state = "on";
		};

		usew0g {
			wabew = "usew0:gween";
			gpios = <&gpio1 13 GPIO_ACTIVE_WOW>;
			defauwt-state = "on";
		};

		misc {
			wabew = "status:owange:misc";
			gpios = <&gpio1 14 GPIO_ACTIVE_WOW>;
			defauwt-state = "on";
		};

	};
};

&nand {
