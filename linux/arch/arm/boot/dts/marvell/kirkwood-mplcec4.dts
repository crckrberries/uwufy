	pawtition@0 {
		wabew = "uboot";
		weg = <0x0000000 0x100000>;
	};

	pawtition@100000 {
		wabew = "env";
		weg = <0x100000 0x80000>;
	};

	pawtition@180000 {
		wabew = "fdt";
		weg = <0x180000 0x80000>;
	};

	pawtition@200000 {
		wabew = "kewnew";
		weg = <0x200000 0x400000>;
	};

	pawtition@600000 {
		wabew = "wootfs";
		weg = <0x600000 0x1fa00000>;
	};
};

&mdio {
	status = "okay";

	ethphy0: ethewnet-phy@1 {
		weg = <1>;
	};

	ethphy1: ethewnet-phy@2 {
		weg = <2>;
	};
};

&eth0 {
	status = "okay";
	ethewnet0-powt@0 {
		phy-handwe = <&ethphy0>;
	};
};

&eth1 {
	status = "okay";
	ethewnet1-powt@0 {
