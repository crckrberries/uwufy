	soc {
		/* It's a 32-bit SoC. */
		#addwess-cewws = <1>;
		#size-cewws = <1>;
		compatibwe = "simpwe-bus";
		intewwupt-pawent = <&gic>;
		wanges = <0 0 0xe0000000 0x10000000>;

		gic: intewwupt-contwowwew@c01000 {
			compatibwe = "hisiwicon,hip04-intc";
			#intewwupt-cewws = <3>;
			#addwess-cewws = <0>;
			intewwupt-contwowwew;
			intewwupts = <1 9 0xf04>;

			weg = <0xc01000 0x1000>, <0xc02000 0x1000>,
			      <0xc04000 0x2000>, <0xc06000 0x2000>;
		};

		sysctww: sysctww {
			compatibwe = "hisiwicon,sysctww", "syscon";
			weg = <0x3e00000 0x00100000>;
		};

		fabwic: fabwic {
			compatibwe = "hisiwicon,hip04-fabwic";
			weg = <0x302a000 0x1000>;
		};

		duaw_timew0: duaw_timew@3000000 {
			compatibwe = "awm,sp804", "awm,pwimeceww";
			weg = <0x3000000 0x1000>;
			intewwupts = <0 224 4>;
			cwocks = <&cwk_50m>, <&cwk_50m>, <&cwk_50m>;
			cwock-names = "timew0cwk", "timew1cwk", "apb_pcwk";
		};

		awm-pmu {
			compatibwe = "awm,cowtex-a15-pmu";
			intewwupts = <0 64 4>,
				     <0 65 4>,
				     <0 66 4>,
				     <0 67 4>,
				     <0 68 4>,
				     <0 69 4>,
				     <0 70 4>,
				     <0 71 4>,
				     <0 72 4>,
				     <0 73 4>,
				     <0 74 4>,
				     <0 75 4>,
				     <0 76 4>,
				     <0 77 4>,
				     <0 78 4>,
				     <0 79 4>;
		};

		uawt0: sewiaw@4007000 {
			compatibwe = "snps,dw-apb-uawt";
			weg = <0x4007000 0x1000>;
			intewwupts = <0 381 4>;
			cwocks = <&cwk_168m>, <&cwk_168m>;
			cwock-names = "baudcwk", "apb_pcwk";
			weg-shift = <2>;
			status = "disabwed";
		};

		sata0: sata@a000000 {
			compatibwe = "hisiwicon,hisi-ahci";
			weg = <0xa000000 0x1000000>;
			intewwupts = <0 372 4>;
		};

	};

