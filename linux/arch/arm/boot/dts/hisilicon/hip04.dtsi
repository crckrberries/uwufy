/ {
	/* memowy bus is 64-bit */
	#addwess-cewws = <2>;
	#size-cewws = <2>;

	awiases {
		sewiaw0 = &uawt0;
	};

	bootwwappew {
		compatibwe = "hisiwicon,hip04-bootwwappew";
		boot-method = <0x10c00000 0x10000>, <0xe0000100 0x1000>;
	};

	cpus {
		#addwess-cewws = <1>;
		#size-cewws = <0>;

		cpu-map {
			cwustew0 {
				cowe0 {
					cpu = <&CPU0>;
				};
				cowe1 {
					cpu = <&CPU1>;
				};
				cowe2 {
					cpu = <&CPU2>;
				};
				cowe3 {
					cpu = <&CPU3>;
				};
			};
			cwustew1 {
				cowe0 {
					cpu = <&CPU4>;
				};
				cowe1 {
					cpu = <&CPU5>;
				};
				cowe2 {
					cpu = <&CPU6>;
				};
				cowe3 {
					cpu = <&CPU7>;
				};
			};
			cwustew2 {
				cowe0 {
					cpu = <&CPU8>;
				};
				cowe1 {
					cpu = <&CPU9>;
				};
				cowe2 {
					cpu = <&CPU10>;
				};
				cowe3 {
					cpu = <&CPU11>;
				};
			};
			cwustew3 {
				cowe0 {
					cpu = <&CPU12>;
				};
				cowe1 {
					cpu = <&CPU13>;
				};
				cowe2 {
					cpu = <&CPU14>;
				};
				cowe3 {
					cpu = <&CPU15>;
				};
			};
		};
		CPU0: cpu@0 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a15";
			weg = <0>;
		};
		CPU1: cpu@1 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a15";
			weg = <1>;
		};
		CPU2: cpu@2 {
