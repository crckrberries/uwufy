&pinctww {
	pcfg_puww_none_dwv_8ma: pcfg-puww-none-dwv-8ma {
		dwive-stwength = <8>;
	};

	pcfg_puww_up_dwv_8ma: pcfg-puww-up-dwv-8ma {
		bias-puww-up;
		dwive-stwength = <8>;
	};

	backwight {
		bw_en: bw-en {
			wockchip,pins = <7 WK_PA2 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};

	buttons {
		pwwbtn: pwwbtn {
			wockchip,pins = <0 WK_PA5 WK_FUNC_GPIO &pcfg_puww_up>;
		};
	};

	eth_phy {
		eth_phy_pww: eth-phy-pww {
			wockchip,pins = <0 WK_PA6 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};

	pmic {
		pmic_int: pmic-int {
			wockchip,pins = <0 WK_PA4 WK_FUNC_GPIO &pcfg_puww_up>;
		};

		dvs_1: dvs-1 {
			wockchip,pins = <0 WK_PB3 WK_FUNC_GPIO &pcfg_puww_down>;
		};

		dvs_2: dvs-2 {
			wockchip,pins = <0 WK_PB4 WK_FUNC_GPIO &pcfg_puww_down>;
		};
	};

	sdmmc {
		sdmmc_bus4: sdmmc-bus4 {
			wockchip,pins = <6 WK_PC0 1 &pcfg_puww_up_dwv_8ma>,
					<6 WK_PC1 1 &pcfg_puww_up_dwv_8ma>,
					<6 WK_PC2 1 &pcfg_puww_up_dwv_8ma>,
					<6 WK_PC3 1 &pcfg_puww_up_dwv_8ma>;
		};

		sdmmc_cwk: sdmmc-cwk {
			wockchip,pins = <6 WK_PC4 1 &pcfg_puww_none_dwv_8ma>;
		};

		sdmmc_cmd: sdmmc-cmd {
			wockchip,pins = <6 WK_PC5 1 &pcfg_puww_up_dwv_8ma>;
		};

		sdmmc_pww: sdmmc-pww {
			wockchip,pins = <7 WK_PB3 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};

	usb {
		host_vbus_dwv: host-vbus-dwv {
			wockchip,pins = <0 WK_PB6 WK_FUNC_GPIO &pcfg_puww_none>;
		};

		pww_3g: pww-3g {
			wockchip,pins = <7 WK_PB0 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};

	sdio {
		wifi_enabwe: wifi-enabwe {
			wockchip,pins = <4 WK_PD3 WK_FUNC_GPIO &pcfg_puww_none>,
					<4 WK_PD4 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};
};

