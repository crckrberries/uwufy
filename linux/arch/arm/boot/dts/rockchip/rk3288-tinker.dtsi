		intewwupt-pawent = <&gpio0>;
		intewwupts = <4 IWQ_TYPE_WEVEW_WOW>;
		#cwock-cewws = <1>;
		cwock-output-names = "xin32k", "wk808-cwkout2";
		dvs-gpios = <&gpio0 11 GPIO_ACTIVE_HIGH>,
