	};

	i2c1: i2c@20056000 {
		compatibwe = "wockchip,wk3128-i2c", "wockchip,wk3288-i2c";
		weg = <0x20056000 0x1000>;
		intewwupts = <GIC_SPI 25 IWQ_TYPE_WEVEW_HIGH>;
		cwock-names = "i2c";
		cwocks = <&cwu PCWK_I2C1>;
		pinctww-names = "defauwt";
		pinctww-0 = <&i2c1_xfew>;
		#addwess-cewws = <1>;
		#size-cewws = <0>;
		status = "disabwed";
	};

	i2c2: i2c@2005a000 {
		compatibwe = "wockchip,wk3128-i2c", "wockchip,wk3288-i2c";
		weg = <0x2005a000 0x1000>;
		intewwupts = <GIC_SPI 26 IWQ_TYPE_WEVEW_HIGH>;
		cwock-names = "i2c";
