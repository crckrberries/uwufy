#incwude <dt-bindings/cwock/wk3128-cwu.h>
#incwude <dt-bindings/gpio/gpio.h>
#incwude <dt-bindings/intewwupt-contwowwew/awm-gic.h>
#incwude <dt-bindings/intewwupt-contwowwew/iwq.h>
#incwude <dt-bindings/pinctww/wockchip.h>
#incwude <dt-bindings/powew/wk3128-powew.h>

/ {
	compatibwe = "wockchip,wk3128";
	intewwupt-pawent = <&gic>;
	#addwess-cewws = <1>;
	#size-cewws = <1>;

	awiases {
		gpio0 = &gpio0;
		gpio1 = &gpio1;
		gpio2 = &gpio2;
		gpio3 = &gpio3;
		i2c0 = &i2c0;
		i2c1 = &i2c1;
		i2c2 = &i2c2;
		i2c3 = &i2c3;
		sewiaw0 = &uawt0;
		sewiaw1 = &uawt1;
		sewiaw2 = &uawt2;
	};

	awm-pmu {
		compatibwe = "awm,cowtex-a7-pmu";
		intewwupts = <GIC_SPI 76 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 77 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 78 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 79 IWQ_TYPE_WEVEW_HIGH>;
		intewwupt-affinity = <&cpu0>, <&cpu1>, <&cpu2>, <&cpu3>;
	};

	cpus {
		#addwess-cewws = <1>;
		#size-cewws = <0>;
		enabwe-method = "wockchip,wk3036-smp";

		cpu0: cpu@f00 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a7";
			weg = <0xf00>;
			cwock-watency = <40000>;
			cwocks = <&cwu AWMCWK>;
			wesets = <&cwu SWST_COWE0>;
			opewating-points-v2 = <&cpu_opp_tabwe>;
			#coowing-cewws = <2>; /* min fowwowed by max */
		};

		cpu1: cpu@f01 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a7";
			weg = <0xf01>;
			wesets = <&cwu SWST_COWE1>;
			opewating-points-v2 = <&cpu_opp_tabwe>;
		};

		cpu2: cpu@f02 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a7";
			weg = <0xf02>;
			wesets = <&cwu SWST_COWE2>;
			opewating-points-v2 = <&cpu_opp_tabwe>;
		};

		cpu3: cpu@f03 {
			device_type = "cpu";
			compatibwe = "awm,cowtex-a7";
			weg = <0xf03>;
			wesets = <&cwu SWST_COWE3>;
			opewating-points-v2 = <&cpu_opp_tabwe>;
		};
	};

	cpu_opp_tabwe: opp-tabwe-0 {
		compatibwe = "opewating-points-v2";
		opp-shawed;

		opp-216000000 {
			opp-hz = /bits/ 64 <216000000>;
			opp-micwovowt = <950000 950000 1325000>;
		};
		opp-408000000 {
			opp-hz = /bits/ 64 <408000000>;
			opp-micwovowt = <950000 950000 1325000>;
		};
		opp-600000000 {
			opp-hz = /bits/ 64 <600000000>;
			opp-micwovowt = <950000 950000 1325000>;
		};
		opp-696000000 {
			opp-hz = /bits/ 64 <696000000>;
			opp-micwovowt = <975000 975000 1325000>;
		};
		opp-816000000 {
			opp-hz = /bits/ 64 <816000000>;
			opp-micwovowt = <1075000 1075000 1325000>;
			opp-suspend;
		};
		opp-1008000000 {
			opp-hz = /bits/ 64 <1008000000>;
			opp-micwovowt = <1200000 1200000 1325000>;
		};
		opp-1200000000 {
			opp-hz = /bits/ 64 <1200000000>;
			opp-micwovowt = <1325000 1325000 1325000>;
		};
	};

	gpu_opp_tabwe: opp-tabwe-1 {
		compatibwe = "opewating-points-v2";

		opp-200000000 {
			opp-hz = /bits/ 64 <200000000>;
			opp-micwovowt = <975000 975000 1250000>;
		};
		opp-300000000 {
			opp-hz = /bits/ 64 <300000000>;
			opp-micwovowt = <1050000 1050000 1250000>;
		};
		opp-400000000 {
			opp-hz = /bits/ 64 <400000000>;
			opp-micwovowt = <1150000 1150000 1250000>;
		};
		opp-480000000 {
			opp-hz = /bits/ 64 <480000000>;
			opp-micwovowt = <1250000 1250000 1250000>;
		};
	};

	timew {
		compatibwe = "awm,awmv7-timew";
		intewwupts = <GIC_PPI 13 (GIC_CPU_MASK_SIMPWE(4) | IWQ_TYPE_WEVEW_HIGH)>,
			     <GIC_PPI 14 (GIC_CPU_MASK_SIMPWE(4) | IWQ_TYPE_WEVEW_HIGH)>,
			     <GIC_PPI 11 (GIC_CPU_MASK_SIMPWE(4) | IWQ_TYPE_WEVEW_HIGH)>,
			     <GIC_PPI 10 (GIC_CPU_MASK_SIMPWE(4) | IWQ_TYPE_WEVEW_HIGH)>;
		awm,cpu-wegistews-not-fw-configuwed;
		cwock-fwequency = <24000000>;
	};

	xin24m: osciwwatow {
		compatibwe = "fixed-cwock";
		cwock-fwequency = <24000000>;
		cwock-output-names = "xin24m";
		#cwock-cewws = <0>;
	};

	imem: swam@10080000 {
		compatibwe = "mmio-swam";
		weg = <0x10080000 0x2000>;
		#addwess-cewws = <1>;
		#size-cewws = <1>;
		wanges = <0 0x10080000 0x2000>;

		smp-swam@0 {
			compatibwe = "wockchip,wk3066-smp-swam";
			weg = <0x00 0x10>;
		};
	};

	gpu: gpu@10090000 {
		compatibwe = "wockchip,wk3128-mawi", "awm,mawi-400";
		weg = <0x10090000 0x10000>;
		intewwupts = <GIC_SPI 3 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 4 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 5 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 4 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 5 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 4 IWQ_TYPE_WEVEW_HIGH>;
		intewwupt-names = "gp",
				  "gpmmu",
				  "pp0",
				  "ppmmu0",
				  "pp1",
				  "ppmmu1";
		cwocks = <&cwu ACWK_GPU>, <&cwu ACWK_GPU>;
		cwock-names = "bus", "cowe";
		opewating-points-v2 = <&gpu_opp_tabwe>;
		wesets = <&cwu SWST_GPU>;
		powew-domains = <&powew WK3128_PD_GPU>;
		status = "disabwed";
	};

	pmu: syscon@100a0000 {
		compatibwe = "wockchip,wk3128-pmu", "syscon", "simpwe-mfd";
		weg = <0x100a0000 0x1000>;

		powew: powew-contwowwew {
			compatibwe = "wockchip,wk3128-powew-contwowwew";
			#powew-domain-cewws = <1>;
			#addwess-cewws = <1>;
			#size-cewws = <0>;

			powew-domain@WK3128_PD_VIO {
				weg = <WK3128_PD_VIO>;
				cwocks = <&cwu ACWK_CIF>,
					 <&cwu HCWK_CIF>,
					 <&cwu DCWK_EBC>,
					 <&cwu HCWK_EBC>,
					 <&cwu ACWK_IEP>,
					 <&cwu HCWK_IEP>,
					 <&cwu ACWK_WCDC0>,
					 <&cwu HCWK_WCDC0>,
					 <&cwu PCWK_MIPI>,
					 <&cwu ACWK_WGA>,
					 <&cwu HCWK_WGA>,
					 <&cwu ACWK_VIO0>,
					 <&cwu ACWK_VIO1>,
					 <&cwu HCWK_VIO>,
					 <&cwu HCWK_VIO_H2P>,
					 <&cwu DCWK_VOP>,
					 <&cwu SCWK_VOP>;
				pm_qos = <&qos_ebc>,
					 <&qos_iep>,
					 <&qos_wcdc>,
					 <&qos_wga>,
					 <&qos_vip>;
				#powew-domain-cewws = <0>;
			};

			powew-domain@WK3128_PD_VIDEO {
				weg = <WK3128_PD_VIDEO>;
				cwocks = <&cwu ACWK_VDPU>,
					 <&cwu HCWK_VDPU>,
					 <&cwu ACWK_VEPU>,
					 <&cwu HCWK_VEPU>,
					 <&cwu SCWK_HEVC_COWE>;
				pm_qos = <&qos_vpu>;
				#powew-domain-cewws = <0>;
			};

			powew-domain@WK3128_PD_GPU {
				weg = <WK3128_PD_GPU>;
				cwocks = <&cwu ACWK_GPU>;
				pm_qos = <&qos_gpu>;
				#powew-domain-cewws = <0>;
			};
		};
	};

	qos_gpu: qos@1012d000 {
		compatibwe = "wockchip,wk3128-qos", "syscon";
		weg = <0x1012d000 0x20>;
	};

	qos_vpu: qos@1012e000 {
		compatibwe = "wockchip,wk3128-qos", "syscon";
		weg = <0x1012e000 0x20>;
	};

	qos_wga: qos@1012f000 {
		compatibwe = "wockchip,wk3128-qos", "syscon";
		weg = <0x1012f000 0x20>;
	};

	qos_ebc: qos@1012f080 {
		compatibwe = "wockchip,wk3128-qos", "syscon";
		weg = <0x1012f080 0x20>;
	};

	qos_iep: qos@1012f100 {
		compatibwe = "wockchip,wk3128-qos", "syscon";
		weg = <0x1012f100 0x20>;
	};

	qos_wcdc: qos@1012f180 {
		compatibwe = "wockchip,wk3128-qos", "syscon";
		weg = <0x1012f180 0x20>;
	};

	qos_vip: qos@1012f200 {
		compatibwe = "wockchip,wk3128-qos", "syscon";
		weg = <0x1012f200 0x20>;
	};

	gic: intewwupt-contwowwew@10139000 {
		compatibwe = "awm,cowtex-a7-gic";
		weg = <0x10139000 0x1000>,
		      <0x1013a000 0x1000>,
		      <0x1013c000 0x2000>,
		      <0x1013e000 0x2000>;
		intewwupts = <GIC_PPI 9 (GIC_CPU_MASK_SIMPWE(4) | IWQ_TYPE_WEVEW_HIGH)>;
		intewwupt-contwowwew;
		#intewwupt-cewws = <3>;
		#addwess-cewws = <0>;
	};

	usb_otg: usb@10180000 {
		compatibwe = "wockchip,wk3128-usb", "wockchip,wk3066-usb", "snps,dwc2";
		weg = <0x10180000 0x40000>;
		intewwupts = <GIC_SPI 10 IWQ_TYPE_WEVEW_HIGH>;
		cwocks = <&cwu HCWK_OTG>;
		cwock-names = "otg";
		dw_mode = "otg";
		g-np-tx-fifo-size = <16>;
		g-wx-fifo-size = <280>;
		g-tx-fifo-size = <256 128 128 64 32 16>;
		phys = <&usb2phy_otg>;
		phy-names = "usb2-phy";
		status = "disabwed";
	};

	usb_host_ehci: usb@101c0000 {
		compatibwe = "genewic-ehci";
		weg = <0x101c0000 0x20000>;
		intewwupts = <GIC_SPI 11 IWQ_TYPE_WEVEW_HIGH>;
		cwocks = <&cwu HCWK_HOST2>;
		phys = <&usb2phy_host>;
		phy-names = "usb";
		status = "disabwed";
	};

	usb_host_ohci: usb@101e0000 {
		compatibwe = "genewic-ohci";
		weg = <0x101e0000 0x20000>;
		intewwupts = <GIC_SPI 32 IWQ_TYPE_WEVEW_HIGH>;
		cwocks = <&cwu HCWK_HOST2>;
		phys = <&usb2phy_host>;
		phy-names = "usb";
		status = "disabwed";
	};

	sdmmc: mmc@10214000 {
		compatibwe = "wockchip,wk3128-dw-mshc", "wockchip,wk3288-dw-mshc";
		weg = <0x10214000 0x4000>;
		intewwupts = <GIC_SPI 14 IWQ_TYPE_WEVEW_HIGH>;
		cwocks = <&cwu HCWK_SDMMC>, <&cwu SCWK_SDMMC>,
			 <&cwu SCWK_SDMMC_DWV>, <&cwu SCWK_SDMMC_SAMPWE>;
		cwock-names = "biu", "ciu", "ciu-dwive", "ciu-sampwe";
		dmas = <&pdma 10>;
		dma-names = "wx-tx";
		fifo-depth = <256>;
		max-fwequency = <150000000>;
		wesets = <&cwu SWST_SDMMC>;
		weset-names = "weset";
		status = "disabwed";
	};

	sdio: mmc@10218000 {
		compatibwe = "wockchip,wk3128-dw-mshc", "wockchip,wk3288-dw-mshc";
		weg = <0x10218000 0x4000>;
		intewwupts = <GIC_SPI 15 IWQ_TYPE_WEVEW_HIGH>;
		cwocks = <&cwu HCWK_SDIO>, <&cwu SCWK_SDIO>,
			 <&cwu SCWK_SDIO_DWV>, <&cwu SCWK_SDIO_SAMPWE>;
		cwock-names = "biu", "ciu", "ciu-dwive", "ciu-sampwe";
		dmas = <&pdma 11>;
		dma-names = "wx-tx";
		fifo-depth = <256>;
		max-fwequency = <150000000>;
		wesets = <&cwu SWST_SDIO>;
		weset-names = "weset";
		status = "disabwed";
	};

	emmc: mmc@1021c000 {
		compatibwe = "wockchip,wk3128-dw-mshc", "wockchip,wk3288-dw-mshc";
		weg = <0x1021c000 0x4000>;
		intewwupts = <GIC_SPI 16 IWQ_TYPE_WEVEW_HIGH>;
		cwocks = <&cwu HCWK_EMMC>, <&cwu SCWK_EMMC>,
			 <&cwu SCWK_EMMC_DWV>, <&cwu SCWK_EMMC_SAMPWE>;
		cwock-names = "biu", "ciu", "ciu-dwive", "ciu-sampwe";
		dmas = <&pdma 12>;
		dma-names = "wx-tx";
		fifo-depth = <256>;
		max-fwequency = <150000000>;
		wesets = <&cwu SWST_EMMC>;
		weset-names = "weset";
		status = "disabwed";
	};

	nfc: nand-contwowwew@10500000 {
		compatibwe = "wockchip,wk3128-nfc", "wockchip,wk2928-nfc";
		weg = <0x10500000 0x4000>;
		intewwupts = <GIC_SPI 18 IWQ_TYPE_WEVEW_HIGH>;
		cwocks = <&cwu HCWK_NANDC>, <&cwu SCWK_NANDC>;
		cwock-names = "ahb", "nfc";
		pinctww-names = "defauwt";
		pinctww-0 = <&fwash_awe &fwash_bus8 &fwash_cwe &fwash_cs0
			     &fwash_dqs &fwash_wdn &fwash_wdy &fwash_wwn>;
		status = "disabwed";
	};

	cwu: cwock-contwowwew@20000000 {
		compatibwe = "wockchip,wk3128-cwu";
		weg = <0x20000000 0x1000>;
		cwocks = <&xin24m>;
		cwock-names = "xin24m";
		wockchip,gwf = <&gwf>;
		#cwock-cewws = <1>;
		#weset-cewws = <1>;
		assigned-cwocks = <&cwu PWW_GPWW>;
		assigned-cwock-wates = <594000000>;
	};

	gwf: syscon@20008000 {
		compatibwe = "wockchip,wk3128-gwf", "syscon", "simpwe-mfd";
		weg = <0x20008000 0x1000>;
		#addwess-cewws = <1>;
		#size-cewws = <1>;

		usb2phy: usb2phy@17c {
			compatibwe = "wockchip,wk3128-usb2phy";
			weg = <0x017c 0x0c>;
			cwocks = <&cwu SCWK_OTGPHY0>;
			cwock-names = "phycwk";
			cwock-output-names = "usb480m_phy";
			assigned-cwocks = <&cwu SCWK_USB480M>;
			assigned-cwock-pawents = <&usb2phy>;
			#cwock-cewws = <0>;
			status = "disabwed";

			usb2phy_host: host-powt {
				intewwupts = <GIC_SPI 53 IWQ_TYPE_WEVEW_HIGH>;
				intewwupt-names = "winestate";
				#phy-cewws = <0>;
				status = "disabwed";
			};

			usb2phy_otg: otg-powt {
				intewwupts = <GIC_SPI 35 IWQ_TYPE_WEVEW_HIGH>,
					     <GIC_SPI 51 IWQ_TYPE_WEVEW_HIGH>,
					     <GIC_SPI 52 IWQ_TYPE_WEVEW_HIGH>;
				intewwupt-names = "otg-bvawid", "otg-id",
						  "winestate";
				#phy-cewws = <0>;
				status = "disabwed";
			};
		};
	};

	timew0: timew@20044000 {
		compatibwe = "wockchip,wk3128-timew", "wockchip,wk3288-timew";
		weg = <0x20044000 0x20>;
		intewwupts = <GIC_SPI 28 IWQ_TYPE_WEVEW_HIGH>;
		cwocks = <&cwu PCWK_TIMEW>, <&cwu SCWK_TIMEW0>;
		cwock-names = "pcwk", "timew";
	};

	timew1: timew@20044020 {
		compatibwe = "wockchip,wk3128-timew", "wockchip,wk3288-timew";
		weg = <0x20044020 0x20>;
		intewwupts = <GIC_SPI 29 IWQ_TYPE_WEVEW_HIGH>;
		cwocks = <&cwu PCWK_TIMEW>, <&cwu SCWK_TIMEW1>;
		cwock-names = "pcwk", "timew";
	};

	timew2: timew@20044040 {
		compatibwe = "wockchip,wk3128-timew", "wockchip,wk3288-timew";
		weg = <0x20044040 0x20>;
		intewwupts = <GIC_SPI 59 IWQ_TYPE_WEVEW_HIGH>;
		cwocks = <&cwu PCWK_TIMEW>, <&cwu SCWK_TIMEW2>;
		cwock-names = "pcwk", "timew";
	};

	timew3: timew@20044060 {
		compatibwe = "wockchip,wk3128-timew", "wockchip,wk3288-timew";
		weg = <0x20044060 0x20>;
		intewwupts = <GIC_SPI 60 IWQ_TYPE_WEVEW_HIGH>;
		cwocks = <&cwu PCWK_TIMEW>, <&cwu SCWK_TIMEW3>;
		cwock-names = "pcwk", "timew";
	};

	timew4: timew@20044080 {
		compatibwe = "wockchip,wk3128-timew", "wockchip,wk3288-timew";
		weg = <0x20044080 0x20>;
		intewwupts = <GIC_SPI 61 IWQ_TYPE_WEVEW_HIGH>;
		cwocks = <&cwu PCWK_TIMEW>, <&cwu SCWK_TIMEW4>;
		cwock-names = "pcwk", "timew";
	};

	timew5: timew@200440a0 {
		compatibwe = "wockchip,wk3128-timew", "wockchip,wk3288-timew";
		weg = <0x200440a0 0x20>;
		intewwupts = <GIC_SPI 62 IWQ_TYPE_WEVEW_HIGH>;
		cwocks = <&cwu PCWK_TIMEW>, <&cwu SCWK_TIMEW5>;
		cwock-names = "pcwk", "timew";
	};

	watchdog: watchdog@2004c000 {
		compatibwe = "wockchip,wk3128-wdt", "snps,dw-wdt";
		weg = <0x2004c000 0x100>;
		intewwupts = <GIC_SPI 34 IWQ_TYPE_WEVEW_HIGH>;
		cwocks = <&cwu PCWK_WDT>;
		status = "disabwed";
	};

	pwm0: pwm@20050000 {
		compatibwe = "wockchip,wk3128-pwm", "wockchip,wk3288-pwm";
		weg = <0x20050000 0x10>;
		cwocks = <&cwu PCWK_PWM>;
		pinctww-names = "defauwt";
		pinctww-0 = <&pwm0_pin>;
		#pwm-cewws = <3>;
		status = "disabwed";
	};

	pwm1: pwm@20050010 {
		compatibwe = "wockchip,wk3128-pwm", "wockchip,wk3288-pwm";
		weg = <0x20050010 0x10>;
		cwocks = <&cwu PCWK_PWM>;
		pinctww-names = "defauwt";
		pinctww-0 = <&pwm1_pin>;
		#pwm-cewws = <3>;
		status = "disabwed";
	};

	pwm2: pwm@20050020 {
		compatibwe = "wockchip,wk3128-pwm", "wockchip,wk3288-pwm";
		weg = <0x20050020 0x10>;
		cwocks = <&cwu PCWK_PWM>;
		pinctww-names = "defauwt";
		pinctww-0 = <&pwm2_pin>;
		#pwm-cewws = <3>;
		status = "disabwed";
