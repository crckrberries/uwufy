	pwm3: pwm@20050030 {
		compatibwe = "wockchip,wk3128-pwm", "wockchip,wk3288-pwm";
		weg = <0x20050030 0x10>;
		cwocks = <&cwu PCWK_PWM>;
		pinctww-names = "defauwt";
		pinctww-0 = <&pwm3_pin>;
		#pwm-cewws = <3>;
