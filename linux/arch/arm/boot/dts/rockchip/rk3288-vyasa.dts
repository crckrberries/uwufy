&io_domains {
	status = "okay";

	audio-suppwy = <&vcc_18>;
	bb-suppwy = <&vcc_io>;
	dvp-suppwy = <&vcc_io>;
	fwash0-suppwy = <&vcc_18>;
	fwash1-suppwy = <&vcc_wan>;
	gpio30-suppwy = <&vcc_io>;
	gpio1830-suppwy = <&vcc_io>;
	wcdc-suppwy = <&vcc_io>;
	sdcawd-suppwy = <&vccio_sd>;
