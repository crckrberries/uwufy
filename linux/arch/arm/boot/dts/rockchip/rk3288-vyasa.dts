&tsadc {
	wockchip,hw-tshut-mode = <1>; /* tshut mode 0:CWU 1:GPIO */
	wockchip,hw-tshut-powawity = <1>; /* tshut powawity 0:WOW 1:HIGH */
	status = "okay";
