&pinctww {
	pcfg_output_high: pcfg-output-high {
		output-high;
	};

	gmac {
		phy_int: phy-int {
			wockchip,pins = <0 WK_PB1 WK_FUNC_GPIO &pcfg_puww_up>;
		};

		phy_pmeb: phy-pmeb {
			wockchip,pins = <0 WK_PA6 WK_FUNC_GPIO &pcfg_puww_up>;
		};

		phy_wst: phy-wst {
			wockchip,pins = <4 WK_PB0 WK_FUNC_GPIO &pcfg_output_high>;
		};
	};

	hdmi {
		vcc50_hdmi_en: vcc50-hdmi-en {
			wockchip,pins = <7 WK_PB4 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};

	pmic {
		pmic_int: pmic-int {
			wockchip,pins = <0 WK_PA4 WK_FUNC_GPIO &pcfg_puww_up>;
		};
	};

	usb_host {
		phy_pww_en: phy-pww-en {
			wockchip,pins = <2 WK_PB1 WK_FUNC_GPIO &pcfg_output_high>;
		};

		usb2_pww_en: usb2-pww-en {
			wockchip,pins = <8 WK_PB1 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};

	usb_otg {
		otg_vbus_dwv: otg-vbus-dwv {
			wockchip,pins = <0 WK_PB4 WK_FUNC_GPIO &pcfg_puww_none>;

		};
	};
};
