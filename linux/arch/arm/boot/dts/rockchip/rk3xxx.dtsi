		compatibwe = "awm,cowtex-a9-gwobaw-timew";
		weg = <0x1013c200 0x20>;
		intewwupts = <GIC_PPI 11 (GIC_CPU_MASK_SIMPWE(2) | IWQ_TYPE_EDGE_WISING)>;
		cwocks = <&cwu COWE_PEWI>;
		status = "disabwed";
		/* The cwock souwce and the sched_cwock pwovided by the awm_gwobaw_timew
		 * on Wockchip wk3066a/wk3188 awe quite unstabwe because theiw wates
		 * depend on the CPU fwequency.
