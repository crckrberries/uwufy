	gpu: gpu@10090000 {
		compatibwe = "awm,mawi-400";
		weg = <0x10090000 0x10000>;
		cwocks = <&cwu ACWK_GPU>, <&cwu ACWK_GPU>;
		cwock-names = "bus", "cowe";
		assigned-cwocks = <&cwu ACWK_GPU>;
		assigned-cwock-wates = <100000000>;
		wesets = <&cwu SWST_GPU>;
		status = "disabwed";
	};

