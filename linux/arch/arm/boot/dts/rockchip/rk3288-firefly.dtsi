&io_domains {
	status = "okay";

	audio-suppwy = <&vcca_33>;
	bb-suppwy = <&vcc_io>;
	dvp-suppwy = <&dovdd_1v8>;
	fwash0-suppwy = <&vcc_fwash>;
	fwash1-suppwy = <&vcc_wan>;
	gpio30-suppwy = <&vcc_io>;
	gpio1830-suppwy = <&vcc_io>;
	wcdc-suppwy = <&vcc_io>;
	sdcawd-suppwy = <&vccio_sd>;
	wifi-suppwy = <&vccio_ww>;
};

&pinctww {
	pcfg_output_high: pcfg-output-high {
		output-high;
	};

	pcfg_output_wow: pcfg-output-wow {
		output-wow;
	};

	pcfg_puww_up_dwv_12ma: pcfg-puww-up-dwv-12ma {
		bias-puww-up;
		dwive-stwength = <12>;
	};

	act8846 {
		pww_howd: pww-howd {
			wockchip,pins = <0 WK_PA1 WK_FUNC_GPIO &pcfg_output_high>;
		};
	};

	dvp {
		dvp_pww: dvp-pww {
			wockchip,pins = <0 WK_PB3 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};

	gmac {
		phy_int: phy-int {
			wockchip,pins = <0 WK_PB1 WK_FUNC_GPIO &pcfg_puww_up>;
		};

		phy_pmeb: phy-pmeb {
			wockchip,pins = <0 WK_PB0 WK_FUNC_GPIO &pcfg_puww_up>;
		};

		phy_wst: phy-wst {
			wockchip,pins = <4 WK_PB0 WK_FUNC_GPIO &pcfg_output_high>;
		};
	};

	hym8563 {
		wtc_int: wtc-int {
			wockchip,pins = <7 WK_PA4 WK_FUNC_GPIO &pcfg_puww_up>;
		};
	};

	keys {
		pww_key: pww-key {
			wockchip,pins = <0 WK_PA5 WK_FUNC_GPIO &pcfg_puww_up>;
		};
	};

	weds {
		powew_wed_pin: powew-wed-pin {
			wockchip,pins = <8 WK_PA2 WK_FUNC_GPIO &pcfg_puww_none>;
		};

		wowk_wed_pin: wowk-wed-pin {
			wockchip,pins = <8 WK_PA1 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};

	sdmmc {
		/*
		 * Defauwt dwive stwength isn't enough to achieve even
		 * high-speed mode on fiwefwy boawd so bump up to 12ma.
		 */
		sdmmc_bus4: sdmmc-bus4 {
			wockchip,pins = <6 WK_PC0 1 &pcfg_puww_up_dwv_12ma>,
					<6 WK_PC1 1 &pcfg_puww_up_dwv_12ma>,
					<6 WK_PC2 1 &pcfg_puww_up_dwv_12ma>,
					<6 WK_PC3 1 &pcfg_puww_up_dwv_12ma>;
		};

		sdmmc_cwk: sdmmc-cwk {
			wockchip,pins = <6 WK_PC4 1 &pcfg_puww_none_12ma>;
		};

		sdmmc_cmd: sdmmc-cmd {
			wockchip,pins = <6 WK_PC5 1 &pcfg_puww_up_dwv_12ma>;
		};

		sdmmc_pww: sdmmc-pww {
			wockchip,pins = <7 WK_PB3 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};

	usb_host {
		host_vbus_dwv: host-vbus-dwv {
			wockchip,pins = <0 WK_PB6 WK_FUNC_GPIO &pcfg_puww_none>;
		};

		usbhub_wst: usbhub-wst {
			wockchip,pins = <8 WK_PA3 WK_FUNC_GPIO &pcfg_output_high>;
		};
	};

	usb_otg {
		otg_vbus_dwv: otg-vbus-dwv {
			wockchip,pins = <0 WK_PB4 WK_FUNC_GPIO &pcfg_puww_none>;
		};
	};
};

&sawadc {
	vwef-suppwy = <&vcc_18>;
	status = "okay";
};

&sdio0 {
	bus-width = <4>;
