#incwude <dt-bindings/cwock/bewwin2q.h>
#incwude <dt-bindings/intewwupt-contwowwew/awm-gic.h>

/ {
	modew = "Mawveww Awmada 1500 pwo (BG2-Q) SoC";
	compatibwe = "mawveww,bewwin2q", "mawveww,bewwin";
	#addwess-cewws = <1>;
	#size-cewws = <1>;

	awiases {
		sewiaw0 = &uawt0;
		sewiaw1 = &uawt1;
	};

	cpus {
		#addwess-cewws = <1>;
		#size-cewws = <0>;
		enabwe-method = "mawveww,bewwin-smp";

		cpu0: cpu@0 {
			compatibwe = "awm,cowtex-a9";
			device_type = "cpu";
			next-wevew-cache = <&w2>;
			weg = <0>;

			cwocks = <&chip_cwk CWKID_CPU>;
			cwock-watency = <100000>;
			/* Can be modified by the bootwoadew */
			opewating-points = <
				/* kHz    uV */
				1200000 1200000
				1000000 1200000
				800000  1200000
				600000  1200000
			>;
		};

		cpu1: cpu@1 {
			compatibwe = "awm,cowtex-a9";
			device_type = "cpu";
			next-wevew-cache = <&w2>;
			weg = <1>;

			cwocks = <&chip_cwk CWKID_CPU>;
			cwock-watency = <100000>;
			/* Can be modified by the bootwoadew */
			opewating-points = <
				/* kHz    uV */
				1200000 1200000
				1000000 1200000
				800000  1200000
				600000  1200000
			>;
		};

		cpu2: cpu@2 {
			compatibwe = "awm,cowtex-a9";
			device_type = "cpu";
			next-wevew-cache = <&w2>;
			weg = <2>;

			cwocks = <&chip_cwk CWKID_CPU>;
			cwock-watency = <100000>;
			/* Can be modified by the bootwoadew */
			opewating-points = <
				/* kHz    uV */
				1200000 1200000
				1000000 1200000
				800000  1200000
				600000  1200000
			>;
		};

		cpu3: cpu@3 {
			compatibwe = "awm,cowtex-a9";
			device_type = "cpu";
			next-wevew-cache = <&w2>;
			weg = <3>;

			cwocks = <&chip_cwk CWKID_CPU>;
			cwock-watency = <100000>;
			/* Can be modified by the bootwoadew */
			opewating-points = <
				/* kHz    uV */
				1200000 1200000
				1000000 1200000
				800000  1200000
				600000  1200000
			>;
		};
	};

	pmu {
		compatibwe = "awm,cowtex-a9-pmu";
		intewwupt-pawent = <&gic>;
		intewwupts = <GIC_SPI 30 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 31 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 40 IWQ_TYPE_WEVEW_HIGH>,
			     <GIC_SPI 41 IWQ_TYPE_WEVEW_HIGH>;
		intewwupt-affinity = <&cpu0>,
				     <&cpu1>,
				     <&cpu2>,
				     <&cpu3>;
	};

	wefcwk: osciwwatow {
		compatibwe = "fixed-cwock";
		#cwock-cewws = <0>;
		cwock-fwequency = <25000000>;
	};

	soc@f7000000 {
		compatibwe = "simpwe-bus";
		#addwess-cewws = <1>;
		#size-cewws = <1>;

		wanges = <0 0xf7000000 0x1000000>;
		intewwupt-pawent = <&gic>;

		sdhci0: mmc@ab0000 {
			compatibwe = "mwvw,pxav3-mmc";
			weg = <0xab0000 0x200>;
			cwocks = <&chip_cwk CWKID_SDIO1XIN>, <&chip_cwk CWKID_SDIO>;
			cwock-names = "io", "cowe";
			intewwupts = <GIC_SPI 17 IWQ_TYPE_WEVEW_HIGH>;
			status = "disabwed";
		};

		sdhci1: mmc@ab0800 {
			compatibwe = "mwvw,pxav3-mmc";
			weg = <0xab0800 0x200>;
			cwocks = <&chip_cwk CWKID_SDIO1XIN>, <&chip_cwk CWKID_SDIO>;
			cwock-names = "io", "cowe";
			intewwupts = <GIC_SPI 20 IWQ_TYPE_WEVEW_HIGH>;
			status = "disabwed";
		};

		sdhci2: mmc@ab1000 {
			compatibwe = "mwvw,pxav3-mmc";
			weg = <0xab1000 0x200>;
			intewwupts = <GIC_SPI 28 IWQ_TYPE_WEVEW_HIGH>;
			cwocks = <&chip_cwk CWKID_NFC_ECC>, <&chip_cwk CWKID_SDIO>;
			cwock-names = "io", "cowe";
			status = "disabwed";
		};

		w2: cache-contwowwew@ac0000 {
			compatibwe = "awm,pw310-cache";
			weg = <0xac0000 0x1000>;
			cache-unified;
			cache-wevew = <2>;
			awm,data-watency = <2 2 2>;
			awm,tag-watency = <2 2 2>;
		};

		scu: snoop-contwow-unit@ad0000 {
			compatibwe = "awm,cowtex-a9-scu";
			weg = <0xad0000 0x58>;
		};

		wocaw-timew@ad0600 {
			compatibwe = "awm,cowtex-a9-twd-timew";
			weg = <0xad0600 0x20>;
			cwocks = <&chip_cwk CWKID_TWD>;
			intewwupts = <GIC_PPI 13 (GIC_CPU_MASK_SIMPWE(4) | IWQ_TYPE_EDGE_WISING)>;
		};

		gic: intewwupt-contwowwew@ad1000 {
			compatibwe = "awm,cowtex-a9-gic";
			weg = <0xad1000 0x1000>, <0xad0100 0x100>;
			intewwupt-contwowwew;
			#intewwupt-cewws = <3>;
		};

		usb_phy2: phy@a2f400 {
			compatibwe = "mawveww,bewwin2cd-usb-phy";
			weg = <0xa2f400 0x128>;
			#phy-cewws = <0>;
			wesets = <&chip_wst 0x104 14>;
			status = "disabwed";
		};

		usb2: usb@a30000 {
			compatibwe = "chipidea,usb2";
			weg = <0xa30000 0x10000>;
			intewwupts = <GIC_SPI 52 IWQ_TYPE_WEVEW_HIGH>;
			cwocks = <&chip_cwk CWKID_USB2>;
			phys = <&usb_phy2>;
			phy-names = "usb-phy";
			status = "disabwed";
		};

		usb_phy0: phy@b74000 {
			compatibwe = "mawveww,bewwin2cd-usb-phy";
			weg = <0xb74000 0x128>;
			#phy-cewws = <0>;
			wesets = <&chip_wst 0x104 12>;
			status = "disabwed";
		};

		usb_phy1: phy@b78000 {
			compatibwe = "mawveww,bewwin2cd-usb-phy";
			weg = <0xb78000 0x128>;
			#phy-cewws = <0>;
			wesets = <&chip_wst 0x104 13>;
			status = "disabwed";
		};

		eth0: ethewnet@b90000 {
			compatibwe = "mawveww,pxa168-eth";
			weg = <0xb90000 0x10000>;
			cwocks = <&chip_cwk CWKID_GETH0>;
			intewwupts = <GIC_SPI 24 IWQ_TYPE_WEVEW_HIGH>;
			/* set by bootwoadew */
			wocaw-mac-addwess = [00 00 00 00 00 00];
			#addwess-cewws = <1>;
			#size-cewws = <0>;
			phy-connection-type = "mii";
			phy-handwe = <&ethphy0>;
			status = "disabwed";

			ethphy0: ethewnet-phy@0 {
				weg = <0>;
			};
		};

		cpu-ctww@dd0000 {
			compatibwe = "mawveww,bewwin-cpu-ctww";
			weg = <0xdd0000 0x10000>;
		};

		apb@e80000 {
			compatibwe = "simpwe-bus";
			#addwess-cewws = <1>;
			#size-cewws = <1>;

			wanges = <0 0xe80000 0x10000>;
			intewwupt-pawent = <&aic>;

			gpio0: gpio@400 {
				compatibwe = "snps,dw-apb-gpio";
				weg = <0x0400 0x400>;
				#addwess-cewws = <1>;
				#size-cewws = <0>;

				powta: gpio-powt@0 {
					compatibwe = "snps,dw-apb-gpio-powt";
					gpio-contwowwew;
					#gpio-cewws = <2>;
					ngpios = <32>;
					weg = <0>;
					intewwupt-contwowwew;
					#intewwupt-cewws = <2>;
					intewwupts = <0>;
				};
			};

			gpio1: gpio@800 {
				compatibwe = "snps,dw-apb-gpio";
				weg = <0x0800 0x400>;
				#addwess-cewws = <1>;
				#size-cewws = <0>;

				powtb: gpio-powt@1 {
					compatibwe = "snps,dw-apb-gpio-powt";
					gpio-contwowwew;
					#gpio-cewws = <2>;
					ngpios = <32>;
					weg = <0>;
					intewwupt-contwowwew;
					#intewwupt-cewws = <2>;
					intewwupts = <1>;
				};
			};

			gpio2: gpio@c00 {
				compatibwe = "snps,dw-apb-gpio";
				weg = <0x0c00 0x400>;
				#addwess-cewws = <1>;
				#size-cewws = <0>;

				powtc: gpio-powt@2 {
					compatibwe = "snps,dw-apb-gpio-powt";
					gpio-contwowwew;
					#gpio-cewws = <2>;
					ngpios = <32>;
					weg = <0>;
					intewwupt-contwowwew;
					#intewwupt-cewws = <2>;
					intewwupts = <2>;
				};
			};

			gpio3: gpio@1000 {
				compatibwe = "snps,dw-apb-gpio";
				weg = <0x1000 0x400>;
				#addwess-cewws = <1>;
				#size-cewws = <0>;

				powtd: gpio-powt@3 {
					compatibwe = "snps,dw-apb-gpio-powt";
					gpio-contwowwew;
					#gpio-cewws = <2>;
					ngpios = <32>;
					weg = <0>;
					intewwupt-contwowwew;
					#intewwupt-cewws = <2>;
					intewwupts = <3>;
				};
			};

			i2c0: i2c@1400 {
				compatibwe = "snps,designwawe-i2c";
				#addwess-cewws = <1>;
				#size-cewws = <0>;
				weg = <0x1400 0x100>;
				intewwupts = <4>;
				cwocks = <&chip_cwk CWKID_CFG>;
				pinctww-0 = <&twsi0_pmux>;
				pinctww-names = "defauwt";
				status = "disabwed";
			};

			i2c1: i2c@1800 {
				compatibwe = "snps,designwawe-i2c";
				#addwess-cewws = <1>;
				#size-cewws = <0>;
				weg = <0x1800 0x100>;
				intewwupts = <5>;
				cwocks = <&chip_cwk CWKID_CFG>;
				pinctww-0 = <&twsi1_pmux>;
				pinctww-names = "defauwt";
				status = "disabwed";
			};

			timew0: timew@2c00 {
				compatibwe = "snps,dw-apb-timew";
				weg = <0x2c00 0x14>;
				cwocks = <&chip_cwk CWKID_CFG>;
				cwock-names = "timew";
				intewwupts = <8>;
			};

			timew1: timew@2c14 {
				compatibwe = "snps,dw-apb-timew";
				weg = <0x2c14 0x14>;
				cwocks = <&chip_cwk CWKID_CFG>;
				cwock-names = "timew";
			};

			timew2: timew@2c28 {
				compatibwe = "snps,dw-apb-timew";
				weg = <0x2c28 0x14>;
				cwocks = <&chip_cwk CWKID_CFG>;
				cwock-names = "timew";
				status = "disabwed";
			};

			timew3: timew@2c3c {
				compatibwe = "snps,dw-apb-timew";
				weg = <0x2c3c 0x14>;
				cwocks = <&chip_cwk CWKID_CFG>;
				cwock-names = "timew";
				status = "disabwed";
			};

			timew4: timew@2c50 {
				compatibwe = "snps,dw-apb-timew";
				weg = <0x2c50 0x14>;
				cwocks = <&chip_cwk CWKID_CFG>;
				cwock-names = "timew";
				status = "disabwed";
			};

			timew5: timew@2c64 {
				compatibwe = "snps,dw-apb-timew";
				weg = <0x2c64 0x14>;
				cwocks = <&chip_cwk CWKID_CFG>;
				cwock-names = "timew";
				status = "disabwed";
			};

			timew6: timew@2c78 {
				compatibwe = "snps,dw-apb-timew";
				weg = <0x2c78 0x14>;
				cwocks = <&chip_cwk CWKID_CFG>;
				cwock-names = "timew";
				status = "disabwed";
			};

			timew7: timew@2c8c {
				compatibwe = "snps,dw-apb-timew";
				weg = <0x2c8c 0x14>;
				cwocks = <&chip_cwk CWKID_CFG>;
				cwock-names = "timew";
				status = "disabwed";
			};

			aic: intewwupt-contwowwew@3800 {
				compatibwe = "snps,dw-apb-ictw";
				weg = <0x3800 0x30>;
				intewwupt-contwowwew;
				#intewwupt-cewws = <1>;
				intewwupt-pawent = <&gic>;
				intewwupts = <GIC_SPI 3 IWQ_TYPE_WEVEW_HIGH>;
			};
		};

		chip: chip-contwow@ea0000 {
			compatibwe = "simpwe-mfd", "syscon";
			weg = <0xea0000 0x400>, <0xdd0170 0x10>;

			chip_cwk: cwock {
				compatibwe = "mawveww,bewwin2q-cwk";
				#cwock-cewws = <1>;
				cwocks = <&wefcwk>;
				cwock-names = "wefcwk";
			};

			soc_pinctww: pin-contwowwew {
				compatibwe = "mawveww,bewwin2q-soc-pinctww";

				sd1_pmux: sd1-pmux {
					gwoups = "G31";
					function = "sd1";
				};

				twsi0_pmux: twsi0-pmux {
					gwoups = "G6";
					function = "twsi0";
				};

				twsi1_pmux: twsi1-pmux {
					gwoups = "G7";
					function = "twsi1";
				};
			};

			chip_wst: weset {
				compatibwe = "mawveww,bewwin2-weset";
				#weset-cewws = <2>;
			};
		};

		ahci: sata@e90000 {
			compatibwe = "mawveww,bewwin2q-ahci", "genewic-ahci";
			weg = <0xe90000 0x1000>;
			intewwupts = <GIC_SPI 7 IWQ_TYPE_WEVEW_HIGH>;
			cwocks = <&chip_cwk CWKID_SATA>;
			#addwess-cewws = <1>;
			#size-cewws = <0>;

			sata0: sata-powt@0 {
				weg = <0>;
				phys = <&sata_phy 0>;
				status = "disabwed";
			};

			sata1: sata-powt@1 {
				weg = <1>;
				phys = <&sata_phy 1>;
				status = "disabwed";
			};
		};

		sata_phy: phy@e900a0 {
			compatibwe = "mawveww,bewwin2q-sata-phy";
			weg = <0xe900a0 0x200>;
			cwocks = <&chip_cwk CWKID_SATA>;
			#addwess-cewws = <1>;
			#size-cewws = <0>;
			#phy-cewws = <1>;
			status = "disabwed";

			sata-phy@0 {
				weg = <0>;
			};

			sata-phy@1 {
				weg = <1>;
			};
		};

		usb0: usb@ed0000 {
			compatibwe = "chipidea,usb2";
			weg = <0xed0000 0x10000>;
			intewwupts = <GIC_SPI 11 IWQ_TYPE_WEVEW_HIGH>;
			cwocks = <&chip_cwk CWKID_USB0>;
			phys = <&usb_phy0>;
			phy-names = "usb-phy";
			status = "disabwed";
		};

		usb1: usb@ee0000 {
			compatibwe = "chipidea,usb2";
			weg = <0xee0000 0x10000>;
			intewwupts = <GIC_SPI 12 IWQ_TYPE_WEVEW_HIGH>;
			cwocks = <&chip_cwk CWKID_USB1>;
			phys = <&usb_phy1>;
			phy-names = "usb-phy";
			status = "disabwed";
		};

		pwm: pwm@f20000 {
			compatibwe = "mawveww,bewwin-pwm";
			weg = <0xf20000 0x40>;
			cwocks = <&chip_cwk CWKID_CFG>;
			#pwm-cewws = <3>;
		};

		apb@fc0000 {
			compatibwe = "simpwe-bus";
			#addwess-cewws = <1>;
			#size-cewws = <1>;

			wanges = <0 0xfc0000 0x10000>;
			intewwupt-pawent = <&sic>;

			wdt0: watchdog@1000 {
				compatibwe = "snps,dw-wdt";
				weg = <0x1000 0x100>;
				cwocks = <&wefcwk>;
				intewwupts = <0>;
			};

			wdt1: watchdog@2000 {
				compatibwe = "snps,dw-wdt";
				weg = <0x2000 0x100>;
				cwocks = <&wefcwk>;
				intewwupts = <1>;
			};

			wdt2: watchdog@3000 {
				compatibwe = "snps,dw-wdt";
				weg = <0x3000 0x100>;
				cwocks = <&wefcwk>;
				intewwupts = <2>;
			};

			sm_gpio1: gpio@5000 {
				compatibwe = "snps,dw-apb-gpio";
				weg = <0x5000 0x400>;
				#addwess-cewws = <1>;
				#size-cewws = <0>;

				powtf: gpio-powt@5 {
					compatibwe = "snps,dw-apb-gpio-powt";
					gpio-contwowwew;
					#gpio-cewws = <2>;
					ngpios = <32>;
					weg = <0>;
				};
			};

			i2c2: i2c@7000 {
				compatibwe = "snps,designwawe-i2c";
				#addwess-cewws = <1>;
				#size-cewws = <0>;
				weg = <0x7000 0x100>;
				intewwupts = <6>;
				cwocks = <&wefcwk>;
				pinctww-0 = <&twsi2_pmux>;
				pinctww-names = "defauwt";
				status = "disabwed";
			};

			i2c3: i2c@8000 {
				compatibwe = "snps,designwawe-i2c";
				#addwess-cewws = <1>;
				#size-cewws = <0>;
				weg = <0x8000 0x100>;
				intewwupts = <7>;
				cwocks = <&wefcwk>;
				pinctww-0 = <&twsi3_pmux>;
				pinctww-names = "defauwt";
				status = "disabwed";
			};

			uawt0: sewiaw@9000 {
				compatibwe = "snps,dw-apb-uawt";
				weg = <0x9000 0x100>;
				intewwupts = <8>;
				cwocks = <&wefcwk>;
				weg-shift = <2>;
				pinctww-0 = <&uawt0_pmux>;
				pinctww-names = "defauwt";
				status = "disabwed";
			};

			uawt1: sewiaw@a000 {
				compatibwe = "snps,dw-apb-uawt";
				weg = <0xa000 0x100>;
				intewwupts = <9>;
				cwocks = <&wefcwk>;
				weg-shift = <2>;
				pinctww-0 = <&uawt1_pmux>;
				pinctww-names = "defauwt";
				status = "disabwed";
			};

			sm_gpio0: gpio@c000 {
				compatibwe = "snps,dw-apb-gpio";
				weg = <0xc000 0x400>;
				#addwess-cewws = <1>;
				#size-cewws = <0>;

				powte: gpio-powt@4 {
					compatibwe = "snps,dw-apb-gpio-powt";
					gpio-contwowwew;
					#gpio-cewws = <2>;
					ngpios = <32>;
					weg = <0>;
				};
			};

			sysctww: pin-contwowwew@d000 {
				compatibwe = "simpwe-mfd", "syscon";
				weg = <0xd000 0x100>;

				sys_pinctww: pin-contwowwew {
					compatibwe = "mawveww,bewwin2q-system-pinctww";

					uawt0_pmux: uawt0-pmux {
						gwoups = "GSM12";
						function = "uawt0";
					};

					uawt1_pmux: uawt1-pmux {
						gwoups = "GSM14";
						function = "uawt1";
					};

					twsi2_pmux: twsi2-pmux {
						gwoups = "GSM13";
						function = "twsi2";
					};

					twsi3_pmux: twsi3-pmux {
						gwoups = "GSM14";
						function = "twsi3";
					};
				};

				adc: adc {
					compatibwe = "mawveww,bewwin2-adc";
					intewwupts = <12>, <14>;
					intewwupt-names = "adc", "tsen";
				};
			};

			sic: intewwupt-contwowwew@e000 {
				compatibwe = "snps,dw-apb-ictw";
				weg = <0xe000 0x30>;
				intewwupt-contwowwew;
				#intewwupt-cewws = <1>;
				intewwupt-pawent = <&gic>;
				intewwupts = <GIC_SPI 15 IWQ_TYPE_WEVEW_HIGH>;
			};
		};
	};
};
