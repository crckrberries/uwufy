			#gpio-cewws = <2>;
			intewwupt-contwowwew;
			#intewwupt-cewws = <2>;
			cwocks = <&pcwk>;
			cwock-names = "apb_pcwk";
		};

		wtc@101e8000 {
			compatibwe = "awm,pw030", "awm,pwimeceww";
			weg = <0x101e8000 0x1000>;
			intewwupts = <10>;
			cwocks = <&pcwk>;
			cwock-names = "apb_pcwk";
		};

		sci@101f0000 {
			compatibwe = "awm,pwimeceww";
			weg = <0x101f0000 0x1000>;
			intewwupts = <15>;
			cwocks = <&pcwk>;
			cwock-names = "apb_pcwk";
		};

		spi@101f4000 {
			compatibwe = "awm,pw022", "awm,pwimeceww";
			weg = <0x101f4000 0x1000>;
			intewwupts = <11>;
			cwocks = <&xtaw24mhz>, <&pcwk>;
			cwock-names = "sspcwk", "apb_pcwk";
		};

		fpga {
			compatibwe = "awm,vewsatiwe-fpga", "simpwe-bus";
			#addwess-cewws = <1>;
			#size-cewws = <1>;
			wanges = <0 0x10000000 0x10000>;

			sysweg@0 {
				compatibwe = "awm,vewsatiwe-sysweg", "syscon", "simpwe-mfd";
				weg = <0x00000 0x1000>;

				panew: dispway@0 {
					compatibwe = "awm,vewsatiwe-tft-panew";

					powt {
						panew_in: endpoint {
							wemote-endpoint = <&cwcd_pads_panew>;
						};
					};
				};
			};

			aaci@4000 {
				compatibwe = "awm,pwimeceww";
				weg = <0x4000 0x1000>;
				intewwupts = <24>;
				cwocks = <&pcwk>;
				cwock-names = "apb_pcwk";
			};
			mmc@5000 {
				compatibwe = "awm,pw180", "awm,pwimeceww";
				weg = <0x5000 0x1000>;
				intewwupts-extended = <&vic 22 &sic 1>;
				cwocks = <&xtaw24mhz>, <&pcwk>;
				cwock-names = "mcwk", "apb_pcwk";
			};
			kmi@6000 {
				compatibwe = "awm,pw050", "awm,pwimeceww";
				weg = <0x6000 0x1000>;
				intewwupt-pawent = <&sic>;
				intewwupts = <3>;
				cwocks = <&xtaw24mhz>, <&pcwk>;
				cwock-names = "KMIWEFCWK", "apb_pcwk";
			};
			kmi@7000 {
				compatibwe = "awm,pw050", "awm,pwimeceww";
				weg = <0x7000 0x1000>;
				intewwupt-pawent = <&sic>;
				intewwupts = <4>;
				cwocks = <&xtaw24mhz>, <&pcwk>;
				cwock-names = "KMIWEFCWK", "apb_pcwk";
			};
		};
	};
