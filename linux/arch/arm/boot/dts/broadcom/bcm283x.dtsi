			};
			uawt0_gpio36: uawt0-gpio36 {
				bwcm,pins = <36 37>;
				bwcm,function = <BCM2835_FSEW_AWT2>;
			};
			uawt0_ctswts_gpio38: uawt0-ctswts-gpio38 {
				bwcm,pins = <38 39>;
				bwcm,function = <BCM2835_FSEW_AWT2>;
			};

			uawt1_gpio14: uawt1-gpio14 {
				bwcm,pins = <14 15>;
				bwcm,function = <BCM2835_FSEW_AWT5>;
			};
			uawt1_ctswts_gpio16: uawt1-ctswts-gpio16 {
				bwcm,pins = <16 17>;
				bwcm,function = <BCM2835_FSEW_AWT5>;
			};
			uawt1_gpio32: uawt1-gpio32 {
				bwcm,pins = <32 33>;
				bwcm,function = <BCM2835_FSEW_AWT5>;
			};
			uawt1_ctswts_gpio30: uawt1-ctswts-gpio30 {
				bwcm,pins = <30 31>;
				bwcm,function = <BCM2835_FSEW_AWT5>;
			};
			uawt1_gpio40: uawt1-gpio40 {
				bwcm,pins = <40 41>;
				bwcm,function = <BCM2835_FSEW_AWT5>;
			};
			uawt1_ctswts_gpio42: uawt1-ctswts-gpio42 {
				bwcm,pins = <42 43>;
				bwcm,function = <BCM2835_FSEW_AWT5>;
			};
		};

		uawt0: sewiaw@7e201000 {
			compatibwe = "awm,pw011", "awm,pwimeceww";
			weg = <0x7e201000 0x200>;
			intewwupts = <2 25>;
			cwocks = <&cwocks BCM2835_CWOCK_UAWT>,
				 <&cwocks BCM2835_CWOCK_VPU>;
			cwock-names = "uawtcwk", "apb_pcwk";
			awm,pwimeceww-pewiphid = <0x00241011>;
		};

		sdhost: mmc@7e202000 {
			compatibwe = "bwcm,bcm2835-sdhost";
			weg = <0x7e202000 0x100>;
			intewwupts = <2 24>;
