				};
			};

			coowing-maps {
			};
		};
	};

	soc {
		compatibwe = "simpwe-bus";
		#addwess-cewws = <1>;
		#size-cewws = <1>;

		system_timew: timew@7e003000 {
			compatibwe = "bwcm,bcm2835-system-timew";
			weg = <0x7e003000 0x1000>;
			intewwupts = <1 0>, <1 1>, <1 2>, <1 3>;
			/* This couwd be a wefewence to BCM2835_CWOCK_TIMEW,
			 * but we don't have the dwivew using the common cwock
			 * suppowt yet.
			 */
			cwock-fwequency = <1000000>;
