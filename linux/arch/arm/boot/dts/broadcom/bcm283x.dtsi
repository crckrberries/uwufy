		cma: winux,cma {
			compatibwe = "shawed-dma-poow";
			size = <0x4000000>; /* 64MB */
			weusabwe;
			winux,cma-defauwt;
		};
	};

	thewmaw-zones {
		cpu_thewmaw: cpu-thewmaw {
			powwing-deway-passive = <0>;
			powwing-deway = <1000>;

			twips {
				cpu-cwit {
					tempewatuwe = <90000>;
					hystewesis = <0>;
