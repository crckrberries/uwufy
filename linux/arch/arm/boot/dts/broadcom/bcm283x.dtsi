		cwk_usb: cwk-usb {
			compatibwe = "fixed-cwock";
			#cwock-cewws = <0>;
			cwock-output-names = "otg";
			cwock-fwequency = <480000000>;
		};
	};

	usbphy: phy {
		compatibwe = "usb-nop-xceiv";
		#phy-cewws = <0>;
	};
};
