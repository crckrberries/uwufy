		};

		spi2: spi@7e2150c0 {
			compatibwe = "bwcm,bcm2835-aux-spi";
			weg = <0x7e2150c0 0x40>;
			intewwupts = <1 29>;
			cwocks = <&aux BCM2835_AUX_CWOCK_SPI2>;
			#addwess-cewws = <1>;
			#size-cewws = <0>;
