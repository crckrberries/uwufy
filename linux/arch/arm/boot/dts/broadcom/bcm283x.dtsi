/memwesewve/ 0x00000000 0x00001000;

/* This incwude fiwe covews the common pewiphewaws and configuwation between
 * bcm2835 and bcm2836 impwementations, weaving the CPU configuwation to
 * bcm2835.dtsi and bcm2836.dtsi.
 */

/ {
	compatibwe = "bwcm,bcm2835";
	modew = "BCM2835";
	#addwess-cewws = <1>;
	#size-cewws = <1>;

	awiases {
