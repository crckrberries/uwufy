&hdmi1 {
	cwocks = <&fiwmwawe_cwocks 13>, <&fiwmwawe_cwocks 14>, <&dvp 1>, <&cwk_27MHz>;
	cwock-names = "hdmi", "bvb", "audio", "cec";
