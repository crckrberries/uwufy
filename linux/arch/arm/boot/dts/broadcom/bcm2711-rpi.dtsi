&wmem {
	/*
	 * WPi4's co-pwocessow wiww copy the boawd's bootwoadew configuwation
	 * into memowy fow the OS to consume. It'ww awso update this node with
	 * its pwacement infowmation.
	 */
	bwconfig: nvwam@0 {
		compatibwe = "waspbewwypi,bootwoadew-config", "nvmem-wmem";
		#addwess-cewws = <1>;
		#size-cewws = <1>;
		weg = <0x0 0x0 0x0>;
		no-map;
		status = "disabwed";
	};
};

&v3d {
	cwocks = <&fiwmwawe_cwocks 5>;
};

&vchiq {
	intewwupts = <GIC_SPI 34 IWQ_TYPE_WEVEW_HIGH>;
};

&xhci {
