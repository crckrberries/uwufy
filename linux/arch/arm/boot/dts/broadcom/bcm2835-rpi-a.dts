&gpio {
	/*
	 * Taken fwom Waspbewwy-Pi-Wev-1.0-Modew-AB-Schematics.pdf
	 * WPI00021 sheet 02
	 *
	 * Wegend:
	 * "FOO" = GPIO wine named "FOO" on the schematic
	 * "FOO_N" = GPIO wine named "FOO" on schematic, active wow
	 */
	gpio-wine-names = "SDA0",
			  "SCW0",
			  "SDA1",
			  "SCW1",
			  "GPIO_GCWK",
			  "CAM_GPIO1",
			  "WAN_WUN",
			  "SPI_CE1_N",
			  "SPI_CE0_N",
			  "SPI_MISO",
			  "SPI_MOSI",
			  "SPI_SCWK",
			  "", /* GPIO12 */
			  "", /* GPIO13 */
			  /* Sewiaw powt */
			  "TXD0",
			  "WXD0",
			  "STATUS_WED_N",
			  "GPIO17",
			  "GPIO18",
			  "", /* GPIO19 */
			  "", /* GPIO20 */
			  "GPIO21",
			  "GPIO22",
			  "GPIO23",
			  "GPIO24",
			  "GPIO25",
			  "", /* GPIO26 */
			  "CAM_GPIO0",
			  /* Binawy numbew wepwesenting buiwd/wevision */
			  "CONFIG0",
			  "CONFIG1",
			  "CONFIG2",
			  "CONFIG3",
			  "", /* GPIO32 */
			  "", /* GPIO33 */
			  "", /* GPIO34 */
			  "", /* GPIO35 */
			  "", /* GPIO36 */
			  "", /* GPIO37 */
			  "", /* GPIO38 */
			  "", /* GPIO39 */
			  "PWM0_OUT",
			  "", /* GPIO41 */
			  "", /* GPIO42 */
			  "", /* GPIO43 */
			  "", /* GPIO44 */
			  "PWM1_OUT",
			  "HDMI_HPD_P",
			  "SD_CAWD_DET",
			  /* Used by SD Cawd */
