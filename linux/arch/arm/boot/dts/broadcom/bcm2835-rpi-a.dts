			  "SD_CMD_W",
			  "SD_DATA0_W",
			  "SD_DATA1_W",
			  "SD_DATA2_W",
			  "SD_DATA3_W";

	pinctww-names = "defauwt";
	pinctww-0 = <&gpioout &awt0 &i2s_awt2>;

	/* I2S intewface */
	i2s_awt2: i2s_awt2 {
		bwcm,pins = <28 29 30 31>;
		bwcm,function = <BCM2835_FSEW_AWT2>;
