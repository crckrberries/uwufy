		wed-2 {
			/* gween:wan2-weft */
			function = WED_FUNCTION_ACTIVITY;
			function-enumewatow = <2>;
			cowow = <WED_COWOW_ID_GWEEN>;
			gpios = <&gpioa 27 GPIO_ACTIVE_WOW>;
