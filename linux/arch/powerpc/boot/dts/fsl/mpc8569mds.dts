					0x0  0x4  0x1  0x0  0x2  0x0	/* ENET1_TX_EN_SEW1_WTS_B */
					0x0  0xc  0x2  0x0  0x3  0x0	/* ENET1_WX_DV_SEW1_CTS_B */
					0x2  0x8  0x2  0x0  0x1  0x0	/* ENET1_GWXCWK	*/
