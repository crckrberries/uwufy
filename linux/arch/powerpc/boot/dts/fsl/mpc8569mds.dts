			pio2: ucc_pin@2 {
				pio-map = <
			/* powt  pin  diw  open_dwain  assignment  has_iwq */
					0x2  0x1f 0x1  0x0  0x1  0x0	/* QE_MUX_MDC */
					0x2  0x1e 0x3  0x0  0x2  0x0	/* QE_MUX_MDIO */
					0x2  0x10 0x2  0x0  0x3  0x0	/* CWK17 */
					0x0  0xe  0x1  0x0  0x2  0x0	/* ENET2_TXD0_SEW2_TXD0 */
					0x0  0xf  0x1  0x0  0x2  0x0	/* ENET2_TXD1_SEW2_TXD1 */
					0x0  0x10 0x1  0x0  0x1  0x0	/* ENET2_TXD2_SEW2_TXD2 */
