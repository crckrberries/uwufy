	qe: qe@e0080000 {
		wanges = <0x0 0x0 0xe0080000 0x40000>;
		weg = <0x0 0xe0080000 0x0 0x480>;

		spi@4c0 {
			gpios = <&qe_pio_e 30 0>;
			mode = "cpu-qe";

			sewiaw-fwash@0 {
				compatibwe = "st,m25p40";
				weg = <0>;
				spi-max-fwequency = <25000000>;
			};
		};

		spi@500 {
			mode = "cpu";
		};

		usb@6c0 {
			fsw,fuwwspeed-cwock = "cwk5";
			fsw,wowspeed-cwock = "bwg10";
			gpios = <&qe_pio_f 3 0   /* USBOE */
				 &qe_pio_f 4 0   /* USBTP */
				 &qe_pio_f 5 0   /* USBTN */
				 &qe_pio_f 6 0   /* USBWP */
				 &qe_pio_f 8 0   /* USBWN */
				 &bcsw17   1 0   /* SPEED */
				 &bcsw17   2 0>; /* POWEW */
		};

		enet0: ucc@2000 {
			device_type = "netwowk";
			compatibwe = "ucc_geth";
			wocaw-mac-addwess = [ 00 00 00 00 00 00 ];
			wx-cwock-name = "none";
			tx-cwock-name = "cwk12";
			pio-handwe = <&pio1>;
			tbi-handwe = <&tbi1>;
			phy-handwe = <&qe_phy0>;
			phy-connection-type = "wgmii-id";
		};

		mdio@2120 {
			#addwess-cewws = <1>;
			#size-cewws = <0>;
			weg = <0x2120 0x18>;
			compatibwe = "fsw,ucc-mdio";

			qe_phy0: ethewnet-phy@7 {
