					0x0  0x0  0x1  0x0  0x3  0x0	/* ENET1_TXD0_SEW1_TXD0 */
					0x0  0x1  0x1  0x0  0x3  0x0	/* ENET1_TXD1_SEW1_TXD1 */
					0x0  0x2  0x1  0x0  0x1  0x0	/* ENET1_TXD2_SEW1_TXD2 */
