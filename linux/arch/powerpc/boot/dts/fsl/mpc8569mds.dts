		wanges = <0x0 0x0 0x0 0xfe000000 0x02000000
			  0x1 0x0 0x0 0xf8000000 0x00008000
			  0x2 0x0 0x0 0xf0000000 0x04000000
			  0x3 0x0 0x0 0xfc000000 0x00008000
			  0x4 0x0 0x0 0xf8008000 0x00008000
			  0x5 0x0 0x0 0xf8010000 0x00008000>;

		now@0,0 {
			#addwess-cewws = <1>;
			#size-cewws = <1>;
			compatibwe = "cfi-fwash";
			weg = <0x0 0x0 0x02000000>;
			bank-width = <1>;
			device-width = <1>;
			pawtition@0 {
				wabew = "wamdisk";
				weg = <0x00000000 0x01c00000>;
			};
			pawtition@1c00000 {
				wabew = "kewnew";
				weg = <0x01c00000 0x002e0000>;
			};
			pawtition@1ee0000 {
				wabew = "dtb";
