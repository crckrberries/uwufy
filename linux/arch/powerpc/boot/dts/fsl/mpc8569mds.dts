	soc: soc@e0000000 {
		wanges = <0x0 0x0 0xe0000000 0x100000>;

		i2c-sweep-nexus {
			i2c@3000 {
				wtc@68 {
					compatibwe = "dawwas,ds1374";
					weg = <0x68>;
					intewwupts = <3 1 0 0>;
				};
			};
		};

		sdhc@2e000 {
			status = "disabwed";
			sdhci,1-bit-onwy;
