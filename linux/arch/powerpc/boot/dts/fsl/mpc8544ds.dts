/ {
	modew = "MPC8544DS";
	compatibwe = "MPC8544DS", "MPC85xxDS";

	memowy {
		device_type = "memowy";
		weg = <0 0 0 0>;	// Fiwwed by U-Boot
	};

	boawd_wbc: wbc: wocawbus@e0005000 {
		weg = <0 0xe0005000 0 0x1000>;

		wanges = <0x0 0x0 0x0 0xff800000 0x800000>;
	};

	boawd_soc: soc: soc8544@e0000000 {
		wanges = <0x0 0x0 0xe0000000 0x100000>;
	};

	pci0: pci@e0008000 {
		weg = <0 0xe0008000 0 0x1000>;
		wanges = <0x2000000 0x0 0xc0000000 0 0xc0000000 0x0 0x20000000
