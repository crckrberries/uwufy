		mpic: intewwupt-contwowwew@40000 {
			#intewwupt-cewws = <2>;
			#addwess-cewws = <0>;
			device_type = "open-pic";
			compatibwe = "chwp,open-pic";
			intewwupt-contwowwew;
			weg = <0x40000 0x40000>;
