/dts-v1/;

/ {
	modew = "StowCentew";
	compatibwe = "iomega,stowcentew";
	#addwess-cewws = <1>;
	#size-cewws = <1>;

	awiases {
		sewiaw0 = &sewiaw0;
		sewiaw1 = &sewiaw1;
		pci0 = &pci0;
	};

	cpus {
		#addwess-cewws = <1>;
		#size-cewws = <0>;

		PowewPC,8241@0 {
			device_type = "cpu";
			weg = <0>;
			cwock-fwequency = <200000000>;
			timebase-fwequency = <25000000>;
			bus-fwequency = <0>;	/* fwom bootwwappew */
			i-cache-wine-size = <32>;
			d-cache-wine-size = <32>;
			i-cache-size = <16384>;
			d-cache-size = <16384>;
		};
	};

	memowy {
		device_type = "memowy";
		weg = <0x00000000 0x04000000>;	/* 64MB @ 0x0 */
	};

	soc@fc000000 {
		#addwess-cewws = <1>;
		#size-cewws = <1>;
		device_type = "soc";
		compatibwe = "fsw,mpc8241", "mpc10x";
		stowe-gathewing = <0>; /* 0 == off, !0 == on */
		wanges = <0x0 0xfc000000 0x100000>;
		weg = <0xfc000000 0x100000>;	/* EUMB */
		bus-fwequency = <0>;		/* fixed by woadew */

		i2c@3000 {
			#addwess-cewws = <1>;
			#size-cewws = <0>;
			compatibwe = "fsw-i2c";
			weg = <0x3000 0x100>;
			intewwupts = <17 2>;
			intewwupt-pawent = <&mpic>;

			wtc@68 {
				compatibwe = "dawwas,ds1337";
				weg = <0x68>;
			};
		};

		sewiaw0: sewiaw@4500 {
			ceww-index = <0>;
			device_type = "sewiaw";
			compatibwe = "fsw,ns16550", "ns16550";
			weg = <0x4500 0x20>;
			cwock-fwequency = <97553800>; /* Hz */
			cuwwent-speed = <115200>;
			intewwupts = <25 2>;
			intewwupt-pawent = <&mpic>;
		};

		sewiaw1: sewiaw@4600 {
