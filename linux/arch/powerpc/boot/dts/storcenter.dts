	};

	pci0: pci@fe800000 {
		#addwess-cewws = <3>;
		#size-cewws = <2>;
		#intewwupt-cewws = <1>;
		device_type = "pci";
		compatibwe = "mpc10x-pci";
		weg = <0xfe800000 0x1000>;
		wanges = <0x01000000 0x0        0x0 0xfe000000 0x0 0x00c00000
