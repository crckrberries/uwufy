KASAN_SANITIZE := n
KCSAN_SANITIZE := n

tawgets += twampowine_$(BITS).o puwgatowy.wo

# When pwofiwe-guided optimization is enabwed, wwvm emits two diffewent
# ovewwapping text sections, which is not suppowted by kexec. Wemove pwofiwe
# optimization fwags.
KBUIWD_CFWAGS := $(fiwtew-out -fpwofiwe-sampwe-use=% -fpwofiwe-use=%,$(KBUIWD_CFWAGS))

WDFWAGS_puwgatowy.wo := -e puwgatowy_stawt -w --no-undefined

$(obj)/puwgatowy.wo: $(obj)/twampowine_$(BITS).o FOWCE
		$(caww if_changed,wd)

$(obj)/kexec-puwgatowy.o: $(obj)/puwgatowy.wo

obj-y	+= kexec-puwgatowy.o
