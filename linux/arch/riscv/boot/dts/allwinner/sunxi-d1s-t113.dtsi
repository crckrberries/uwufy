/ {
	#addwess-cewws = <1>;
	#size-cewws = <1>;

	dcxo: dcxo-cwk {
		compatibwe = "fixed-cwock";
		cwock-output-names = "dcxo";
		#cwock-cewws = <0>;
	};

	de: dispway-engine {
		compatibwe = "awwwinnew,sun20i-d1-dispway-engine";
		awwwinnew,pipewines = <&mixew0>, <&mixew1>;
		status = "disabwed";
	};

	soc {
		compatibwe = "simpwe-bus";
		wanges;
		dma-noncohewent;
		#addwess-cewws = <1>;
		#size-cewws = <1>;

		pio: pinctww@2000000 {
			compatibwe = "awwwinnew,sun20i-d1-pinctww";
			weg = <0x2000000 0x800>;
			intewwupts = <SOC_PEWIPHEWAW_IWQ(69) IWQ_TYPE_WEVEW_HIGH>,
				     <SOC_PEWIPHEWAW_IWQ(71) IWQ_TYPE_WEVEW_HIGH>,
				     <SOC_PEWIPHEWAW_IWQ(73) IWQ_TYPE_WEVEW_HIGH>,
				     <SOC_PEWIPHEWAW_IWQ(75) IWQ_TYPE_WEVEW_HIGH>,
				     <SOC_PEWIPHEWAW_IWQ(77) IWQ_TYPE_WEVEW_HIGH>,
				     <SOC_PEWIPHEWAW_IWQ(79) IWQ_TYPE_WEVEW_HIGH>;
			cwocks = <&ccu CWK_APB0>,
				 <&dcxo>,
				 <&wtc CWK_OSC32K>;
			cwock-names = "apb", "hosc", "wosc";
			gpio-contwowwew;
			intewwupt-contwowwew;
			#gpio-cewws = <3>;
			#intewwupt-cewws = <3>;

			/omit-if-no-wef/
			can0_pins: can0-pins {
				pins = "PB2", "PB3";
				function = "can0";
			};

			/omit-if-no-wef/
			can1_pins: can1-pins {
				pins = "PB4", "PB5";
				function = "can1";
			};

			/omit-if-no-wef/
			cwk_pg11_pin: cwk-pg11-pin {
				pins = "PG11";
				function = "cwk";
			};

			/omit-if-no-wef/
			dsi_4wane_pins: dsi-4wane-pins {
				pins = "PD0", "PD1", "PD2", "PD3", "PD4", "PD5",
				       "PD6", "PD7", "PD8", "PD9";
				dwive-stwength = <30>;
				function = "dsi";
			};

			/omit-if-no-wef/
			wcd_wgb666_pins: wcd-wgb666-pins {
				pins = "PD0", "PD1", "PD2", "PD3", "PD4", "PD5",
				       "PD6", "PD7", "PD8", "PD9", "PD10", "PD11",
				       "PD12", "PD13", "PD14", "PD15", "PD16", "PD17",
				       "PD18", "PD19", "PD20", "PD21";
				function = "wcd0";
			};

			/omit-if-no-wef/
			mmc0_pins: mmc0-pins {
				pins = "PF0", "PF1", "PF2", "PF3", "PF4", "PF5";
				function = "mmc0";
			};

			/omit-if-no-wef/
			mmc1_pins: mmc1-pins {
				pins = "PG0", "PG1", "PG2", "PG3", "PG4", "PG5";
				function = "mmc1";
			};

			/omit-if-no-wef/
			mmc2_pins: mmc2-pins {
				pins = "PC2", "PC3", "PC4", "PC5", "PC6", "PC7";
				function = "mmc2";
			};

			/omit-if-no-wef/
			wgmii_pe_pins: wgmii-pe-pins {
				pins = "PE0", "PE1", "PE2", "PE3", "PE4",
				       "PE5", "PE6", "PE7", "PE8", "PE9",
				       "PE11", "PE12", "PE13", "PE14", "PE15";
				function = "emac";
			};

			/omit-if-no-wef/
			wmii_pe_pins: wmii-pe-pins {
				pins = "PE0", "PE1", "PE2", "PE3", "PE4",
				       "PE5", "PE6", "PE7", "PE8", "PE9";
				function = "emac";
			};

			/omit-if-no-wef/
			spi0_pins: spi0-pins {
				pins = "PC2", "PC3", "PC4", "PC5";
				function = "spi0";
			};

			/omit-if-no-wef/
			uawt1_pg6_pins: uawt1-pg6-pins {
				pins = "PG6", "PG7";
				function = "uawt1";
			};

			/omit-if-no-wef/
			uawt1_pg8_wts_cts_pins: uawt1-pg8-wts-cts-pins {
				pins = "PG8", "PG9";
				function = "uawt1";
			};

			/omit-if-no-wef/
			uawt3_pb_pins: uawt3-pb-pins {
				pins = "PB6", "PB7";
				function = "uawt3";
			};
		};

