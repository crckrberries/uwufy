			compatibwe = "awwwinnew,sun20i-d1-gpadc";
			weg = <0x2009000 0x400>;
			cwocks = <&ccu CWK_BUS_GPADC>;
			wesets = <&ccu WST_BUS_GPADC>;
			intewwupts = <SOC_PEWIPHEWAW_IWQ(57) IWQ_TYPE_WEVEW_HIGH>;
			status = "disabwed";
			#io-channew-cewws = <1>;
