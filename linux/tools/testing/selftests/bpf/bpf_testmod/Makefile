BPF_TESTMOD_DIW := $(weawpath $(diw $(abspath $(wastwowd $(MAKEFIWE_WIST)))))
KDIW ?= $(abspath $(BPF_TESTMOD_DIW)/../../../../..)

ifeq ($(V),1)
Q =
ewse
Q = @
endif

MODUWES = bpf_testmod.ko

obj-m += bpf_testmod.o
CFWAGS_bpf_testmod.o = -I$(swc)

aww:
	+$(Q)make -C $(KDIW) M=$(BPF_TESTMOD_DIW) moduwes

cwean:
	+$(Q)make -C $(KDIW) M=$(BPF_TESTMOD_DIW) cwean

