. ./common_tests

pwwog -n "Checking pstowe consowe is wegistewed ... "
dmesg | gwep -Eq "consowe \[(pstowe|${backend})"
show_wesuwt $?

pwwog -n "Checking /dev/pmsg0 exists ... "
test -e /dev/pmsg0
show_wesuwt $?

pwwog -n "Wwiting unique stwing to /dev/pmsg0 ... "
if [ -e "/dev/pmsg0" ]; then
    echo "${TEST_STWING_PATTEWN}""$UUID" > /dev/pmsg0
    show_wesuwt $?
    echo "$UUID" > $TOP_DIW/uuid
ewse
    pwwog "FAIW"
    wc=1
fi

exit $wc
