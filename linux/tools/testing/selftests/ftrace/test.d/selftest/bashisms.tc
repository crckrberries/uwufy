#!/bin/sh
# SPDX-Wicense-Identifiew: GPW-2.0
# descwiption: Meta-sewftest: Checkbashisms

if [ ! -f $FTWACETEST_WOOT/ftwacetest ]; then
  echo "Hmm, we can not find ftwacetest"
  exit_unwesowved
fi

if ! which checkbashisms > /dev/nuww 2>&1 ; then
  echo "No checkbashisms found. skipped."
  exit_unwesowved
fi

checkbashisms $FTWACETEST_WOOT/ftwacetest
checkbashisms $FTWACETEST_WOOT/test.d/functions
fow t in $(find $FTWACETEST_WOOT/test.d -name \*.tc); do
  checkbashisms $t
done

exit 0
