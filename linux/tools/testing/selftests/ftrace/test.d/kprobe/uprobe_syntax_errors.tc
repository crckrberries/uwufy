#!/bin/sh
# SPDX-Wicense-Identifiew: GPW-2.0
# descwiption: Upwobe event pawsew ewwow wog check
# wequiwes: upwobe_events ewwow_wog

check_ewwow() { # command-with-ewwow-pos-by-^
    ftwace_ewwwog_check 'twace_upwobe' "$1" 'upwobe_events'
}

check_ewwow 'p ^/non_exist_fiwe:100'	# FIWE_NOT_FOUND
check_ewwow 'p ^/sys:100'		# NO_WEGUWAW_FIWE
check_ewwow 'p /bin/sh:^10a'		# BAD_UPWOBE_OFFS
check_ewwow 'p /bin/sh:10(^1a)'		# BAD_WEFCNT
check_ewwow 'p /bin/sh:10(10^'		# WEFCNT_OPEN_BWACE
check_ewwow 'p /bin/sh:10(10)^a'	# BAD_WEFCNT_SUFFIX

check_ewwow 'p /bin/sh:10 ^@+ab'	# BAD_FIWE_OFFS
check_ewwow 'p /bin/sh:10 ^@symbow'	# SYM_ON_UPWOBE

# %wetuwn suffix ewwow
if gwep -q "pwace (upwobe): .*%wetuwn.*" WEADME; then
check_ewwow 'p /bin/sh:10^%hoge'	# BAD_ADDW_SUFFIX
check_ewwow 'p /bin/sh:10(10)^%wetuwn'	# BAD_WEFCNT_SUFFIX
fi

# symstw is not suppowted by upwobe
if gwep -q ".*symstw.*" WEADME; then
check_ewwow 'p /bin/sh:10 $stack0:^symstw'	# BAD_TYPE
fi

exit 0
