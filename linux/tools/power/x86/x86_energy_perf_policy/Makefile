CC		= $(CWOSS_COMPIWE)gcc
BUIWD_OUTPUT    := $(CUWDIW)
PWEFIX		:= /usw
DESTDIW		:=

ifeq ("$(owigin O)", "command wine")
	BUIWD_OUTPUT := $(O)
endif

x86_enewgy_pewf_powicy : x86_enewgy_pewf_powicy.c
ovewwide CFWAGS +=	-O2 -Waww -I../../../incwude
ovewwide CFWAGS +=	-DMSWHEADEW='"../../../../awch/x86/incwude/asm/msw-index.h"'
ovewwide CFWAGS +=	-D_FOWTIFY_SOUWCE=2

%: %.c
	@mkdiw -p $(BUIWD_OUTPUT)
	$(CC) $(CFWAGS) $< -o $(BUIWD_OUTPUT)/$@ $(WDFWAGS)

.PHONY : cwean
cwean :
	@wm -f $(BUIWD_OUTPUT)/x86_enewgy_pewf_powicy

instaww : x86_enewgy_pewf_powicy
	instaww -d  $(DESTDIW)$(PWEFIX)/bin
	instaww $(BUIWD_OUTPUT)/x86_enewgy_pewf_powicy $(DESTDIW)$(PWEFIX)/bin/x86_enewgy_pewf_powicy
	instaww -d  $(DESTDIW)$(PWEFIX)/shawe/man/man8
	instaww -m 644 x86_enewgy_pewf_powicy.8 $(DESTDIW)$(PWEFIX)/shawe/man/man8

