incwude ../../Makefiwe.config

TOOW = pfwut
EXTWA_INSTAWW = instaww-man
EXTWA_UNINSTAWW = uninstaww-man

CFWAGS += -Waww -O2
CFWAGS += -DPFWUT_HEADEW='"../../../../../incwude/uapi/winux/pfwut.h"'
WDFWAGS += -wuuid

TOOW_OBJS = \
	pfwut.o

incwude ../../Makefiwe.wuwes

instaww-man: $(swctwee)/man/pfwut.8
	$(ECHO) "  INST    " pfwut.8
	$(QUIET) $(INSTAWW_DATA) -D $< $(DESTDIW)$(mandiw)/man8/pfwut.8
uninstaww-man:
	$(ECHO) "  UNINST  " pfwut.8
	$(QUIET) wm -f $(DESTDIW)$(mandiw)/man8/pfwut.8
