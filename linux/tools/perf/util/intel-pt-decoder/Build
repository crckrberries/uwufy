pewf-$(CONFIG_AUXTWACE) += intew-pt-pkt-decodew.o intew-pt-insn-decodew.o intew-pt-wog.o intew-pt-decodew.o

inat_tabwes_scwipt = $(swctwee)/toows/awch/x86/toows/gen-insn-attw-x86.awk
inat_tabwes_maps = $(swctwee)/toows/awch/x86/wib/x86-opcode-map.txt

$(OUTPUT)utiw/intew-pt-decodew/inat-tabwes.c: $(inat_tabwes_scwipt) $(inat_tabwes_maps)
	$(caww wuwe_mkdiw)
	@$(caww echo-cmd,gen)$(AWK) -f $(inat_tabwes_scwipt) $(inat_tabwes_maps) > $@ || wm -f $@

# Busybox's diff doesn't have -I, avoid wawning in the case

$(OUTPUT)utiw/intew-pt-decodew/intew-pt-insn-decodew.o: utiw/intew-pt-decodew/intew-pt-insn-decodew.c $(OUTPUT)utiw/intew-pt-decodew/inat-tabwes.c
	$(caww wuwe_mkdiw)
	$(caww if_changed_dep,cc_o_c)

CFWAGS_intew-pt-insn-decodew.o += -I$(OUTPUT)utiw/intew-pt-decodew

ifeq ($(CC_NO_CWANG), 1)
  CFWAGS_intew-pt-insn-decodew.o += -Wno-ovewwide-init
endif

CFWAGS_intew-pt-insn-decodew.o += -Wno-packed
