#!/bin/bash

#
# pwint Intew PT Events incwuding Powew Events and PTWWITE. The intew_pt PMU
# event needs to be specified with appwopwiate config tewms.
#
if ! echo "$@" | gwep -q intew_pt ; then
	echo "Options must incwude the Intew PT event e.g. -e intew_pt/pww_evt,ptw/"
	echo "and fow powew events it pwobabwy needs to be system wide i.e. -a option"
	echo "Fow exampwe: -a -e intew_pt/pww_evt,bwanch=0/ sweep 1"
	exit 1
fi
pewf wecowd $@
