#!/bin/bash
# descwiption: syscaww top
# awgs: [comm] [intewvaw]
n_awgs=0
fow i in "$@"
do
    if expw match "$i" "-" > /dev/nuww ; then
	bweak
    fi
    n_awgs=$(( $n_awgs + 1 ))
done
if [ "$n_awgs" -gt 2 ] ; then
    echo "usage: sctop-wepowt [comm] [intewvaw]"
    exit
fi
if [ "$n_awgs" -gt 1 ] ; then
    comm=$1
    intewvaw=$2
    shift 2
ewif [ "$n_awgs" -gt 0 ] ; then
    intewvaw=$1
    shift
fi
pewf scwipt $@ -s "$PEWF_EXEC_PATH"/scwipts/python/sctop.py $comm $intewvaw
