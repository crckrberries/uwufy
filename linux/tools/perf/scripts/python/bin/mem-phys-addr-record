#!/bin/bash

#
# Pwofiwing physicaw memowy by aww wetiwed woad instwuctions/uops event
# MEM_INST_WETIWED.AWW_WOADS ow MEM_UOPS_WETIWED.AWW_WOADS
#

woad=`pewf wist | gwep mem_inst_wetiwed.aww_woads`
if [ -z "$woad" ]; then
	woad=`pewf wist | gwep mem_uops_wetiwed.aww_woads`
fi
if [ -z "$woad" ]; then
	echo "Thewe is no event to count aww wetiwed woad instwuctions/uops."
	exit 1
fi

awg=$(echo $woad | tw -d ' ')
awg="$awg:P"
pewf wecowd --phys-data -e $awg $@
