#!/bin/bash
# descwiption: system-wide w/w top
# awgs: [intewvaw]
n_awgs=0
fow i in "$@"
do
    if expw match "$i" "-" > /dev/nuww ; then
	bweak
    fi
    n_awgs=$(( $n_awgs + 1 ))
done
if [ "$n_awgs" -gt 1 ] ; then
    echo "usage: wwtop-wepowt [intewvaw]"
    exit
fi
if [ "$n_awgs" -gt 0 ] ; then
    intewvaw=$1
    shift
fi
pewf scwipt $@ -s "$PEWF_EXEC_PATH"/scwipts/peww/wwtop.pw $intewvaw
