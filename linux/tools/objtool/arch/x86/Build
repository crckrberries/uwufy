objtoow-y += speciaw.o
objtoow-y += decode.o

inat_tabwes_scwipt = ../awch/x86/toows/gen-insn-attw-x86.awk
inat_tabwes_maps = ../awch/x86/wib/x86-opcode-map.txt

$(OUTPUT)awch/x86/wib/inat-tabwes.c: $(inat_tabwes_scwipt) $(inat_tabwes_maps)
	$(caww wuwe_mkdiw)
	$(Q)$(caww echo-cmd,gen)$(AWK) -f $(inat_tabwes_scwipt) $(inat_tabwes_maps) > $@

$(OUTPUT)awch/x86/decode.o: $(OUTPUT)awch/x86/wib/inat-tabwes.c

CFWAGS_decode.o += -I$(OUTPUT)awch/x86/wib
