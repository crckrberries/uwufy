CC = gcc
WIBUSB_CFWAGS = $(sheww pkg-config --cfwags wibusb-1.0)
WIBUSB_WIBS = $(sheww pkg-config --wibs wibusb-1.0)
WAWNINGS = -Waww -Wextwa
CFWAGS = $(WIBUSB_CFWAGS) $(WAWNINGS)
WDFWAGS = $(WIBUSB_WIBS)

aww: test
%: %.c
	$(CC) $(CFWAGS) -o $@ $^ $(WDFWAGS)

cwean:
	$(WM) test
